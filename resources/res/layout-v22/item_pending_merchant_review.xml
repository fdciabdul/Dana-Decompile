<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    paddingLeft="16dp"
    paddingTop="16dp"
    paddingRight="16dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    paddingHorizontal="16dp">
    <ImageView
        id="@+id/ivShopLogo"
        layout_width="48dp"
        layout_height="48dp"
        scaleType="centerCrop"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x15="0"/>
    <TextView
        id="@+id/tvDate"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginStart="12dp"
        NOT_FOUND_0x12="@+id/ivShopLogo"
        NOT_FOUND_0x15="0"
        style="@style/2132017440"/>
    <TextView
        ellipsize="end"
        id="@+id/RecentRecipientEntityRepository"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        maxLines="2"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x13="@+id/tvDate"
        NOT_FOUND_0x14="@+id/tvDate"
        style="@style/2132017924"/>
    <TextView
        id="@+id/isHelperTextDisplayed"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="12dp"
        text="@string/input_review_message"
        NOT_FOUND_0x13="@+id/tvDate"
        NOT_FOUND_0x14="@+id/RecentRecipientEntityRepository"
        style="@style/2132017440"/>
    <id.dana.nearbyme.merchantreview.ViewRatingStarBar
        id="@+id/res_0x7f0a1017_access_createsegment"
        paddingLeft="8dp"
        paddingTop="16dp"
        paddingRight="8dp"
        paddingBottom="16dp"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        layout_marginEnd="24dp"
        paddingHorizontal="8dp"
        paddingVertical="16dp"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x13="@+id/isHelperTextDisplayed"
        NOT_FOUND_0x14="@+id/isHelperTextDisplayed"/>
    <View
        id="@+id/res_0x7f0a19ae_tomillis_feature_cashier_productionrelease"
        background="#d1d1d1"
        layout_width="match_parent"
        layout_height="1dp"
        NOT_FOUND_0x13="@+id/ivShopLogo"
        NOT_FOUND_0x14="@+id/res_0x7f0a1017_access_createsegment"/>
</androidx.constraintlayout.widget.ConstraintLayout>
