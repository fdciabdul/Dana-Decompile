<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    paddingTop="16dp"
    paddingBottom="16dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    paddingVertical="16dp">
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/ProductFlavor"
        layout_width="100dp"
        layout_height="100dp"
        NOT_FOUND_0xa="0"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0x10="@drawable/ic_maintenance_page"/>
    <androidx.appcompat.widget.AppCompatTextView
        textColor="#313131"
        id="@+id/tvErrorTitle"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="12dp"
        text="@string/promo_discovery_error_general_title"
        NOT_FOUND_0xa="0"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xc="@+id/ProductFlavor"
        style="@style/2132017932"/>
    <androidx.appcompat.widget.AppCompatTextView
        textColor="#525252"
        id="@+id/MerchantReviewFormAnalyticTracker"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        text="@string/promo_discovery_error_general_description"
        textAlignment="center"
        NOT_FOUND_0xa="0"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xc="@+id/tvErrorTitle"
        style="@style/2132017405"/>
    <id.dana.component.buttoncomponent.DanaButtonPrimaryView
        id="@+id/btnError"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="12dp"
        NOT_FOUND_0xa="0"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xc="@+id/MerchantReviewFormAnalyticTracker"
        NOT_FOUND_0xe="@string/try_again"
        NOT_FOUND_0xf="1"/>
</androidx.constraintlayout.widget.ConstraintLayout>
