<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="@drawable/bg_card_rounded_10dp"
    paddingTop="16dp"
    paddingBottom="16dp"
    layout_width="match_parent"
    layout_height="wrap_content">
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/viewIvHeaderSkeleton"
        background="@drawable/bg_circle_grey_20"
        layout_width="32dp"
        layout_height="32dp"
        layout_marginStart="16dp"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x11="0"/>
    <View
        id="@+id/viewTvHeaderSkeleton"
        background="@drawable/square_rounded_skeleton"
        layout_width="0dp"
        layout_height="18dp"
        layout_marginStart="8dp"
        layout_marginEnd="16dp"
        NOT_FOUND_0xc="@+id/viewIvHeaderSkeleton"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0xe="@+id/viewIvHeaderSkeleton"
        NOT_FOUND_0x11="@+id/viewIvHeaderSkeleton"/>
    <View
        id="@+id/viewSeparator"
        background="#e3e3e3"
        layout_width="match_parent"
        layout_height="1dp"
        layout_marginTop="12dp"
        NOT_FOUND_0x10="@+id/viewIvHeaderSkeleton"/>
    <View
        id="@+id/viewSelectedCountrySkeleton"
        background="@drawable/square_rounded_skeleton"
        layout_width="0dp"
        layout_height="56dp"
        layout_marginLeft="16dp"
        layout_marginTop="16dp"
        layout_marginRight="16dp"
        layout_marginHorizontal="@dimen/BuiltInFictitiousFunctionClassFactory_res_0x7f070055"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x11="@+id/viewSeparator"/>
    <include
        id="@+id/viewEditText1"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_marginTop="16dp"
        layout_marginRight="16dp"
        layout_marginHorizontal="@dimen/BuiltInFictitiousFunctionClassFactory_res_0x7f070055"
        NOT_FOUND_0x10="@+id/viewSelectedCountrySkeleton"
        layout="@layout/view_recipient_form_edit_text_skeleton"/>
    <include
        id="@+id/viewEditText2"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_marginTop="16dp"
        layout_marginRight="16dp"
        layout_marginHorizontal="@dimen/BuiltInFictitiousFunctionClassFactory_res_0x7f070055"
        NOT_FOUND_0x10="@+id/viewEditText1"
        layout="@layout/view_recipient_form_edit_text_skeleton"/>
    <include
        id="@+id/viewEditText3"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_marginTop="16dp"
        layout_marginRight="16dp"
        layout_marginHorizontal="@dimen/BuiltInFictitiousFunctionClassFactory_res_0x7f070055"
        NOT_FOUND_0x10="@+id/viewEditText2"
        layout="@layout/view_recipient_form_edit_text_skeleton"/>
    <include
        id="@+id/viewEditText4"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_marginTop="16dp"
        layout_marginRight="16dp"
        layout_marginHorizontal="@dimen/BuiltInFictitiousFunctionClassFactory_res_0x7f070055"
        NOT_FOUND_0x10="@+id/viewEditText3"
        layout="@layout/view_recipient_form_edit_text_skeleton"/>
    <include
        id="@+id/viewEditText5"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_marginTop="16dp"
        layout_marginRight="16dp"
        layout_marginHorizontal="@dimen/BuiltInFictitiousFunctionClassFactory_res_0x7f070055"
        NOT_FOUND_0x10="@+id/viewEditText4"
        layout="@layout/view_recipient_form_edit_text_skeleton"/>
    <include
        id="@+id/viewEditText6"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_marginTop="16dp"
        layout_marginRight="16dp"
        layout_marginHorizontal="@dimen/BuiltInFictitiousFunctionClassFactory_res_0x7f070055"
        NOT_FOUND_0x10="@+id/viewEditText5"
        layout="@layout/view_recipient_form_edit_text_skeleton"/>
</androidx.constraintlayout.widget.ConstraintLayout>
