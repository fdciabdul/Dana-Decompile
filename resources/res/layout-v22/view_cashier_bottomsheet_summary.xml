<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_gravity="bottom"
    orientation="vertical"
    background="#ffffff"
    layout_width="match_parent"
    layout_height="wrap_content">
    <View
        background="#d1d1d1"
        layout_width="match_parent"
        layout_height="0.5dp"
        layout_marginBottom="8dp"/>
    <LinearLayout
        layout_gravity="center_horizontal"
        orientation="horizontal"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_marginRight="16dp"
        layout_marginHorizontal="16dp">
        <ImageView
            src="@drawable/ic_info_filled"
            style="@style/2132018187"/>
        <TextView
            layout_gravity="center_vertical"
            layout_width="wrap_content"
            layout_height="wrap_content"
            text="@string/send_money_summary_bottom_sheet_description"
            textAlignment="center"
            layout_marginStart="8dp"
            style="@style/2132017439"/>
    </LinearLayout>
    <id.dana.component.buttoncomponent.DanaButtonPrimaryView
        id="@+id/btnNextCashierNative"
        layout_width="match_parent"
        layout_height="44dp"
        layout_marginLeft="16dp"
        layout_marginTop="8dp"
        layout_marginRight="16dp"
        layout_marginBottom="16dp"
        layout_marginHorizontal="16dp"
        NOT_FOUND_0xf="@string/sendmoney_summary_continue"
        NOT_FOUND_0x10="0"/>
</LinearLayout>
