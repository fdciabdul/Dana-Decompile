<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_gravity="center_horizontal"
    layout_width="wrap_content"
    layout_height="wrap_content">
    <id.dana.component.buttoncomponent.DanaButtonSecondaryView
        id="@+id/btn_resend"
        layout_width="wrap_content"
        layout_height="wrap_content"
        translationZ="1dp"
        NOT_FOUND_0x12="@+id/ll_countdown"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x16="resend"/>
    <androidx.appcompat.widget.LinearLayoutCompat
        orientation="horizontal"
        id="@+id/ll_countdown"
        background="@drawable/rounded_right_background"
        layout_width="wrap_content"
        layout_height="wrap_content"
        translationX="-3dp"
        NOT_FOUND_0x10="@+id/btn_resend"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x13="@+id/btn_resend"
        NOT_FOUND_0x15="@+id/btn_resend">
        <androidx.appcompat.widget.AppCompatTextView
            gravity="center"
            paddingTop="13dp"
            paddingBottom="13dp"
            layout_width="wrap_content"
            layout_height="wrap_content"
            text="00:09"
            layout_marginStart="10dp"
            paddingVertical="13dp"
            style="@style/2132017894"/>
        <ProgressBar
            layout_gravity="center"
            layout_width="12dp"
            layout_height="12dp"
            layout_marginStart="6dp"
            layout_marginEnd="10dp"
            indeterminateTint="#ff8c04"/>
    </androidx.appcompat.widget.LinearLayoutCompat>
</androidx.constraintlayout.widget.ConstraintLayout>
