<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/container"
    layout_width="match_parent"
    layout_height="match_parent">
    <include
        id="@+id/action_bar"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x15="0"
        layout="@layout/layout_toolbar"/>
    <com.airbnb.lottie.LottieAnimationView
        id="@+id/lottie_icon"
        layout_width="200dp"
        layout_height="200dp"
        NOT_FOUND_0x11="@+id/tv_title"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x14="@+id/action_bar"
        NOT_FOUND_0x16="2"
        NOT_FOUND_0x17="true"
        NOT_FOUND_0x18="json/lottie/transfer_account_pending.lottie"
        NOT_FOUND_0x19="true"/>
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/iv_icon"
        visibility="gone"
        layout_width="200dp"
        layout_height="200dp"
        NOT_FOUND_0x11="@+id/tv_title"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x14="@+id/action_bar"
        NOT_FOUND_0x16="2"
        NOT_FOUND_0x1c="@drawable/ic_failed_transfer_account"/>
    <androidx.appcompat.widget.AppCompatTextView
        textSize="20sp"
        textColor="#313131"
        id="@+id/tv_title"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="12dp"
        text="@string/transfer_account_status_title_pending"
        fontFamily="@font/2131296266"
        NOT_FOUND_0x11="@+id/tv_subtitle"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x14="@+id/lottie_icon"/>
    <androidx.appcompat.widget.AppCompatTextView
        textSize="16sp"
        textColor="#727272"
        gravity="center"
        id="@+id/tv_subtitle"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginLeft="24dp"
        layout_marginTop="8dp"
        layout_marginRight="24dp"
        text="@string/transfer_account_status_subtitle_pending"
        layout_marginHorizontal="24dp"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x14="@+id/tv_title"/>
    <id.dana.component.buttoncomponent.DanaButtonPrimaryView
        id="@+id/btn_call_cs"
        visibility="gone"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginBottom="50dp"
        layout_marginStart="16dp"
        layout_marginEnd="16dp"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x1a="@string/transfer_account_status_button_failed"
        NOT_FOUND_0x1b="1"/>
</androidx.constraintlayout.widget.ConstraintLayout>
