<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="#ffffff"
    layout_width="match_parent"
    layout_height="match_parent">
    <include
        id="@+id/viewToolbarCardBinding"
        layout="@layout/layout_toolbar"/>
    <ScrollView
        layout_width="match_parent"
        layout_height="0dp"
        NOT_FOUND_0xd="@+id/clStickySection"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="@+id/viewToolbarCardBinding">
        <androidx.constraintlayout.widget.ConstraintLayout
            layout_width="match_parent"
            layout_height="wrap_content">
            <androidx.appcompat.widget.AppCompatImageView
                id="@+id/ivBackground"
                background="#108ee9"
                layout_width="match_parent"
                layout_height="98dp"
                NOT_FOUND_0xe="0"
                NOT_FOUND_0x11="0"
                NOT_FOUND_0x13="0"/>
            <include
                id="@+id/viewCardBinding"
                layout_width="match_parent"
                layout_height="0dp"
                NOT_FOUND_0x13="0"
                layout="@layout/view_card_binding"/>
            <id.dana.cardbinding.view.DisclaimerView
                id="@+id/GetPersonalizationOptions"
                paddingBottom="16dp"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginTop="16dp"
                layout_marginBottom="16dp"
                layout_marginVertical="@dimen/BuiltInFictitiousFunctionClassFactory_res_0x7f070055"
                NOT_FOUND_0xe="0"
                NOT_FOUND_0x11="0"
                NOT_FOUND_0x12="@+id/viewCardBinding"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
    <androidx.constraintlayout.widget.ConstraintLayout
        id="@+id/clStickySection"
        background="#ffffff"
        layout_width="match_parent"
        layout_height="wrap_content"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x11="0">
        <id.dana.cardbinding.view.DisclaimerView
            id="@+id/oneklikDisclaimerView"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="16dp"
            layout_marginBottom="16dp"
            layout_marginVertical="@dimen/BuiltInFictitiousFunctionClassFactory_res_0x7f070055"
            NOT_FOUND_0xd="@+id/viewDivider"
            NOT_FOUND_0xe="0"
            NOT_FOUND_0x11="0"/>
        <id.dana.cardbinding.view.SupportedCardsView
            id="@+id/supportedCardsView"
            background="@drawable/bg_rounded_grey"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginLeft="16dp"
            layout_marginRight="16dp"
            layout_marginBottom="16dp"
            layout_marginHorizontal="@dimen/BuiltInFictitiousFunctionClassFactory_res_0x7f070055"
            NOT_FOUND_0xd="@+id/viewDivider"
            NOT_FOUND_0xe="0"
            NOT_FOUND_0x11="0"/>
        <View
            id="@+id/viewDivider"
            background="#d1d1d1"
            layout_width="wrap_content"
            layout_height="1dp"
            layout_marginBottom="8.5dp"
            NOT_FOUND_0xd="@+id/res_0x7f0a0140_verifypinstatemanager_executeriskchallenge_2_1"
            NOT_FOUND_0xf="0"
            NOT_FOUND_0x10="0"/>
        <id.dana.component.buttoncomponent.DanaButtonPrimaryView
            id="@+id/res_0x7f0a0140_verifypinstatemanager_executeriskchallenge_2_1"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginLeft="16dp"
            layout_marginTop="8dp"
            layout_marginRight="16dp"
            layout_marginBottom="16dp"
            layout_marginHorizontal="@dimen/BuiltInFictitiousFunctionClassFactory_res_0x7f070055"
            NOT_FOUND_0xc="0"
            NOT_FOUND_0xe="0"
            NOT_FOUND_0x11="0"
            NOT_FOUND_0x14="@string/card_binding_add_new_card_button"
            NOT_FOUND_0x15="0"
            NOT_FOUND_0x16="@string/cardbind_btn_add_card"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <include
        id="@+id/viewOnBoarding"
        visibility="gone"
        layout_width="match_parent"
        layout_height="0dp"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0x12="@+id/viewToolbarCardBinding"
        layout="@layout/activity_onboarding_card_binding"/>
</androidx.constraintlayout.widget.ConstraintLayout>
