<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/cl_my_bills_monthly_container"
    paddingTop="8dp"
    layout_width="match_parent"
    layout_height="wrap_content">
    <TextView
        id="@+id/tv_my_bills_monthly_count"
        layout_width="0dp"
        layout_height="wrap_content"
        text="@string/my_bills_monthly_count"
        layout_marginStart="16dp"
        layout_marginEnd="16dp"
        NOT_FOUND_0xf="@+id/tv_view_all"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x13="0"
        style="@style/2132017433"/>
    <id.dana.richview.CurrencyTextView
        id="@+id/tv_my_bills_monthly_total_amount"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        layout_marginEnd="16dp"
        NOT_FOUND_0xf="@+id/tv_view_all"
        NOT_FOUND_0x11="@+id/tv_my_bills_monthly_count"
        NOT_FOUND_0x12="@+id/tv_my_bills_monthly_count"
        style="@style/2132017924"/>
    <TextView
        id="@+id/tv_view_all"
        layout_width="0dp"
        layout_height="wrap_content"
        text="@string/view_all"
        layout_marginEnd="16dp"
        NOT_FOUND_0xd="@+id/tv_my_bills_monthly_total_amount"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x13="@+id/tv_my_bills_monthly_count"
        style="@style/2132017919"/>
    <View
        id="@+id/divider_line"
        background="#d1d1d1"
        layout_width="match_parent"
        layout_height="0.5dp"
        layout_marginTop="8dp"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x12="@+id/tv_my_bills_monthly_total_amount"/>
    <androidx.recyclerview.widget.RecyclerView
        id="@+id/rv_my_bills_monthly"
        layout_width="match_parent"
        layout_height="wrap_content"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="@+id/divider_line"/>
    <View
        id="@+id/divider_bottom_line"
        background="#d1d1d1"
        layout_width="match_parent"
        layout_height="0.5dp"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x12="@+id/rv_my_bills_monthly"/>
    <id.dana.component.buttoncomponent.DanaButtonPrimaryView
        id="@+id/btn_my_bills_add_bill"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_marginTop="16dp"
        layout_marginRight="16dp"
        layout_marginBottom="16dp"
        layout_marginHorizontal="@dimen/BuiltInFictitiousFunctionClassFactory_res_0x7f070055"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="@+id/divider_bottom_line"
        NOT_FOUND_0x14="10"
        NOT_FOUND_0x15="ADD NEW BILL"
        NOT_FOUND_0x16="1"/>
</androidx.constraintlayout.widget.ConstraintLayout>
