<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.card.MaterialCardView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_width="match_parent"
    layout_height="wrap_content"
    NOT_FOUND_0xc="@dimen/2131166055"
    NOT_FOUND_0xd="0dp">
    <FrameLayout
        background="@drawable/bg_card_send_money"
        layout_width="match_parent"
        layout_height="match_parent"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        padding="20dp"
        layout_width="match_parent"
        layout_height="wrap_content">
        <androidx.appcompat.widget.AppCompatTextView
            id="@+id/tvBannerTitle"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginLeft="24dp"
            layout_marginRight="24dp"
            text="@string/sendmoney_to_link_banner_title"
            textAlignment="center"
            layout_marginHorizontal="24dp"
            NOT_FOUND_0xf="@+id/tvBannerDesc"
            NOT_FOUND_0x10="0"
            NOT_FOUND_0x11="0"
            NOT_FOUND_0x13="0"
            style="@style/2132017924"/>
        <androidx.appcompat.widget.AppCompatTextView
            id="@+id/tvBannerDesc"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginLeft="24dp"
            layout_marginTop="12dp"
            layout_marginRight="24dp"
            text="@string/sendmoney_to_link_banner_desc"
            textAlignment="center"
            layout_marginHorizontal="24dp"
            NOT_FOUND_0x10="0"
            NOT_FOUND_0x11="0"
            NOT_FOUND_0x12="@+id/tvBannerTitle"
            style="@style/2132017405"/>
        <androidx.appcompat.widget.AppCompatImageView
            id="@+id/ivBannerIllustration"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="16dp"
            adjustViewBounds="true"
            NOT_FOUND_0xe="0"
            NOT_FOUND_0x10="0"
            NOT_FOUND_0x11="0"
            NOT_FOUND_0x12="@+id/tvBannerDesc"
            NOT_FOUND_0x14="@drawable/bg_illustration_send_to_chat_banner"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</com.google.android.material.card.MaterialCardView>
