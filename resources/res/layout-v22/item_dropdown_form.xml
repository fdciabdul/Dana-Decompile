<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    orientation="vertical"
    id="@+id/llParent"
    layout_width="match_parent"
    layout_height="wrap_content">
    <TextView
        textColor="#727272"
        id="@+id/tvTitle"
        layout_width="match_parent"
        layout_height="wrap_content"
        style="@style/2132017894"/>
    <LinearLayout
        orientation="horizontal"
        id="@+id/checkParameterIsNotNull_res_0x7f0a0d49"
        background="@drawable/bg_card_rounded_stroke_grey30_4dp"
        paddingLeft="16dp"
        paddingTop="10dp"
        paddingRight="16dp"
        paddingBottom="10dp"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        paddingHorizontal="@dimen/BuiltInFictitiousFunctionClassFactory_res_0x7f070055"
        paddingVertical="@dimen/getAuthRequestContext_res_0x7f070052">
        <de.hdodenhof.circleimageview.CircleImageView
            layout_gravity="center"
            id="@+id/ivIcon"
            visibility="gone"
            layout_width="20dp"
            layout_height="20dp"
            layout_marginEnd="8dp"/>
        <TextView
            textColor="#313131"
            layout_gravity="center"
            id="@+id/tvSelectedValue"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_weight="7"
            style="@style/2132017407"/>
        <ImageView
            layout_gravity="center"
            layout_width="20dp"
            layout_height="20dp"
            src="@drawable/ic_chevron_down_grey50"
            layout_weight="1"/>
    </LinearLayout>
    <TextView
        id="@+id/resetSizeCounter"
        visibility="gone"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        drawableEnd="@drawable/ic_warning_red"
        NOT_FOUND_0x18="0"
        NOT_FOUND_0x1a="0"
        NOT_FOUND_0x1b="@+id/etForm"
        style="@style/2132017435"/>
    <androidx.cardview.widget.CardView
        id="@+id/cvInfo"
        visibility="gone"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="10dp"
        NOT_FOUND_0x15="6dp"
        NOT_FOUND_0x16="0dp">
        <androidx.constraintlayout.widget.ConstraintLayout
            id="@+id/getSupportButtonTintMode_res_0x7f0a0331"
            background="#e9f5fe"
            paddingLeft="12dp"
            paddingTop="8dp"
            paddingRight="12dp"
            paddingBottom="8dp"
            layout_width="match_parent"
            layout_height="wrap_content"
            paddingHorizontal="@dimen/2131165267"
            paddingVertical="@dimen/MyBillsEntityDataFactory_res_0x7f07005f">
            <androidx.appcompat.widget.AppCompatImageView
                id="@+id/ivInfo"
                layout_width="14dp"
                layout_height="14dp"
                NOT_FOUND_0x17="0"
                NOT_FOUND_0x1a="0"
                NOT_FOUND_0x1c="0"
                NOT_FOUND_0x1d="@drawable/ic_info_blue"/>
            <TextView
                layout_width="0dp"
                layout_height="wrap_content"
                text="Biaya Transaksi: Rp50.000"
                layout_marginStart="8dp"
                NOT_FOUND_0x17="@+id/ivInfo"
                NOT_FOUND_0x18="0"
                NOT_FOUND_0x19="@+id/ivInfo"
                NOT_FOUND_0x1c="@+id/ivInfo"
                style="@style/2132017433"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>
</LinearLayout>
