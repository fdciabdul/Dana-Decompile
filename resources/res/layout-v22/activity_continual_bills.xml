<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="#f5f5f5"
    style="@style/2132017531">
    <include
        id="@+id/SecurityGuardProfileProvider"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x13="0"
        layout="@layout/layout_toolbar"/>
    <androidx.core.widget.NestedScrollView
        layout_width="match_parent"
        layout_height="0dp"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x12="@+id/SecurityGuardProfileProvider">
        <androidx.appcompat.widget.LinearLayoutCompat
            orientation="vertical"
            layout_width="match_parent"
            layout_height="wrap_content">
            <androidx.constraintlayout.widget.ConstraintLayout
                background="#108ee9"
                layout_width="match_parent"
                layout_height="wrap_content">
                <TextView
                    textSize="16sp"
                    textStyle="bold"
                    textColor="#ffffff"
                    id="@+id/getQualifiedNames"
                    layout_width="wrap_content"
                    layout_height="wrap_content"
                    layout_margin="16dp"
                    NOT_FOUND_0x11="0"
                    NOT_FOUND_0x13="0"/>
                <TextView
                    textSize="16sp"
                    textColor="#ffffff"
                    id="@+id/tv_date_range"
                    layout_width="wrap_content"
                    layout_height="wrap_content"
                    layout_marginTop="4dp"
                    layout_marginStart="16dp"
                    NOT_FOUND_0x11="0"
                    NOT_FOUND_0x12="@+id/getQualifiedNames"/>
                <id.dana.mybills.view.TotalEstimatedBillsView
                    id="@+id/totalEstimatedBillView"
                    layout_width="match_parent"
                    layout_height="wrap_content"
                    layout_margin="16dp"
                    NOT_FOUND_0xf="0"
                    NOT_FOUND_0x10="0"
                    NOT_FOUND_0x11="0"
                    NOT_FOUND_0x12="@+id/tv_date_range"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.recyclerview.widget.RecyclerView
                id="@+id/rvBillPaymentStatus"
                scrollbars="none"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginLeft="16dp"
                layout_marginRight="16dp"
                layout_marginHorizontal="16dp"/>
        </androidx.appcompat.widget.LinearLayoutCompat>
    </androidx.core.widget.NestedScrollView>
    <id.dana.mybills.view.FooterMonthlyView
        id="@+id/footerMyBillsView"
        layout_width="match_parent"
        layout_height="wrap_content"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="0"/>
</androidx.constraintlayout.widget.ConstraintLayout>
