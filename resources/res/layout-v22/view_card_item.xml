<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/cl_root"
    background="@drawable/bg_rounded_white_8dp"
    paddingLeft="16dp"
    paddingTop="8dp"
    paddingRight="16dp"
    paddingBottom="8dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    paddingHorizontal="16dp"
    paddingVertical="8dp">
    <androidx.appcompat.widget.AppCompatTextView
        ellipsize="end"
        id="@+id/tv_title"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        maxLines="1"
        layout_marginStart="8dp"
        layout_marginEnd="8dp"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x18="@+id/res_0x7f0a0a9f_otpregistrationpresenter_input_1"
        NOT_FOUND_0x1b="0"
        style="@style/2132017908"/>
    <androidx.appcompat.widget.AppCompatTextView
        ellipsize="end"
        id="@+id/tv_description"
        layout_width="0dp"
        layout_height="wrap_content"
        maxLines="1"
        layout_marginStart="8dp"
        layout_marginEnd="8dp"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x18="@+id/res_0x7f0a0a9f_otpregistrationpresenter_input_1"
        NOT_FOUND_0x1a="@+id/tv_title"
        style="@style/2132017439"/>
    <androidx.appcompat.widget.AppCompatTextView
        textColor="#00ce64"
        gravity="center_vertical"
        id="@+id/tv_status"
        visibility="gone"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        text="@string/cards_completed"
        drawablePadding="5dp"
        layout_marginStart="8dp"
        layout_marginEnd="8dp"
        NOT_FOUND_0x15="@drawable/ic_cards_success"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x18="@+id/res_0x7f0a0a9f_otpregistrationpresenter_input_1"
        NOT_FOUND_0x1a="@+id/tv_description"
        style="@style/2132017894"/>
    <androidx.appcompat.widget.AppCompatTextView
        id="@+id/tv_remaining_time"
        visibility="gone"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        layout_marginStart="8dp"
        layout_marginEnd="8dp"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x18="@+id/res_0x7f0a0a9f_otpregistrationpresenter_input_1"
        NOT_FOUND_0x1a="@+id/tv_status"
        style="@style/2132017440"/>
    <com.google.android.material.imageview.ShapeableImageView
        id="@+id/res_0x7f0a0a9f_otpregistrationpresenter_input_1"
        background="#badffa"
        layout_width="60dp"
        layout_height="60dp"
        adjustViewBounds="true"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x19="0"
        NOT_FOUND_0x1b="0"
        NOT_FOUND_0x1c="@style/2132017586"/>
</androidx.constraintlayout.widget.ConstraintLayout>
