<?xml version="1.0" encoding="utf-8"?>
<inset xmlns:android="http://schemas.android.com/apk/res/android"
    insetLeft="4dp"
    insetRight="4dp"
    insetTop="10dp"
    insetBottom="7dp">
    <selector>
        <item state_enabled="false">
            <nine-patch
                src="@drawable/abc_textfield_default_mtrl_alpha"
                tint="?attr/2130968960"
                alpha="?android:attr/disabledAlpha"/>
        </item>
        <item
            state_focused="false"
            state_pressed="false">
            <nine-patch
                src="@drawable/abc_textfield_default_mtrl_alpha"
                tint="?attr/2130968960"/>
        </item>
        <item>
            <nine-patch
                src="@drawable/abc_textfield_activated_mtrl_alpha"
                tint="?attr/2130968958"/>
        </item>
    </selector>
</inset>
