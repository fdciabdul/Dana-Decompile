<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" color="#108ee9">
    <item id="@android:id/mask">
        <shape shape="rectangle">
            <corners radius="12dp"/>
            <solid color="#e3e3e3"/>
            <stroke
                width="1dp"
                color="#e3e3e3"/>
        </shape>
    </item>
    <item id="@android:id/background">
        <shape shape="rectangle">
            <corners radius="12dp"/>
            <solid color="#14000000"/>
            <stroke
                width="1dp"
                color="#14000000"/>
        </shape>
    </item>
</ripple>
