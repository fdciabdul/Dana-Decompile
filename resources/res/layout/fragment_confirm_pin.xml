<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" style="@style/2132017551">
    <TextView
        gravity="center"
        id="@+id/header_title"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_marginTop="32dp"
        layout_marginRight="16dp"
        text="@string/confirm_pin_title"
        layout_marginStart="16dp"
        layout_marginEnd="16dp"
        layout_alignParentStart="true"
        layout_alignParentEnd="true"
        style="@style/2132017915"/>
    <TextView
        gravity="center"
        id="@+id/header_description"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_marginTop="8dp"
        layout_marginRight="16dp"
        text="@string/confirm_pin_description"
        layout_below="@+id/header_title"
        layout_marginStart="16dp"
        layout_marginEnd="16dp"
        layout_alignParentStart="true"
        layout_alignParentEnd="true"
        style="@style/2132017404"/>
    <id.dana.core.ui.richview.PinEntryEditText
        textSize="14sp"
        textStyle="bold"
        textColor="#313131"
        id="@+id/res_0x7f0a0905_networkuserentitydata_externalsyntheticlambda8"
        paddingTop="30dp"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginLeft="15dp"
        layout_marginTop="46dp"
        layout_marginRight="15dp"
        singleLine="true"
        android:maxLength="6"
        digits="1234567890"
        layout_below="@+id/header_description"
        layout_centerHorizontal="true"
        inputType="textVisiblePassword|number"
        imeOptions="actionDone"
        textIsSelectable="false"
        layout_marginStart="15dp"
        layout_marginEnd="15dp"
        NOT_FOUND_0x22="@drawable/bg_pin"
        NOT_FOUND_0x23="4dp"
        NOT_FOUND_0x24="@android:color/transparent"
        NOT_FOUND_0x25="16dp"/>
    <androidx.appcompat.widget.AppCompatTextView
        layout_gravity="center"
        id="@+id/res_0x7f0a0208_networkuserentitydata_externalsyntheticlambda0"
        background="@drawable/bg_button_white_border_rounded"
        paddingTop="4dp"
        paddingBottom="4dp"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="26dp"
        text="@string/show_pin"
        layout_below="@+id/res_0x7f0a0905_networkuserentitydata_externalsyntheticlambda8"
        layout_centerHorizontal="true"
        contentDescription="@string/btn_pin_masking"
        textAllCaps="true"
        paddingStart="12dp"
        paddingEnd="12dp"
        style="@style/2132017899"/>
    <ProgressBar
        id="@+id/progress_bar"
        visibility="gone"
        indeterminate="true"
        layout_centerInParent="true"
        style="@style/2132018187"/>
</RelativeLayout>
