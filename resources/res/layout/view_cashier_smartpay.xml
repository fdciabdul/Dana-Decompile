<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/item_add_new_card"
    layout_width="match_parent"
    layout_height="wrap_content">
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/ivIconAsset"
        layout_width="28dp"
        layout_height="28dp"
        layout_marginTop="20dp"
        layout_marginBottom="20dp"
        layout_marginStart="56dp"
        NOT_FOUND_0xb="@+id/isHelperTextDisplayed"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x12="@+id/tvTitle"
        NOT_FOUND_0x14="@drawable/ic_chip_smart_pay"/>
    <androidx.appcompat.widget.AppCompatTextView
        id="@+id/tvTitle"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="10dp"
        text="@string/smart_pay"
        contentDescription="@string/lbl_smart_pay"
        layout_marginStart="8dp"
        NOT_FOUND_0xc="@+id/isHelperTextDisplayed"
        NOT_FOUND_0xe="@+id/ivInfo"
        NOT_FOUND_0xf="@+id/ivIconAsset"
        NOT_FOUND_0x12="0"
        style="@style/2132017413"/>
    <androidx.appcompat.widget.AppCompatTextView
        textColor="#727272"
        id="@+id/isHelperTextDisplayed"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        text="@string/smart_pay_description"
        layout_marginStart="8dp"
        NOT_FOUND_0xe="@+id/ivInfo"
        NOT_FOUND_0xf="@+id/ivIconAsset"
        NOT_FOUND_0x11="@+id/tvTitle"
        style="@style/2132017440"/>
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/ivInfo"
        layout_width="wrap_content"
        layout_height="wrap_content"
        contentDescription="@string/icon_info_smartpay"
        layout_marginEnd="16dp"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x13="0.488"
        NOT_FOUND_0x14="@drawable/ic_info_blue"/>
    <View
        background="#b8b8b8"
        layout_width="match_parent"
        layout_height="0.5dp"
        layout_marginTop="10dp"
        layout_marginStart="16dp"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="@+id/isHelperTextDisplayed"/>
</androidx.constraintlayout.widget.ConstraintLayout>
