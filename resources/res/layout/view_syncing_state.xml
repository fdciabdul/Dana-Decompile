<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/cl_activate_state"
    background="#f5f5f5"
    padding="26dp"
    layout_width="match_parent"
    layout_height="match_parent">
    <androidx.constraintlayout.widget.ConstraintLayout
        layout_width="wrap_content"
        layout_height="wrap_content"
        NOT_FOUND_0xa="0"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xe="0">
        <androidx.appcompat.widget.AppCompatImageView
            id="@+id/iv_activate"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginBottom="125dp"
            NOT_FOUND_0xa="0"
            NOT_FOUND_0xb="0"
            NOT_FOUND_0xc="0"
            NOT_FOUND_0xe="0"
            NOT_FOUND_0xf="@drawable/ic_activatefeed"/>
        <TextView
            textColor="#000000"
            id="@+id/tv_activate_title"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="16dp"
            text="@string/social_feeds_syncing_state_title"
            NOT_FOUND_0xb="0"
            NOT_FOUND_0xc="0"
            NOT_FOUND_0xd="@+id/iv_activate"
            style="@style/2132017932"/>
        <TextView
            id="@+id/tv_activate_desc"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="8dp"
            text="@string/social_feeds_syncing_state_desc"
            textAlignment="center"
            NOT_FOUND_0xb="@+id/tv_activate_title"
            NOT_FOUND_0xc="@+id/tv_activate_title"
            NOT_FOUND_0xd="@+id/tv_activate_title"
            style="@style/2132017414"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
