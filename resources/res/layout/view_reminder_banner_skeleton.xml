<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_width="match_parent"
    layout_height="match_parent"
    NOT_FOUND_0xb="8dp"
    NOT_FOUND_0xc="0dp">
    <androidx.constraintlayout.widget.ConstraintLayout
        background="@drawable/rounded_white_border"
        layout_width="match_parent"
        layout_height="match_parent">
        <View
            id="@+id/viewArrowSkeleton"
            background="@drawable/square_rounded_skeleton"
            layout_width="24dp"
            layout_height="24dp"
            layout_marginEnd="13dp"
            NOT_FOUND_0xd="@+id/viewBannerSkeleton"
            NOT_FOUND_0xe="0"
            NOT_FOUND_0x13="@+id/viewBannerSkeleton"/>
        <androidx.appcompat.widget.AppCompatTextView
            textSize="16sp"
            id="@+id/tvBannerTitleSkeleton"
            background="@drawable/square_rounded_skeleton"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_marginTop="13dp"
            fontFamily="@font/sf_pro_semibold"
            layout_marginStart="16dp"
            layout_marginEnd="16dp"
            NOT_FOUND_0xf="@+id/viewArrowSkeleton"
            NOT_FOUND_0x10="@+id/viewBannerSkeleton"
            NOT_FOUND_0x13="0"/>
        <androidx.appcompat.widget.AppCompatTextView
            textSize="12sp"
            textColor="#727272"
            id="@+id/tvBannerSubtitleSkeleton"
            background="@drawable/square_rounded_skeleton"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_marginBottom="8dp"
            fontFamily="@font/sf_pro_regular"
            layout_marginStart="16dp"
            layout_marginEnd="16dp"
            NOT_FOUND_0xd="0"
            NOT_FOUND_0xf="@+id/viewArrowSkeleton"
            NOT_FOUND_0x10="@+id/viewBannerSkeleton"
            NOT_FOUND_0x12="@+id/tvBannerTitleSkeleton"/>
        <View
            id="@+id/viewBannerSkeleton"
            background="@drawable/square_rounded_reminder_banner_skeleton"
            layout_width="56dp"
            layout_height="56dp"
            layout_marginTop="10dp"
            layout_marginBottom="10dp"
            layout_marginStart="13dp"
            NOT_FOUND_0xd="0"
            NOT_FOUND_0x11="0"
            NOT_FOUND_0x13="0"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>
