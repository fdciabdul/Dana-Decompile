<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/clContentWrapper"
    layout_width="match_parent"
    layout_height="match_parent">
    <androidx.constraintlayout.widget.ConstraintLayout
        background="@drawable/bg_card_rounded"
        layout_width="match_parent"
        layout_height="wrap_content">
        <View
            id="@+id/tv_bank_selection_label"
            background="@drawable/square_rounded_skeleton"
            paddingLeft="4dp"
            paddingRight="4dp"
            layout_width="140dp"
            layout_height="12dp"
            layout_marginLeft="16dp"
            layout_marginTop="16dp"
            layout_marginRight="16dp"
            NOT_FOUND_0xb="@+id/detvBankSelection"
            NOT_FOUND_0xc="0"
            NOT_FOUND_0xd="0"
            NOT_FOUND_0xe="0"
            NOT_FOUND_0x10="0"/>
        <View
            id="@+id/tvBankNumberLabel"
            background="@drawable/square_rounded_skeleton"
            paddingLeft="4dp"
            paddingRight="4dp"
            layout_width="140dp"
            layout_height="12dp"
            layout_marginLeft="16dp"
            layout_marginTop="24dp"
            layout_marginRight="16dp"
            NOT_FOUND_0xb="@+id/detvBankNumber"
            NOT_FOUND_0xe="0"
            NOT_FOUND_0xf="@+id/detvBankSelection"/>
        <View
            id="@+id/tvBankNumberError"
            background="@drawable/square_rounded_skeleton"
            layout_width="120dp"
            layout_height="10dp"
            layout_marginLeft="16dp"
            layout_marginTop="8dp"
            layout_marginRight="16dp"
            layout_marginBottom="16dp"
            NOT_FOUND_0xa="0"
            NOT_FOUND_0xc="0"
            NOT_FOUND_0xd="0"
            NOT_FOUND_0xe="0"
            NOT_FOUND_0xf="@+id/detvBankNumber"/>
        <View
            id="@+id/detvBankSelection"
            background="@drawable/square_rounded_skeleton"
            layout_width="match_parent"
            layout_height="28dp"
            layout_marginLeft="16dp"
            layout_marginTop="16dp"
            layout_marginRight="16dp"
            NOT_FOUND_0xb="@+id/tvBankNumberLabel"
            NOT_FOUND_0xc="0"
            NOT_FOUND_0xd="0.5"
            NOT_FOUND_0xe="0"
            NOT_FOUND_0xf="@+id/tv_bank_selection_label"/>
        <View
            id="@+id/detvBankNumber"
            background="@drawable/square_rounded_skeleton"
            layout_width="0dp"
            layout_height="28dp"
            layout_marginLeft="16dp"
            layout_marginTop="16dp"
            layout_marginRight="16dp"
            NOT_FOUND_0xb="@+id/tvBankNumberError"
            NOT_FOUND_0xc="0"
            NOT_FOUND_0xe="0"
            NOT_FOUND_0xf="@+id/tvBankNumberLabel"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>
