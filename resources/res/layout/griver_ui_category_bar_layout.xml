<?xml version="1.0" encoding="utf-8"?>
<com.alibaba.griver.ui.ant.layout.AULinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:aui="http://schemas.android.com/apk/res-auto"
    orientation="vertical"
    layout_width="wrap_content"
    layout_height="wrap_content">
    <RelativeLayout
        layout_width="wrap_content"
        layout_height="43dp">
        <HorizontalScrollView
            id="@+id/horizontalScrollView"
            background="@android:color/white"
            scrollbars="none"
            fadingEdge="horizontal"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_toLeftOf="@+id/addIcon">
            <com.alibaba.griver.ui.ant.layout.AULinearLayout
                orientation="vertical"
                layout_width="wrap_content"
                layout_height="wrap_content">
                <com.alibaba.griver.ui.ant.layout.AUScrollLayout
                    orientation="horizontal"
                    id="@+id/scroll_layout"
                    layout_width="wrap_content"
                    layout_height="41dp"/>
                <ImageView
                    id="@+id/menu_bar"
                    background="#108ee9"
                    visibility="invisible"
                    layout_width="0dp"
                    layout_height="2dp"
                    contentDescription=""/>
            </com.alibaba.griver.ui.ant.layout.AULinearLayout>
        </HorizontalScrollView>
        <com.alibaba.griver.ui.ant.text.AUTextView
            background="@drawable/griver_ui_segment_shadow_gradient_left"
            layout_width="14dp"
            layout_height="match_parent"
            layout_alignParentLeft="true"/>
        <com.alibaba.griver.ui.ant.text.AUTextView
            background="@drawable/griver_ui_segment_shadow_gradient_right"
            layout_width="14dp"
            layout_height="match_parent"
            layout_alignRight="@+id/horizontalScrollView"/>
        <com.alibaba.griver.ui.ant.text.AUIconView
            gravity="center"
            id="@+id/addIcon"
            background="#fff"
            visibility="gone"
            layout_width="48dp"
            layout_height="match_parent"
            layout_alignParentRight="true"
            NOT_FOUND_0xe="@color/2131100025"
            NOT_FOUND_0xf="@dimen/2131165509"
            NOT_FOUND_0x10="@string/griver_iconfont_add"/>
    </RelativeLayout>
    <View
        id="@+id/bottomLine"
        background="#eeeeee"
        layout_width="match_parent"
        layout_height="1px"/>
</com.alibaba.griver.ui.ant.layout.AULinearLayout>
