<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    gravity="center"
    orientation="vertical"
    layout_width="match_parent"
    layout_height="match_parent">
    <com.airbnb.lottie.LottieAnimationView
        id="@+id/lavAnimation"
        layout_width="295dp"
        layout_height="220dp"
        layout_marginStart="40dp"
        layout_marginEnd="40dp"
        NOT_FOUND_0xe="@+id/tvTitle"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x13="true"
        NOT_FOUND_0x14="true"/>
    <androidx.appcompat.widget.AppCompatTextView
        textSize="20sp"
        textColor="#313131"
        ellipsize="end"
        id="@+id/tvTitle"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="32dp"
        lines="1"
        fontFamily="@font/2131296266"
        layout_marginStart="24dp"
        layout_marginEnd="24dp"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="@+id/lavAnimation"/>
    <androidx.appcompat.widget.AppCompatTextView
        id="@+id/isHelperTextDisplayed"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        fontFamily="@font/sf_pro_regular"
        textAlignment="center"
        layout_marginStart="24dp"
        layout_marginEnd="24dp"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="@+id/tvTitle"
        style="@style/2132017414"/>
</LinearLayout>
