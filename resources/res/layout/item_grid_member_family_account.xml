<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.card.MaterialCardView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_width="match_parent"
    layout_height="134dp"
    layout_margin="3dp"
    NOT_FOUND_0x13="8dp"
    NOT_FOUND_0x14="0dp"
    NOT_FOUND_0x1d="@color/2131100473"
    NOT_FOUND_0x1e="1dp">
    <androidx.constraintlayout.widget.ConstraintLayout
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginStart="12dp">
        <id.dana.richview.CircleImageSelectionView
            id="@+id/ivMemberImage"
            layout_width="32dp"
            layout_height="32dp"
            layout_marginTop="12dp"
            NOT_FOUND_0x19="0"
            NOT_FOUND_0x1b="0"/>
        <de.hdodenhof.circleimageview.CircleImageView
            id="@+id/ivMemberStatusIndicator"
            visibility="gone"
            layout_width="12dp"
            layout_height="12dp"
            src="@drawable/ic_organizer_mark_family_account"
            NOT_FOUND_0x16="@+id/ivMemberImage"
            NOT_FOUND_0x17="@+id/ivMemberImage"/>
        <androidx.appcompat.widget.AppCompatImageView
            id="@+id/ivThreeDots"
            layout_width="24dp"
            layout_height="24dp"
            layout_marginTop="16dp"
            layout_marginStart="12dp"
            layout_marginEnd="6dp"
            NOT_FOUND_0x17="0"
            NOT_FOUND_0x18="1"
            NOT_FOUND_0x1b="0"
            NOT_FOUND_0x1c="@drawable/ic_vertical_offset_grey"/>
        <TextView
            ellipsize="end"
            id="@+id/tvMemberName"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="8dp"
            maxLines="2"
            layout_marginEnd="8dp"
            NOT_FOUND_0x17="0"
            NOT_FOUND_0x19="0"
            NOT_FOUND_0x1a="@+id/ivMemberImage"
            style="@style/2132017900"/>
        <TextView
            id="@+id/tvMemberInfo"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="4dp"
            singleLine="true"
            layout_marginEnd="8dp"
            NOT_FOUND_0x17="0"
            NOT_FOUND_0x19="0"
            NOT_FOUND_0x1a="@+id/tvMemberName"
            style="@style/2132017425"/>
        <androidx.recyclerview.widget.RecyclerView
            orientation="horizontal"
            id="@+id/rvServices"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="4dp"
            layout_marginBottom="8dp"
            layout_marginEnd="12dp"
            NOT_FOUND_0x15="androidx.recyclerview.widget.LinearLayoutManager"
            NOT_FOUND_0x16="0"
            NOT_FOUND_0x17="0"
            NOT_FOUND_0x19="0"
            NOT_FOUND_0x1a="@+id/tvMemberInfo"/>
        <TextView
            textColor="#108ee9"
            gravity="center"
            id="@+id/btnResend"
            background="@drawable/bg_dana_button_secondary_active"
            visibility="gone"
            layout_width="match_parent"
            layout_height="32dp"
            layout_marginTop="4dp"
            layout_marginBottom="12dp"
            text="@string/family_account_resend_invitation"
            maxLines="1"
            textAllCaps="true"
            layout_marginEnd="12dp"
            NOT_FOUND_0x16="0"
            NOT_FOUND_0x17="0"
            NOT_FOUND_0x19="0"
            NOT_FOUND_0x1a="@+id/tvMemberInfo"
            style="@style/2132017900"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</com.google.android.material.card.MaterialCardView>
