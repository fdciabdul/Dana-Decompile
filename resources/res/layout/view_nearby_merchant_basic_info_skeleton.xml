<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="@android:color/white"
    layout_width="match_parent"
    layout_height="wrap_content">
    <View
        id="@+id/vLogoShimmer"
        background="@drawable/square_rounded_skeleton_4dp_grey20"
        layout_width="64dp"
        layout_height="64dp"
        layout_marginTop="12dp"
        layout_marginStart="16dp"
        NOT_FOUND_0x7="0"
        NOT_FOUND_0x9="0"/>
    <View
        id="@+id/vNameShimmer"
        background="@drawable/square_rounded_skeleton_4dp_grey20"
        layout_width="192dp"
        layout_height="22dp"
        layout_marginStart="12dp"
        NOT_FOUND_0x6="@+id/vLogoShimmer"
        NOT_FOUND_0x9="@+id/vLogoShimmer"/>
    <View
        id="@+id/vRatingShimmer"
        background="@drawable/square_rounded_skeleton_4dp_grey20"
        layout_width="80dp"
        layout_height="16dp"
        layout_marginTop="4dp"
        layout_marginStart="12dp"
        NOT_FOUND_0x6="@+id/vLogoShimmer"
        NOT_FOUND_0x8="@+id/vNameShimmer"/>
    <View
        id="@+id/vCategoryShimmer"
        background="@drawable/square_rounded_skeleton_4dp_grey20"
        layout_width="80dp"
        layout_height="14dp"
        layout_marginTop="8dp"
        layout_marginStart="12dp"
        NOT_FOUND_0x6="@+id/vLogoShimmer"
        NOT_FOUND_0x8="@+id/vRatingShimmer"/>
    <View
        id="@+id/HoldLogin_Factory"
        background="#d1d1d1"
        layout_width="0dp"
        layout_height="0.5dp"
        layout_marginTop="12dp"
        NOT_FOUND_0x7="@+id/vCategoryShimmer"
        NOT_FOUND_0x8="@+id/vCategoryShimmer"/>
</androidx.constraintlayout.widget.ConstraintLayout>
