<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/layout_qr"
    layout_width="match_parent"
    layout_height="wrap_content">
    <ImageView
        id="@+id/iv_qr"
        layout_width="@dimen/2131166089"
        layout_height="@dimen/2131166089"
        scaleType="fitCenter"
        adjustViewBounds="true"
        layout_centerInParent="true"/>
    <RelativeLayout
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_centerInParent="true">
        <de.hdodenhof.circleimageview.CircleImageView
            id="@+id/imageview_user_profile"
            background="@drawable/white_circle"
            layout_width="40dp"
            layout_height="40dp"
            src="@drawable/res_0x7f080891_networkuserentitydata_externalsyntheticlambda7"
            layout_centerInParent="true"
            contentDescription="@null"/>
        <ImageView
            id="@+id/view_kyc"
            visibility="invisible"
            layout_width="15dp"
            layout_height="15dp"
            layout_marginLeft="5dp"
            layout_marginTop="5dp"
            layout_above="@+id/imageview_user_profile"
            layout_alignRight="@+id/imageview_user_profile"
            layout_alignBottom="@+id/imageview_user_profile"
            layout_marginStart="5dp"
            layout_alignEnd="@+id/imageview_user_profile"
            NOT_FOUND_0x11="@drawable/verify"/>
    </RelativeLayout>
    <ImageView
        id="@+id/iv_refresh"
        background="@drawable/white_circle"
        visibility="gone"
        layout_width="44dp"
        layout_height="44dp"
        src="@drawable/refresh"
        layout_centerInParent="true"
        contentDescription="@null"/>
    <View
        id="@+id/view_qr_skeleton"
        background="@drawable/square_skeleton"
        visibility="gone"
        layout_width="@dimen/2131166089"
        layout_height="@dimen/2131166089"
        scaleType="fitXY"
        layout_centerInParent="true"/>
</RelativeLayout>
