<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/card_view_dana_deal"
    layout_marginLeft="16dp"
    layout_marginRight="16dp"
    NOT_FOUND_0x17="6dp"
    NOT_FOUND_0x18="0dp"
    style="@style/2132017490">
    <androidx.constraintlayout.widget.ConstraintLayout
        background="@drawable/bg_rounded_with_border"
        layout_width="match_parent"
        layout_height="wrap_content">
        <ImageView
            id="@+id/iv_dana_deals_image"
            layout_width="match_parent"
            layout_height="0dp"
            layout_margin="1dp"
            scaleType="centerCrop"
            adjustViewBounds="true"
            NOT_FOUND_0x19="@+id/tv_voucher_description"
            NOT_FOUND_0x1d="0"
            NOT_FOUND_0x1e="0"
            NOT_FOUND_0x22="0"
            NOT_FOUND_0x23="@drawable/ic_header_null"/>
        <FrameLayout
            id="@+id/fl_merchant_logo"
            background="@drawable/bg_bottom_right_rounded"
            padding="8dp"
            layout_width="wrap_content"
            layout_height="wrap_content"
            NOT_FOUND_0x1d="0"
            NOT_FOUND_0x22="0">
            <ImageView
                id="@+id/iv_merchant_logo"
                layout_width="wrap_content"
                layout_height="wrap_content"
                scaleType="fitXY"
                adjustViewBounds="true"
                maxWidth="32dp"
                maxHeight="32dp"/>
        </FrameLayout>
        <FrameLayout
            id="@+id/fl_voucher_value"
            background="@drawable/bg_bottom_left_rounded"
            padding="8dp"
            layout_width="wrap_content"
            layout_height="wrap_content"
            NOT_FOUND_0x1e="0"
            NOT_FOUND_0x22="0">
            <TextView
                id="@+id/tv_voucher_percentage_value"
                layout_width="wrap_content"
                layout_height="wrap_content"
                style="@style/2132017897"/>
        </FrameLayout>
        <TextView
            id="@+id/tv_voucher_title"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_marginStart="64dp"
            layout_marginEnd="16dp"
            NOT_FOUND_0x1b="0"
            NOT_FOUND_0x1f="@+id/fl_merchant_logo"
            NOT_FOUND_0x21="@+id/fl_voucher_value"
            style="@style/2132017404"/>
        <TextView
            id="@+id/tv_voucher_price"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_marginTop="4dp"
            layout_marginEnd="16dp"
            NOT_FOUND_0x1b="0"
            NOT_FOUND_0x20="@+id/tv_voucher_title"
            NOT_FOUND_0x21="@+id/tv_voucher_title"
            style="@style/2132017931"/>
        <TextView
            textColor="@android:color/white"
            id="@+id/tv_voucher_description"
            paddingBottom="22dp"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_marginTop="4dp"
            layout_marginEnd="16dp"
            NOT_FOUND_0x1b="0"
            NOT_FOUND_0x20="@+id/tv_voucher_title"
            NOT_FOUND_0x21="@+id/tv_voucher_price"
            style="@style/2132017440"/>
        <TextView
            id="@+id/tv_voucher_price_label"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="8dp"
            layout_marginBottom="4dp"
            text="@string/dana_deals_voucher_price_label"
            layout_below="@+id/iv_dana_deals_image"
            layout_marginStart="16dp"
            layout_alignParentStart="true"
            NOT_FOUND_0x1a="@+id/tv_voucher_price_value"
            NOT_FOUND_0x1d="0"
            NOT_FOUND_0x21="@+id/iv_dana_deals_image"
            style="@style/2132017900"/>
        <TextView
            id="@+id/tv_voucher_price_value"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_marginBottom="8dp"
            layout_below="@+id/tv_voucher_price_label"
            layout_marginEnd="16dp"
            NOT_FOUND_0x19="0"
            NOT_FOUND_0x1c="@+id/btn_buy_now"
            NOT_FOUND_0x20="@+id/tv_voucher_price_label"
            NOT_FOUND_0x21="@+id/tv_voucher_price_label"
            style="@style/2132017913"/>
        <Button
            id="@+id/btn_buy_now"
            layout_width="96dp"
            layout_height="32dp"
            text="@string/dana_deals_buy_now_text"
            layout_alignParentBottom="true"
            textAllCaps="true"
            layout_marginEnd="17dp"
            NOT_FOUND_0x19="0"
            NOT_FOUND_0x1e="0"
            NOT_FOUND_0x21="@+id/iv_dana_deals_image"
            style="@style/2132017655"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>
