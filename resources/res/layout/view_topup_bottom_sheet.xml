<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_gravity="bottom"
    background="@drawable/rounded_top_background"
    paddingBottom="16dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    NOT_FOUND_0x19="0">
    <TextView
        id="@+id/tvTitle"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="16dp"
        text="@string/family_account_text_how_much_do_you_want_to_top_up"
        layout_marginStart="16dp"
        NOT_FOUND_0x1e="0"
        NOT_FOUND_0x1f="@+id/secondaryHash_res_0x7f0a08d3"
        style="@style/2132017916"/>
    <TextView
        id="@+id/tvDescError"
        visibility="invisible"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_marginRight="16dp"
        text="@string/family_account_text_top_up_max_balance"
        NOT_FOUND_0x18="@drawable/ic_warning_14"
        NOT_FOUND_0x1b="0"
        NOT_FOUND_0x1c="0"
        NOT_FOUND_0x1d="0"
        NOT_FOUND_0x1f="@+id/detTopup"
        style="@style/2132017435"/>
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/secondaryHash_res_0x7f0a08d3"
        layout_width="56dp"
        layout_height="4dp"
        layout_margin="8dp"
        NOT_FOUND_0x1c="0"
        NOT_FOUND_0x1d="0"
        NOT_FOUND_0x20="0"
        NOT_FOUND_0x24="@drawable/ic_flat_dark"/>
    <id.dana.core.ui.richview.CurrencyEditText
        enabled="true"
        textSize="27sp"
        textStyle="bold"
        id="@+id/detTopup"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_marginTop="16dp"
        layout_marginRight="16dp"
        maxLines="1"
        android:maxLength="13"
        inputType="numberPassword"
        imeOptions="actionDone"
        contentDescription="@string/txt_nominal_topup"
        textCursorDrawable="@drawable/cursor_tangerine"
        paddingStart="0dp"
        NOT_FOUND_0x17="@color/getErrorConfigVersion_res_0x7f060305"
        NOT_FOUND_0x1a="0"
        NOT_FOUND_0x1e="0"
        NOT_FOUND_0x1f="@+id/tvTitle"/>
    <id.dana.component.buttoncomponent.DanaButtonPrimaryView
        id="@+id/btnTopUpContinue"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_margin="16dp"
        NOT_FOUND_0x1f="@+id/nvFamilyAccountTopUp"
        NOT_FOUND_0x21="@string/family_account_text_continue"
        NOT_FOUND_0x22="0"
        NOT_FOUND_0x23="@string/btn_continue_topup"/>
    <id.dana.core.ui.richview.NumpadView
        id="@+id/nvFamilyAccountTopUp"
        layout_width="match_parent"
        layout_height="wrap_content"
        NOT_FOUND_0x1a="0"
        NOT_FOUND_0x1e="0"
        NOT_FOUND_0x1f="@+id/tvDescError"/>
</androidx.constraintlayout.widget.ConstraintLayout>
