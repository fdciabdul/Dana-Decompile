<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/allowedTargets"
    background="#f5f5f5"
    layout_width="match_parent"
    layout_height="match_parent">
    <include
        id="@+id/toolbar_container"
        NOT_FOUND_0x10="@+id/nested_sv"
        layout="@layout/layout_toolbar"/>
    <androidx.core.widget.NestedScrollView
        id="@+id/nested_sv"
        layout_width="match_parent"
        layout_height="0dp"
        fillViewport="true"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x13="@+id/toolbar_container">
        <LinearLayout
            orientation="vertical"
            layout_width="match_parent"
            layout_height="match_parent">
            <include
                id="@+id/res_0x7f0a0438_sightcameraview_icameraframelistener"
                visibility="gone"
                layout="@layout/view_empty_state_try_again"/>
            <LinearLayout
                orientation="vertical"
                id="@+id/cl_content_group_detail"
                layout_width="match_parent"
                layout_height="match_parent"
                layout_marginTop="16dp">
                <TextView
                    textSize="16sp"
                    textStyle="bold"
                    textColor="#000000"
                    id="@+id/tvHeader"
                    layout_width="wrap_content"
                    layout_height="wrap_content"
                    layout_marginLeft="12dp"
                    layout_marginTop="-4dp"
                    layout_marginRight="12dp"
                    layout_marginBottom="12dp"
                    text="Friends Activity"
                    NOT_FOUND_0x12="0"
                    NOT_FOUND_0x13="@+id/toolbar_container"/>
                <androidx.recyclerview.widget.RecyclerView
                    id="@+id/BlockedFragmentComponent"
                    layout_width="match_parent"
                    layout_height="match_parent"/>
            </LinearLayout>
        </LinearLayout>
    </androidx.core.widget.NestedScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>
