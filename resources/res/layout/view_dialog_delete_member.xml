<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_gravity="center"
    orientation="vertical"
    id="@+id/ll_simple_dialog"
    background="@drawable/rounded_white"
    paddingLeft="12dp"
    paddingTop="8dp"
    paddingRight="12dp"
    paddingBottom="12dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    layout_margin="20dp">
    <TextView
        gravity="center"
        id="@+id/tvTitle"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        style="@style/2132017916"/>
    <TextView
        gravity="center"
        id="@+id/isHelperTextDisplayed"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        style="@style/2132017439"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        id="@+id/cl_action_dialog"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="16dp">
        <Button
            id="@+id/btn_negative"
            background="@drawable/bg_dana_button_ghost_active"
            layout_width="0dp"
            layout_height="32dp"
            text="@string/cancel"
            textAllCaps="true"
            NOT_FOUND_0xf="0"
            NOT_FOUND_0x11="@+id/btn_positive"
            NOT_FOUND_0x13="0"
            style="@style/2132017895"/>
        <Button
            id="@+id/btn_positive"
            background="@drawable/bg_dana_button_primary_active"
            layout_width="0dp"
            layout_height="32dp"
            text="OK"
            textAllCaps="true"
            NOT_FOUND_0x10="@+id/btn_negative"
            NOT_FOUND_0x12="0"
            NOT_FOUND_0x13="0"
            style="@style/2132017899"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</LinearLayout>
