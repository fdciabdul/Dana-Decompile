<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    orientation="vertical"
    background="#000"
    layout_width="match_parent"
    layout_height="match_parent">
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/iv_media_preview"
        background="#000"
        layout_width="0dp"
        layout_height="0dp"
        scaleType="fitCenter"
        NOT_FOUND_0xb="@+id/cl_media_info"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x10="0"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        id="@+id/cl_media_info"
        background="@drawable/bg_gradient_alpha_black"
        layout_width="0dp"
        layout_height="wrap_content"
        NOT_FOUND_0xa="0"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xe="0">
        <TextView
            id="@+id/tv_media_info_title"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_marginTop="16dp"
            layout_marginStart="16dp"
            layout_marginEnd="16dp"
            NOT_FOUND_0xc="0"
            NOT_FOUND_0xe="0"
            NOT_FOUND_0x10="0"
            style="@style/2132017412"/>
        <TextView
            id="@+id/tv_media_info_subtitle"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_marginStart="8dp"
            layout_marginEnd="16dp"
            NOT_FOUND_0xa="@+id/DebugCoroutineInfoImpl_res_0x7f0a031b"
            NOT_FOUND_0xc="0"
            NOT_FOUND_0xd="@+id/DebugCoroutineInfoImpl_res_0x7f0a031b"
            NOT_FOUND_0x10="@+id/DebugCoroutineInfoImpl_res_0x7f0a031b"
            style="@style/2132017438"/>
        <de.hdodenhof.circleimageview.CircleImageView
            id="@+id/DebugCoroutineInfoImpl_res_0x7f0a031b"
            layout_width="24dp"
            layout_height="24dp"
            layout_marginTop="8dp"
            layout_marginBottom="30dp"
            layout_marginStart="16dp"
            NOT_FOUND_0xa="0"
            NOT_FOUND_0xe="0"
            NOT_FOUND_0xf="@+id/tv_media_info_title"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
