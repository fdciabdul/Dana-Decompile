<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    orientation="vertical"
    background="#ffffff"
    layout_width="match_parent"
    layout_height="match_parent">
    <FrameLayout
        id="@+id/h5_nav_back"
        clickable="true"
        layout_width="40dp"
        layout_height="47dp"
        layout_centerVertical="true"
        contentDescription="@string/griver_backward">
        <ImageView
            gravity="center"
            layout_gravity="center"
            id="@+id/tv_back_button"
            layout_width="wrap_content"
            layout_height="32dp"
            src="@drawable/griver_ui_ic_header_back"
            maxWidth="44dp"
            maxLines="1"/>
    </FrameLayout>
    <FrameLayout
        layout_width="match_parent"
        layout_height="match_parent">
        <com.alibaba.griver.ui.splash.SplashLoadingView
            id="@+id/view_splash_loading"
            layout_width="match_parent"
            layout_height="match_parent"/>
        <include
            id="@+id/view_splash_error"
            visibility="gone"
            layout="@layout/griver_ui_splash_error"/>
    </FrameLayout>
</LinearLayout>
