<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_gravity="center"
    orientation="vertical"
    id="@+id/card_view"
    clipToPadding="false"
    layout_width="match_parent"
    layout_height="wrap_content"
    layout_margin="36dp"
    NOT_FOUND_0x13="8dp"
    NOT_FOUND_0x14="0dp"
    NOT_FOUND_0x15="12dp"
    NOT_FOUND_0x16="12dp"
    NOT_FOUND_0x17="12dp"
    NOT_FOUND_0x18="12dp">
    <androidx.constraintlayout.widget.ConstraintLayout
        layout_gravity="center"
        orientation="vertical"
        id="@+id/constrainLayout"
        layout_width="match_parent"
        layout_height="wrap_content">
        <androidx.appcompat.widget.AppCompatImageView
            id="@+id/iv_image"
            layout_width="124dp"
            layout_height="0dp"
            layout_marginTop="16dp"
            maxHeight="124dp"
            NOT_FOUND_0x19="1:1"
            NOT_FOUND_0x1a="0"
            NOT_FOUND_0x1b="0"
            NOT_FOUND_0x1d="0"
            NOT_FOUND_0x1e="@drawable/bg_skeleton_banner"/>
        <TextView
            gravity="center"
            id="@+id/tv_title"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_marginTop="8dp"
            text="@string/login_with_face_verification"
            lineSpacingExtra="4dp"
            NOT_FOUND_0x1a="0"
            NOT_FOUND_0x1b="0"
            NOT_FOUND_0x1c="@+id/iv_image"
            style="@style/2132017916"/>
        <TextView
            gravity="center"
            id="@+id/tv_description"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_marginTop="8dp"
            text="@string/login_with_face_description"
            lineSpacingExtra="4dp"
            NOT_FOUND_0x1a="0"
            NOT_FOUND_0x1b="0"
            NOT_FOUND_0x1c="@+id/tv_title"
            style="@style/2132017440"/>
        <id.dana.component.formcomponent.DanaCheckboxButtonView
            textAppearance="@style/2132017439"
            id="@+id/cb_no_dialog_anymore"
            visibility="gone"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_marginTop="10dp"
            NOT_FOUND_0x1a="0"
            NOT_FOUND_0x1b="0"
            NOT_FOUND_0x1c="@+id/tv_description"
            style="@style/2132017439"/>
        <LinearLayout
            orientation="horizontal"
            id="@+id/view_action_dialog"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="8dp"
            weightSum="2"
            NOT_FOUND_0x1a="0"
            NOT_FOUND_0x1b="0"
            NOT_FOUND_0x1c="@+id/cb_no_dialog_anymore">
            <Button
                textSize="12sp"
                textColor="#108ee9"
                id="@+id/btn_negative"
                visibility="gone"
                layout_width="match_parent"
                layout_height="32dp"
                layout_margin="3dp"
                layout_weight="1"
                lineSpacingExtra="4sp"
                fontFamily="@font/2131296265"
                style="@style/2132017583"/>
            <Button
                textSize="12sp"
                textColor="#108ee9"
                id="@+id/btn_negative_ghost"
                visibility="gone"
                layout_width="match_parent"
                layout_height="32dp"
                layout_margin="3dp"
                layout_weight="1"
                lineSpacingExtra="4sp"
                fontFamily="@font/2131296265"
                style="@style/2132017582"/>
            <Button
                textSize="12sp"
                id="@+id/btn_positive"
                visibility="gone"
                layout_width="match_parent"
                layout_height="32dp"
                layout_margin="3dp"
                layout_weight="1"
                lineSpacingExtra="4sp"
                style="@style/2132017655"/>
            <id.dana.richview.LogoProgressView
                id="@+id/lpv_loading_dialog"
                visibility="gone"
                layout_width="match_parent"
                layout_height="32dp"
                layout_weight="1"/>
        </LinearLayout>
        <TextView
            gravity="center"
            id="@+id/tv_dialog_footer_text"
            visibility="gone"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="12dp"
            NOT_FOUND_0x1a="0"
            NOT_FOUND_0x1b="0"
            NOT_FOUND_0x1c="@+id/view_action_dialog"
            style="@style/2132017440"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>
