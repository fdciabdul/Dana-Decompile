<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/ll_linked_merchant_skeleton_container"
    background="@android:color/white"
    layout_width="match_parent"
    layout_height="70dp">
    <View
        id="@+id/viewSkeleton"
        background="@drawable/square_rounded_skeleton"
        layout_width="28dp"
        layout_height="28dp"
        layout_marginStart="16dp"
        NOT_FOUND_0x7="0"
        NOT_FOUND_0xa="0"
        NOT_FOUND_0xb="0"/>
    <LinearLayout
        orientation="vertical"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginStart="16dp"
        NOT_FOUND_0x7="0"
        NOT_FOUND_0x9="@+id/viewSkeleton"
        NOT_FOUND_0xb="0">
        <View
            background="@drawable/square_rounded_skeleton"
            layout_width="180dp"
            layout_height="18dp"/>
        <View
            background="@drawable/square_rounded_skeleton"
            layout_width="123dp"
            layout_height="14dp"
            layout_marginTop="2dp"/>
    </LinearLayout>
    <View
        layout_width="0dp"
        NOT_FOUND_0x7="0"
        NOT_FOUND_0x8="0"
        NOT_FOUND_0xa="0"
        style="@style/2132017967"/>
</androidx.constraintlayout.widget.ConstraintLayout>
