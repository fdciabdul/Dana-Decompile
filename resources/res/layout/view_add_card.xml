<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_gravity="center"
    id="@+id/card_add"
    background="@drawable/bg_new_card"
    foregroundGravity="center"
    style="@style/2132017531">
    <RelativeLayout
        paddingLeft="16dp"
        paddingTop="16dp"
        paddingRight="16dp"
        paddingBottom="6dp"
        contentDescription="@string/btn_add_new_card_offline"
        style="@style/2132017531">
        <LinearLayout
            gravity="center"
            orientation="vertical"
            id="@+id/dana_card"
            padding="15dp"
            layout_margin="10dp"
            layout_centerInParent="true"
            style="@style/2132017490">
            <ImageView
                id="@+id/add_new_card"
                src="@drawable/add_card_vertical"
                style="@style/2132018187"/>
            <TextView
                textSize="18sp"
                textColor="#000"
                gravity="center_horizontal"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginTop="12dp"
                text="@string/tap_to_add_new_card"
                lineSpacingExtra="6sp"
                textAllCaps="true"
                fontFamily="@font/2131296266"/>
            <TextView
                textSize="12sp"
                textColor="#313131"
                gravity="center"
                layout_width="wrap_content"
                layout_height="wrap_content"
                text="@string/enjoy_the_secure_and_seamless_payment_experience"
                lineSpacingExtra="4sp"/>
            <ImageView
                id="@+id/iv_expresspay"
                visibility="gone"
                layout_marginTop="6dp"
                NOT_FOUND_0x25="@drawable/expresspay_blue"
                style="@style/2132018187"/>
        </LinearLayout>
    </RelativeLayout>
    <LinearLayout
        gravity="center"
        orientation="horizontal"
        background="#ffffff"
        paddingTop="8dp"
        paddingBottom="16dp"
        layout_width="match_parent"
        layout_height="wrap_content"
        weightSum="4"
        layout_alignParentBottom="true"
        paddingStart="16dp"
        paddingEnd="16dp">
        <id.dana.richview.DanaProtectionView
            layout_gravity="center"
            id="@+id/view_dana_protection"
            visibility="gone"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_weight="1"
            NOT_FOUND_0x22="small_dark"
            NOT_FOUND_0x23="0"
            NOT_FOUND_0x24="0"/>
        <ImageView
            layout_gravity="center"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_weight="0.5"
            NOT_FOUND_0x25="@drawable/visa"/>
        <ImageView
            layout_gravity="center"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_weight="0.5"
            NOT_FOUND_0x25="@drawable/mastercard"/>
        <ImageView
            layout_gravity="center"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_weight="0.5"
            NOT_FOUND_0x25="@drawable/jcb"/>
        <ImageView
            layout_gravity="center"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_weight="0.5"
            NOT_FOUND_0x25="@drawable/amex"/>
        <ImageView
            layout_gravity="center"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginLeft="10dp"
            layout_marginRight="20dp"
            layout_marginStart="10dp"
            layout_marginEnd="20dp"
            NOT_FOUND_0x25="@drawable/gpn"/>
        <ImageView
            layout_gravity="center"
            layout_width="0dp"
            layout_height="10dp"
            layout_weight="2"
            NOT_FOUND_0x25="@drawable/expresspay_blue"/>
    </LinearLayout>
</RelativeLayout>
