<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    NOT_FOUND_0xe="8dp"
    NOT_FOUND_0xf="0dp"
    style="@style/2132017490">
    <LinearLayout
        orientation="vertical"
        background="@drawable/rounded_white_border"
        layout_width="match_parent"
        layout_height="wrap_content">
        <LinearLayout
            gravity="center_horizontal"
            orientation="vertical"
            background="@drawable/split_bill_section_square"
            style="@style/2132017490">
            <TextView
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginTop="16dp"
                text="@string/split_bill_entry_title"
                lineSpacingExtra="4sp"
                style="@style/2132017439"/>
            <TextView
                layout_width="wrap_content"
                layout_height="wrap_content"
                text="@string/split_bill_entry_description"
                lineSpacingExtra="4sp"
                style="@style/2132017439"/>
            <Button
                id="@+id/btn_init_split_bill"
                layout_marginTop="10dp"
                text="@string/label_split_bill"
                style="@style/2132017581"/>
        </LinearLayout>
        <id.dana.splitbill.view.SplitBillHistoryView
            id="@+id/sbhv_bill_histories"
            paddingLeft="16dp"
            paddingRight="16dp"
            paddingBottom="10dp"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="28dp"
            paddingStart="16dp"
            paddingEnd="16dp"
            NOT_FOUND_0x10="false"/>
    </LinearLayout>
</androidx.cardview.widget.CardView>
