<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="#ffffff"
    paddingLeft="16dp"
    paddingTop="8dp"
    paddingRight="16dp"
    paddingBottom="8dp"
    layout_width="match_parent"
    layout_height="wrap_content">
    <TextView
        id="@+id/ContentDeliveryCacheEntityDataFactory"
        layout_width="0dp"
        layout_height="wrap_content"
        text="@string/family_member_transaction_set_limit_title"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x17="@+id/ivCurrencyBorder"
        NOT_FOUND_0x19="0"
        style="@style/2132017916"/>
    <TextView
        id="@+id/tvCurrency"
        layout_width="wrap_content"
        layout_height="wrap_content"
        text="@string/rupiah_short"
        NOT_FOUND_0x14="@+id/ivCurrencyBorder"
        NOT_FOUND_0x15="@+id/ivCurrencyBorder"
        NOT_FOUND_0x17="@+id/ivCurrencyBorder"
        NOT_FOUND_0x19="@+id/ivCurrencyBorder"
        style="@style/2132017900"/>
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/ivCurrencyBorder"
        layout_width="24dp"
        layout_height="24dp"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x19="0"
        NOT_FOUND_0x1a="@drawable/bg_transparent_circle_grey20_8dp"/>
    <id.dana.core.ui.richview.CurrencyEditText
        enabled="true"
        textSize="27sp"
        textStyle="bold"
        id="@+id/etAmount"
        layout_width="0dp"
        layout_height="wrap_content"
        maxLines="1"
        android:maxLength="10"
        inputType="numberPassword"
        imeOptions="actionDone"
        contentDescription="@string/txt_member_limit"
        textCursorDrawable="@drawable/cursor_tangerine"
        layout_marginStart="12dp"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x16="@+id/ivCurrencyBorder"
        NOT_FOUND_0x18="@+id/ContentDeliveryCacheEntityDataFactory"/>
    <TextView
        id="@+id/tvHelper"
        layout_width="0dp"
        layout_height="wrap_content"
        NOT_FOUND_0x13="@drawable/ic_info_grey_20"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x17="@+id/ContentDeliveryCacheEntityDataFactory"
        NOT_FOUND_0x18="@+id/etAmount"
        style="@style/2132017440"/>
</androidx.constraintlayout.widget.ConstraintLayout>
