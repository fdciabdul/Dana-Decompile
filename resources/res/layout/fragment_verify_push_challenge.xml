<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="@drawable/rounded_popup_white"
    layout_width="match_parent"
    layout_height="wrap_content">
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/iv_icon_bottomsheet"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="42dp"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x14="@drawable/ic_general_device_not_recognize"/>
    <androidx.appcompat.widget.AppCompatTextView
        gravity="center"
        id="@+id/tv_title_bottomsheet"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="16dp"
        text="@string/twilio_bottomsheet_title"
        layout_marginStart="16dp"
        layout_marginEnd="16dp"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="@+id/iv_icon_bottomsheet"
        style="@style/2132017932"/>
    <androidx.appcompat.widget.AppCompatTextView
        gravity="center"
        id="@+id/tv_desc_bottomsheet"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="16dp"
        text="@string/twilio_bottomsheet_description"
        layout_marginStart="16dp"
        layout_marginEnd="16dp"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="@+id/tv_title_bottomsheet"
        style="@style/2132017405"/>
    <androidx.appcompat.widget.AppCompatTextView
        gravity="center"
        id="@+id/tv_countdown_bottomsheet"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="68dp"
        layout_marginBottom="12dp"
        layout_marginStart="16dp"
        layout_marginEnd="16dp"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="@+id/tv_desc_bottomsheet"
        style="@style/2132017914"/>
    <LinearLayout
        orientation="vertical"
        id="@+id/ll_retry"
        visibility="invisible"
        layout_width="match_parent"
        layout_height="wrap_content"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="@+id/tv_desc_bottomsheet">
        <androidx.appcompat.widget.AppCompatTextView
            gravity="center"
            id="@+id/tv_left_attempt"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="20dp"
            layout_marginBottom="8dp"
            layout_marginStart="16dp"
            layout_marginEnd="16dp"
            NOT_FOUND_0xd="@+id/btn_resend"
            NOT_FOUND_0xe="0"
            NOT_FOUND_0xf="0"
            NOT_FOUND_0x11="0"
            style="@style/2132017440"/>
        <id.dana.component.buttoncomponent.DanaButtonPrimaryView
            gravity="center"
            id="@+id/btn_resend"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginBottom="16dp"
            layout_marginStart="16dp"
            layout_marginEnd="16dp"
            NOT_FOUND_0xc="0"
            NOT_FOUND_0xe="0"
            NOT_FOUND_0xf="0"
            NOT_FOUND_0x12="@string/twilio_bottomsheet_resend_button"
            NOT_FOUND_0x13="1"/>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
