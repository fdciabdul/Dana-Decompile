<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    orientation="vertical"
    layout_width="match_parent"
    layout_height="match_parent">
    <View
        background="@drawable/square_rounded_skeleton"
        layout_width="match_parent"
        layout_height="40dp"
        layout_marginLeft="16dp"
        layout_marginTop="12dp"
        layout_marginRight="16dp"
        layout_marginBottom="12dp"/>
    <include
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="16dp"
        layout_marginBottom="16dp"
        layout_marginStart="16dp"
        layout_marginEnd="4dp"
        layout="@layout/view_add_new_recipient_skeleton_global"/>
    <View
        background="#f5f5f5"
        layout_width="match_parent"
        layout_height="8dp"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        layout_width="match_parent"
        layout_height="wrap_content">
        <View
            id="@+id/viewHeaderSkeletonLeft"
            background="@drawable/square_rounded_skeleton"
            layout_width="0dp"
            layout_height="16dp"
            layout_marginTop="12dp"
            layout_marginBottom="12dp"
            layout_marginStart="16dp"
            layout_marginEnd="10dp"
            NOT_FOUND_0xb="0"
            NOT_FOUND_0xd="@+id/viewHeaderSkeletonRight"
            NOT_FOUND_0xe="1.5"
            NOT_FOUND_0x10="0"
            NOT_FOUND_0x11="0"/>
        <View
            id="@+id/viewHeaderSkeletonRight"
            background="@drawable/square_rounded_skeleton"
            layout_width="0dp"
            layout_height="16dp"
            layout_marginTop="12dp"
            layout_marginBottom="12dp"
            layout_marginEnd="16dp"
            NOT_FOUND_0xb="0"
            NOT_FOUND_0xc="0"
            NOT_FOUND_0xe="1"
            NOT_FOUND_0xf="@+id/viewHeaderSkeletonLeft"
            NOT_FOUND_0x11="0"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <include layout="@layout/view_list_recipient_skeleton_global"/>
    <include layout="@layout/view_list_recipient_skeleton_global"/>
    <include layout="@layout/view_list_recipient_skeleton_global"/>
    <include layout="@layout/view_list_recipient_skeleton_global"/>
    <include layout="@layout/view_list_recipient_skeleton_global"/>
    <include layout="@layout/view_list_recipient_skeleton_global"/>
    <include layout="@layout/view_list_recipient_skeleton_global"/>
    <include layout="@layout/view_list_recipient_skeleton_global"/>
    <include layout="@layout/view_list_recipient_skeleton_global"/>
    <include layout="@layout/view_list_recipient_skeleton_global"/>
</androidx.appcompat.widget.LinearLayoutCompat>
