<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="#f5f5f5"
    focusable="true"
    clickable="true"
    layout_width="match_parent"
    layout_height="match_parent">
    <include
        id="@+id/toolbar"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x19="0"
        NOT_FOUND_0x1b="0"
        layout="@layout/view_cashier_toolbar"/>
    <ScrollView
        id="@+id/sv_installment"
        layout_width="match_parent"
        layout_height="0dp"
        fillViewport="true"
        NOT_FOUND_0x15="@+id/ll_continue_btn"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x19="0"
        NOT_FOUND_0x1a="@+id/toolbar">
        <androidx.constraintlayout.widget.ConstraintLayout
            orientation="vertical"
            id="@+id/ll_installment"
            layout_width="match_parent"
            layout_height="0dp"
            layout_marginBottom="8dp">
            <androidx.constraintlayout.widget.ConstraintLayout
                id="@+id/cl_total_amount"
                background="#ffffff"
                layout_width="match_parent"
                layout_height="wrap_content"
                NOT_FOUND_0x16="0"
                NOT_FOUND_0x19="0"
                NOT_FOUND_0x1b="0">
                <androidx.appcompat.widget.AppCompatTextView
                    id="@+id/appCompatTextView"
                    layout_width="wrap_content"
                    layout_height="wrap_content"
                    layout_marginTop="20dp"
                    layout_marginBottom="20dp"
                    text="@string/total_price"
                    contentDescription="@string/sdet_lbl_total_price_title"
                    layout_marginStart="16dp"
                    NOT_FOUND_0x14="0"
                    NOT_FOUND_0x19="0"
                    NOT_FOUND_0x1b="0"
                    style="@style/2132017413"/>
                <androidx.appcompat.widget.AppCompatTextView
                    gravity="end"
                    id="@+id/res_0x7f0a1618_snapshotcontroller_1"
                    layout_width="0dp"
                    layout_height="wrap_content"
                    contentDescription="@string/sdet_lbl_total_amount"
                    textAlignment="textEnd"
                    layout_marginStart="16dp"
                    layout_marginEnd="16dp"
                    NOT_FOUND_0x14="@+id/appCompatTextView"
                    NOT_FOUND_0x16="0"
                    NOT_FOUND_0x18="@+id/appCompatTextView"
                    NOT_FOUND_0x1b="@+id/appCompatTextView"
                    style="@style/2132017953"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <id.dana.cashier.view.PaylaterInstallmentView
                id="@+id/paylater_installment_view"
                background="#ffffff"
                visibility="visible"
                layout_width="match_parent"
                layout_height="wrap_content"
                NOT_FOUND_0x15="@+id/cl_agreement"
                NOT_FOUND_0x16="0"
                NOT_FOUND_0x19="0"
                NOT_FOUND_0x1a="@+id/cl_total_amount"
                NOT_FOUND_0x1c="0"/>
            <androidx.constraintlayout.widget.ConstraintLayout
                id="@+id/cl_agreement"
                background="@drawable/bg_white_rounded_bottom"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginTop="64dp"
                layout_marginBottom="8dp"
                layout_marginStart="16dp"
                layout_marginEnd="16dp"
                NOT_FOUND_0x14="@+id/ll_installment"
                NOT_FOUND_0x16="0"
                NOT_FOUND_0x19="0">
                <androidx.appcompat.widget.AppCompatCheckBox
                    id="@+id/cb_paylater_agreement_tnc"
                    layout_width="28dp"
                    layout_height="28dp"
                    layout_marginTop="8dp"
                    layout_marginBottom="8dp"
                    button="@drawable/selector_addon_check"
                    contentDescription="@string/sdet_checkbox_tnc"
                    layout_marginStart="8dp"
                    NOT_FOUND_0x14="0"
                    NOT_FOUND_0x17="@+id/tv_paylater_agreement_desc"
                    NOT_FOUND_0x19="0"
                    NOT_FOUND_0x1b="0"/>
                <androidx.appcompat.widget.AppCompatTextView
                    id="@+id/tv_paylater_agreement_desc"
                    layout_width="0dp"
                    layout_height="wrap_content"
                    layout_marginTop="8dp"
                    layout_marginBottom="8dp"
                    contentDescription="@string/sdet_tnc_link"
                    textAlignment="textStart"
                    layout_marginStart="4dp"
                    layout_marginEnd="12dp"
                    NOT_FOUND_0x14="@+id/cb_paylater_agreement_tnc"
                    NOT_FOUND_0x16="0"
                    NOT_FOUND_0x18="@+id/cb_paylater_agreement_tnc"
                    NOT_FOUND_0x1b="@+id/cb_paylater_agreement_tnc"
                    style="@style/2132017440"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
    <id.dana.richview.DanaProtectionLoadingView
        id="@+id/dana_protect_loading_view"
        layout_width="0dp"
        layout_height="0dp"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x19="0"
        NOT_FOUND_0x1b="0"/>
    <LinearLayout
        orientation="vertical"
        id="@+id/ll_continue_btn"
        background="#ffffff"
        layout_width="match_parent"
        layout_height="wrap_content"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x19="0">
        <id.dana.component.buttoncomponent.DanaButtonPrimaryView
            id="@+id/res_0x7f0a01c7_otpregistrationpresenter_input_2"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginLeft="16dp"
            layout_marginTop="8dp"
            layout_marginRight="16dp"
            layout_marginBottom="16dp"
            contentDescription="@string/sdet_btn_continue"
            NOT_FOUND_0x1d="@string/btn_continue"
            NOT_FOUND_0x1e="0"/>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
