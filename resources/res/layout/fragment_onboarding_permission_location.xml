<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_gravity="top"
    id="@+id/fl_container"
    background="#108ee9"
    layout_width="match_parent"
    layout_height="wrap_content"
    NOT_FOUND_0x13="@string/bottom_sheet_behavior">
    <ScrollView
        layout_gravity="top"
        layout_width="match_parent"
        layout_height="match_parent"
        layout_marginBottom="140dp"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x18="0"
        NOT_FOUND_0x1a="0">
        <androidx.constraintlayout.widget.ConstraintLayout
            layout_width="match_parent"
            layout_height="wrap_content">
            <com.airbnb.lottie.LottieAnimationView
                layout_gravity="center_horizontal"
                id="@+id/lottie_intro_animation"
                layout_width="match_parent"
                layout_height="wrap_content"
                scaleType="centerCrop"
                NOT_FOUND_0x16="0"
                NOT_FOUND_0x18="0"
                NOT_FOUND_0x1a="0"
                NOT_FOUND_0x1b="json/lottie/location_permission.lottie"
                NOT_FOUND_0x1c="true"/>
            <ImageView
                id="@+id/iv_padlock"
                layout_width="wrap_content"
                layout_height="wrap_content"
                paddingStart="16dp"
                paddingEnd="16dp"
                NOT_FOUND_0x14="@+id/tv_first"
                NOT_FOUND_0x18="0"
                NOT_FOUND_0x19="@+id/tv_subtitle"
                NOT_FOUND_0x1a="@+id/tv_first"
                NOT_FOUND_0x1f="@drawable/ic_padlock"/>
            <ImageView
                id="@+id/iv_promo"
                layout_width="wrap_content"
                layout_height="wrap_content"
                paddingStart="16dp"
                paddingEnd="16dp"
                NOT_FOUND_0x14="@+id/tv_second"
                NOT_FOUND_0x18="0"
                NOT_FOUND_0x19="@+id/tv_first"
                NOT_FOUND_0x1a="@+id/tv_second"
                NOT_FOUND_0x1f="@drawable/ic_promo_24"/>
            <ImageView
                id="@+id/iv_merchant"
                layout_width="wrap_content"
                layout_height="wrap_content"
                paddingStart="16dp"
                paddingEnd="16dp"
                NOT_FOUND_0x14="@+id/tv_third"
                NOT_FOUND_0x18="0"
                NOT_FOUND_0x19="@+id/tv_second"
                NOT_FOUND_0x1a="@+id/tv_third"
                NOT_FOUND_0x1f="@drawable/ic_merchant"/>
            <TextView
                textColor="#ffffff"
                id="@+id/tv_first"
                paddingTop="8dp"
                layout_width="0dp"
                layout_height="wrap_content"
                text="@string/onboarding_permission_location_first"
                paddingEnd="16dp"
                NOT_FOUND_0x16="0"
                NOT_FOUND_0x17="@+id/iv_padlock"
                NOT_FOUND_0x19="@+id/tv_subtitle"
                style="@style/2132017400"/>
            <TextView
                textColor="#ffffff"
                id="@+id/tv_subtitle"
                paddingTop="12dp"
                layout_width="match_parent"
                layout_height="wrap_content"
                text="@string/onboarding_permission_location_subtitle"
                paddingStart="16dp"
                paddingEnd="16dp"
                NOT_FOUND_0x19="@+id/tv_title"
                style="@style/2132017400"/>
            <TextView
                textColor="#ffffff"
                id="@+id/tv_second"
                paddingTop="8dp"
                layout_width="0dp"
                layout_height="wrap_content"
                text="@string/onboarding_permission_location_seconds"
                paddingEnd="16dp"
                NOT_FOUND_0x16="0"
                NOT_FOUND_0x17="@+id/iv_promo"
                NOT_FOUND_0x19="@+id/tv_first"
                style="@style/2132017400"/>
            <TextView
                textColor="#ffffff"
                id="@+id/tv_title"
                paddingTop="16dp"
                layout_width="match_parent"
                layout_height="wrap_content"
                text="@string/onboarding_permission_location_title"
                paddingStart="16dp"
                paddingEnd="16dp"
                NOT_FOUND_0x19="@+id/lottie_intro_animation"
                style="@style/2132017926"/>
            <TextView
                textColor="#ffffff"
                id="@+id/tv_third"
                paddingTop="8dp"
                layout_width="0dp"
                layout_height="wrap_content"
                text="@string/onboarding_permission_location_third"
                paddingEnd="16dp"
                NOT_FOUND_0x16="0"
                NOT_FOUND_0x17="@+id/iv_merchant"
                NOT_FOUND_0x19="@+id/tv_second"
                style="@style/2132017400"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
    <androidx.appcompat.widget.LinearLayoutCompat
        layout_gravity="bottom"
        orientation="vertical"
        id="@+id/res_0x7f0a041b_fontscontractcompat_fontrequestcallback"
        layout_width="match_parent"
        layout_height="wrap_content">
        <Button
            textSize="16sp"
            textColor="#108ee9"
            id="@+id/btn_allow"
            visibility="visible"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginLeft="16dp"
            layout_marginRight="16dp"
            layout_marginBottom="16dp"
            text="@string/PlaceComponentResult"
            lineSpacingExtra="4sp"
            fontFamily="@font/open_sans_bold"
            NOT_FOUND_0x14="0"
            NOT_FOUND_0x16="0"
            NOT_FOUND_0x18="0"
            style="@style/2132017657"/>
        <id.dana.component.buttoncomponent.DanaButtonPrimaryView
            id="@+id/btn_later"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginLeft="16dp"
            layout_marginRight="16dp"
            layout_marginBottom="16dp"
            NOT_FOUND_0x15="@+id/btn_ask_pin"
            NOT_FOUND_0x16="0"
            NOT_FOUND_0x18="0"
            NOT_FOUND_0x1d="@string/later"
            NOT_FOUND_0x1e="1"/>
    </androidx.appcompat.widget.LinearLayoutCompat>
</FrameLayout>
