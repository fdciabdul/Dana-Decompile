<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="#ffffff"
    style="@style/2132017531">
    <include
        id="@+id/layoutToolbar"
        layout="@layout/layout_toolbar"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        orientation="vertical"
        id="@+id/layout_value"
        layout_width="match_parent"
        layout_height="0dp"
        layout_marginLeft="16dp"
        layout_marginTop="?attr/2130968583"
        layout_marginRight="16dp"
        layout_above="@+id/rl_sendmoney_container"
        contentDescription="@string/txtAreaAmount"
        NOT_FOUND_0x1b="@+id/rl_sendmoney_container"
        NOT_FOUND_0x1c="0"
        NOT_FOUND_0x1d="0"
        NOT_FOUND_0x20="0"
        NOT_FOUND_0x21="16dp">
        <TextView
            id="@+id/tv_enter_amount"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="12dp"
            text="@string/enter_amount"
            textAllCaps="true"
            textAlignment="center"
            NOT_FOUND_0x1b="@+id/crv_amount_value"
            NOT_FOUND_0x1e="0"
            NOT_FOUND_0x20="0"
            style="@style/2132017432"/>
        <id.dana.richview.CurrencyTextView
            id="@+id/crv_amount_value"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="8dp"
            contentDescription="@string/txtTotalAmount"
            NOT_FOUND_0x1e="0"
            NOT_FOUND_0x1f="@+id/tv_enter_amount"
            style="@style/2132017964"/>
        <TextView
            textSize="10sp"
            textColor="#ff5d55"
            gravity="center"
            id="@+id/tv_error_msg"
            visibility="gone"
            drawablePadding="5dp"
            layout_below="@+id/crv_amount_value"
            fontFamily="@font/2131296265"
            layout_alignParentEnd="true"
            NOT_FOUND_0x19="@drawable/ic_warning_14"
            NOT_FOUND_0x1e="@+id/crv_amount_value"
            NOT_FOUND_0x1f="@+id/crv_amount_value"
            style="@style/2132018187"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <RelativeLayout
        id="@+id/rl_sendmoney_container"
        paddingLeft="15dp"
        paddingRight="15dp"
        paddingBottom="10dp"
        NOT_FOUND_0x1a="0"
        NOT_FOUND_0x1c="0"
        NOT_FOUND_0x1d="0"
        style="@style/2132017490">
        <id.dana.richview.calculator.CalculatorView
            id="@+id/cv_sendmoney_pad"
            NOT_FOUND_0x22="11"
            style="@style/2132018187"/>
        <id.dana.component.buttoncomponent.DanaButtonPrimaryView
            id="@+id/btn_action"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_below="@+id/cv_sendmoney_pad"
            contentDescription="@string/btnContinue"
            NOT_FOUND_0x23="@string/option_continue"
            NOT_FOUND_0x24="0"/>
        <RelativeLayout
            id="@+id/rl_shimmer_container"
            padding="15dp"
            layout_marginTop="2dp"
            layout_below="@+id/cv_sendmoney_pad"
            style="@style/2132017490"/>
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
