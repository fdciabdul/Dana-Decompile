<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/cl_action_dialog"
    layout_width="match_parent"
    layout_height="wrap_content"
    layout_marginTop="16dp">
    <Button
        id="@+id/btn_custom_cancel"
        background="@drawable/bg_dana_button_ghost_active"
        layout_width="0dp"
        layout_height="32dp"
        text="Cancel"
        textAllCaps="true"
        layout_marginEnd="8dp"
        NOT_FOUND_0x8="0"
        NOT_FOUND_0xa="@+id/btn_custom_ok"
        NOT_FOUND_0xc="0"
        style="@style/2132017895"/>
    <Button
        id="@+id/btn_custom_ok"
        background="@drawable/bg_dana_button_primary_active"
        layout_width="0dp"
        layout_height="32dp"
        text="OK"
        textAllCaps="true"
        NOT_FOUND_0x9="@+id/btn_custom_cancel"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xc="0"
        style="@style/2132017899"/>
</androidx.constraintlayout.widget.ConstraintLayout>
