<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" style="@style/2132017551">
    <TextView
        id="@+id/steps"
        layout_marginTop="12dp"
        layout_marginBottom="12dp"
        layout_centerHorizontal="true"
        style="@style/2132017552"/>
    <TextView
        id="@+id/header"
        text="@string/create_profile_title"
        layout_below="@+id/steps"
        layout_centerHorizontal="true"
        style="@style/2132017550"/>
    <de.hdodenhof.circleimageview.CircleImageView
        layout_gravity="center"
        id="@+id/res_0x7f0a0fda_abstractbinaryclassannotationandconstantloader_loadannotationsandinitializers_1_annotationvisitorformethod"
        layout_width="80dp"
        layout_height="80dp"
        layout_margin="48dp"
        src="@drawable/ic_empty_profile"
        layout_below="@+id/header"
        layout_centerHorizontal="true"/>
    <EditText
        textSize="24sp"
        textColor="#ffffff"
        textColorHint="#80ffffff"
        gravity="center"
        id="@+id/profile_name"
        background="@null"
        layout_width="match_parent"
        layout_height="wrap_content"
        hint="@string/profile_hint"
        maxLines="2"
        android:maxLength="32"
        digits=" abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789"
        layout_below="@+id/res_0x7f0a0fda_abstractbinaryclassannotationandconstantloader_loadannotationsandinitializers_1_annotationvisitorformethod"
        inputType="textMultiLine"
        imeOptions="actionDone"
        NOT_FOUND_0x16="@font/open_sans_light"/>
    <View
        background="#ffffff"
        layout_width="40dp"
        layout_height="2dp"
        layout_marginTop="8dp"
        layout_below="@+id/profile_name"
        layout_centerHorizontal="true"/>
</RelativeLayout>
