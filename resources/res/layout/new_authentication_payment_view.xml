<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/cl_payment_auth"
    style="@style/2132017490">
    <LinearLayout
        orientation="vertical"
        layout_width="match_parent"
        layout_height="wrap_content"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x17="0">
        <RelativeLayout
            orientation="horizontal"
            background="#ffffff"
            paddingLeft="12dp"
            paddingTop="20dp"
            paddingRight="12dp"
            paddingBottom="20dp"
            NOT_FOUND_0x13="0"
            NOT_FOUND_0x14="0"
            NOT_FOUND_0x16="0"
            NOT_FOUND_0x17="0"
            style="@style/2132017490">
            <androidx.appcompat.widget.AppCompatTextView
                id="@+id/tv_title"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginLeft="4dp"
                text="@string/pin_verification"
                layout_alignParentLeft="true"
                layout_centerInParent="true"
                layout_marginStart="4dp"
                layout_alignParentStart="true"
                style="@style/2132017413"/>
            <androidx.appcompat.widget.SwitchCompat
                id="@+id/switch_button"
                checked="false"
                layout_alignParentRight="true"
                layout_centerInParent="true"
                contentDescription="@string/toggle_pin_verification"
                layout_alignParentEnd="true"
                style="@style/2132017659"/>
        </RelativeLayout>
        <View
            NOT_FOUND_0x14="0"
            NOT_FOUND_0x16="0"
            NOT_FOUND_0x17="@+id/rl_normal_menu"
            style="@style/2132017967"/>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
