<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="@drawable/home_component_border"
    layout_width="match_parent"
    layout_height="80dp"
    contentDescription="@string/btnBubbleActivity">
    <ImageView
        id="@+id/iv_feeds_widget_profile"
        layout_width="56dp"
        layout_height="56dp"
        scaleType="fitXY"
        contentDescription="@string/imgProfileAvatar"
        layout_marginStart="16dp"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x13="0"/>
    <LinearLayout
        orientation="vertical"
        id="@+id/ll_container"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="22dp"
        layout_marginBottom="20dp"
        layout_marginStart="20dp"
        layout_marginEnd="24dp"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="@+id/iv_feeds_widget_profile"
        NOT_FOUND_0x13="0">
        <TextView
            ellipsize="end"
            id="@+id/tv_feeds_widget_activities"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginBottom="4dp"
            maxLines="2"
            style="@style/2132017439"/>
        <TextView
            textSize="12sp"
            id="@+id/saveMonthOfFetchFreeTransfer"
            layout_width="match_parent"
            layout_height="wrap_content"
            NOT_FOUND_0x10="@+id/iv_feeds_widget_profile"
            NOT_FOUND_0x12="@+id/tv_feeds_widget_activities"/>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
