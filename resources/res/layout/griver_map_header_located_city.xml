<?xml version="1.0" encoding="utf-8"?>
<com.alibaba.griver.ui.ant.layout.AURelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    background="@drawable/griver_map_location_list_bg_selector"
    layout_width="match_parent"
    layout_height="64dp"
    minHeight="64dp">
    <com.alibaba.griver.ui.ant.text.AUTextView
        textSize="16dp"
        textColor="#333333"
        id="@+id/poiselect_header_title"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_marginRight="78dp"
        singleLine="true"
        layout_centerVertical="true"/>
    <com.alibaba.griver.ui.ant.layout.AULinearLayout
        gravity="right"
        orientation="horizontal"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_alignParentRight="true"
        layout_centerVertical="true">
        <com.alibaba.griver.ui.ant.AUImageView
            id="@+id/poiselect_selection_mark"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginRight="16dp"
            src="@drawable/griver_map_icon_selection"/>
        <com.alibaba.griver.ui.ant.AUImageView
            id="@+id/action_goto"
            visibility="gone"
            layout_width="19dp"
            layout_height="19dp"
            layout_marginRight="16dp"
            src="@drawable/griver_map_icon_goto_2"/>
    </com.alibaba.griver.ui.ant.layout.AULinearLayout>
    <com.alibaba.griver.ui.ant.AUImageView
        background="#eeeeee"
        layout_width="match_parent"
        layout_height="1px"
        layout_alignParentBottom="true"/>
</com.alibaba.griver.ui.ant.layout.AURelativeLayout>
