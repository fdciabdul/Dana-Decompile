<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/container_loading"
    layout_width="match_parent"
    layout_height="match_parent">
    <include
        id="@+id/toolbar"
        NOT_FOUND_0x8="0"
        NOT_FOUND_0x9="0"
        NOT_FOUND_0xb="0"
        layout="@layout/carrier_identification_toolbar"/>
    <com.airbnb.lottie.LottieAnimationView
        id="@+id/lv_avatar_user"
        layout_width="200dp"
        layout_height="200dp"
        layout_marginTop="60dp"
        NOT_FOUND_0x8="0"
        NOT_FOUND_0x9="0"
        NOT_FOUND_0xa="@+id/toolbar"
        NOT_FOUND_0xc="true"
        NOT_FOUND_0xd="json/lottie/verifying_boku.lottie"
        NOT_FOUND_0xe="true"/>
    <androidx.appcompat.widget.AppCompatTextView
        gravity="center"
        id="@+id/tv_title_verify"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="24dp"
        text="@string/carrier_identification_verify_loading_title"
        layout_marginStart="20dp"
        layout_marginEnd="20dp"
        NOT_FOUND_0x8="0"
        NOT_FOUND_0x9="0"
        NOT_FOUND_0xa="@+id/lv_avatar_user"
        style="@style/2132017931"/>
    <androidx.appcompat.widget.AppCompatTextView
        gravity="center"
        id="@+id/tv_desc_verify"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        text="@string/carrier_identification_verify_loading_desc"
        layout_marginStart="20dp"
        layout_marginEnd="20dp"
        NOT_FOUND_0x8="0"
        NOT_FOUND_0x9="0"
        NOT_FOUND_0xa="@+id/tv_title_verify"
        style="@style/2132017412"/>
</androidx.constraintlayout.widget.ConstraintLayout>
