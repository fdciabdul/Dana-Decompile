<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/vg_home_widget_base_container"
    paddingTop="4dp"
    paddingBottom="4dp"
    clipChildren="false"
    clipToPadding="false"
    layout_width="match_parent"
    layout_height="wrap_content"
    paddingStart="12dp"
    paddingEnd="12dp">
    <LinearLayout
        orientation="vertical"
        id="@+id/ll_home_widget_base"
        background="@drawable/bg_home_widget_white_rounded"
        paddingTop="12dp"
        paddingBottom="12dp"
        layout_width="match_parent"
        layout_height="wrap_content">
        <androidx.constraintlayout.widget.ConstraintLayout
            id="@+id/cl_base_home_widget"
            layout_width="match_parent"
            layout_height="match_parent">
            <TextView
                id="@+id/tv_widget_title"
                layout_width="wrap_content"
                layout_height="wrap_content"
                contentDescription="@string/lblFeedTitle"
                layout_marginStart="12dp"
                NOT_FOUND_0x10="0"
                NOT_FOUND_0x12="0"
                style="@style/2132017916"/>
            <TextView
                id="@+id/tv_widget_description"
                layout_width="0dp"
                layout_height="wrap_content"
                contentDescription="@string/lblFeedSubTitle"
                layout_marginStart="12dp"
                NOT_FOUND_0xf="@+id/ReferralMapper_Factory_res_0x7f0a024f"
                NOT_FOUND_0x10="0"
                NOT_FOUND_0x11="@+id/tv_widget_title"
                style="@style/2132017439"/>
            <id.dana.component.buttoncomponent.DanaButtonSecondaryView
                id="@+id/ReferralMapper_Factory_res_0x7f0a024f"
                layout_width="88dp"
                layout_height="wrap_content"
                contentDescription="@string/btnExplore"
                layout_marginEnd="12dp"
                NOT_FOUND_0xe="0"
                NOT_FOUND_0x12="0"
                NOT_FOUND_0x13="10"
                NOT_FOUND_0x14="1"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </LinearLayout>
</FrameLayout>
