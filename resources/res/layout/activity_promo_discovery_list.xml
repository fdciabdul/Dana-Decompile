<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="#f5f5f5"
    layout_width="match_parent"
    layout_height="match_parent">
    <include
        id="@+id/tb_promo_center"
        layout_width="0dp"
        layout_height="wrap_content"
        NOT_FOUND_0x1c="@+id/pcv_promo_center"
        NOT_FOUND_0x1d="0"
        NOT_FOUND_0x20="0"
        NOT_FOUND_0x22="0"
        layout="@layout/layout_toolbar"/>
    <id.dana.richview.promocenter.PromoCategoryView
        id="@+id/pcv_promo_center"
        layout_width="0dp"
        layout_height="wrap_content"
        contentDescription="@string/cd_promo_center_category_filter"
        NOT_FOUND_0x1c="@+id/rv_regular_promo"
        NOT_FOUND_0x1d="0"
        NOT_FOUND_0x20="0"
        NOT_FOUND_0x21="@+id/tb_promo_center"/>
    <androidx.recyclerview.widget.RecyclerView
        id="@+id/rv_regular_promo"
        clipToPadding="false"
        layout_width="match_parent"
        layout_height="0dp"
        layout_marginTop="4dp"
        NOT_FOUND_0x1c="@+id/pcb_view"
        NOT_FOUND_0x1d="0"
        NOT_FOUND_0x20="0"
        NOT_FOUND_0x21="@+id/pcv_promo_center"
        NOT_FOUND_0x23="1"/>
    <View
        background="@drawable/bg_top_shadow_gradient"
        layout_width="match_parent"
        layout_height="8dp"
        NOT_FOUND_0x1c="@+id/pcb_view"/>
    <id.dana.promocenter.views.PromoCenterBottomMenuView
        id="@+id/pcb_view"
        layout_width="match_parent"
        layout_height="wrap_content"
        NOT_FOUND_0x1b="0"/>
    <LinearLayout
        gravity="center"
        orientation="vertical"
        id="@+id/res_0x7f0a0ddb_daggernewservicecardcomponent_newservicecardcomponentimpl_threadexecutorprovider"
        background="#f5f5f5"
        layout_width="match_parent"
        layout_height="0dp"
        NOT_FOUND_0x1c="@+id/pcb_view"
        NOT_FOUND_0x1e="0"
        NOT_FOUND_0x1f="0"
        NOT_FOUND_0x21="@+id/pcv_promo_center"
        NOT_FOUND_0x23="1">
        <ImageView
            layout_gravity="center_horizontal"
            id="@+id/iv_error_promocenter"
            layout_width="160dp"
            layout_height="160dp"
            src="@drawable/ic_no_connection"/>
        <TextView
            textSize="20sp"
            textStyle="bold"
            textColor="#000000"
            id="@+id/getFeatureForcedFirstCard"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="16dp"
            text="@string/error_promo_title"
            fontFamily="@font/2131296266"
            textAlignment="center"/>
        <TextView
            textSize="16sp"
            textColor="#727272"
            id="@+id/tv_error_description"
            paddingLeft="16sp"
            paddingRight="16sp"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="8dp"
            text="@string/error_promo_description"
            fontFamily="@font/2131296265"
            textAlignment="center"
            paddingStart="16sp"
            paddingEnd="16sp"
            layout_marginStart="26dp"
            layout_marginEnd="26dp"/>
    </LinearLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        id="@+id/clPromoErrorButton"
        background="#ffffff"
        paddingBottom="34dp"
        visibility="gone"
        clipToPadding="false"
        layout_width="match_parent"
        layout_height="wrap_content"
        NOT_FOUND_0x1b="0">
        <View
            id="@+id/separator"
            background="#d1d1d1"
            layout_width="match_parent"
            layout_height="1dp"
            NOT_FOUND_0x1d="0"
            NOT_FOUND_0x20="0"
            NOT_FOUND_0x22="0"/>
        <id.dana.component.buttoncomponent.DanaButtonPrimaryView
            id="@+id/btnError"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginLeft="16dp"
            layout_marginTop="9dp"
            layout_marginRight="16dp"
            NOT_FOUND_0x21="@+id/separator"
            NOT_FOUND_0x24="Back"
            NOT_FOUND_0x25="1"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
