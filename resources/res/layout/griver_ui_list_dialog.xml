<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    orientation="vertical"
    id="@+id/dialog_bg"
    background="@drawable/griver_ui_dialog_bg"
    layout_width="match_parent"
    layout_height="wrap_content">
    <include
        id="@+id/title_container"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout="@layout/griver_ui_list_dialog_head"/>
    <View
        id="@+id/head_divider"
        background="#dddddd"
        visibility="gone"
        layout_width="match_parent"
        layout_height="1px"/>
    <com.alibaba.griver.ui.ant.listview.AUMaxItemCornerListView
        scrollbarThumbVertical="@drawable/griver_ui_list_dialog_scrollbar_thumb"
        id="@+id/dialog_listView"
        focusable="true"
        focusableInTouchMode="true"
        scrollbars="vertical"
        layout_width="match_parent"
        layout_height="wrap_content"
        listSelector="@android:color/transparent"
        divider="#dddddd"
        dividerHeight="1px"
        overScrollMode="never"/>
    <View
        id="@+id/bottom_divider"
        background="#dddddd"
        visibility="gone"
        layout_width="match_parent"
        layout_height="1px"/>
    <include
        id="@+id/bottom_container"
        visibility="gone"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout="@layout/griver_ui_base_dialog_button"/>
</LinearLayout>
