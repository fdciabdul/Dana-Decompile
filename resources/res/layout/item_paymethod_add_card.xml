<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="?android:attr/selectableItemBackground"
    padding="16dp"
    focusable="true"
    clickable="true"
    layout_width="match_parent"
    layout_height="wrap_content">
    <ImageView
        id="@+id/SendMoneyHomePresenter"
        layout_width="40dp"
        layout_height="40dp"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x19="0"
        NOT_FOUND_0x1a="@drawable/ic_add_debit_card"/>
    <TextView
        textSize="16sp"
        textColor="#4a4a4a"
        ellipsize="end"
        id="@+id/tv_title"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginLeft="12dp"
        text="@string/add_new_debit_card"
        maxLines="1"
        fontFamily="@font/2131296266"
        layout_marginStart="12dp"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x15="@+id/SendMoneyHomePresenter"
        NOT_FOUND_0x16="@+id/rb_selected_card"
        NOT_FOUND_0x19="0"/>
    <ImageView
        id="@+id/iv_prefix_description"
        layout_width="wrap_content"
        layout_height="0dp"
        layout_marginLeft="12dp"
        layout_marginTop="4dp"
        layout_marginStart="12dp"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x15="@+id/SendMoneyHomePresenter"
        NOT_FOUND_0x18="@+id/tv_title"/>
    <TextView
        textSize="12sp"
        id="@+id/tv_divider"
        paddingLeft="4dp"
        paddingRight="4dp"
        layout_width="0dp"
        layout_height="wrap_content"
        text="@string/divider"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x15="@+id/iv_prefix_description"
        NOT_FOUND_0x18="@+id/tv_title"/>
    <TextView
        textSize="12sp"
        textColor="#4a4a4a"
        id="@+id/tv_description"
        layout_width="0dp"
        layout_height="wrap_content"
        text="@string/add_and_use_this_card"
        fontFamily="@font/2131296265"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x15="@+id/tv_divider"
        NOT_FOUND_0x16="@+id/rb_selected_card"
        NOT_FOUND_0x18="@+id/tv_title"/>
    <RadioButton
        id="@+id/rb_selected_card"
        layout_width="wrap_content"
        layout_height="0dp"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x19="0"
        style="@style/2132017498"/>
</androidx.constraintlayout.widget.ConstraintLayout>
