<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="#fafafa"
    style="@style/2132017531">
    <androidx.core.widget.NestedScrollView
        id="@+id/nsv_profile"
        layout_width="0dp"
        layout_height="0dp"
        fillViewport="true"
        NOT_FOUND_0x18="0"
        NOT_FOUND_0x19="0"
        NOT_FOUND_0x1a="0"
        NOT_FOUND_0x1b="0">
        <LinearLayout
            orientation="vertical"
            background="#f5f5f5"
            descendantFocusability="blocksDescendants"
            style="@style/2132017490">
            <id.dana.richview.SimpleProfileWithQrView
                id="@+id/profile"
                style="@style/2132017490"/>
            <Button
                textSize="16sp"
                textColor="#f8f8f8"
                id="@+id/btn_verify"
                background="@drawable/btn_topup_blue"
                paddingLeft="16dp"
                paddingTop="5dp"
                paddingRight="16dp"
                paddingBottom="5dp"
                visibility="gone"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginTop="8dp"
                layout_marginBottom="10dp"
                drawableRight="@drawable/kyc_default_png"
                contentDescription="@string/btn_kyc"
                textAllCaps="true"
                drawableEnd="@drawable/kyc_default_png"
                layout_marginStart="16dp"
                layout_marginEnd="16dp"
                style="@style/2132017915"/>
            <View style="@style/2132017967"/>
            <androidx.recyclerview.widget.RecyclerView
                id="@+id/rv_profile_setting"
                scrollbars="vertical"
                layout_marginBottom="120dp"
                overScrollMode="never"
                style="@style/2132017490"/>
        </LinearLayout>
    </androidx.core.widget.NestedScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>
