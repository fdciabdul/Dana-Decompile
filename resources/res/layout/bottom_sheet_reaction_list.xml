<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    orientation="vertical"
    id="@+id/vg_reaction_list_root"
    background="@drawable/rounded_top_background"
    layout_width="match_parent"
    layout_height="wrap_content">
    <ImageView
        layout_gravity="center_horizontal"
        id="@+id/secondaryHash_res_0x7f0a08d3"
        layout_width="56dp"
        layout_height="4dp"
        layout_marginLeft="8dp"
        layout_marginTop="8dp"
        layout_marginRight="8dp"
        layout_marginStart="8dp"
        layout_marginEnd="8dp"
        NOT_FOUND_0x14="@drawable/ic_flat_dark"/>
    <TextView
        textAppearance="?attr/2130970172"
        textStyle="bold"
        id="@+id/tv_reaction_list_title"
        paddingBottom="8dp"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_marginTop="10dp"
        text="@string/lbl_add_saymoji"/>
    <androidx.recyclerview.widget.RecyclerView
        id="@+id/rv_reaction_list"
        paddingLeft="10dp"
        paddingTop="8dp"
        paddingRight="8dp"
        paddingBottom="16dp"
        clipToPadding="false"
        layout_width="match_parent"
        layout_height="wrap_content"
        minHeight="185dp"/>
</LinearLayout>
