<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/viewRiskAmount"
    layout_width="match_parent"
    layout_height="wrap_content">
    <include
        id="@+id/toolbar"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x12="0"
        layout="@layout/view_cashier_toolbar"/>
    <include
        id="@+id/cancelEventDelivery"
        layout_width="match_parent"
        layout_height="wrap_content"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="@+id/toolbar"
        layout="@layout/view_cashier_header"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        id="@+id/clTotalAmount"
        background="#ffffff"
        layout_width="match_parent"
        layout_height="wrap_content"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="@+id/cancelEventDelivery">
        <androidx.appcompat.widget.AppCompatTextView
            id="@+id/appCompatTextView"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="20dp"
            layout_marginBottom="20dp"
            text="@string/total_price"
            layout_marginStart="16dp"
            NOT_FOUND_0xd="0"
            NOT_FOUND_0x10="0"
            NOT_FOUND_0x12="0"
            style="@style/2132017413"/>
        <androidx.appcompat.widget.AppCompatTextView
            gravity="end"
            id="@+id/tvAmount"
            layout_width="0dp"
            layout_height="wrap_content"
            textAlignment="textEnd"
            layout_marginStart="16dp"
            layout_marginEnd="16dp"
            NOT_FOUND_0xd="@+id/appCompatTextView"
            NOT_FOUND_0xe="0"
            NOT_FOUND_0xf="@+id/appCompatTextView"
            NOT_FOUND_0x12="@+id/appCompatTextView"
            style="@style/2132017953"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.Group
        id="@+id/groupRiskTotalAmountView"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        NOT_FOUND_0xc="layoutHeader, clTotalAmount"/>
</androidx.constraintlayout.widget.ConstraintLayout>
