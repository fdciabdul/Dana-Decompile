<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="#008ef0"
    padding="16dp"
    layout_width="match_parent"
    layout_height="wrap_content">
    <ImageView
        id="@+id/iv_hide_balance"
        layout_width="32dp"
        layout_height="32dp"
        NOT_FOUND_0x9="@+id/tv_hideable_balance"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0x10="@+id/tv_hideable_balance"
        NOT_FOUND_0x11="@drawable/ic_visibility"/>
    <ImageView
        id="@+id/iv_hide_balance_loading"
        visibility="gone"
        layout_width="32dp"
        layout_height="32dp"
        NOT_FOUND_0x9="@+id/tv_hideable_balance"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0x10="@+id/tv_hideable_balance"
        NOT_FOUND_0x11="@drawable/ic_visibility_disabled"/>
    <id.dana.richview.HideableBalanceView
        id="@+id/tv_hideable_balance"
        layout_width="0dp"
        layout_height="wrap_content"
        NOT_FOUND_0xa="@+id/iv_hide_balance"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xc="@+id/iv_hide_balance"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0xf="@+id/computeBackingRowMap"/>
    <TextView
        id="@+id/PushVerifyPreferenceEntityDataFactory"
        visibility="gone"
        layout_width="0dp"
        layout_height="32dp"
        layout_marginTop="4dp"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xc="@+id/iv_hide_balance"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0xf="@+id/computeBackingRowMap"/>
    <TextView
        id="@+id/computeBackingRowMap"
        layout_width="wrap_content"
        layout_height="wrap_content"
        text="@string/total_savings"
        maxLines="1"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x10="0"
        style="@style/2132017438"/>
</androidx.constraintlayout.widget.ConstraintLayout>
