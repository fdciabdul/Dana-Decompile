<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    layout_width="match_parent"
    layout_height="match_parent">
    <ImageView
        id="@+id/com_mixpanel_android_notification_gradient"
        layout_width="match_parent"
        layout_height="match_parent"
        contentDescription="@string/com_mixpanel_android_notification_image"/>
    <LinearLayout
        id="@+id/com_mixpanel_android_button_exit_wrapper"
        padding="10dp"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_alignParentRight="true">
        <ImageView
            id="@+id/com_mixpanel_android_image_close"
            layout_width="23dp"
            layout_height="23dp"
            src="@drawable/com_mixpanel_android_close_new"
            contentDescription="@string/com_mixpanel_android_close"/>
    </LinearLayout>
    <LinearLayout
        orientation="vertical"
        id="@+id/com_mixpanel_android_notification_bottom_wrapper"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="0dp"
        layout_alignParentBottom="true">
        <TextView
            textSize="22sp"
            textStyle="bold"
            textColor="#ffffff"
            gravity="center_horizontal"
            id="@+id/res_0x7f0a04f6_networkuserentitydata_externalsyntheticlambda0"
            paddingLeft="20dp"
            paddingRight="20dp"
            layout_width="match_parent"
            layout_height="wrap_content"/>
        <TextView
            textSize="17sp"
            textColor="#ffffff"
            gravity="center_horizontal"
            id="@+id/com_mixpanel_android_notification_subtext"
            paddingLeft="20dp"
            paddingRight="20dp"
            layout_width="match_parent"
            layout_height="wrap_content"/>
        <LinearLayout
            gravity="center_horizontal"
            orientation="horizontal"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginLeft="30dp"
            layout_marginTop="18dp"
            layout_marginRight="30dp"
            layout_marginBottom="40dp"
            weightSum="2">
            <Button
                textSize="17sp"
                textStyle="bold"
                textColor="@android:color/white"
                ellipsize="end"
                layout_gravity="center"
                id="@+id/com_mixpanel_android_notification_button"
                paddingLeft="20dp"
                paddingTop="10dp"
                paddingRight="20dp"
                paddingBottom="10dp"
                layout_width="0dp"
                layout_height="wrap_content"
                minWidth="150dp"
                text="@string/com_mixpanel_android_done"
                maxLines="1"
                layout_weight="1"/>
            <Button
                textSize="17sp"
                textStyle="bold"
                textColor="@android:color/white"
                ellipsize="end"
                layout_gravity="center"
                id="@+id/com_mixpanel_android_notification_second_button"
                paddingLeft="20dp"
                paddingTop="10dp"
                paddingRight="20dp"
                paddingBottom="10dp"
                layout_width="0dp"
                layout_height="wrap_content"
                layout_marginLeft="15dp"
                minWidth="150dp"
                text="@string/com_mixpanel_android_done"
                maxLines="1"
                layout_weight="1"/>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        gravity="center"
        orientation="horizontal"
        layout_width="match_parent"
        layout_height="match_parent"
        layout_marginTop="50dp"
        layout_above="@+id/com_mixpanel_android_notification_bottom_wrapper">
        <com.mixpanel.android.takeoverinapp.FadingImageView
            id="@+id/com_mixpanel_android_notification_image"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginLeft="35dp"
            layout_marginRight="35dp"
            src="@drawable/com_mixpanel_android_logo"
            contentDescription="@string/com_mixpanel_android_notification_image"/>
    </LinearLayout>
</RelativeLayout>
