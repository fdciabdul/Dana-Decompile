<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="#bf000000"
    layout_width="match_parent"
    layout_height="match_parent">
    <Button
        textColor="#ffffff"
        id="@+id/btnOkCardRotateTips"
        background="@drawable/bg_button_secondary_white"
        layout_width="110dp"
        layout_height="wrap_content"
        layout_marginTop="16dp"
        text="@string/card_binding_ok_button"
        fontFamily="@font/sf_pro_regular"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="@+id/ivCardRotateCircle"/>
    <TextView
        id="@+id/tvContentTips"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginBottom="48dp"
        text="@string/card_binding_tips_content"
        textAlignment="center"
        layout_marginStart="32dp"
        layout_marginEnd="32dp"
        NOT_FOUND_0xd="@+id/ivCardRotateTips"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0xf="0"
        style="@style/2132017438"/>
    <TextView
        id="@+id/tvHeaderTips"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="120dp"
        layout_marginBottom="16dp"
        text="@string/card_binding_tips_title"
        NOT_FOUND_0xd="@+id/tvContentTips"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0xf="0"
        style="@style/2132017947"/>
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/ivCardRotateCircle"
        layout_marginTop="16dp"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="@+id/ivCardRotateTips"
        NOT_FOUND_0x12="@drawable/ic_card_rotate_circle"
        style="@style/2132018187"/>
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/ivCardRotateTips"
        layout_width="150dp"
        layout_height="150dp"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="@drawable/ic_card_rotate_tips"/>
</androidx.constraintlayout.widget.ConstraintLayout>
