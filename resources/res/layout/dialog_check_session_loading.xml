<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="@drawable/bg_rounded_white_12dp"
    paddingBottom="26dp"
    layout_width="match_parent"
    layout_height="match_parent">
    <com.airbnb.lottie.LottieAnimationView
        id="@+id/loading_animation"
        layout_width="100dp"
        layout_height="100dp"
        layout_marginTop="26dp"
        layout_marginStart="50dp"
        layout_marginEnd="50dp"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="true"
        NOT_FOUND_0x11="json/lottie/check_session_loading.lottie"
        NOT_FOUND_0x12="true"/>
    <androidx.appcompat.widget.AppCompatTextView
        textSize="12sp"
        gravity="center"
        layout_width="176dp"
        layout_height="wrap_content"
        layout_marginTop="16dp"
        text="@string/warm_start_loading_description"
        fontFamily="@font/sf_pro_regular"
        layout_marginStart="12dp"
        layout_marginEnd="12dp"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0xe="@+id/loading_animation"/>
</androidx.constraintlayout.widget.ConstraintLayout>
