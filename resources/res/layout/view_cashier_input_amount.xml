<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="#ffffff"
    paddingLeft="16dp"
    paddingTop="8dp"
    paddingRight="16dp"
    paddingBottom="8dp"
    layout_width="match_parent"
    layout_height="wrap_content">
    <TextView
        textStyle="bold"
        id="@+id/tvCurrency"
        layout_width="wrap_content"
        layout_height="wrap_content"
        text="@string/rupiah_short"
        NOT_FOUND_0x15="@+id/ivCurrencyBorder"
        NOT_FOUND_0x16="@+id/ivCurrencyBorder"
        NOT_FOUND_0x18="@+id/ivCurrencyBorder"
        NOT_FOUND_0x1a="@+id/ivCurrencyBorder"
        style="@style/2132017405"/>
    <TextView
        id="@+id/ContentDeliveryCacheEntityDataFactory"
        layout_width="0dp"
        layout_height="wrap_content"
        text="@string/price_amount"
        layout_marginStart="12dp"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x17="@+id/ivCurrencyBorder"
        NOT_FOUND_0x1a="0"
        style="@style/2132017901"/>
    <TextView
        id="@+id/tvErrorHelper"
        layout_width="0dp"
        layout_height="wrap_content"
        text="@string/input_amount_note"
        NOT_FOUND_0x14="@drawable/ic_info_grey_40"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x18="@+id/ContentDeliveryCacheEntityDataFactory"
        NOT_FOUND_0x19="@+id/etAmount"
        style="@style/2132017440"/>
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/ivCurrencyBorder"
        layout_width="30dp"
        layout_height="30dp"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x18="0"
        NOT_FOUND_0x1a="0"
        NOT_FOUND_0x1b="@drawable/bg_transparent_circle_grey20_8dp"/>
    <id.dana.core.ui.richview.CurrencyEditText
        enabled="true"
        textSize="27sp"
        textStyle="bold"
        id="@+id/etAmount"
        paddingTop="4dp"
        longClickable="false"
        layout_width="0dp"
        layout_height="wrap_content"
        maxLines="1"
        android:maxLength="12"
        inputType="numberPassword"
        imeOptions="actionDone"
        textCursorDrawable="@drawable/cursor_tangerine"
        paddingStart="0dp"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x18="@+id/ContentDeliveryCacheEntityDataFactory"
        NOT_FOUND_0x19="@+id/ContentDeliveryCacheEntityDataFactory"/>
</androidx.constraintlayout.widget.ConstraintLayout>
