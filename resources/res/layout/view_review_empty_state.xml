<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_gravity="center_horizontal"
    orientation="vertical"
    background="@android:color/white"
    paddingTop="16dp"
    paddingBottom="16dp"
    layout_width="match_parent"
    layout_height="wrap_content">
    <androidx.appcompat.widget.AppCompatImageView
        layout_width="match_parent"
        layout_height="wrap_content"
        NOT_FOUND_0xd="@drawable/ic_review_empty_state"/>
    <TextView
        gravity="center_horizontal"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        text="@string/review_empty_state_title"
        layout_marginStart="16dp"
        layout_marginEnd="16dp"
        style="@style/2132017916"/>
    <TextView
        gravity="center"
        layout_gravity="center"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        text="@string/review_empty_state_message"
        lineSpacingExtra="4sp"
        layout_marginStart="16dp"
        layout_marginEnd="16dp"
        style="@style/2132017406"/>
</LinearLayout>
