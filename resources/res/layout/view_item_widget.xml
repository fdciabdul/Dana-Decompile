<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/cv_widget_item"
    focusable="true"
    visibility="gone"
    clickable="true"
    layout_margin="8dp"
    NOT_FOUND_0x13="6dp"
    NOT_FOUND_0x14="0dp"
    style="@style/2132017490">
    <androidx.constraintlayout.widget.ConstraintLayout
        id="@+id/cv_content"
        background="@drawable/home_component_border"
        paddingBottom="12dp"
        style="@style/2132017490">
        <ImageView
            id="@+id/iv_widget_background"
            layout_width="match_parent"
            layout_height="wrap_content"
            scaleType="centerCrop"
            maxHeight="170dp"
            minHeight="154dp"
            NOT_FOUND_0x15="0"
            NOT_FOUND_0x16="0"
            NOT_FOUND_0x18="0"/>
        <TextView
            id="@+id/tv_widget_owner"
            background="@drawable/bg_white_round_bottom_left"
            padding="7dp"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            drawablePadding="2dp"
            NOT_FOUND_0x15="0"
            NOT_FOUND_0x18="0"
            style="@style/2132017431"/>
        <TextView
            ellipsize="end"
            id="@+id/tv_widget_title"
            visibility="gone"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginLeft="12dp"
            layout_marginTop="8dp"
            layout_marginRight="12dp"
            lines="2"
            NOT_FOUND_0x16="0"
            NOT_FOUND_0x17="@+id/iv_widget_background"
            style="@style/2132017916"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>
