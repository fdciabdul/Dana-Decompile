<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/item_pay_later"
    layout_width="match_parent"
    layout_height="wrap_content">
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/ivIconAsset"
        layout_width="36dp"
        layout_height="28dp"
        layout_marginStart="56dp"
        NOT_FOUND_0xe="@+id/isHelperTextDisplayed"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x15="@+id/tvTitle"
        NOT_FOUND_0x17="@drawable/res_0x7f080891_networkuserentitydata_externalsyntheticlambda7"/>
    <androidx.appcompat.widget.AppCompatTextView
        id="@+id/tvSecondaryDescription"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        layout_marginStart="8dp"
        layout_marginEnd="4dp"
        NOT_FOUND_0xf="@+id/divider"
        NOT_FOUND_0x11="@+id/rgPaymentMethod"
        NOT_FOUND_0x12="@+id/ivIconAsset"
        NOT_FOUND_0x14="@+id/isHelperTextDisplayed"/>
    <androidx.appcompat.widget.AppCompatTextView
        id="@+id/tvTitle"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="9.5dp"
        layout_marginStart="8dp"
        layout_marginEnd="4dp"
        NOT_FOUND_0xf="@+id/tvTitle"
        NOT_FOUND_0x11="@+id/tvNew"
        NOT_FOUND_0x12="@+id/ivIconAsset"
        NOT_FOUND_0x15="0"
        style="@style/2132017440"/>
    <androidx.appcompat.widget.AppCompatTextView
        textSize="10sp"
        textColor="#ffffff"
        id="@+id/tvNewOrPromo"
        background="@drawable/bg_rounded_blue"
        visibility="invisible"
        fontFamily="@font/2131296265"
        paddingStart="4dp"
        paddingEnd="4dp"
        layout_marginStart="4dp"
        NOT_FOUND_0xe="@+id/tvTitle"
        NOT_FOUND_0x12="@+id/tvTitle"
        NOT_FOUND_0x15="@+id/tvTitle"
        style="@style/2132018188"/>
    <androidx.appcompat.widget.AppCompatTextView
        id="@+id/isHelperTextDisplayed"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        layout_marginStart="8dp"
        layout_marginEnd="4dp"
        NOT_FOUND_0x11="@+id/rgPaymentMethod"
        NOT_FOUND_0x12="@+id/ivIconAsset"
        NOT_FOUND_0x14="@+id/tvTitle"
        style="@style/2132017413"/>
    <RadioButton
        id="@+id/rgPaymentMethod"
        layout_width="wrap_content"
        layout_height="0dp"
        button="@drawable/selector_cashier_button_radio"
        layout_marginEnd="16dp"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x16="0"/>
    <View
        id="@+id/divider"
        background="#b8b8b8"
        layout_width="match_parent"
        layout_height="0.5dp"
        layout_marginTop="16dp"
        layout_marginStart="16dp"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x14="@+id/tvSecondaryDescription"/>
</androidx.constraintlayout.widget.ConstraintLayout>
