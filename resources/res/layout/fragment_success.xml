<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_width="match_parent"
    layout_height="match_parent">
    <TextView
        textSize="30sp"
        textColor="#222b45"
        gravity="center_horizontal"
        id="@+id/tvTitle"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="60dp"
        textAlignment="center"
        NOT_FOUND_0x9="@+id/ivIcon"
        NOT_FOUND_0xa="0"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0xe="0.95"/>
    <ImageView
        id="@+id/ivIcon"
        layout_width="78dp"
        layout_height="78dp"
        layout_marginTop="45dp"
        src="@drawable/ic_success"
        NOT_FOUND_0x9="@+id/tvRedirectMessage"
        NOT_FOUND_0xa="0"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xc="@+id/tvTitle"/>
    <TextView
        textSize="@dimen/2131166065"
        textColor="@android:color/black"
        gravity="center"
        id="@+id/tvRedirectMessage"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="45dp"
        NOT_FOUND_0xa="0"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xc="@+id/ivIcon"
        NOT_FOUND_0xe="0.8"/>
</androidx.constraintlayout.widget.ConstraintLayout>
