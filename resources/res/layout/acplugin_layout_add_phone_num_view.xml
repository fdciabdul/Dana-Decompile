<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    gravity="center_vertical"
    orientation="horizontal"
    id="@+id/add_phone_num_view"
    background="@drawable/acplugin_phone_num_list_bg"
    layout_width="match_parent"
    layout_height="64dp">
    <ImageView
        layout_width="20dp"
        layout_height="20dp"
        layout_marginLeft="16dp"
        layout_marginRight="10dp"
        src="@drawable/acplugin_add_phone_num_icon"/>
    <TextView
        textSize="16dp"
        textColor="#131314"
        id="@+id/add_phone_num_text"
        layout_width="0dp"
        layout_height="wrap_content"
        text="@string/acplugin_add_phone"
        includeFontPadding="false"
        layout_weight="1"/>
    <ImageView
        layout_width="22dp"
        layout_height="22dp"
        layout_marginLeft="12dp"
        layout_marginRight="16dp"
        src="@drawable/acplugin_list_arrow"/>
</LinearLayout>
