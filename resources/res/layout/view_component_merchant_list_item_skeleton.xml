<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/cl_merchant_skeleton"
    paddingTop="16dp"
    paddingBottom="16dp"
    layout_width="match_parent"
    layout_height="wrap_content">
    <View
        id="@+id/view_merchant_logo_skeleton"
        background="@drawable/bg_rounded_4dp_grey30"
        layout_width="64dp"
        layout_height="64dp"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xe="0"/>
    <View
        id="@+id/view_merchant_name_skeleton"
        background="@drawable/bg_rounded_4dp_grey30"
        layout_width="180dp"
        layout_height="18dp"
        layout_marginStart="16dp"
        layout_marginEnd="78dp"
        NOT_FOUND_0xb="@+id/view_merchant_logo_skeleton"
        NOT_FOUND_0xe="0"/>
    <View
        id="@+id/view_rating_skeleton"
        background="@drawable/bg_rounded_4dp_grey30"
        layout_width="48dp"
        layout_height="24dp"
        NOT_FOUND_0xa="0"
        NOT_FOUND_0xe="0"/>
    <View
        id="@+id/view_merchant_category_skeleton"
        background="@drawable/bg_rounded_4dp_grey30"
        layout_width="130dp"
        layout_height="16dp"
        layout_marginTop="4dp"
        layout_marginStart="16dp"
        NOT_FOUND_0xb="@+id/view_merchant_logo_skeleton"
        NOT_FOUND_0xd="@+id/view_merchant_name_skeleton"/>
    <View
        id="@+id/view_white_divider_skeleton"
        background="@drawable/bg_rounded_4dp_grey30"
        layout_width="0dp"
        layout_height="0.5dp"
        layout_marginTop="8dp"
        layout_marginStart="16dp"
        layout_marginEnd="16dp"
        NOT_FOUND_0xa="0"
        NOT_FOUND_0xb="@+id/view_merchant_logo_skeleton"
        NOT_FOUND_0xd="@+id/view_merchant_category_skeleton"/>
    <View
        id="@+id/view_merchant_address_skeleton"
        background="@drawable/bg_rounded_4dp_grey30"
        layout_width="235dp"
        layout_height="32dp"
        layout_marginTop="8dp"
        layout_marginStart="16dp"
        NOT_FOUND_0xb="@+id/view_merchant_logo_skeleton"
        NOT_FOUND_0xd="@+id/view_white_divider_skeleton"/>
    <View
        id="@+id/view_marker_icon_skeleton"
        background="@drawable/bg_rounded_4dp_grey30"
        layout_width="57dp"
        layout_height="16dp"
        layout_marginTop="8dp"
        layout_marginStart="16dp"
        NOT_FOUND_0x9="0"
        NOT_FOUND_0xb="@+id/view_merchant_logo_skeleton"
        NOT_FOUND_0xd="@+id/view_merchant_address_skeleton"/>
    <View
        id="@+id/view_top_up_ready_skeleton"
        background="@drawable/bg_rounded_4dp_grey30"
        layout_width="72dp"
        layout_height="16dp"
        layout_marginTop="8dp"
        layout_marginStart="12dp"
        NOT_FOUND_0xb="@+id/view_marker_icon_skeleton"
        NOT_FOUND_0xd="@+id/view_merchant_address_skeleton"/>
</androidx.constraintlayout.widget.ConstraintLayout>
