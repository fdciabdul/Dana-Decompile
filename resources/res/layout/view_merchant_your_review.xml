<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    orientation="vertical"
    id="@+id/res_0x7f0a0de3_swipehelper_underlaybuttonclicklistener"
    layout_width="match_parent"
    layout_height="wrap_content">
    <LinearLayout
        orientation="horizontal"
        id="@+id/ll_user"
        layout_width="match_parent"
        layout_height="wrap_content">
        <ImageView
            id="@+id/iv_avatar_user"
            background="@drawable/shape_oval_grey"
            layout_width="36dp"
            layout_height="37dp"
            adjustViewBounds="true"/>
        <FrameLayout
            layout_width="0dp"
            layout_height="wrap_content"
            layout_weight="1">
            <LinearLayout
                orientation="vertical"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginStart="5dp">
                <TextView
                    textSize="14sp"
                    textColor="#000000"
                    gravity="center"
                    id="@+id/getSecondaryProgressTintMode"
                    layout_width="wrap_content"
                    layout_height="wrap_content"
                    layout_marginStart="5dp"
                    style="@style/2132017901"/>
                <TextView
                    id="@+id/saveMonthOfFetchFreeTransfer"
                    layout_width="match_parent"
                    layout_height="wrap_content"
                    layout_marginStart="5dp"
                    style="@style/2132017439"/>
            </LinearLayout>
        </FrameLayout>
        <FrameLayout
            layout_width="wrap_content"
            layout_height="wrap_content">
            <id.dana.nearbyme.merchantreview.ViewRatingStarBar
                id="@+id/rating_star_bar"
                layout_width="132dp"
                layout_height="36dp"
                NOT_FOUND_0xd="9dp"
                NOT_FOUND_0xe="13dp"
                NOT_FOUND_0xf="13dp"
                NOT_FOUND_0x10="9dp"/>
        </FrameLayout>
    </LinearLayout>
    <TextView
        id="@+id/tv_review_description"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="15dp"
        style="@style/2132017405"/>
    <androidx.recyclerview.widget.RecyclerView
        orientation="horizontal"
        id="@+id/rv_photo_review"
        scrollbars="none"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="12dp"
        NOT_FOUND_0x11="androidx.recyclerview.widget.LinearLayoutManager"/>
    <View
        background="#d1d1d1"
        layout_width="match_parent"
        layout_height="0.5dp"/>
</LinearLayout>
