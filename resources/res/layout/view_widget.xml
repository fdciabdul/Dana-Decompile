<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    orientation="vertical"
    id="@+id/res_0x7f0a10c6_groupdetailactivity_initdeletegroupdialog_2"
    visibility="gone"
    layout_marginTop="4dp"
    style="@style/2132017490">
    <TextView
        id="@+id/tv_widget_title"
        layout_width="0dp"
        layout_height="wrap_content"
        text="@string/donation_title"
        fontFamily="@font/2131296266"
        layout_marginStart="16dp"
        layout_marginEnd="16dp"
        NOT_FOUND_0x13="@+id/btn_view_all"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x17="0"
        style="@style/2132017916"/>
    <TextView
        textSize="12sp"
        textColor="#313131"
        ellipsize="end"
        id="@+id/tv_widget_description"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="2dp"
        text="@string/donation_description"
        maxLines="1"
        lineSpacingExtra="4sp"
        layout_marginStart="16dp"
        layout_marginEnd="16dp"
        NOT_FOUND_0x13="@+id/btn_view_all"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x16="@+id/tv_widget_title"
        style="@style/2132017439"/>
    <Button
        textSize="12sp"
        id="@+id/btn_widget_view_all"
        layout_width="wrap_content"
        text="@string/view_all"
        layout_marginEnd="16dp"
        NOT_FOUND_0x12="@+id/tv_widget_description"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x17="@+id/tv_widget_title"
        style="@style/2132017482"/>
    <androidx.recyclerview.widget.RecyclerView
        id="@+id/res_0x7f0a11a7_daggercontinualbillscomponent_continualbillscomponentimpl_sslpinningrepositoryprovider"
        clipToPadding="false"
        layout_marginTop="12dp"
        paddingStart="8dp"
        paddingEnd="8dp"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x16="@+id/tv_widget_description"
        style="@style/2132017490"/>
</androidx.constraintlayout.widget.ConstraintLayout>
