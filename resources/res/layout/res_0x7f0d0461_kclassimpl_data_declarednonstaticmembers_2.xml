<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">
    <FrameLayout
        id="@+id/wrapper_controls"
        background="#fff"
        layout_width="match_parent"
        layout_height="64dp"
        layout_above="@+id/wrapper_states">
        <LinearLayout
            gravity="center"
            orientation="horizontal"
            id="@+id/layout_aspect_ratio"
            layout_width="match_parent"
            layout_height="match_parent"/>
        <include
            id="@+id/layout_rotate_wheel"
            layout="@layout/ucrop_layout_rotate_wheel"/>
        <include
            id="@+id/layout_scale_wheel"
            layout="@layout/ucrop_layout_scale_wheel"/>
    </FrameLayout>
    <ImageView
        background="@drawable/ucrop_shadow_upside"
        layout_width="match_parent"
        layout_height="3dp"
        layout_above="@+id/wrapper_states"/>
    <LinearLayout
        gravity="center"
        orientation="horizontal"
        id="@+id/wrapper_states"
        background="#fff"
        layout_width="match_parent"
        layout_height="72dp"
        baselineAligned="false"
        layout_alignParentBottom="true">
        <FrameLayout
            id="@+id/res_0x7f0a1272_networkuserentitydata_externalsyntheticlambda2"
            style="@style/2132018234">
            <ImageView
                id="@+id/image_view_state_scale"
                src="@drawable/ucrop_ic_scale"
                style="@style/2132018231"/>
        </FrameLayout>
        <FrameLayout
            id="@+id/state_aspect_ratio"
            style="@style/2132018234">
            <ImageView
                id="@+id/image_view_state_aspect_ratio"
                src="@drawable/ucrop_ic_crop"
                style="@style/2132018231"/>
        </FrameLayout>
        <FrameLayout
            id="@+id/initRecordTimeStamp_res_0x7f0a1271"
            style="@style/2132018234">
            <ImageView
                id="@+id/res_0x7f0a08db_kclassimpl_data_declarednonstaticmembers_2"
                src="@drawable/ucrop_ic_rotate"
                style="@style/2132018231"/>
        </FrameLayout>
    </LinearLayout>
</merge>
