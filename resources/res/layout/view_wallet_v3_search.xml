<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/res_0x7f0a04ae_eventloopimplbase_delayedrunnabletask"
    background="@drawable/bg_rounded_8dp_border_grey20"
    layout_width="match_parent"
    layout_height="wrap_content">
    <androidx.appcompat.widget.AppCompatImageView
        layout_gravity="center"
        id="@+id/iv_search_toolbar_wallet_search"
        layout_width="18dp"
        layout_height="18dp"
        layout_marginTop="15dp"
        layout_marginBottom="15dp"
        src="@drawable/ic_search_grey"
        layout_marginStart="15dp"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x18="0"
        NOT_FOUND_0x19="0"/>
    <EditText
        enabled="true"
        textColorHint="#4c000000"
        layout_gravity="center"
        id="@+id/et_wallet_search_toolbar"
        background="#00000000"
        clickable="true"
        layout_width="0dp"
        layout_height="wrap_content"
        hint="@string/wallet_search_bar_hint_v3"
        maxLines="1"
        android:maxLength="40"
        inputType="textNoSuggestions"
        imeOptions="actionSearch"
        textCursorDrawable="@drawable/cursor_tangerine"
        layout_marginStart="11dp"
        layout_marginEnd="20dp"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x16="@+id/iv_clear_toolbar_wallet_search"
        NOT_FOUND_0x17="@+id/iv_search_toolbar_wallet_search"
        NOT_FOUND_0x19="0"
        style="@style/2132017407"/>
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/iv_clear_toolbar_wallet_search"
        visibility="gone"
        layout_width="20dp"
        layout_height="20dp"
        src="@drawable/ic_close_filled_grey"
        layout_marginEnd="14dp"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x19="0"/>
</androidx.constraintlayout.widget.ConstraintLayout>
