<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="?android:attr/selectableItemBackground"
    focusable="true"
    clickable="true"
    contentDescription="btn-add-recipient"
    style="@style/2132017490">
    <View
        id="@+id/divider_top"
        background="#b8b8b8"
        layout_width="match_parent"
        layout_height="0.5dp"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x12="0"/>
    <ImageView
        id="@+id/avatar"
        layout_marginLeft="16dp"
        layout_marginStart="16dp"
        NOT_FOUND_0xe="@+id/tv_add_number_msg"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x12="@+id/tv_phone_number"
        NOT_FOUND_0x13="@drawable/ic_unreg_user_without_shadow"
        style="@style/2132018187"/>
    <TextView
        textSize="16sp"
        textColor="#4a4a4a"
        id="@+id/tv_phone_number"
        layout_marginLeft="16dp"
        layout_marginTop="10dp"
        fontFamily="@font/2131296265"
        layout_marginStart="16dp"
        NOT_FOUND_0xf="@+id/avatar"
        NOT_FOUND_0x12="@+id/divider_top"
        style="@style/2132018187"/>
    <TextView
        textSize="12sp"
        textColor="#b8b8b8"
        id="@+id/tv_add_number_msg"
        layout_marginLeft="16dp"
        text="@string/add_phone_number"
        fontFamily="@font/2131296265"
        layout_marginStart="16dp"
        NOT_FOUND_0xf="@+id/avatar"
        NOT_FOUND_0x11="@+id/tv_phone_number"
        style="@style/2132018187"/>
    <View
        background="#b8b8b8"
        layout_width="match_parent"
        layout_height="0.5dp"
        layout_marginTop="10dp"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="@+id/tv_add_number_msg"/>
</androidx.constraintlayout.widget.ConstraintLayout>
