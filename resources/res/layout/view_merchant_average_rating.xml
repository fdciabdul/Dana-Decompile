<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    orientation="vertical"
    id="@+id/ll_average_rating_area"
    background="#ffffff"
    layout_width="match_parent"
    layout_height="wrap_content"
    paddingStart="16dp"
    paddingEnd="16dp">
    <LinearLayout
        orientation="vertical"
        id="@+id/ll_all_rating_container"
        paddingTop="16dp"
        paddingBottom="16dp"
        layout_width="match_parent"
        layout_height="wrap_content">
        <LinearLayout
            orientation="horizontal"
            layout_width="match_parent"
            layout_height="wrap_content">
            <TextView
                gravity="start"
                id="@+id/tv_title_average_rating"
                layout_width="match_parent"
                layout_height="wrap_content"
                text="@string/merchant_rating_title"
                layout_weight="1"
                textAllCaps="true"
                style="@style/2132017900"/>
            <TextView
                gravity="end"
                id="@+id/tv_see_all_rating"
                layout_width="match_parent"
                layout_height="wrap_content"
                text="@string/see_all"
                layout_weight="1"
                textAllCaps="true"
                style="@style/2132017895"/>
        </LinearLayout>
        <LinearLayout
            orientation="horizontal"
            id="@+id/ll_average_rating_container"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="16dp">
            <ImageView
                layout_gravity="center"
                layout_width="25dp"
                layout_height="25dp"
                src="@drawable/ic_rating_star"
                layout_marginEnd="5dp"/>
            <TextView
                gravity="center"
                id="@+id/tv_rating"
                layout_width="wrap_content"
                layout_height="wrap_content"
                style="@style/2132017956"/>
            <TextView
                textSize="14sp"
                textColor="#000000"
                gravity="center"
                layout_width="wrap_content"
                layout_height="wrap_content"
                text="@string/rating_max_label"
                fontFamily="@font/2131296266"
                layout_marginStart="5dp"/>
            <TextView
                layout_gravity="bottom"
                id="@+id/res_0x7f0a1803_r8_lambda_bz3pxigslnoepxjsarblztaebgk"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginBottom="2dp"
                layout_marginStart="5dp"/>
        </LinearLayout>
    </LinearLayout>
    <View
        id="@+id/view_divider_rating"
        background="#d1d1d1"
        layout_width="match_parent"
        layout_height="0.5dp"/>
    <LinearLayout
        orientation="vertical"
        id="@+id/your_review_section"
        layout_width="match_parent"
        layout_height="wrap_content">
        <LinearLayout
            orientation="vertical"
            id="@+id/user_review_section"
            visibility="gone"
            layout_width="match_parent"
            layout_height="wrap_content">
            <TextView
                gravity="start"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginTop="12dp"
                layout_marginBottom="12dp"
                text="@string/merchant_your_review_label"
                textAllCaps="true"
                style="@style/2132017900"/>
            <id.dana.nearbyme.merchantdetail.viewcomponent.MerchantYourReviewView
                id="@+id/QueryPayMethodModule_ProvidePresenterFactory"
                paddingTop="12dp"
                paddingBottom="16dp"
                visibility="visible"
                layout_width="match_parent"
                layout_height="wrap_content"/>
        </LinearLayout>
        <LinearLayout
            orientation="vertical"
            id="@+id/input_review_section"
            visibility="gone"
            layout_width="match_parent"
            layout_height="wrap_content">
            <TextView
                gravity="start"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginTop="12dp"
                layout_marginBottom="12dp"
                text="@string/merchant_your_review_label"
                textAllCaps="true"
                style="@style/2132017900"/>
            <TextView
                layout_width="match_parent"
                layout_height="wrap_content"
                text="@string/merchant_review_title"
                style="@style/2132017916"/>
            <TextView
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginBottom="12dp"
                text="@string/merchant_review_subtitle"
                style="@style/2132017901"/>
            <id.dana.nearbyme.merchantreview.ViewRatingStarBar
                id="@+id/getOnProductFieldClicked"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginBottom="24dp"/>
            <View
                background="#d1d1d1"
                layout_width="match_parent"
                layout_height="0.5dp"/>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
