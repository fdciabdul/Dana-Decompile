<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    orientation="vertical"
    layout_width="match_parent"
    layout_height="match_parent">
    <include
        id="@+id/include_toolbar"
        layout="@layout/layout_toolbar"/>
    <id.dana.richview.WebProgressView
        id="@+id/view_web_progress"
        layout_width="match_parent"
        layout_height="0dp"
        layout_weight="1"/>
    <View
        id="@+id/view_separator"
        background="#d1d1d1"
        layout_width="match_parent"
        layout_height="0.5dp"
        NOT_FOUND_0xb="0"/>
    <Button
        id="@+id/btn_ipg_agree"
        background="@drawable/btn_common"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_marginTop="16sp"
        layout_marginRight="16dp"
        layout_marginBottom="8dp"
        text="@string/ipg_terms_and_conditions_agree"
        style="@style/2132017915"/>
</LinearLayout>
