<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="?android:attr/selectableItemBackground"
    layout_width="match_parent"
    layout_height="wrap_content">
    <RelativeLayout
        orientation="horizontal"
        id="@+id/rl_add"
        padding="16dp"
        layout_width="0dp"
        layout_height="60dp"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x16="0">
        <ImageView
            id="@+id/iv_display_photo"
            layout_width="28dp"
            layout_height="28dp"
            scaleType="fitXY"
            NOT_FOUND_0x17="@drawable/ic_add_blue"/>
        <TextView
            textSize="16sp"
            textColor="#108ee9"
            id="@+id/tv_add_new"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginLeft="16dp"
            text="@string/send_to_bank_account"
            layout_toRightOf="@+id/iv_display_photo"
            layout_centerVertical="true"
            fontFamily="@font/2131296266"
            layout_marginStart="16dp"
            layout_toEndOf="@+id/iv_display_photo"/>
        <ImageView
            layout_gravity="center"
            id="@+id/iv_change_icon"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_alignParentRight="true"
            layout_centerVertical="true"
            layout_alignParentEnd="true"
            NOT_FOUND_0x17="@drawable/res_0x7f080796_networkuserentitydata_externalsyntheticlambda1"/>
    </RelativeLayout>
    <View
        id="@+id/view_bottom_divider"
        background="#b8b8b8"
        layout_width="match_parent"
        layout_height="0.5dp"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x15="@+id/rl_add"/>
</androidx.constraintlayout.widget.ConstraintLayout>
