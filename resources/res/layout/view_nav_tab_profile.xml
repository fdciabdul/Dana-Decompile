<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/cl_view_nav"
    layout_width="match_parent"
    layout_height="56dp">
    <androidx.appcompat.widget.AppCompatImageView
        layout_gravity="center_horizontal"
        id="@+id/nav_tab_icon"
        layout_width="24dp"
        layout_height="24dp"
        layout_marginTop="8dp"
        layout_marginBottom="4dp"
        layout_alignParentTop="true"
        layout_centerHorizontal="true"
        contentDescription="@string/navigation_tab_icon"
        NOT_FOUND_0x13="@drawable/ic_nav_me"/>
    <de.hdodenhof.circleimageview.CircleImageView
        layout_gravity="center_horizontal"
        id="@+id/nav_tab_user_avatar"
        visibility="visible"
        layout_width="24dp"
        layout_height="24dp"
        layout_marginTop="8dp"
        layout_marginBottom="4dp"
        layout_alignParentTop="true"
        layout_centerHorizontal="true"
        contentDescription="@string/navigation_tab_icon"
        NOT_FOUND_0x11="@color/black_text"
        NOT_FOUND_0x12="1dp"/>
    <androidx.appcompat.widget.AppCompatImageView
        layout_gravity="center_horizontal"
        id="@+id/nav_tab_user_verified_indicator"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_toLeftOf="@+id/nav_tab_user_avatar"
        layout_above="@+id/nav_tab_title"
        contentDescription="@string/navigation_tab_icon"
        layout_alignEnd="@+id/nav_tab_user_avatar"
        NOT_FOUND_0x13="@drawable/ic_verified_user"/>
    <TextView
        gravity="center"
        layout_gravity="center_horizontal"
        id="@+id/nav_tab_title"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginBottom="4dp"
        text="@string/nav_me_title"
        layout_below="@+id/nav_tab_icon"
        layout_alignParentBottom="true"
        layout_centerHorizontal="true"
        style="@style/2132017901"/>
</RelativeLayout>
