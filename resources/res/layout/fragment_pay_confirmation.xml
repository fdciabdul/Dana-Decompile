<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    focusable="true"
    clickable="true"
    layout_width="match_parent"
    layout_height="match_parent">
    <include
        id="@+id/toolbar"
        NOT_FOUND_0x1b="0"
        NOT_FOUND_0x1e="0"
        NOT_FOUND_0x20="0"
        layout="@layout/view_cashier_toolbar"/>
    <androidx.coordinatorlayout.widget.CoordinatorLayout
        orientation="vertical"
        background="#f5f5f5"
        layout_width="match_parent"
        layout_height="0dp"
        NOT_FOUND_0x19="0"
        NOT_FOUND_0x1b="0"
        NOT_FOUND_0x1e="0"
        NOT_FOUND_0x1f="@+id/toolbar">
        <androidx.core.widget.NestedScrollView
            id="@+id/setBackgroundColor"
            scrollbars="vertical"
            layout_width="match_parent"
            layout_height="wrap_content">
            <LinearLayout
                orientation="vertical"
                layout_width="match_parent"
                layout_height="wrap_content">
                <include
                    id="@+id/cancelEventDelivery"
                    layout="@layout/view_cashier_header"/>
                <androidx.constraintlayout.widget.ConstraintLayout
                    id="@+id/clTotalPrice"
                    background="#ffffff"
                    layout_width="match_parent"
                    layout_height="wrap_content">
                    <androidx.appcompat.widget.AppCompatTextView
                        id="@+id/res_0x7f0a15d1_setparenthandle_kotlinx_coroutines_core"
                        layout_width="0dp"
                        layout_height="wrap_content"
                        layout_marginTop="20dp"
                        layout_marginBottom="20dp"
                        text="@string/total_price"
                        contentDescription="@string/sdet_lbl_total_price_title"
                        layout_marginStart="16dp"
                        NOT_FOUND_0x19="0"
                        NOT_FOUND_0x1c="@+id/tvAmount"
                        NOT_FOUND_0x1e="0"
                        NOT_FOUND_0x20="0"
                        NOT_FOUND_0x21="0.3"
                        style="@style/2132017413"/>
                    <androidx.appcompat.widget.AppCompatTextView
                        gravity="end"
                        id="@+id/tvAmount"
                        layout_width="0dp"
                        layout_height="wrap_content"
                        contentDescription="@string/sdet_lbl_total_amount"
                        textAlignment="textEnd"
                        layout_marginStart="16dp"
                        layout_marginEnd="16dp"
                        NOT_FOUND_0x19="@+id/res_0x7f0a15d1_setparenthandle_kotlinx_coroutines_core"
                        NOT_FOUND_0x1b="0"
                        NOT_FOUND_0x1d="@+id/res_0x7f0a15d1_setparenthandle_kotlinx_coroutines_core"
                        NOT_FOUND_0x20="@+id/res_0x7f0a15d1_setparenthandle_kotlinx_coroutines_core"
                        style="@style/2132017953"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <include
                    id="@+id/RepaymentInfoResultMapperKt"
                    visibility="gone"
                    layout="@layout/view_global_send_detail"/>
                <include
                    id="@+id/layoutPromo"
                    layout="@layout/view_pay_confirmation_promo"/>
                <id.dana.cashier.view.CashierAddOnView
                    id="@+id/res_0x7f0a029c_cyfmonitor_challengeactioncallback"
                    visibility="gone"
                    layout_width="match_parent"
                    layout_height="wrap_content"/>
                <include
                    id="@+id/layoutPaylaterInstallmentDetail"
                    visibility="gone"
                    layout="@layout/view_paylater_installment_detail"/>
                <LinearLayout
                    orientation="horizontal"
                    id="@+id/res_0x7f0a0d1f_networkutils_networkdetailcallback"
                    background="@drawable/bg_rounded_8dp_border_grey20"
                    paddingTop="8dp"
                    paddingBottom="8dp"
                    visibility="gone"
                    layout_marginTop="8dp"
                    layout_marginBottom="36dp"
                    weightSum="1"
                    contentDescription="@string/sdet_lbl_additional_information"
                    paddingStart="10dp"
                    paddingEnd="10dp"
                    layout_marginStart="16dp"
                    layout_marginEnd="16dp"
                    style="@style/2132017490">
                    <androidx.appcompat.widget.AppCompatImageView
                        layout_width="wrap_content"
                        layout_height="wrap_content"
                        NOT_FOUND_0x22="@drawable/ic_info_filled_16_grey50"/>
                    <TextView
                        id="@+id/tvAdditionalInfo"
                        layout_width="0dp"
                        layout_height="wrap_content"
                        layout_marginRight="8dp"
                        layout_weight="1"
                        layout_marginStart="8dp"
                        style="@style/2132017439"/>
                </LinearLayout>
                <include
                    id="@+id/res_0x7f0a0cc5_r8_lambda_dcc0u2cqt0pulhxlkkhxodzfkss"
                    visibility="gone"
                    layout_width="match_parent"
                    layout_height="wrap_content"
                    layout="@layout/layout_dana_plus_term_and_condition"/>
                <id.dana.cashier.view.CashierPayWithCvvView
                    id="@+id/cashierPayWithCvvView"
                    visibility="gone"
                    layout_width="match_parent"
                    layout_height="wrap_content"
                    layout_marginTop="4dp"/>
                <View
                    layout_width="match_parent"
                    layout_height="150dp"/>
            </LinearLayout>
        </androidx.core.widget.NestedScrollView>
        <ScrollView
            layout_gravity="bottom"
            id="@+id/svPayMethod"
            layout_width="match_parent"
            layout_height="wrap_content">
            <androidx.constraintlayout.widget.ConstraintLayout
                id="@+id/clPaymentMethod"
                layout_width="match_parent"
                layout_height="wrap_content">
                <include
                    id="@+id/RiskChallengeTrackerImpl_Factory"
                    layout_width="match_parent"
                    layout_height="wrap_content"
                    NOT_FOUND_0x19="0"
                    NOT_FOUND_0x1b="0"
                    NOT_FOUND_0x1e="0"
                    NOT_FOUND_0x20="0"
                    layout="@layout/view_cashier_change_pay_method"/>
                <View
                    id="@+id/layoutShimmerPayMethod"
                    layout_width="match_parent"
                    layout_height="wrap_content"
                    layout_marginTop="20dp"
                    NOT_FOUND_0x1a="@+id/SelfUnfreeze_Factory"
                    NOT_FOUND_0x1b="0"
                    NOT_FOUND_0x1e="0"
                    NOT_FOUND_0x20="0"/>
                <View
                    id="@+id/SelfUnfreeze_Factory"
                    layout_width="match_parent"
                    layout_height="wrap_content"
                    NOT_FOUND_0x19="0"
                    NOT_FOUND_0x1b="0"
                    NOT_FOUND_0x1e="0"
                    NOT_FOUND_0x1f="@+id/layoutShimmerPayMethod"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </ScrollView>
    </androidx.coordinatorlayout.widget.CoordinatorLayout>
    <FrameLayout
        id="@+id/flDanaLoading"
        background="#80ffffff"
        visibility="gone"
        layout_width="match_parent"
        layout_height="match_parent">
        <id.dana.richview.LogoProgressView
            layout_gravity="center"
            id="@+id/lpvLoading"
            layout_width="wrap_content"
            layout_height="wrap_content"/>
    </FrameLayout>
    <id.dana.richview.DanaProtectionLoadingView
        id="@+id/danaProtectLoadingView"
        contentDescription="@string/icon_secure_lottie"
        NOT_FOUND_0x19="0"
        NOT_FOUND_0x1b="0"
        NOT_FOUND_0x1e="0"
        NOT_FOUND_0x20="0"
        style="@style/2132017531"/>
</androidx.constraintlayout.widget.ConstraintLayout>
