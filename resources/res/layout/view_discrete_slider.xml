<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_width="match_parent"
    layout_height="wrap_content">
    <androidx.appcompat.widget.AppCompatSeekBar
        id="@+id/discreteSlider"
        visibility="visible"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="52dp"
        max="10"
        progress="7"
        indeterminate="false"
        thumbOffset="8dp"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="@null"
        style="@style/2132017614"/>
    <TextView
        id="@+id/tvStartLabelBelow"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        text="@string/nps_survey_slider_label_no"
        layout_marginStart="20dp"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0xf="@+id/discreteSlider"
        style="@style/2132017435"/>
    <TextView
        id="@+id/tvEndLabelBelow"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        text="@string/nps_survey_slider_label_yes"
        layout_marginEnd="18dp"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0xf="@+id/discreteSlider"
        style="@style/2132017434"/>
</androidx.constraintlayout.widget.ConstraintLayout>
