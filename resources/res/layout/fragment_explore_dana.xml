<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="#f5f5f5"
    style="@style/2132017531">
    <androidx.constraintlayout.widget.ConstraintLayout
        id="@+id/res_0x7f0a04ae_eventloopimplbase_delayedrunnabletask"
        background="#108ee9"
        focusable="true"
        focusableInTouchMode="true"
        layout_width="match_parent"
        layout_height="wrap_content"
        contentDescription="@string/btn_search_bar"
        NOT_FOUND_0x1d="0"
        NOT_FOUND_0x20="0"
        NOT_FOUND_0x21="@+id/toolbar_layout">
        <LinearLayout
            background="@drawable/searchbox_rounded_white"
            padding="10dp"
            layout_marginBottom="8dp"
            layout_marginStart="16dp"
            layout_marginEnd="16dp"
            NOT_FOUND_0x1c="0"
            NOT_FOUND_0x1e="0"
            NOT_FOUND_0x1f="0"
            NOT_FOUND_0x22="0"
            style="@style/2132017490">
            <androidx.appcompat.widget.AppCompatImageView
                layout_gravity="center"
                id="@+id/iv_search_toolbar_global_search"
                layout_width="20dp"
                layout_height="20dp"
                src="@drawable/ic_search_grey"
                contentDescription="@string/icon_search"/>
            <FrameLayout
                layout_width="0dp"
                layout_height="match_parent"
                layout_weight="1">
                <EditText
                    layout_gravity="center"
                    id="@+id/et_global_search_toolbar"
                    background="#00000000"
                    focusable="false"
                    focusableInTouchMode="false"
                    layout_width="match_parent"
                    layout_height="wrap_content"
                    hint="@string/text_looking_for_anything_in_DANA"
                    maxLines="1"
                    android:maxLength="25"
                    inputType="textNoSuggestions"
                    imeOptions="actionSearch"
                    contentDescription="@string/lbl_search_bar"
                    paddingStart="8dp"
                    paddingEnd="20dp"
                    style="@style/2132017399"/>
            </FrameLayout>
        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.core.widget.NestedScrollView
        id="@+id/nested_category"
        layout_width="match_parent"
        layout_height="0dp"
        animateLayoutChanges="true"
        layout_marginStart="16dp"
        layout_marginEnd="16dp"
        NOT_FOUND_0x1c="0"
        NOT_FOUND_0x1d="0"
        NOT_FOUND_0x20="0"
        NOT_FOUND_0x21="@+id/res_0x7f0a04ae_eventloopimplbase_delayedrunnabletask">
        <androidx.constraintlayout.widget.ConstraintLayout
            id="@+id/fl_explore_widget"
            layout_width="match_parent"
            layout_height="wrap_content"
            animateLayoutChanges="true">
            <LinearLayout
                orientation="horizontal"
                id="@+id/ll_personalization_entry"
                visibility="gone"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginTop="16dp"
                NOT_FOUND_0x1e="0"
                NOT_FOUND_0x22="0">
                <include layout="@layout/view_user_personalization_entry_point"/>
            </LinearLayout>
            <androidx.appcompat.widget.LinearLayoutCompat
                orientation="vertical"
                id="@+id/llc_explore_widget_container"
                background="#f5f5f5"
                layout_width="match_parent"
                layout_height="wrap_content"
                NOT_FOUND_0x1e="0"
                NOT_FOUND_0x21="@+id/ll_personalization_entry">
                <LinearLayout
                    orientation="vertical"
                    id="@+id/ll_container_popular"
                    layout_width="match_parent"
                    layout_height="wrap_content"
                    layout_marginTop="16dp">
                    <TextView
                        layout_width="wrap_content"
                        layout_height="wrap_content"
                        text="@string/popular_search"
                        contentDescription="@string/lbl_top_searches_title"
                        style="@style/2132017901"/>
                    <androidx.recyclerview.widget.RecyclerView
                        id="@+id/rv_popular_search"
                        paddingTop="10dp"
                        layout_width="match_parent"
                        layout_height="wrap_content"/>
                </LinearLayout>
                <TextView
                    layout_width="wrap_content"
                    layout_height="wrap_content"
                    layout_marginTop="24dp"
                    text="@string/explore_more"
                    contentDescription="@string/lbl_discover_more_title"
                    style="@style/2132017901"/>
                <LinearLayout
                    orientation="vertical"
                    id="@+id/intersectTypesWithoutIntersectionType"
                    paddingBottom="10dp"
                    layout_width="match_parent"
                    layout_height="wrap_content">
                    <id.dana.explore.popularplaces.PopularPlacesView
                        id="@+id/res_0x7f0a0fc8_userpocketassetsdao_impl_2"
                        layout_width="match_parent"
                        layout_height="wrap_content"
                        layout_marginTop="8dp"/>
                    <id.dana.explore.service.ExploreServiceView
                        id="@+id/explore_service_view"
                        layout_width="match_parent"
                        layout_height="wrap_content"
                        layout_marginTop="8dp"/>
                    <id.dana.explore.view.ExploreOnlineMerchantView
                        id="@+id/explore_online_merchant"
                        layout_width="match_parent"
                        layout_height="wrap_content"
                        layout_marginTop="8dp"/>
                    <id.dana.explore.view.ExploreProductView
                        id="@+id/explore_product_view"
                        layout_width="match_parent"
                        layout_height="wrap_content"
                        layout_marginTop="8dp"/>
                    <id.dana.explore.view.ExplorePromoView
                        id="@+id/explore_promo_view"
                        layout_width="match_parent"
                        layout_height="wrap_content"
                        layout_marginTop="8dp"/>
                </LinearLayout>
            </androidx.appcompat.widget.LinearLayoutCompat>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.core.widget.NestedScrollView>
    <include
        id="@+id/toolbar_layout"
        NOT_FOUND_0x1d="0"
        NOT_FOUND_0x20="0"
        NOT_FOUND_0x22="0"
        layout="@layout/layout_toolbar"/>
</androidx.constraintlayout.widget.ConstraintLayout>
