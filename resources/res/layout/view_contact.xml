<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/cl_contact"
    paddingTop="10dp"
    focusable="false"
    layout_width="match_parent"
    layout_height="wrap_content">
    <id.dana.richview.CircleImageSelectionView
        id="@+id/cisv_display_photo"
        layout_width="28dp"
        layout_height="28dp"
        layout_marginLeft="16dp"
        layout_marginStart="16dp"
        NOT_FOUND_0x14="@+id/tv_phone_number"
        NOT_FOUND_0x1c="0"
        NOT_FOUND_0x1e="0"
        NOT_FOUND_0x1f="9dp"/>
    <TextView
        textSize="16sp"
        textColor="#4a4a4a"
        ellipsize="end"
        id="@+id/tv_display_name"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_marginRight="10dp"
        maxLines="1"
        fontFamily="@font/2131296265"
        layout_marginStart="16dp"
        layout_marginEnd="10dp"
        NOT_FOUND_0x15="@+id/tv_phone_number"
        NOT_FOUND_0x17="@+id/fl_icon_container"
        NOT_FOUND_0x18="0"
        NOT_FOUND_0x1b="@+id/cisv_display_photo"
        NOT_FOUND_0x1e="0"/>
    <TextView
        textSize="12sp"
        textColor="#b8b8b8"
        ellipsize="end"
        id="@+id/tv_phone_number"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginBottom="8dp"
        maxLines="1"
        fontFamily="@font/2131296265"
        layout_marginStart="16dp"
        layout_marginEnd="10dp"
        NOT_FOUND_0x14="@+id/view_divider"
        NOT_FOUND_0x16="@+id/tv_display_name"
        NOT_FOUND_0x18="0"
        NOT_FOUND_0x1b="@+id/cisv_display_photo"
        NOT_FOUND_0x1d="@+id/tv_display_name"/>
    <FrameLayout
        id="@+id/fl_icon_container"
        paddingBottom="10sp"
        layout_width="wrap_content"
        layout_height="wrap_content"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x17="@+id/CustomPopMenuExtension"
        NOT_FOUND_0x1e="0">
        <ImageView
            layout_gravity="center_vertical"
            id="@+id/iv_dana_user_logo"
            visibility="gone"
            layout_width="55dp"
            layout_height="16dp"
            layout_marginRight="16dp"
            layout_marginEnd="16dp"
            NOT_FOUND_0x23="@drawable/ic_ico_danauser"/>
        <androidx.constraintlayout.widget.ConstraintLayout
            layout_gravity="center_vertical"
            id="@+id/cl_invite_user"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginEnd="16dp">
            <id.dana.component.buttoncomponent.DanaButtonSecondaryView
                id="@+id/btn_invite_user"
                visibility="visible"
                layout_width="wrap_content"
                layout_height="wrap_content"
                NOT_FOUND_0x14="0"
                NOT_FOUND_0x19="0"
                NOT_FOUND_0x1a="0"
                NOT_FOUND_0x1e="0"
                NOT_FOUND_0x20="10"
                NOT_FOUND_0x21="@string/invite"
                NOT_FOUND_0x22="1"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </FrameLayout>
    <ImageView
        id="@+id/CustomPopMenuExtension"
        visibility="gone"
        layout_width="24dp"
        layout_height="24dp"
        layout_marginRight="4dp"
        layout_marginEnd="4dp"
        NOT_FOUND_0x14="@+id/tv_phone_number"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x1e="@+id/tv_display_name"
        NOT_FOUND_0x23="@drawable/ic_arrow_right_mini"/>
    <View
        id="@+id/view_divider"
        background="#b8b8b8"
        layout_width="match_parent"
        layout_height="0.5dp"
        layout_marginLeft="16dp"
        layout_marginTop="10dp"
        layout_marginStart="16dp"
        NOT_FOUND_0x1c="@+id/cisv_display_photo"
        NOT_FOUND_0x1d="@+id/tv_phone_number"/>
</androidx.constraintlayout.widget.ConstraintLayout>
