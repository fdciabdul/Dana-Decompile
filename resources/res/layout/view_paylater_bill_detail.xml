<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_width="match_parent"
    layout_height="wrap_content">
    <androidx.appcompat.widget.AppCompatTextView
        id="@+id/tv_paylater_promo"
        background="@drawable/bg_rounded_yellow_50_paylater_promo"
        paddingLeft="16dp"
        paddingTop="8dp"
        paddingRight="16dp"
        paddingBottom="8dp"
        visibility="gone"
        layout_width="match_parent"
        layout_height="wrap_content"
        drawablePadding="4dp"
        NOT_FOUND_0x12="@drawable/ic_paylater_promo"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x17="0"
        style="@style/2132017899"/>
    <LinearLayout
        orientation="vertical"
        id="@+id/ll_paylater_bill_detail"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_marginTop="8dp"
        layout_marginRight="16dp"
        layout_marginBottom="16dp"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x16="@+id/tv_paylater_promo">
        <androidx.appcompat.widget.AppCompatTextView
            id="@+id/tv_paylater_bill_detail_title"
            layout_width="wrap_content"
            layout_height="wrap_content"
            text="@string/paylater_bill_detail_title"
            contentDescription="@string/sdet_title_bill_detail_info"
            style="@style/2132017916"/>
        <androidx.appcompat.widget.AppCompatTextView
            id="@+id/tv_paylater_bill_detail_subtitle"
            paddingTop="4dp"
            layout_width="wrap_content"
            layout_height="wrap_content"
            text="@string/paylater_bill_detail_subtitle"
            contentDescription="@string/sdet_lbl_bill_detail_info"
            style="@style/2132017440"/>
        <androidx.recyclerview.widget.RecyclerView
            id="@+id/rv_paylater_bill_detail"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="8dp"
            overScrollMode="never"/>
        <id.dana.cashier.view.PaylaterAgreementView
            id="@+id/res_0x7f0a0f94_userpocketassetsdao_impl_1"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="8dp"
            layout_marginBottom="16dp"
            NOT_FOUND_0x13="0"
            NOT_FOUND_0x14="0"
            NOT_FOUND_0x15="0"
            NOT_FOUND_0x16="@+id/ll_paylater_bill_detail"/>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
