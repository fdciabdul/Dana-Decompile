<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/headerSection"
    style="@style/2132017490">
    <RelativeLayout
        orientation="horizontal"
        id="@+id/rlHeaderSection"
        paddingLeft="16dp"
        paddingRight="16dp"
        paddingBottom="4dp"
        layout_marginTop="16dp"
        paddingStart="16dp"
        paddingEnd="16dp"
        style="@style/2132017490">
        <LinearLayout
            id="@+id/llSortSection"
            focusable="true"
            clickable="true"
            layout_alignParentEnd="true"
            style="@style/2132018187">
            <TextView
                id="@+id/tvSortSection"
                layout_width="wrap_content"
                layout_height="wrap_content"
                textAllCaps="true"
                textAlignment="center"
                style="@style/2132017895"/>
            <androidx.appcompat.widget.AppCompatImageView
                id="@+id/ivChevronSor"
                layout_width="16dp"
                layout_height="16dp"
                layout_marginStart="4dp"
                NOT_FOUND_0x13="@drawable/ic_chevron_down_18dp_blue50"/>
        </LinearLayout>
        <TextView
            id="@+id/tvHeaderTitle"
            layout_width="wrap_content"
            layout_height="wrap_content"
            textAllCaps="true"
            textAlignment="center"
            style="@style/2132017901"/>
    </RelativeLayout>
    <View
        id="@+id/viewDivider"
        background="#b8b8b8"
        visibility="gone"
        layout_width="match_parent"
        layout_height="0.5dp"
        layout_below="@+id/rlHeaderSection"/>
</RelativeLayout>
