<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="@android:color/white"
    layout_width="match_parent"
    layout_height="match_parent">
    <include
        id="@+id/SecurityGuardProfileProvider"
        NOT_FOUND_0x18="0"
        NOT_FOUND_0x1a="0"
        layout="@layout/layout_toolbar"/>
    <View
        id="@+id/v_dim_header"
        background="#7a000000"
        visibility="gone"
        layout_width="match_parent"
        layout_height="?attr/2130968583"
        NOT_FOUND_0x18="0"
        NOT_FOUND_0x1a="0"/>
    <id.dana.richview.splitbill.AmountDisplayView
        id="@+id/adv_split_bill"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_marginTop="16dp"
        layout_marginRight="16dp"
        layout_marginStart="16dp"
        layout_marginEnd="16dp"
        NOT_FOUND_0x18="0"
        NOT_FOUND_0x19="@+id/SecurityGuardProfileProvider"/>
    <id.dana.sendmoney.remarks.NoteView
        id="@+id/rv_remarks"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginLeft="10dp"
        layout_marginTop="15dp"
        layout_marginRight="10dp"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x18="0"
        NOT_FOUND_0x19="@+id/adv_split_bill"
        NOT_FOUND_0x1b="6dp"/>
    <include
        id="@+id/view_divider"
        layout_width="0dp"
        layout_height="1dp"
        layout_marginLeft="16dp"
        layout_marginTop="8dp"
        layout_marginRight="16dp"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x18="0"
        NOT_FOUND_0x19="@+id/rv_remarks"
        layout="@layout/view_header"/>
    <TextView
        textColor="#bfbfbf"
        id="@+id/tv_title_split_bill"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_marginTop="16dp"
        text="@string/split_bill_with"
        textAllCaps="true"
        fontFamily="@font/2131296266"
        layout_marginStart="16dp"
        NOT_FOUND_0x18="0"
        NOT_FOUND_0x19="@+id/view_divider"/>
    <id.dana.splitbill.view.SplitBillPayersView
        id="@+id/sbpv_payers"
        focusable="true"
        focusableInTouchMode="true"
        layout_width="match_parent"
        layout_height="0dp"
        NOT_FOUND_0x16="@+id/fl_btn_container"
        NOT_FOUND_0x18="@+id/bpv_selection"
        NOT_FOUND_0x19="@+id/tv_title_split_bill"/>
    <FrameLayout
        id="@+id/fl_btn_container"
        layout_width="match_parent"
        layout_height="wrap_content"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x18="0">
        <id.dana.richview.LogoProgressView
            layout_gravity="center"
            id="@+id/progress_view"
            visibility="gone"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_margin="16dp"/>
        <Button
            enabled="false"
            textSize="16sp"
            layout_gravity="center"
            id="@+id/btn_split_bill"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_margin="16dp"
            text="@string/split_bill_now"
            style="@style/2132017655"/>
    </FrameLayout>
    <View
        id="@+id/v_dim_body"
        background="#7a000000"
        visibility="gone"
        clickable="true"
        layout_width="match_parent"
        layout_height="match_parent"/>
    <id.dana.richview.splitbill.BillPayerView
        id="@+id/bpv_selection"
        layout_width="match_parent"
        layout_height="0dp"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x19="@+id/SecurityGuardProfileProvider"/>
    <View
        id="@+id/v_touch_blocker"
        background="#00000000"
        visibility="gone"
        clickable="true"
        layout_width="match_parent"
        layout_height="match_parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>
