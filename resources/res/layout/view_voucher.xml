<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/cl_voucher"
    background="@drawable/bg_idle_voucher"
    paddingLeft="8dp"
    paddingRight="8dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    layout_marginBottom="8dp"
    NOT_FOUND_0x1d="0"
    NOT_FOUND_0x22="0">
    <ImageView
        id="@+id/iv_dana_logo"
        layout_width="28dp"
        layout_height="28dp"
        layout_marginLeft="8dp"
        layout_marginTop="16dp"
        layout_alignParentLeft="true"
        layout_alignParentTop="true"
        layout_marginStart="8dp"
        layout_alignParentStart="true"
        NOT_FOUND_0x1e="0"
        NOT_FOUND_0x22="0"
        NOT_FOUND_0x23="@drawable/res_0x7f080633_networkuserentitydata_externalsyntheticlambda2"/>
    <View
        id="@+id/view_divider"
        background="@drawable/vertical_dotted_line"
        layout_width="2dp"
        layout_height="0dp"
        layout_marginLeft="16dp"
        layout_toRightOf="@+id/iv_dana_logo"
        layout_marginStart="16dp"
        layout_toEndOf="@+id/iv_dana_logo"
        NOT_FOUND_0x1d="0"
        NOT_FOUND_0x1f="@+id/iv_dana_logo"
        NOT_FOUND_0x22="0"/>
    <ImageView
        id="@+id/StatementViewType"
        visibility="gone"
        layout_width="24dp"
        layout_height="24dp"
        layout_marginLeft="8dp"
        layout_marginTop="8dp"
        layout_alignParentTop="true"
        layout_alignParentRight="true"
        layout_marginStart="8dp"
        layout_alignParentEnd="true"
        NOT_FOUND_0x20="0"
        NOT_FOUND_0x22="0"
        NOT_FOUND_0x23="@drawable/ic_referral_tracker_success"/>
    <TextView
        textSize="16sp"
        textColor="#4a4a4a"
        ellipsize="end"
        id="@+id/tv_title"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_marginTop="8dp"
        maxLines="1"
        layout_toLeftOf="@+id/StatementViewType"
        layout_toRightOf="@+id/view_divider"
        layout_alignParentTop="true"
        fontFamily="@font/2131296266"
        layout_marginStart="16dp"
        layout_toStartOf="@+id/StatementViewType"
        layout_toEndOf="@+id/view_divider"
        NOT_FOUND_0x1f="@+id/view_divider"
        NOT_FOUND_0x22="0"/>
    <TextView
        textSize="16sp"
        textColor="#fb8b01"
        id="@+id/tv_balance"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_toRightOf="@+id/view_divider"
        layout_below="@+id/tv_title"
        fontFamily="@font/2131296266"
        layout_marginStart="16dp"
        layout_toEndOf="@+id/view_divider"
        NOT_FOUND_0x1f="@+id/view_divider"
        NOT_FOUND_0x21="@+id/tv_title"/>
    <TextView
        textSize="12sp"
        textColor="#4a4a4a"
        id="@+id/tv_description"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_toRightOf="@+id/view_divider"
        layout_below="@+id/tv_balance"
        fontFamily="@font/2131296266"
        layout_marginStart="16dp"
        layout_toEndOf="@+id/view_divider"
        NOT_FOUND_0x1f="@+id/view_divider"
        NOT_FOUND_0x21="@+id/tv_balance"/>
    <TextView
        textSize="12sp"
        textColor="#ff5d55"
        id="@+id/tv_expire_time"
        paddingTop="4dp"
        paddingBottom="8dp"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_toRightOf="@+id/view_divider"
        layout_below="@+id/tv_description"
        fontFamily="@font/2131296266"
        layout_marginStart="16dp"
        layout_toEndOf="@+id/view_divider"
        NOT_FOUND_0x1f="@+id/view_divider"
        NOT_FOUND_0x21="@+id/tv_description"/>
    <TextView
        textSize="12sp"
        textColor="#108ee9"
        id="@+id/tv_show_detail"
        paddingTop="4dp"
        paddingBottom="8dp"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginLeft="4dp"
        text="@string/show_detail"
        layout_toRightOf="@+id/tv_expire_time"
        layout_below="@+id/tv_description"
        fontFamily="@font/2131296266"
        layout_marginStart="4dp"
        layout_toEndOf="@+id/tv_expire_time"
        NOT_FOUND_0x1f="@+id/tv_expire_time"
        NOT_FOUND_0x21="@+id/tv_description"/>
</androidx.constraintlayout.widget.ConstraintLayout>
