<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="@drawable/bg_rounded_white_with_border"
    paddingLeft="16dp"
    paddingTop="16dp"
    paddingRight="16dp"
    paddingBottom="10dp"
    layout_width="match_parent"
    layout_height="wrap_content">
    <TextView
        id="@+id/tv_title_kyb_transaction_history"
        layout_width="0dp"
        layout_height="wrap_content"
        text="@string/transaction_history_title"
        contentDescription="@string/lbl_transaction_history"
        NOT_FOUND_0x12="@+id/btn_see_all"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x15="0"
        style="@style/2132017916"/>
    <TextView
        id="@+id/tv_kyb_transaction_count"
        visibility="gone"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        contentDescription="@string/lbl_recent_transaction"
        NOT_FOUND_0x12="@+id/btn_see_all"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x14="@+id/tv_title_kyb_transaction_history"
        style="@style/2132017440"/>
    <TextView
        gravity="center"
        id="@+id/tv_info_empty_state_kyb_transaction_history"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        layout_marginBottom="10dp"
        text="@string/kyb_transaction_history_empty_state_info_text"
        layout_marginStart="8dp"
        layout_marginEnd="8dp"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x14="@+id/iv_logo_empty_state_kyb_transaction_history"
        style="@style/2132017439"/>
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/iv_logo_empty_state_kyb_transaction_history"
        visibility="gone"
        layout_width="80dp"
        layout_height="80dp"
        layout_marginTop="20dp"
        layout_marginStart="1dp"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x14="@+id/tv_kyb_transaction_count"
        NOT_FOUND_0x19="@drawable/ic_illustration_negative_state_empty_merchant"/>
    <androidx.recyclerview.widget.RecyclerView
        id="@+id/rv_kyb_transaction_history"
        visibility="visible"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="10dp"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x14="@+id/tv_kyb_transaction_count"/>
    <id.dana.component.buttoncomponent.DanaButtonSecondaryView
        id="@+id/btn_see_all"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        contentDescription="@string/btn_see_all_transaction"
        NOT_FOUND_0x10="@+id/tv_kyb_transaction_count"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x15="@+id/tv_title_kyb_transaction_history"
        NOT_FOUND_0x16="10"
        NOT_FOUND_0x17="@string/see_all_text"
        NOT_FOUND_0x18="1"/>
    <ImageView
        id="@+id/iv_red_dot_btn_view_all"
        visibility="gone"
        layout_width="10dp"
        layout_height="10dp"
        layout_marginTop="4dp"
        contentDescription="@string/icon_red_dot_view_all"
        layout_marginEnd="8dp"
        NOT_FOUND_0x11="@+id/btn_see_all"
        NOT_FOUND_0x15="@+id/btn_see_all"
        NOT_FOUND_0x19="@drawable/red_dot"/>
</androidx.constraintlayout.widget.ConstraintLayout>
