<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="@android:color/white"
    style="@style/2132017531">
    <include
        id="@+id/split_bill_toolbar"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x12="0"
        layout="@layout/layout_toolbar"/>
    <androidx.core.widget.NestedScrollView
        id="@+id/nsv_split_bill_summary"
        layout_width="match_parent"
        layout_height="0dp"
        NOT_FOUND_0xe="@+id/ssv_split_bill_summary"
        NOT_FOUND_0xf="@+id/ssv_split_bill_summary"
        NOT_FOUND_0x10="@+id/ssv_split_bill_summary"
        NOT_FOUND_0x11="@+id/split_bill_toolbar">
        <include layout="@layout/view_split_bill_details"/>
    </androidx.core.widget.NestedScrollView>
    <id.dana.richview.socialshare.SocialShareView
        id="@+id/ssv_split_bill_summary"
        background="@drawable/bg_rounded_with_border"
        paddingLeft="26dp"
        paddingTop="22dp"
        paddingRight="26dp"
        paddingBottom="19dp"
        visibility="gone"
        layout_margin="8dp"
        paddingStart="26dp"
        paddingEnd="26dp"
        NOT_FOUND_0xc="true"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x13="1"
        style="@style/2132017490"/>
    <View
        id="@+id/v_dim_body"
        background="#7a000000"
        visibility="gone"
        layout_width="match_parent"
        layout_height="match_parent"/>
    <include
        visibility="gone"
        layout="@layout/view_split_bill_detail_empty_state"/>
</androidx.constraintlayout.widget.ConstraintLayout>
