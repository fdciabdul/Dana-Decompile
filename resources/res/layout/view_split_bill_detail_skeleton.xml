<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    orientation="vertical"
    style="@style/2132017490">
    <ImageView
        layout_width="match_parent"
        layout_height="180dp"
        src="@drawable/square_skeleton"
        scaleType="fitXY"/>
    <TextView
        textSize="12sp"
        textColor="#bfbfbf"
        gravity="center_horizontal"
        layout_gravity="center_horizontal"
        layout_marginTop="8dp"
        text="@string/bill_amount"
        lineSpacingExtra="4sp"
        textAllCaps="true"
        fontFamily="@font/2131296266"
        style="@style/2132017490"/>
    <View
        layout_gravity="center_horizontal"
        background="@drawable/square_skeleton"
        layout_width="150dp"
        layout_height="40dp"
        layout_marginTop="10dp"/>
    <View
        layout_gravity="center_horizontal"
        background="@drawable/square_skeleton"
        layout_width="120dp"
        layout_height="18dp"
        layout_marginTop="10dp"
        layout_marginBottom="16dp"/>
    <TextView
        textSize="12sp"
        textColor="#727272"
        layout_marginLeft="16dp"
        layout_marginTop="8dp"
        layout_marginRight="16dp"
        layout_marginBottom="8dp"
        text="@string/split_bill_with"
        lineSpacingExtra="4sp"
        textAllCaps="true"
        fontFamily="@font/2131296266"
        layout_marginStart="16dp"
        layout_marginEnd="16dp"
        style="@style/2132018187"/>
    <View
        layout_marginLeft="16dp"
        layout_marginRight="16dp"
        layout_marginBottom="10dp"
        layout_marginStart="16dp"
        layout_marginEnd="16dp"
        style="@style/2132017967"/>
    <include layout="@layout/view_item_split_bill_detail_skeleton"/>
    <include layout="@layout/view_item_split_bill_detail_skeleton"/>
    <include layout="@layout/view_item_split_bill_detail_skeleton"/>
    <include layout="@layout/view_item_split_bill_detail_skeleton"/>
    <include layout="@layout/view_item_split_bill_detail_skeleton"/>
</LinearLayout>
