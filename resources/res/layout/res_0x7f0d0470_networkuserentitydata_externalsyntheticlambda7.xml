<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    orientation="vertical"
    layout_width="match_parent"
    layout_height="wrap_content">
    <androidx.constraintlayout.widget.ConstraintLayout
        id="@+id/clAddToMyBills"
        paddingLeft="16dp"
        paddingRight="16dp"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginBottom="16dp">
        <LinearLayout
            gravity="center"
            orientation="horizontal"
            layout_width="wrap_content"
            layout_height="wrap_content"
            NOT_FOUND_0x16="@+id/res_0x7f0a12bb_securitysettingsactivity_createpinlauncher_2_2"
            NOT_FOUND_0x18="0"
            NOT_FOUND_0x19="@+id/res_0x7f0a12bb_securitysettingsactivity_createpinlauncher_2_2">
            <TextView
                textColor="#313131"
                id="@+id/res_0x7f0a13aa_splitmerchantconfigentity_getmerchantdetailconfig_2_invokesuspend_lambda_0_inlined_getconfigsonceready_default_1_1"
                layout_width="wrap_content"
                layout_height="wrap_content"
                text="@string/my_bills_title_add_to_my_bill"
                textAlignment="viewStart"
                style="@style/2132017413"/>
            <ImageView
                id="@+id/ivInfo"
                padding="4dp"
                layout_width="wrap_content"
                layout_height="wrap_content"
                src="@drawable/ic_circle_information"
                contentDescription="@string/my_bills_content_description_btn"/>
        </LinearLayout>
        <com.google.android.material.switchmaterial.SwitchMaterial
            id="@+id/res_0x7f0a12bb_securitysettingsactivity_createpinlauncher_2_2"
            layout_width="wrap_content"
            layout_height="wrap_content"
            thumb="@drawable/bg_switch_thumb"
            NOT_FOUND_0x16="0"
            NOT_FOUND_0x17="0"
            NOT_FOUND_0x19="0"
            NOT_FOUND_0x1a="52dp"
            NOT_FOUND_0x1b="@drawable/bg_switch_track"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <id.dana.mybills.ui.customview.CustomEditTextView
        id="@+id/QrExpiredActivity_res_0x7f0a066f"
        visibility="gone"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_marginRight="16dp"
        NOT_FOUND_0x15="@string/my_bills_label_bill_name"/>
    <id.dana.mybills.ui.customview.CustomEditTextView
        id="@+id/etBillingDate"
        visibility="gone"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_marginTop="16dp"
        layout_marginRight="16dp"
        NOT_FOUND_0x14="@drawable/ic_calendar_grey"
        NOT_FOUND_0x15="@string/my_bills_label_bill_date"/>
    <id.dana.mybills.ui.customview.CustomDropdownView
        id="@+id/dropdownPaymentType"
        visibility="gone"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_margin="16dp"/>
</androidx.appcompat.widget.LinearLayoutCompat>
