<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    background="#00000000"
    layout_width="match_parent"
    layout_height="match_parent">
    <RelativeLayout
        id="@+id/fragment_container"
        background="#00000000"
        layout_width="match_parent"
        layout_height="match_parent"
        layout_above="@+id/tab_container"/>
    <RelativeLayout
        id="@+id/tab_container"
        visibility="gone"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_alignParentBottom="true"/>
    <RelativeLayout
        id="@+id/content_id"
        layout_width="200dp"
        layout_height="300dp"
        layout_centerInParent="true"/>
    <RelativeLayout
        id="@+id/splash_container"
        layout_width="match_parent"
        layout_height="match_parent"/>
    <RelativeLayout
        id="@+id/videoLayout"
        visibility="gone"
        layout_width="match_parent"
        layout_height="match_parent"/>
    <ViewStub
        id="@+id/view_stub_diagnostic_tool"
        layout="@layout/griver_diagnostic_tool_layout"
        layout_width="48dp"
        layout_height="48dp"
        layout_marginRight="30dp"
        layout_marginBottom="100dp"
        layout_alignParentRight="true"
        layout_alignParentBottom="true"/>
</RelativeLayout>
