<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/container"
    layout_width="match_parent"
    layout_height="wrap_content">
    <id.dana.core.ui.richview.PinEntryEditText
        textSize="28sp"
        textStyle="normal"
        id="@+id/isLayoutRequested_res_0x7f0a0904"
        background="#f5f5f5"
        padding="10dp"
        layout_width="220dp"
        layout_height="64dp"
        layout_marginTop="24dp"
        layout_marginBottom="32dp"
        cursorVisible="false"
        android:maxLength="4"
        digits="1234567890"
        inputType="number"
        contentDescription="@string/txt_area_otp"
        textIsSelectable="false"
        textAlignment="center"
        NOT_FOUND_0x1f="0"
        NOT_FOUND_0x25="0"
        NOT_FOUND_0x26="@+id/iv_otp_channel"
        NOT_FOUND_0x28="@drawable/bg_pin"
        NOT_FOUND_0x29="true"
        NOT_FOUND_0x2a="8dp"
        NOT_FOUND_0x2b="12dp"/>
    <ImageView
        id="@+id/ic_success"
        visibility="invisible"
        layout_width="64dp"
        layout_height="64dp"
        layout_marginLeft="8dp"
        layout_marginRight="8dp"
        contentDescription="@null"
        layout_marginStart="8dp"
        layout_marginEnd="8dp"
        NOT_FOUND_0x1e="@+id/isLayoutRequested_res_0x7f0a0904"
        NOT_FOUND_0x1f="@+id/isLayoutRequested_res_0x7f0a0904"
        NOT_FOUND_0x25="@+id/isLayoutRequested_res_0x7f0a0904"
        NOT_FOUND_0x27="@+id/isLayoutRequested_res_0x7f0a0904"
        NOT_FOUND_0x2c="@drawable/ic_success_64dp"/>
    <RelativeLayout
        id="@+id/ic_progress"
        visibility="visible"
        layout_marginLeft="8dp"
        layout_marginRight="8dp"
        layout_centerInParent="true"
        contentDescription="@null"
        layout_marginStart="8dp"
        layout_marginEnd="8dp"
        NOT_FOUND_0x1e="@+id/isLayoutRequested_res_0x7f0a0904"
        NOT_FOUND_0x1f="@+id/isLayoutRequested_res_0x7f0a0904"
        NOT_FOUND_0x25="@+id/isLayoutRequested_res_0x7f0a0904"
        NOT_FOUND_0x27="@+id/isLayoutRequested_res_0x7f0a0904"
        style="@style/2132018187">
        <ImageView
            layout_gravity="center"
            id="@+id/ic_image"
            layout_width="64dp"
            layout_height="64dp"
            layout_marginBottom="10dp"
            layout_centerInParent="true"
            contentDescription="ic logo"
            NOT_FOUND_0x2c="@drawable/ic_loading_bg"/>
        <ImageView
            layout_gravity="center"
            id="@+id/ic_image_fg"
            layout_width="50dp"
            layout_height="50dp"
            layout_marginBottom="10dp"
            layout_centerInParent="true"
            contentDescription="ic logo"
            NOT_FOUND_0x2c="@drawable/loading_spinner"/>
    </RelativeLayout>
    <TextView
        textSize="12sp"
        id="@+id/resend_otp"
        paddingTop="8dp"
        paddingBottom="8dp"
        visibility="gone"
        layout_width="0dp"
        layout_marginTop="24dp"
        text="@string/resend_otp_sms"
        contentDescription="@string/btn_resend_otp"
        textAllCaps="true"
        NOT_FOUND_0x1f="@+id/resend_middle_line"
        NOT_FOUND_0x20="@+id/resend_otp_whatsapp"
        NOT_FOUND_0x21="0.5"
        NOT_FOUND_0x25="0"
        NOT_FOUND_0x27="@+id/resend_middle_line"
        style="@style/2132017571"/>
    <TextView
        textSize="12sp"
        id="@+id/resend_otp_whatsapp"
        paddingTop="8dp"
        paddingBottom="8dp"
        visibility="gone"
        layout_width="0dp"
        layout_marginTop="24dp"
        text="@string/resend_otp_whatsapp"
        contentDescription="@string/btn_resend_otp_wa"
        textAllCaps="true"
        NOT_FOUND_0x1f="0"
        NOT_FOUND_0x21="0.5"
        NOT_FOUND_0x24="@+id/resend_otp"
        NOT_FOUND_0x27="@+id/resend_middle_line"
        style="@style/2132017571"/>
    <TextView
        textSize="@dimen/2131166005"
        textColor="#313131"
        gravity="center_horizontal"
        id="@+id/title_top"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginLeft="8dp"
        layout_marginRight="8dp"
        text="@string/enter_otp"
        fontFamily="@font/2131296266"
        layout_marginStart="8dp"
        layout_marginEnd="8dp"
        NOT_FOUND_0x1f="0"
        NOT_FOUND_0x25="0"
        NOT_FOUND_0x27="0"/>
    <TextView
        textSize="14sp"
        id="@+id/title"
        layout_width="0dp"
        layout_marginLeft="8dp"
        layout_marginTop="8dp"
        layout_marginRight="8dp"
        text="@string/popup_otp_title"
        layout_marginStart="8dp"
        layout_marginEnd="8dp"
        NOT_FOUND_0x1f="0"
        NOT_FOUND_0x25="0"
        NOT_FOUND_0x26="@+id/title_top"
        style="@style/2132017573"/>
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/iv_otp_channel"
        layout_width="wrap_content"
        layout_height="20dp"
        layout_marginTop="4dp"
        NOT_FOUND_0x1f="0"
        NOT_FOUND_0x25="0"
        NOT_FOUND_0x26="@+id/title"
        NOT_FOUND_0x2c="@drawable/ic_otp_sms"/>
    <View
        id="@+id/resend_middle_line"
        background="#d6d6d6"
        visibility="gone"
        layout_width="1dp"
        layout_height="0dp"
        layout_marginTop="24dp"
        layout_marginBottom="8dp"
        NOT_FOUND_0x22="0"
        NOT_FOUND_0x23="0"
        NOT_FOUND_0x26="@+id/isLayoutRequested_res_0x7f0a0904"
        style="@style/2132017642"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        id="@+id/cl_timer_and_changephonenumber_container"
        paddingBottom="28dp"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="24dp"
        NOT_FOUND_0x1f="0"
        NOT_FOUND_0x25="0"
        NOT_FOUND_0x26="@+id/isLayoutRequested_res_0x7f0a0904">
        <TextView
            textSize="12sp"
            textColor="#108ee9"
            id="@+id/tv_help"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_marginLeft="8dp"
            layout_marginTop="10dp"
            layout_marginRight="8dp"
            layout_marginBottom="8dp"
            text="@string/tooltip_help"
            contentDescription="@string/btn_help_otp"
            textAllCaps="true"
            fontFamily="@font/2131296266"
            textAlignment="center"
            NOT_FOUND_0x1e="0"
            NOT_FOUND_0x20="@+id/timer"
            NOT_FOUND_0x21="0.5"
            NOT_FOUND_0x25="0"
            NOT_FOUND_0x27="0"/>
        <TextView
            textSize="12sp"
            id="@+id/timer"
            visibility="visible"
            layout_width="0dp"
            layout_marginTop="10dp"
            layout_marginRight="8dp"
            layout_marginBottom="8dp"
            textAllCaps="true"
            NOT_FOUND_0x1e="0"
            NOT_FOUND_0x1f="0"
            NOT_FOUND_0x21="0.5"
            NOT_FOUND_0x24="@+id/tv_help"
            NOT_FOUND_0x27="0"
            style="@style/2132017572"/>
        <View
            id="@+id/view_separator_lost_phone_number"
            background="#d6d6d6"
            visibility="invisible"
            layout_width="1dp"
            layout_height="0dp"
            NOT_FOUND_0x1e="0"
            NOT_FOUND_0x22="0"
            NOT_FOUND_0x23="0"
            NOT_FOUND_0x27="0"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <TextView
        textSize="12sp"
        textColor="#108ee9"
        id="@+id/tv_change_phonenumber"
        paddingBottom="32dp"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        text="@string/lost_phone_number"
        textAllCaps="true"
        fontFamily="@font/2131296266"
        textAlignment="center"
        NOT_FOUND_0x1f="0"
        NOT_FOUND_0x25="0"
        NOT_FOUND_0x26="@+id/cl_timer_and_changephonenumber_container"/>
</androidx.constraintlayout.widget.ConstraintLayout>
