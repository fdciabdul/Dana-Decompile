<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    orientation="vertical"
    background="#f5f5f5"
    focusable="true"
    clickable="true"
    layout_width="match_parent"
    layout_height="match_parent">
    <include
        id="@+id/toolbar"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x13="0"
        layout="@layout/view_cashier_toolbar"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        id="@+id/clHeaderContainer"
        background="@drawable/bg_gradient_top_up_and_pay"
        layout_width="match_parent"
        layout_height="wrap_content"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="@+id/toolbar">
        <androidx.appcompat.widget.AppCompatImageView
            id="@+id/ivHeader"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="6dp"
            NOT_FOUND_0x10="0"
            NOT_FOUND_0x11="0"
            NOT_FOUND_0x13="0"
            NOT_FOUND_0x14="@drawable/ic_topup_and_pay_header"/>
        <androidx.appcompat.widget.AppCompatTextView
            id="@+id/tv_header_title"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_marginLeft="16dp"
            layout_marginTop="8dp"
            layout_marginRight="16dp"
            layout_marginBottom="6dp"
            text="@string/topup_and_pay_header_title"
            textAlignment="center"
            NOT_FOUND_0xf="0"
            NOT_FOUND_0x10="0"
            NOT_FOUND_0x11="0"
            NOT_FOUND_0x12="@+id/ivHeader"
            style="@style/2132017438"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.core.widget.NestedScrollView
        layout_width="match_parent"
        layout_height="0dp"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="@+id/clHeaderContainer">
        <LinearLayout
            orientation="vertical"
            id="@+id/topUpAndPayMethodContainer"
            layout_width="match_parent"
            layout_height="wrap_content">
            <id.dana.cashier.view.TopUpAndPayMethodView
                id="@+id/res_0x7f0a1372_shareactivitybottomsheetdialog_setavatar_1"
                layout_width="match_parent"
                layout_height="wrap_content"/>
            <id.dana.cashier.view.TopUpAndPayMethodView
                id="@+id/res_0x7f0a1373_flowkt__zipkt_combinetransform_7"
                layout_width="match_parent"
                layout_height="wrap_content"/>
            <id.dana.cashier.view.TopUpAndPayMethodView
                id="@+id/res_0x7f0a1371_shareactivitybottomsheetdialog_setavatar_2"
                layout_width="match_parent"
                layout_height="wrap_content"/>
        </LinearLayout>
    </androidx.core.widget.NestedScrollView>
    <FrameLayout
        id="@+id/flDanaLoading"
        background="#80ffffff"
        visibility="gone"
        layout_width="match_parent"
        layout_height="match_parent">
        <id.dana.richview.LogoProgressView
            layout_gravity="center"
            id="@+id/lpvLoading"
            layout_width="wrap_content"
            layout_height="wrap_content"/>
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
