<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="#108ee9"
    paddingBottom="16dp"
    layout_width="match_parent"
    layout_height="wrap_content">
    <ImageView
        layout_width="0dp"
        layout_height="0dp"
        src="@drawable/bg_my_profile_account"
        scaleType="centerCrop"
        NOT_FOUND_0x19="0"
        NOT_FOUND_0x1a="0"
        NOT_FOUND_0x1c="0"
        NOT_FOUND_0x1e="0"/>
    <de.hdodenhof.circleimageview.CircleImageView
        id="@+id/res_0x7f0a0fda_abstractbinaryclassannotationandconstantloader_loadannotationsandinitializers_1_annotationvisitorformethod"
        layout_width="72dp"
        layout_height="72dp"
        layout_marginTop="17dp"
        src="@drawable/avatar_placeholder"
        layout_marginStart="16dp"
        NOT_FOUND_0x17="@color/2131100488"
        NOT_FOUND_0x18="1dp"
        NOT_FOUND_0x1c="0"
        NOT_FOUND_0x1e="0"/>
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/res_0x7f0a0fdb_daggerpromocentercomponent_promocentercomponentimpl_settingrepositoryprovider"
        visibility="visible"
        layout_width="72dp"
        layout_height="72dp"
        layout_marginTop="17dp"
        scaleType="fitEnd"
        layout_marginStart="16dp"
        NOT_FOUND_0x1c="0"
        NOT_FOUND_0x1e="0"
        NOT_FOUND_0x21="@drawable/bg_avatar_overlay_oval"/>
    <ImageView
        id="@+id/iv_dana_premium"
        visibility="invisible"
        layout_width="71dp"
        layout_height="24dp"
        layout_marginTop="60dp"
        NOT_FOUND_0x1a="@+id/res_0x7f0a0fda_abstractbinaryclassannotationandconstantloader_loadannotationsandinitializers_1_annotationvisitorformethod"
        NOT_FOUND_0x1c="@+id/res_0x7f0a0fda_abstractbinaryclassannotationandconstantloader_loadannotationsandinitializers_1_annotationvisitorformethod"
        NOT_FOUND_0x1e="@+id/res_0x7f0a0fda_abstractbinaryclassannotationandconstantloader_loadannotationsandinitializers_1_annotationvisitorformethod"
        NOT_FOUND_0x21="@drawable/ic_dana_premium_profile"/>
    <id.dana.richview.ProfileNameTextView
        textStyle="bold"
        id="@+id/text_user_name"
        paddingLeft="16dp"
        paddingRight="16dp"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_alignParentTop="true"
        textAlignment="textStart"
        NOT_FOUND_0x1a="0"
        NOT_FOUND_0x1b="@+id/res_0x7f0a0fda_abstractbinaryclassannotationandconstantloader_loadannotationsandinitializers_1_annotationvisitorformethod"
        NOT_FOUND_0x1e="@+id/res_0x7f0a0fda_abstractbinaryclassannotationandconstantloader_loadannotationsandinitializers_1_annotationvisitorformethod"
        NOT_FOUND_0x20="1"
        style="@style/2132017915"/>
    <TextView
        textColor="#ffffff"
        id="@+id/tv_phone_number"
        paddingLeft="16dp"
        paddingRight="16dp"
        visibility="gone"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        layout_below="@+id/text_user_name"
        NOT_FOUND_0x1a="0"
        NOT_FOUND_0x1b="@+id/res_0x7f0a0fda_abstractbinaryclassannotationandconstantloader_loadannotationsandinitializers_1_annotationvisitorformethod"
        NOT_FOUND_0x1d="@+id/text_user_name"/>
    <LinearLayout
        orientation="horizontal"
        id="@+id/vg_profile_with_qr_show"
        background="@drawable/bg_outline_white_rad_15dp"
        layout_width="wrap_content"
        layout_height="30dp"
        layout_marginTop="6dp"
        paddingStart="10dp"
        paddingEnd="6dp"
        layout_marginStart="16dp"
        NOT_FOUND_0x1b="@+id/res_0x7f0a0fda_abstractbinaryclassannotationandconstantloader_loadannotationsandinitializers_1_annotationvisitorformethod"
        NOT_FOUND_0x1d="@+id/tv_phone_number"
        NOT_FOUND_0x1f="9dp">
        <androidx.appcompat.widget.AppCompatImageView
            layout_gravity="center_vertical"
            layout_width="20dp"
            layout_height="20dp"
            src="@drawable/ic_nav_pay"/>
        <TextView
            layout_gravity="center_vertical"
            layout_width="wrap_content"
            layout_height="wrap_content"
            text="@string/show_my_qr"
            textAllCaps="true"
            paddingStart="4dp"
            paddingEnd="4dp"
            style="@style/2132017899"/>
        <androidx.appcompat.widget.AppCompatImageView
            layout_gravity="center_vertical"
            layout_width="20dp"
            layout_height="20dp"
            NOT_FOUND_0x21="@drawable/ic_arrow_right_me_page"
            NOT_FOUND_0x22="@color/2131100488"/>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
