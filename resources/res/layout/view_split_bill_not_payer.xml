<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" style="@style/2132017531">
    <LinearLayout
        orientation="vertical"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_centerInParent="true">
        <ImageView
            layout_gravity="center"
            id="@+id/iv_not_included_bill"
            NOT_FOUND_0xf="@drawable/not_included"
            style="@style/2132018187"/>
        <TextView
            textSize="16sp"
            textColor="@android:color/black"
            layout_gravity="center"
            id="@+id/tv_sorry"
            text="@string/split_bill_non_payer_title"
            lineSpacingExtra="4sp"
            fontFamily="@font/2131296266"
            style="@style/2132018187"/>
        <TextView
            textSize="14sp"
            textColor="#313131"
            gravity="center"
            layout_gravity="center"
            layout_marginTop="8dp"
            text="@string/split_bill_non_payer_description"
            lineSpacingExtra="4sp"
            fontFamily="@font/2131296265"
            style="@style/2132018187"/>
    </LinearLayout>
    <Button
        id="@+id/btn_ok"
        layout_margin="16dp"
        text="@android:string/ok"
        layout_alignParentBottom="true"
        fontFamily="@font/2131296266"
        style="@style/2132017655"/>
</RelativeLayout>
