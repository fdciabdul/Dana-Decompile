<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    orientation="vertical"
    id="@+id/cl_latest_review_container"
    paddingTop="16dp"
    layout_width="match_parent"
    layout_height="wrap_content">
    <LinearLayout
        orientation="horizontal"
        id="@+id/ll_user"
        layout_width="match_parent"
        layout_height="wrap_content"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x19="0">
        <ImageView
            id="@+id/iv_avatar_user"
            background="@drawable/shape_oval_grey"
            layout_width="36dp"
            layout_height="37dp"
            adjustViewBounds="true"/>
        <FrameLayout
            layout_width="match_parent"
            layout_height="wrap_content">
            <LinearLayout
                orientation="vertical"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginStart="5dp">
                <TextView
                    textSize="14sp"
                    textColor="#000000"
                    gravity="center"
                    id="@+id/getSecondaryProgressTintMode"
                    layout_width="wrap_content"
                    layout_height="wrap_content"
                    fontFamily="@font/2131296266"
                    layout_marginStart="5dp"/>
                <TextView
                    id="@+id/saveMonthOfFetchFreeTransfer"
                    layout_width="match_parent"
                    layout_height="wrap_content"
                    layout_marginStart="5dp"
                    style="@style/2132017439"/>
            </LinearLayout>
            <TextView
                layout_gravity="right"
                id="@+id/tv_rating"
                layout_width="wrap_content"
                layout_height="wrap_content"
                drawablePadding="3dp"
                drawableStart="@drawable/ic_rating_star"
                style="@style/2132017900"/>
        </FrameLayout>
    </LinearLayout>
    <TextView
        id="@+id/tv_review_description"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="15dp"
        NOT_FOUND_0x15="@+id/rv_photo_review"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x18="@+id/ll_user"
        style="@style/2132017405"/>
    <androidx.recyclerview.widget.RecyclerView
        orientation="horizontal"
        id="@+id/rv_photo_review"
        scrollbars="none"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="16dp"
        NOT_FOUND_0x13="androidx.recyclerview.widget.LinearLayoutManager"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x18="@+id/tv_review_description"/>
    <View
        id="@+id/view_divider"
        background="#d1d1d1"
        layout_width="match_parent"
        layout_height="0.5dp"
        layout_marginTop="16dp"
        NOT_FOUND_0x18="@+id/rv_photo_review"/>
    <Button
        id="@+id/btn_see_other_review"
        background="@drawable/btn_semi_rounded_white"
        visibility="gone"
        layout_width="match_parent"
        layout_height="32dp"
        layout_marginTop="15dp"
        text="@string/merchant_detail_see_other_reviews"
        NOT_FOUND_0x18="@+id/view_divider"
        style="@style/2132017658"/>
</LinearLayout>
