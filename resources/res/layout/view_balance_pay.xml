<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/layout_parent"
    padding="16dp"
    style="@style/2132017531">
    <ImageView
        id="@+id/pay_logo"
        src="@drawable/logo_balance_pay"
        scaleType="fitStart"
        contentDescription="@string/pay"
        NOT_FOUND_0x13="@+id/view_dana_protection"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x19="0"
        NOT_FOUND_0x1b="0"
        style="@style/2132017455"/>
    <id.dana.pay.view.QrBarcodePayView
        id="@+id/pay_qr_barcode"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="16dp"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x15="2"
        NOT_FOUND_0x19="0"
        NOT_FOUND_0x1c="1"
        style="@style/2132017579"/>
    <TextView
        textSize="@dimen/2131166136"
        textColor="#ffffff"
        id="@+id/pay_title"
        visibility="invisible"
        text="@string/balance_pay_title"
        lineSpacingExtra="0sp"
        fontFamily="@font/2131296265"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x1a="@+id/pay_logo"
        style="@style/2132018187"/>
    <TextView
        id="@+id/pay_id"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="@dimen/2131166133"
        lineSpacingExtra="0sp"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x1a="@+id/pay_title"
        style="@style/2132017915"/>
    <TextView
        id="@+id/pay_content_identifier"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="@dimen/2131166129"
        text="@string/balance"
        lineSpacingExtra="1sp"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x1a="@+id/pay_id"
        style="@style/2132017438"/>
    <id.dana.richview.ToggleBalanceView
        id="@+id/tbv_pay_content"
        layout_width="wrap_content"
        layout_height="wrap_content"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x1a="@+id/pay_content_identifier"
        NOT_FOUND_0x1d="0"/>
    <TextView
        gravity="center"
        id="@+id/pay_topup_button"
        background="@drawable/btn_pay_topup_round"
        layout_width="wrap_content"
        layout_height="wrap_content"
        text="@string/pay_top_up"
        fontFamily="@font/2131296266"
        NOT_FOUND_0x11="@+id/tbv_pay_content"
        NOT_FOUND_0x18="0"
        NOT_FOUND_0x1b="@+id/pay_content_identifier"
        style="@style/2132017899"/>
    <id.dana.richview.VerticalTextView
        gravity="bottom"
        id="@+id/tv_scan_info"
        layout_width="wrap_content"
        layout_height="wrap_content"
        text="@string/pay_scan_info"
        textAllCaps="true"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x14="0.7"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x17="@+id/pay_qr_barcode"
        style="@style/2132017430"/>
</androidx.constraintlayout.widget.ConstraintLayout>
