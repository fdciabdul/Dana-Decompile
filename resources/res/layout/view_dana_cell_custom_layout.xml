<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="#ffffff"
    paddingTop="4dp"
    paddingBottom="4dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    paddingStart="12dp"
    paddingEnd="12dp">
    <LinearLayout
        orientation="vertical"
        paddingLeft="8dp"
        layout_width="0dp"
        layout_height="wrap_content"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x10="@+id/tv_value"
        NOT_FOUND_0x12="@+id/iv_icon"
        NOT_FOUND_0x13="0">
        <androidx.appcompat.widget.AppCompatTextView
            id="@+id/tv_label"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            style="@style/2132017900"/>
        <androidx.appcompat.widget.AppCompatTextView
            id="@+id/tv_description"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="2dp"
            style="@style/2132017432"/>
    </LinearLayout>
    <androidx.appcompat.widget.AppCompatTextView
        id="@+id/tv_value"
        visibility="gone"
        layout_width="0dp"
        layout_height="wrap_content"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x13="0"
        style="@style/2132017900"/>
    <de.hdodenhof.circleimageview.CircleImageView
        id="@+id/iv_icon"
        visibility="gone"
        layout_width="28dp"
        layout_height="28dp"
        NOT_FOUND_0xc="@color/2131099990"
        NOT_FOUND_0xd="1dp"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x13="0"/>
</androidx.constraintlayout.widget.ConstraintLayout>
