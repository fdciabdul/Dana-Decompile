<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    gravity="center"
    orientation="vertical"
    background="@android:color/white"
    visibility="visible"
    layout_width="match_parent"
    layout_height="match_parent"
    layout_weight="1">
    <LinearLayout
        gravity="center"
        orientation="vertical"
        layout_width="match_parent"
        layout_height="match_parent">
        <androidx.appcompat.widget.AppCompatImageView
            layout_width="wrap_content"
            layout_height="wrap_content"
            NOT_FOUND_0x12="@drawable/ic_merchant_detail_error_illustration"/>
        <TextView
            gravity="center"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="16dp"
            text="@string/merchant_detail_error_state_title"
            layout_marginStart="16dp"
            layout_marginEnd="16dp"
            style="@style/2132017932"/>
        <TextView
            gravity="center"
            id="@+id/tv_error_message_nearby_detail"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="8dp"
            text="@string/merchant_detail_error_state_message"
            lineSpacingExtra="4sp"
            layout_marginStart="16dp"
            layout_marginEnd="16dp"
            style="@style/2132017414"/>
    </LinearLayout>
    <id.dana.component.buttoncomponent.DanaButtonPrimaryView
        id="@+id/btnTryAgain"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginBottom="16dp"
        layout_marginStart="16dp"
        layout_marginEnd="16dp"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="@string/try_again"
        NOT_FOUND_0x11="1"/>
</androidx.constraintlayout.widget.ConstraintLayout>
