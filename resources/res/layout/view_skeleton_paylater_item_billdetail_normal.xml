<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="@drawable/bg_rounded_white_4dp"
    layout_width="match_parent"
    layout_height="wrap_content">
    <id.dana.cashier.view.PaylaterTimelineView
        id="@+id/paylater_timeline_view_normal"
        layout_width="0dp"
        layout_height="match_parent"
        NOT_FOUND_0x8="@color/2131099990"
        NOT_FOUND_0x9="true"
        NOT_FOUND_0xa="0"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="0.1"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="2dp"
        NOT_FOUND_0x13="@drawable/bg_circle_marker_grey20"
        NOT_FOUND_0x14="20dp"
        NOT_FOUND_0x15="true"
        NOT_FOUND_0x16="@color/2131099990"/>
    <View
        id="@+id/vTitle"
        background="@drawable/bg_rounded_grey_20_4dp"
        layout_width="0dp"
        layout_height="20dp"
        layout_marginTop="8dp"
        layout_marginBottom="8dp"
        layout_marginStart="8dp"
        layout_marginEnd="0dp"
        NOT_FOUND_0xc="@+id/paylater_timeline_view_normal"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="0.2"/>
    <View
        id="@+id/vSubtitle"
        background="@drawable/bg_rounded_grey_20_4dp"
        layout_width="0dp"
        layout_height="20dp"
        layout_marginTop="4dp"
        layout_marginBottom="4dp"
        layout_marginStart="8dp"
        layout_marginEnd="0dp"
        NOT_FOUND_0xa="0"
        NOT_FOUND_0xc="@+id/paylater_timeline_view_normal"
        NOT_FOUND_0xe="@+id/vTitle"
        NOT_FOUND_0x10="0.4"/>
    <View
        id="@+id/vAmount"
        background="@drawable/bg_rounded_grey_20_4dp"
        layout_width="0dp"
        layout_height="20dp"
        NOT_FOUND_0xa="0"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="0.2"/>
</androidx.constraintlayout.widget.ConstraintLayout>
