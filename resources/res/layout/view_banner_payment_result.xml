<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.card.MaterialCardView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    theme="@style/2132017169"
    layout_width="match_parent"
    layout_height="wrap_content"
    NOT_FOUND_0x14="8dp"
    NOT_FOUND_0x15="0dp"
    NOT_FOUND_0x1f="@color/2131099990"
    NOT_FOUND_0x20="1dp">
    <androidx.constraintlayout.widget.ConstraintLayout
        layout_width="match_parent"
        layout_height="wrap_content">
        <TextView
            id="@+id/tvBannerTitle"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_marginTop="24dp"
            fontFamily="@font/2131296257"
            layout_marginStart="16dp"
            NOT_FOUND_0x18="@+id/vertical_guideline"
            NOT_FOUND_0x1c="@+id/res_0x7f0a094f_glideexperiments_builder"
            NOT_FOUND_0x1e="@+id/res_0x7f0a094f_glideexperiments_builder"
            style="@style/2132017923"/>
        <androidx.appcompat.widget.AppCompatImageView
            id="@+id/res_0x7f0a094f_glideexperiments_builder"
            layout_width="match_parent"
            layout_height="0dp"
            src="#fca73e"
            scaleType="fitXY"
            adjustViewBounds="false"
            contentDescription="@string/img_banner_success"
            NOT_FOUND_0x16="0"
            NOT_FOUND_0x17="0"
            NOT_FOUND_0x1c="0"
            NOT_FOUND_0x1e="0"/>
        <androidx.constraintlayout.widget.Guideline
            orientation="vertical"
            id="@+id/vertical_guideline"
            layout_width="wrap_content"
            layout_height="wrap_content"
            NOT_FOUND_0x19="0.6"
            NOT_FOUND_0x1a="0"
            NOT_FOUND_0x1b="0"/>
        <androidx.constraintlayout.widget.Guideline
            orientation="horizontal"
            id="@+id/vertical_guideline_horizontal"
            layout_width="wrap_content"
            layout_height="wrap_content"
            NOT_FOUND_0x19="0.5"
            NOT_FOUND_0x1a="0"
            NOT_FOUND_0x1b="0"/>
        <com.google.android.material.card.MaterialCardView
            id="@+id/readMicros"
            layout_width="wrap_content"
            layout_height="32dp"
            layout_marginTop="16dp"
            layout_marginBottom="16dp"
            layout_marginStart="16dp"
            NOT_FOUND_0x13="@color/2131100488"
            NOT_FOUND_0x14="4dp"
            NOT_FOUND_0x15="0dp"
            NOT_FOUND_0x16="0"
            NOT_FOUND_0x1c="@+id/res_0x7f0a094f_glideexperiments_builder"
            NOT_FOUND_0x1d="@+id/vertical_guideline_horizontal"
            NOT_FOUND_0x1f="@color/2131099816"
            NOT_FOUND_0x20="1dp">
            <LinearLayout
                paddingLeft="16dp"
                paddingTop="8dp"
                paddingRight="16dp"
                paddingBottom="8dp"
                layout_width="match_parent"
                layout_height="match_parent">
                <TextView
                    layout_gravity="center"
                    id="@+id/tvBtnCta"
                    layout_width="wrap_content"
                    layout_height="wrap_content"
                    contentDescription="@string/btn_banner_success"
                    textAllCaps="true"
                    style="@style/2132017895"/>
            </LinearLayout>
        </com.google.android.material.card.MaterialCardView>
    </androidx.constraintlayout.widget.ConstraintLayout>
</com.google.android.material.card.MaterialCardView>
