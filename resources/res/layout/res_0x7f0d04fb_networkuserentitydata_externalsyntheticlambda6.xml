<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/cl_root"
    background="@drawable/bg_rounded_white_8dp"
    layout_width="match_parent"
    layout_height="wrap_content">
    <androidx.appcompat.widget.AppCompatTextView
        ellipsize="end"
        id="@+id/tv_title"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="12dp"
        maxLines="1"
        layout_marginStart="8dp"
        layout_marginEnd="16dp"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x14="@+id/res_0x7f0a0a9f_otpregistrationpresenter_input_1"
        NOT_FOUND_0x17="0"
        style="@style/2132017908"/>
    <androidx.appcompat.widget.AppCompatTextView
        ellipsize="end"
        id="@+id/tv_description"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginBottom="8dp"
        maxLines="1"
        layout_marginStart="8dp"
        layout_marginEnd="16dp"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x14="@+id/res_0x7f0a0a9f_otpregistrationpresenter_input_1"
        NOT_FOUND_0x16="@+id/tv_title"
        NOT_FOUND_0x18="0"
        style="@style/2132017439"/>
    <androidx.appcompat.widget.AppCompatTextView
        textColor="#00ce64"
        gravity="center_vertical"
        id="@+id/tv_status"
        visibility="gone"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        text="@string/cards_completed"
        drawablePadding="5dp"
        layout_marginStart="8dp"
        layout_marginEnd="8dp"
        NOT_FOUND_0x11="@drawable/ic_cards_success"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x14="@+id/res_0x7f0a0a9f_otpregistrationpresenter_input_1"
        NOT_FOUND_0x16="@+id/tv_description"
        style="@style/2132017894"/>
    <androidx.appcompat.widget.AppCompatTextView
        id="@+id/tv_remaining_time"
        visibility="gone"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        layout_marginStart="8dp"
        layout_marginEnd="8dp"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x14="@+id/res_0x7f0a0a9f_otpregistrationpresenter_input_1"
        NOT_FOUND_0x16="@+id/tv_status"
        style="@style/2132017440"/>
    <com.google.android.material.imageview.ShapeableImageView
        id="@+id/res_0x7f0a0a9f_otpregistrationpresenter_input_1"
        background="#badffa"
        layout_width="60dp"
        layout_height="60dp"
        layout_marginTop="8dp"
        layout_marginBottom="8dp"
        adjustViewBounds="true"
        layout_marginStart="16dp"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x19="@style/2132017586"/>
    <View
        id="@+id/view_dummy_stack"
        visibility="gone"
        clickable="false"
        layout_width="0dp"
        layout_height="0dp"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x17="0"/>
</androidx.constraintlayout.widget.ConstraintLayout>
