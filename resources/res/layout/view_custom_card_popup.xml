<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    layout_gravity="center"
    orientation="vertical"
    id="@+id/ll_simple_dialog"
    background="@drawable/rounded_white_border"
    paddingLeft="12dp"
    paddingTop="8dp"
    paddingRight="12dp"
    paddingBottom="12dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    layout_margin="40dp">
    <RelativeLayout
        layout_gravity="center"
        id="@+id/getValueOfTouchPositionAbsolute_res_0x7f0a109e"
        background="@drawable/bg_card_popup_icon"
        padding="3dp"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="20dp">
        <androidx.appcompat.widget.AppCompatImageView
            id="@+id/res_0x7f0a0ad6_networkuserentitydata_externalsyntheticlambda3"
            layout_width="80dp"
            layout_height="80dp"
            layout_centerHorizontal="true"/>
    </RelativeLayout>
    <TextView
        textColor="#313131"
        gravity="center"
        id="@android:id/title"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="12dp"
        style="@style/2132017916"/>
    <TextView
        gravity="center"
        id="@android:id/summary"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        style="@style/2132017405"/>
    <include layout="@layout/view_custom_action_card_popup"/>
</LinearLayout>
