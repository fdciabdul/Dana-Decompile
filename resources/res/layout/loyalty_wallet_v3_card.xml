<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    focusable="true"
    clickable="true"
    layout_width="match_parent"
    layout_height="wrap_content">
    <com.google.android.material.card.MaterialCardView
        layout_gravity="center"
        orientation="vertical"
        id="@+id/cv_pocket_loyalty"
        layout_width="0dp"
        layout_height="0dp"
        NOT_FOUND_0x12="@dimen/MyBillsEntityDataFactory_res_0x7f07005f"
        NOT_FOUND_0x13="0dp"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x16="1:0.6"
        NOT_FOUND_0x19="0"
        NOT_FOUND_0x1a="0"
        NOT_FOUND_0x1d="0"
        NOT_FOUND_0x1f="@color/2131099990"
        NOT_FOUND_0x20="1dp">
        <androidx.constraintlayout.widget.ConstraintLayout
            id="@+id/cl_pocket_loyalty"
            layout_width="match_parent"
            layout_height="match_parent"
            NOT_FOUND_0x14="0"
            NOT_FOUND_0x19="0"
            NOT_FOUND_0x1a="0"
            NOT_FOUND_0x1d="0">
            <androidx.appcompat.widget.AppCompatImageView
                id="@+id/iv_pocket_loyalty_background"
                background="#4a4a4a"
                layout_width="match_parent"
                layout_height="match_parent"
                scaleType="centerCrop"
                adjustViewBounds="false"
                NOT_FOUND_0x14="0"
                NOT_FOUND_0x19="0"
                NOT_FOUND_0x1a="0"
                NOT_FOUND_0x1d="0"/>
            <androidx.appcompat.widget.AppCompatImageView
                id="@+id/iv_pocket_loyalty"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginTop="16dp"
                scaleType="fitStart"
                adjustViewBounds="false"
                maxHeight="60dp"
                minWidth="50dp"
                minHeight="50dp"
                layout_marginStart="16dp"
                NOT_FOUND_0x14="0"
                NOT_FOUND_0x18="0"
                NOT_FOUND_0x19="0"
                NOT_FOUND_0x1a="0"
                NOT_FOUND_0x1d="0"
                NOT_FOUND_0x1e="0"/>
            <TextView
                id="@+id/tv_pocket_loyalty_points"
                layout_width="36dp"
                layout_height="16dp"
                layout_marginTop="16dp"
                text="@string/loyalty_asset_points_label"
                NOT_FOUND_0x15="@+id/tv_pocket_loyalty_points_value"
                NOT_FOUND_0x17="@+id/tv_pocket_loyalty_points_value"
                NOT_FOUND_0x1d="0"
                style="@style/2132017438"/>
            <TextView
                id="@+id/tv_pocket_loyalty_points_value"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginEnd="16dp"
                NOT_FOUND_0x17="0"
                NOT_FOUND_0x18="1"
                NOT_FOUND_0x1b="0"
                NOT_FOUND_0x1c="@+id/tv_pocket_loyalty_points"
                style="@style/2132017947"/>
            <TextView
                id="@+id/tv_pocket_loyalty_number"
                layout_width="wrap_content"
                layout_height="wrap_content"
                text="@string/loyalty_asset_number_label"
                NOT_FOUND_0x15="@+id/tv_pocket_loyalty_number_value"
                NOT_FOUND_0x1b="@+id/tv_pocket_loyalty_number_value"
                style="@style/2132017438"/>
            <TextView
                id="@+id/tv_pocket_loyalty_number_value"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginTop="2dp"
                layout_marginBottom="16dp"
                layout_marginStart="16dp"
                NOT_FOUND_0x14="0"
                NOT_FOUND_0x17="0"
                NOT_FOUND_0x18="0"
                NOT_FOUND_0x1b="0"
                style="@style/2132017915"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </com.google.android.material.card.MaterialCardView>
</androidx.constraintlayout.widget.ConstraintLayout>
