<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="#f5f5f5"
    focusable="true"
    clickable="true"
    layout_width="match_parent"
    layout_height="match_parent">
    <include
        id="@+id/onekliKChallengeHeader"
        NOT_FOUND_0x24="0"
        NOT_FOUND_0x26="0"
        NOT_FOUND_0x28="0"
        layout="@layout/view_cashier_toolbar"/>
    <TextView
        id="@+id/tvOneklikChallengeTitle"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="28dp"
        text="@string/please_choose_your_phone_number"
        NOT_FOUND_0x24="0"
        NOT_FOUND_0x26="0"
        NOT_FOUND_0x27="@+id/onekliKChallengeHeader"
        style="@style/2132017916"/>
    <TextView
        id="@+id/tvOneklikChallengeTitleDescription"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        text="@string/pilih_nomor_hp_terdaftar_di_bank"
        NOT_FOUND_0x24="0"
        NOT_FOUND_0x26="0"
        NOT_FOUND_0x27="@+id/tvOneklikChallengeTitle"
        style="@style/2132017440"/>
    <LinearLayout
        orientation="horizontal"
        id="@+id/llOneklikChallengePhoneNumber"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="12dp"
        NOT_FOUND_0x24="0"
        NOT_FOUND_0x26="0"
        NOT_FOUND_0x27="@+id/tvOneklikChallengeTitleDescription">
        <androidx.constraintlayout.widget.ConstraintLayout
            background="#ffffff"
            layout_width="242dp"
            layout_height="40dp">
            <TextView
                id="@+id/oneklikChallengeSpinnerPrefix"
                background="@drawable/bg_rounded_4dp_grey50"
                paddingLeft="4dp"
                paddingRight="4dp"
                layout_width="wrap_content"
                layout_height="wrap_content"
                text="@string/indonesian_phone_prefix"
                layout_marginStart="16dp"
                NOT_FOUND_0x23="0"
                NOT_FOUND_0x26="0"
                NOT_FOUND_0x28="0"
                style="@style/2132017412"/>
            <Spinner
                theme="@style/2132017852"
                id="@+id/oneklikChallengeSpinner"
                layout_width="0dp"
                layout_height="0dp"
                spinnerMode="dropdown"
                layout_marginStart="8dp"
                NOT_FOUND_0x23="0"
                NOT_FOUND_0x24="0"
                NOT_FOUND_0x25="@+id/oneklikChallengeSpinnerPrefix"
                NOT_FOUND_0x28="0"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <id.dana.component.buttoncomponent.DanaButtonSecondaryView
            layout_gravity="center_vertical"
            id="@+id/btnOneklikChallengeSendOtp"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginStart="16dp"
            NOT_FOUND_0x2b="10"
            NOT_FOUND_0x2c="Send OTP"
            NOT_FOUND_0x2d="1"/>
        <id.dana.component.buttoncomponent.DanaButtonGhostView
            layout_gravity="center_vertical"
            id="@+id/btnOneklikChallengeCountdown"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginStart="16dp"
            NOT_FOUND_0x20="10"
            NOT_FOUND_0x21="Send OTP"
            NOT_FOUND_0x22="0"/>
    </LinearLayout>
    <TextView
        id="@+id/tvOneklikChallengeOtpTitle"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="40dp"
        text="@string/please_enter_your_otp"
        NOT_FOUND_0x24="0"
        NOT_FOUND_0x26="0"
        NOT_FOUND_0x27="@+id/llOneklikChallengePhoneNumber"
        style="@style/2132017916"/>
    <id.dana.core.ui.richview.PinEntryEditText
        textSize="24sp"
        textStyle="normal"
        textColor="#000000"
        gravity="center"
        id="@+id/etInputOneklikOtp"
        background="#b8f1d4"
        paddingTop="15dp"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginLeft="45dp"
        layout_marginTop="16dp"
        layout_marginRight="45dp"
        cursorVisible="false"
        android:maxLength="6"
        digits="1234567890"
        inputType="number"
        imeOptions="actionNone"
        textIsSelectable="false"
        textAlignment="center"
        NOT_FOUND_0x24="0"
        NOT_FOUND_0x26="0"
        NOT_FOUND_0x27="@+id/tvOneklikChallengeOtpTitle"
        NOT_FOUND_0x29="@drawable/bg_pin_cashier"
        NOT_FOUND_0x2a="8dp"/>
    <TextView
        gravity="center_horizontal"
        id="@+id/tvOneklikChallengeOtpError"
        visibility="gone"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginLeft="45dp"
        layout_marginTop="16dp"
        layout_marginRight="45dp"
        text="@string/oneklik_error_text"
        NOT_FOUND_0x24="0"
        NOT_FOUND_0x26="0"
        NOT_FOUND_0x27="@+id/etInputOneklikOtp"
        style="@style/2132017409"/>
    <androidx.constraintlayout.widget.Group
        id="@+id/groupOtpInput"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        NOT_FOUND_0x1f="tvOneklikChallengeOtpTitle, etInputOneklikOtp"/>
    <FrameLayout
        id="@+id/flDanaLoading"
        background="#80ffffff"
        visibility="gone"
        layout_width="match_parent"
        layout_height="match_parent">
        <id.dana.richview.LogoProgressView
            layout_gravity="center"
            id="@+id/lpvLoading"
            layout_width="wrap_content"
            layout_height="wrap_content"/>
    </FrameLayout>
    <id.dana.richview.DanaProtectionLoadingView
        id="@+id/danaProtectLoadingView"
        contentDescription="@string/icon_secure_lottie"
        NOT_FOUND_0x23="0"
        NOT_FOUND_0x24="0"
        NOT_FOUND_0x26="0"
        NOT_FOUND_0x28="0"
        style="@style/2132017531"/>
</androidx.constraintlayout.widget.ConstraintLayout>
