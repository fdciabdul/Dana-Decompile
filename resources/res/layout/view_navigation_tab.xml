<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/cl_view_nav"
    layout_width="match_parent"
    layout_height="56dp">
    <ImageView
        layout_gravity="center_horizontal"
        id="@+id/nav_tab_icon"
        layout_width="24dp"
        layout_height="24dp"
        layout_marginTop="8dp"
        layout_marginBottom="4dp"
        contentDescription="@string/navigation_tab_icon"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x17="0"/>
    <TextView
        textSize="12sp"
        textColor="@drawable/nav_title_color"
        gravity="center"
        layout_gravity="center_horizontal"
        id="@+id/nav_tab_title"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginBottom="4dp"
        lineSpacingExtra="4sp"
        fontFamily="@font/2131296265"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x14="@+id/nav_tab_icon"
        NOT_FOUND_0x15="@+id/nav_tab_icon"
        NOT_FOUND_0x16="@+id/nav_tab_icon"/>
    <TextView
        textSize="10sp"
        textStyle="bold"
        textColor="#ffffff"
        gravity="center"
        id="@+id/nav_tab_notif"
        background="@drawable/bg_rounded_red_notification"
        paddingLeft="4sp"
        paddingRight="4sp"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginLeft="15dp"
        layout_marginStart="15dp"
        NOT_FOUND_0x15="@+id/nav_tab_icon"
        NOT_FOUND_0x17="0"
        style="@style/2132018187"/>
</androidx.constraintlayout.widget.ConstraintLayout>
