<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    paddingTop="16dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    paddingStart="12dp">
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/ivChainQueryIcon"
        layout_width="0dp"
        layout_height="wrap_content"
        NOT_FOUND_0xd="@+id/tvViewAll"
        NOT_FOUND_0xe="100dp"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x14="@+id/res_0x7f0a1400_resetpinstatemanager_resetpinstate_failed"
        NOT_FOUND_0x15="@drawable/ic_chain_query"/>
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/ivChevronRight"
        layout_width="24dp"
        layout_height="24dp"
        layout_marginEnd="8dp"
        NOT_FOUND_0xd="@+id/tvViewAll"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x14="@+id/res_0x7f0a1400_resetpinstatemanager_resetpinstate_failed"
        NOT_FOUND_0x15="@drawable/ic_arrow_right_mini"/>
    <TextView
        id="@+id/res_0x7f0a1400_resetpinstatemanager_resetpinstate_failed"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginStart="12dp"
        layout_marginEnd="12dp"
        NOT_FOUND_0x10="@+id/ivChevronRight"
        NOT_FOUND_0x11="@+id/ivChainQueryIcon"
        NOT_FOUND_0x14="0"
        style="@style/2132017924"/>
    <TextView
        textSize="12sp"
        textColor="#727272"
        id="@+id/tvViewAll"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        text="@string/chain_query_description"
        fontFamily="@font/sf_pro_regular"
        layout_marginEnd="12dp"
        NOT_FOUND_0x10="@+id/ivChevronRight"
        NOT_FOUND_0x12="@+id/res_0x7f0a1400_resetpinstatemanager_resetpinstate_failed"
        NOT_FOUND_0x13="@+id/res_0x7f0a1400_resetpinstatemanager_resetpinstate_failed"
        style="@style/2132017908"/>
    <View
        background="#d1d1d1"
        layout_width="0dp"
        layout_height="0.5dp"
        layout_marginTop="13dp"
        NOT_FOUND_0x12="@+id/res_0x7f0a1400_resetpinstatemanager_resetpinstate_failed"
        NOT_FOUND_0x13="@+id/tvViewAll"/>
</androidx.constraintlayout.widget.ConstraintLayout>
