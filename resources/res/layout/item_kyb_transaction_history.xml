<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_width="match_parent"
    layout_height="wrap_content">
    <TextView
        ellipsize="end"
        id="@+id/tv_kyb_order_type"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="9dp"
        contentDescription="@string/lbl_title_transaction_widget"
        fontFamily="@font/sf_pro_regular"
        layout_marginStart="12dp"
        NOT_FOUND_0x14="@+id/iv_kyb_order_type"
        NOT_FOUND_0x17="0"
        style="@style/2132017405"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        id="@+id/vg_offus_status"
        background="@drawable/bg_rounded_4dp_grey_10"
        paddingTop="2dp"
        paddingBottom="2dp"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        paddingStart="4dp"
        paddingEnd="4dp"
        layout_marginStart="4dp"
        NOT_FOUND_0x12="@+id/tv_kyb_order_type"
        NOT_FOUND_0x14="@+id/tv_kyb_order_type"
        NOT_FOUND_0x17="@+id/tv_kyb_order_type">
        <TextView
            id="@+id/tv_offus_status"
            layout_width="wrap_content"
            layout_height="wrap_content"
            text="@string/kyb_offus_in_progress"
            contentDescription="@string/lbl_in_progress_transaction"
            fontFamily="@font/sf_pro_regular"
            NOT_FOUND_0x12="0"
            NOT_FOUND_0x13="0"
            NOT_FOUND_0x15="0"
            NOT_FOUND_0x17="0"
            style="@style/2132017432"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <TextView
        gravity="end"
        id="@+id/tv_kyb_order_title"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        singleLine="true"
        contentDescription="@string/lbl_source_widget"
        fontFamily="@font/sf_pro_regular"
        layout_marginStart="16dp"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x14="@+id/tv_kyb_order_time"
        NOT_FOUND_0x16="@+id/tv_kyb_order_amount"
        style="@style/2132017440"/>
    <com.google.android.material.imageview.ShapeableImageView
        id="@+id/iv_kyb_order_type"
        layout_width="24dp"
        layout_height="24dp"
        NOT_FOUND_0x12="@+id/tv_kyb_order_time"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x17="@+id/tv_kyb_order_type"
        NOT_FOUND_0x18="@style/2132018200"/>
    <TextView
        id="@+id/tv_kyb_order_amount"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="9dp"
        contentDescription="@string/lbl_amount_widget"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x17="0"
        style="@style/2132017908"/>
    <TextView
        id="@+id/tv_kyb_order_time"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        layout_marginBottom="8.5dp"
        contentDescription="@string/lbl_date_time_widget"
        fontFamily="@font/sf_pro_regular"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x15="@+id/tv_kyb_order_type"
        NOT_FOUND_0x16="@+id/tv_kyb_order_type"
        style="@style/2132017440"/>
</androidx.constraintlayout.widget.ConstraintLayout>
