<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:aui="http://schemas.android.com/apk/res-auto"
    orientation="horizontal"
    background="#00000000"
    layout_width="wrap_content"
    layout_height="wrap_content">
    <LinearLayout
        gravity="center_vertical"
        orientation="horizontal"
        id="@+id/custom_callout_container"
        background="@drawable/griver_map_dark_bg"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginBottom="3dp">
        <LinearLayout
            gravity="center"
            orientation="vertical"
            id="@+id/custom_callout_left_layout"
            paddingTop="1dp"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginLeft="5dp"
            layout_marginRight="8dp">
            <TextView
                textSize="15sp"
                textStyle="bold"
                textColor="#ffffff"
                id="@+id/custom_callout_left_value"
                layout_width="wrap_content"
                layout_height="wrap_content"/>
            <TextView
                textSize="10sp"
                textColor="#ffffff"
                id="@+id/custom_callout_left_value_unit"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginTop="-2dp"
                text="Minutes"/>
        </LinearLayout>
        <View
            id="@+id/custom_callout_split_line"
            background="#8b919b"
            paddingTop="1dp"
            layout_width="1dp"
            layout_height="22dp"/>
        <LinearLayout
            gravity="center_vertical"
            orientation="horizontal"
            paddingTop="1dp"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginLeft="8dp">
            <TextView
                textSize="14sp"
                ellipsize="end"
                id="@+id/custom_callout_right_desc"
                layout_width="wrap_content"
                layout_height="wrap_content"
                text=""
                maxLines="1"/>
            <com.alibaba.griver.ui.ant.text.AUIconView
                id="@+id/custom_callout_right_arrow"
                layout_width="14dp"
                layout_height="15dp"
                layout_marginLeft="5dp"
                layout_marginRight="2dp"
                NOT_FOUND_0x11="@color/2131100066"
                NOT_FOUND_0x12="î˜©"/>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
