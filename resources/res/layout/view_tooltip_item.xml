<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_width="match_parent"
    layout_height="wrap_content">
    <ImageView
        id="@+id/image"
        layout_width="80dp"
        layout_height="80dp"
        layout_marginLeft="12dp"
        layout_marginRight="12dp"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x12="0"/>
    <TextView
        textSize="16sp"
        textStyle="bold"
        ellipsize="end"
        id="@+id/title"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_marginRight="28dp"
        layout_marginStart="16dp"
        layout_marginEnd="28dp"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xd="@+id/image"
        NOT_FOUND_0xf="@+id/image"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x13="20sp"
        style="@style/2132017916"/>
    <TextView
        ellipsize="end"
        id="@+id/description"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        layout_marginRight="28dp"
        layout_marginEnd="28dp"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xc="@+id/title"
        NOT_FOUND_0xe="@+id/title"
        NOT_FOUND_0x10="@+id/title"
        NOT_FOUND_0x11="@+id/title"
        NOT_FOUND_0x13="18sp"
        style="@style/2132017406"/>
</androidx.constraintlayout.widget.ConstraintLayout>
