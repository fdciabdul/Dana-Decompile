<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    gravity="center"
    orientation="horizontal"
    id="@+id/checkAppxSupportCrossVersionSnapshot"
    layout_width="match_parent"
    layout_height="32dp"
    layout_marginTop="4dp"
    layout_marginBottom="4dp">
    <de.hdodenhof.circleimageview.CircleImageView
        layout_gravity="start|center_vertical"
        id="@+id/iv_avatar"
        layout_width="24dp"
        layout_height="24dp"
        contentDescription="@string/imgProfileAvatar"
        layout_marginEnd="9dp"
        NOT_FOUND_0xd="1dp"/>
    <TextView
        ellipsize="end"
        id="@+id/tv_feed_content"
        layout_width="0dp"
        layout_height="wrap_content"
        maxLines="1"
        layout_weight="1"
        contentDescription="@string/lblActivity"
        layout_marginEnd="8dp"
        style="@style/2132017439"/>
    <ViewSwitcher
        id="@+id/button_view_switch"
        layout_width="wrap_content"
        layout_height="wrap_content">
        <com.airbnb.lottie.LottieAnimationView
            layout_gravity="end|center_vertical"
            id="@+id/res_0x7f0a0f22_daggermybillscomponent_mybillscomponentimpl_providefamilyaccountrepositoryprovider"
            layout_width="24dp"
            layout_height="24dp"
            NOT_FOUND_0xe="json/lottie/feed_widget_arrow_button_animation.lottie"
            NOT_FOUND_0xf="true"
            NOT_FOUND_0x10="2"/>
        <TextView
            ellipsize="end"
            gravity="end|center_vertical"
            layout_gravity="center_vertical"
            id="@+id/tv_time_stamp"
            layout_width="wrap_content"
            layout_height="wrap_content"
            contentDescription="@string/lblTimeActivity"
            style="@style/2132017440"/>
    </ViewSwitcher>
</LinearLayout>
