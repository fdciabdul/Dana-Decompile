<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    clipToPadding="false"
    layout_width="match_parent"
    layout_height="wrap_content"
    layout_marginTop="12dp"
    layout_marginBottom="12dp">
    <ImageView
        id="@+id/alertCameraNoPermissionDialog"
        layout_width="wrap_content"
        layout_height="wrap_content"
        src="@drawable/ic_rectangle_cross_20"
        layout_marginEnd="10dp"
        NOT_FOUND_0xa="0"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xe="0"/>
    <TextView
        ellipsize="end"
        id="@+id/isHelperTextDisplayed"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        layout_marginStart="8dp"
        layout_marginEnd="18dp"
        NOT_FOUND_0xa="0"
        NOT_FOUND_0xc="@+id/alertCameraNoPermissionDialog"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0xf="18sp"
        style="@style/2132017399"/>
</androidx.constraintlayout.widget.ConstraintLayout>
