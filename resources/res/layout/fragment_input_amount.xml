<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    orientation="vertical"
    background="#ffffff"
    focusable="true"
    clickable="true"
    layout_width="match_parent"
    layout_height="match_parent">
    <include
        id="@+id/toolbar"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x12="0"
        layout="@layout/view_cashier_toolbar"/>
    <LinearLayout
        orientation="vertical"
        id="@+id/llConfirmBtn"
        background="#ffffff"
        layout_width="match_parent"
        layout_height="wrap_content"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="0">
        <id.dana.component.buttoncomponent.DanaButtonPrimaryView
            id="@+id/res_0x7f0a0140_verifypinstatemanager_executeriskchallenge_2_1"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginLeft="16dp"
            layout_marginTop="8dp"
            layout_marginRight="16dp"
            layout_marginBottom="16dp"
            NOT_FOUND_0x13="@string/btn_continue"
            NOT_FOUND_0x14="0"/>
    </LinearLayout>
    <View
        id="@+id/vShimmerBtn"
        layout_width="match_parent"
        layout_height="wrap_content"
        NOT_FOUND_0xd="@+id/llConfirmBtn"
        NOT_FOUND_0xf="@+id/llConfirmBtn"
        NOT_FOUND_0x10="@+id/llConfirmBtn"
        NOT_FOUND_0x12="@+id/llConfirmBtn"/>
    <ScrollView
        id="@+id/scrollView"
        layout_width="match_parent"
        layout_height="0dp"
        NOT_FOUND_0xe="@+id/numpadView"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="@+id/toolbar">
        <androidx.constraintlayout.widget.ConstraintLayout
            layout_width="match_parent"
            layout_height="wrap_content">
            <include
                id="@+id/cancelEventDelivery"
                layout_width="match_parent"
                layout_height="wrap_content"
                NOT_FOUND_0xf="0"
                NOT_FOUND_0x10="0"
                NOT_FOUND_0x12="0"
                layout="@layout/view_cashier_header"/>
            <id.dana.cashier.view.CashierInputAmountView
                id="@+id/inputAmountView"
                layout_width="match_parent"
                layout_height="wrap_content"
                NOT_FOUND_0xf="0"
                NOT_FOUND_0x10="0"
                NOT_FOUND_0x11="@+id/cancelEventDelivery"/>
            <View
                id="@+id/vShimmerInputAmount"
                layout_width="match_parent"
                layout_height="wrap_content"
                NOT_FOUND_0xd="@+id/inputAmountView"
                NOT_FOUND_0xf="@+id/inputAmountView"
                NOT_FOUND_0x10="@+id/inputAmountView"
                NOT_FOUND_0x12="@+id/inputAmountView"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
    <id.dana.core.ui.richview.NumpadView
        id="@+id/numpadView"
        visibility="gone"
        layout_width="match_parent"
        layout_height="wrap_content"
        NOT_FOUND_0xe="@+id/llConfirmBtn"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="@+id/scrollView"/>
    <FrameLayout
        id="@+id/flDanaLoading"
        background="#80ffffff"
        visibility="gone"
        layout_width="match_parent"
        layout_height="match_parent">
        <id.dana.richview.LogoProgressView
            layout_gravity="center"
            id="@+id/lpvLoading"
            layout_width="wrap_content"
            layout_height="wrap_content"/>
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
