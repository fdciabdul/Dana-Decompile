<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_width="match_parent"
    layout_height="match_parent">
    <androidx.appcompat.widget.AppCompatTextView
        id="@+id/tv_title"
        layout_width="wrap_content"
        layout_height="wrap_content"
        text="@string/feed_highlight_title"
        layout_marginStart="8dp"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0xf="0"
        style="@style/2132017916"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        id="@+id/cl_loading"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="16dp"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0xe="@+id/tv_title">
        <View
            id="@+id/first_skeleton"
            background="@drawable/bg_rounded_8dp_grey30"
            layout_width="287dp"
            layout_height="80dp"
            NOT_FOUND_0xd="0"
            NOT_FOUND_0xf="0"/>
        <View
            id="@+id/second_skeleton"
            background="@drawable/bg_rounded_8dp_grey30"
            layout_width="287dp"
            layout_height="80dp"
            layout_marginStart="12dp"
            NOT_FOUND_0xc="@+id/first_skeleton"
            NOT_FOUND_0xf="0"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        id="@+id/cl_highlight"
        visibility="gone"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="12dp"
        layout_marginBottom="4dp"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0xe="@+id/tv_title">
        <androidx.recyclerview.widget.RecyclerView
            orientation="horizontal"
            id="@+id/rv_highlight"
            layout_width="match_parent"
            layout_height="92dp"
            NOT_FOUND_0xa="androidx.recyclerview.widget.LinearLayoutManager"
            NOT_FOUND_0xb="0"
            NOT_FOUND_0xd="0"
            NOT_FOUND_0xf="0"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
