<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    layout_gravity="bottom"
    orientation="horizontal"
    background="@drawable/com_mixpanel_android_mini_inapp_rounded"
    visibility="gone"
    layout_width="match_parent"
    layout_height="65dp"
    layout_marginLeft="10dp"
    layout_marginRight="10dp"
    layout_marginBottom="10dp">
    <com.mixpanel.android.takeoverinapp.MiniCircleImageView
        id="@+id/com_mixpanel_android_notification_image"
        layout_width="75dp"
        layout_height="match_parent"
        scaleType="centerInside"
        contentDescription="@string/com_mixpanel_android_notification_image"/>
    <TextView
        textSize="14sp"
        textColor="#ffffff"
        gravity="center_vertical"
        id="@+id/res_0x7f0a04f6_networkuserentitydata_externalsyntheticlambda0"
        paddingLeft="0dp"
        paddingRight="15dp"
        layout_width="0dp"
        layout_height="match_parent"
        layout_weight="100"/>
</LinearLayout>
