<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/cl_explore_service_container"
    background="@drawable/bg_home_widget_white_rounded"
    clipToPadding="false"
    layout_width="match_parent"
    layout_height="wrap_content">
    <TextView
        textColor="#108ee9"
        gravity="center_horizontal"
        id="@+id/tv_all_service"
        background="@drawable/bg_rounded_blue_transparent"
        paddingLeft="12dp"
        paddingTop="9dp"
        paddingRight="12dp"
        paddingBottom="9dp"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        text="@string/explore_service_all_services"
        lineSpacingExtra="4sp"
        contentDescription="@string/btn_services"
        layout_marginEnd="16dp"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x18="@+id/title"
        style="@style/2132017895"/>
    <androidx.recyclerview.widget.RecyclerView
        id="@+id/res_0x7f0a1193_access_openbottomsheetonboardingviewlist"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="16dp"
        layout_marginStart="12dp"
        NOT_FOUND_0x17="@+id/subtitle"/>
    <com.google.android.material.textview.MaterialTextView
        textColor="#000000"
        id="@+id/title"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="12dp"
        text="@string/explore_service_title"
        contentDescription="@string/lbl_recommended_services_title"
        layout_marginStart="12dp"
        NOT_FOUND_0x13="@+id/tv_all_service"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x18="0"
        style="@style/2132017924"/>
    <com.google.android.material.textview.MaterialTextView
        id="@+id/subtitle"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="2dp"
        text="@string/explore_service_description"
        includeFontPadding="false"
        contentDescription="@string/lbl_recommended_services_description"
        fontFamily="@font/sf_pro_regular"
        layout_marginStart="12dp"
        NOT_FOUND_0x13="@+id/tv_all_service"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x17="@+id/title"
        style="@style/2132017440"/>
</androidx.constraintlayout.widget.ConstraintLayout>
