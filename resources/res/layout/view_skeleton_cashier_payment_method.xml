<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/item_pay_later"
    layout_width="match_parent"
    layout_height="wrap_content">
    <View
        id="@+id/vIcon"
        background="@drawable/bg_rounded_grey_20_4dp"
        layout_width="36dp"
        layout_height="24dp"
        layout_marginTop="18dp"
        layout_marginStart="56dp"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xe="0"/>
    <View
        id="@+id/vTitle"
        background="@drawable/bg_rounded_grey_20_4dp"
        layout_width="0dp"
        layout_height="16dp"
        layout_marginTop="10dp"
        layout_marginStart="8dp"
        NOT_FOUND_0xb="@+id/vIcon"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0xf="0.3"/>
    <View
        id="@+id/vSubtitle"
        background="@drawable/bg_rounded_grey_20_4dp"
        layout_width="0dp"
        layout_height="16dp"
        layout_marginTop="8dp"
        NOT_FOUND_0xa="@+id/radioBtn"
        NOT_FOUND_0xc="@+id/vTitle"
        NOT_FOUND_0xd="@+id/vTitle"/>
    <de.hdodenhof.circleimageview.CircleImageView
        id="@+id/radioBtn"
        layout_width="24dp"
        layout_height="24dp"
        src="#e3e3e3"
        layout_marginStart="12dp"
        layout_marginEnd="16dp"
        NOT_FOUND_0x8="@+id/vSubtitle"
        NOT_FOUND_0x9="0"
        NOT_FOUND_0xb="@+id/vSubtitle"
        NOT_FOUND_0xe="@+id/vTitle"/>
    <View
        id="@+id/divider"
        background="#b8b8b8"
        layout_width="match_parent"
        layout_height="0.5dp"
        layout_marginTop="24dp"
        layout_marginStart="16dp"
        NOT_FOUND_0x8="0"
        NOT_FOUND_0x9="0"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xd="@+id/radioBtn"/>
</androidx.constraintlayout.widget.ConstraintLayout>
