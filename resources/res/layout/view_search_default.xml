<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    focusableInTouchMode="true"
    layout_width="match_parent"
    layout_height="wrap_content">
    <EditText
        textSize="16sp"
        textColorHint="#b8b8b8"
        ellipsize="end"
        id="@+id/etSearch"
        background="@drawable/search_view_background"
        paddingLeft="16dp"
        paddingTop="8dp"
        paddingRight="16dp"
        paddingBottom="8dp"
        layout_width="match_parent"
        layout_height="wrap_content"
        hint="@string/sendmoneygroup_x2p_searchview_hint"
        android:maxLength="50"
        drawablePadding="8dp"
        inputType="textNoSuggestions"
        drawableStart="@drawable/ic_search"
        paddingStart="16dp"
        paddingEnd="16dp"
        NOT_FOUND_0x15="16sp"
        NOT_FOUND_0x16="12sp"
        NOT_FOUND_0x17="1"
        NOT_FOUND_0x18="0"
        NOT_FOUND_0x1a="0"
        NOT_FOUND_0x1b="0"/>
    <ImageView
        id="@+id/iv_clear"
        padding="8dp"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        NOT_FOUND_0x18="@+id/etSearch"
        NOT_FOUND_0x19="@+id/etSearch"
        NOT_FOUND_0x1b="@+id/etSearch"
        NOT_FOUND_0x1c="@drawable/res_0x7f08084c_networkuserentitydata_externalsyntheticlambda7"/>
</androidx.constraintlayout.widget.ConstraintLayout>
