<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        left="-300dp"
        right="-300dp">
        <rotate
            android:fromDegrees="90"
            android:toDegrees="90">
            <shape shape="line">
                <stroke
                    width="2dp"
                    color="#ededed"
                    dashWidth="4dp"
                    dashGap="4dp"/>
            </shape>
        </rotate>
    </item>
</layer-list>
