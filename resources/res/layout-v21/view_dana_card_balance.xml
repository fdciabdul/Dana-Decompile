<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_gravity="center"
    id="@+id/res_0x7f0a0414_imagecaptureviewmodel_gettipuploadphoto_1"
    visibility="gone"
    layout_width="match_parent"
    layout_height="wrap_content"
    foregroundGravity="center">
    <androidx.cardview.widget.CardView
        layout_width="match_parent"
        layout_height="0dp"
        layout_margin="8dp"
        NOT_FOUND_0x1b="10dp"
        NOT_FOUND_0x1c="6dp"
        NOT_FOUND_0x1d="0"
        NOT_FOUND_0x1f="7:4"
        NOT_FOUND_0x21="0"
        NOT_FOUND_0x22="0"
        NOT_FOUND_0x25="0">
        <androidx.constraintlayout.widget.ConstraintLayout
            background="@drawable/bg_balance"
            layout_width="match_parent"
            layout_height="match_parent">
            <ImageView
                id="@+id/iv_card_logo"
                layout_marginLeft="12dp"
                layout_marginTop="16dp"
                layout_centerVertical="true"
                layout_marginStart="12dp"
                NOT_FOUND_0x1e="@+id/iv_path"
                NOT_FOUND_0x23="0"
                NOT_FOUND_0x25="0"
                NOT_FOUND_0x26="@drawable/group_6"
                style="@style/2132018187"/>
            <ImageView
                id="@+id/iv_path"
                visibility="invisible"
                layout_marginTop="16dp"
                NOT_FOUND_0x1e="@+id/tv_mask_number"
                NOT_FOUND_0x23="@+id/iv_card_logo"
                NOT_FOUND_0x24="@+id/iv_card_logo"
                NOT_FOUND_0x26="@drawable/ic_line"
                style="@style/2132018187"/>
            <TextView
                textSize="16sp"
                textColor="#ffffff"
                id="@+id/tv_mask_number"
                layout_marginTop="8dp"
                lineSpacingExtra="8sp"
                fontFamily="@font/2131296266"
                NOT_FOUND_0x1e="@+id/tv_balance"
                NOT_FOUND_0x23="@+id/iv_card_logo"
                NOT_FOUND_0x24="@+id/iv_path"
                style="@style/2132018187"/>
            <TextView
                textSize="12sp"
                textColor="#ccfcfcfc"
                id="@+id/tv_balance"
                layout_marginTop="8dp"
                text="@string/balance"
                lineSpacingExtra="4sp"
                fontFamily="@font/2131296265"
                NOT_FOUND_0x1e="@+id/tbv_pay_content"
                NOT_FOUND_0x23="@+id/iv_card_logo"
                NOT_FOUND_0x24="@+id/tv_mask_number"
                style="@style/2132018187"/>
            <id.dana.richview.ToggleBalanceView
                id="@+id/tbv_pay_content"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginBottom="17dp"
                lineSpacingExtra="8sp"
                NOT_FOUND_0x1d="0"
                NOT_FOUND_0x21="0"
                NOT_FOUND_0x23="@+id/iv_card_logo"
                NOT_FOUND_0x24="@+id/tv_balance"
                NOT_FOUND_0x27="0"/>
            <TextView
                textSize="12sp"
                textColor="#ffffff"
                gravity="center"
                id="@+id/SubscriptionIntervalType"
                background="@drawable/btn_qris_pay_topup_round"
                layout_marginBottom="17dp"
                minWidth="100dp"
                text="@string/pay_top_up"
                layout_alignParentRight="true"
                layout_alignParentBottom="true"
                lineSpacingExtra="4sp"
                textAllCaps="true"
                fontFamily="@font/2131296266"
                textAlignment="center"
                layout_marginEnd="12dp"
                layout_alignParentEnd="true"
                letterSpacing="-0.02"
                NOT_FOUND_0x1d="0"
                NOT_FOUND_0x20="0"
                style="@style/2132018187"/>
