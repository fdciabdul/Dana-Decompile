<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/res_0x7f0a1a4f_daggersmartpaycomponent_smartpaycomponentimpl_userrepositoryprovider"
    layout_width="match_parent"
    layout_height="match_parent">
    <FrameLayout
        id="@+id/vg_social_feed_avatar_container"
        layout_width="76dp"
        layout_height="74dp"
        layout_marginStart="12dp"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x17="0">
        <de.hdodenhof.circleimageview.CircleImageView
            layout_gravity="center_horizontal"
            id="@+id/iv_feeds_widget_profile"
            layout_width="56dp"
            layout_height="56dp"
            src="@drawable/avatar_placeholder"
            NOT_FOUND_0xf="@color/2131099990"
            NOT_FOUND_0x10="1dp"/>
        <androidx.appcompat.widget.AppCompatImageView
            layout_gravity="bottom"
            id="@+id/iv_feeds_widget_mini_avatar_1"
            layout_width="24dp"
            layout_height="24dp"
            layout_marginStart="10dp"
            NOT_FOUND_0x18="@drawable/ic_avatar_illustration_photo_red_shirt"/>
        <androidx.appcompat.widget.AppCompatImageView
            layout_gravity="bottom|center_horizontal"
            id="@+id/iv_feeds_widget_mini_avatar_2"
            layout_width="24dp"
            layout_height="24dp"
            NOT_FOUND_0x18="@drawable/ic_avatar_illustration_photo_green_shirt"/>
        <androidx.appcompat.widget.AppCompatImageView
            layout_gravity="end|bottom"
            id="@+id/res_0x7f0a0b22_hereauthenticationpreference_savebackoffdata_1"
            layout_width="24dp"
            layout_height="24dp"
            src="@drawable/ic_bubble_number_9"
            layout_marginEnd="10dp"/>
    </FrameLayout>
    <LinearLayout
        orientation="vertical"
        id="@+id/ll_container"
        background="@drawable/home_component_border"
        padding="8dp"
        layout_width="0dp"
        layout_height="76dp"
        layout_marginBottom="20dp"
        layout_marginStart="8dp"
        layout_marginEnd="12dp"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x13="@+id/vg_social_feed_avatar_container"
        NOT_FOUND_0x16="@+id/vg_social_feed_avatar_container"
        NOT_FOUND_0x17="0">
        <TextView
            ellipsize="end"
            id="@+id/tv_feeds_widget_activities"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginBottom="4dp"
            maxLines="2"
            minLines="2"
            style="@style/2132017405"/>
        <TextView
            id="@+id/saveMonthOfFetchFreeTransfer"
            layout_width="match_parent"
            layout_height="0dp"
            layout_weight="1"
            NOT_FOUND_0x13="@+id/iv_feeds_widget_profile"
            NOT_FOUND_0x15="@+id/tv_feeds_widget_activities"
            style="@style/2132017440"/>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
