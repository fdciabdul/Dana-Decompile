<?xml version="1.0" encoding="utf-8"?>
<com.alibaba.griver.ui.ant.layout.AULinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:au="http://schemas.android.com/apk/res-auto"
    orientation="vertical"
    layout_width="match_parent"
    layout_height="match_parent">
    <com.alibaba.griver.ui.ant.AUTitleBar
        id="@+id/title_bar"
        layout_width="match_parent"
        layout_height="wrap_content"/>
    <com.alibaba.griver.ui.ant.layout.AULinearLayout
        orientation="vertical"
        layout_width="match_parent"
        layout_height="match_parent">
        <com.alibaba.griver.ui.ant.layout.AUFrameLayout
            id="@+id/map_container"
            layout_width="match_parent"
            layout_height="240dp">
            <com.alipay.mobile.apmap.AdapterMapView
                id="@+id/map"
                layout_width="match_parent"
                layout_height="match_parent"/>
            <com.alibaba.griver.ui.ant.AUImageView
                layout_gravity="bottom|left"
                id="@+id/map_my_location"
                background="@drawable/griver_map_my_poi_btn_selector"
                layout_width="42dp"
                layout_height="42dp"
                layout_margin="3.3dp"
                contentDescription="@string/griver_map_my_location"/>
            <com.alibaba.griver.ui.ant.AUImageView
                layout_gravity="bottom"
                background="#eeeeee"
                layout_width="match_parent"
                layout_height="1px"/>
        </com.alibaba.griver.ui.ant.layout.AUFrameLayout>
        <com.alibaba.griver.ui.ant.listview.AUListView
            id="@+id/poi_list"
            layout_width="match_parent"
            layout_height="match_parent"
            divider="@null"
            dividerHeight="0px"
            choiceMode="singleChoice"/>
    </com.alibaba.griver.ui.ant.layout.AULinearLayout>
</com.alibaba.griver.ui.ant.layout.AULinearLayout>
