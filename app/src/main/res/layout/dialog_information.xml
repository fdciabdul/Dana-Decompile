<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="#ffffff"
    paddingTop="24dp"
    style="@style/2132017490">
    <TextView
        textSize="17sp"
        textColor="#030303"
        id="@+id/tv_header"
        paddingLeft="15.5dp"
        paddingRight="15.5dp"
        visibility="gone"
        layout_marginTop="19dp"
        fontFamily="@font/open_sans_bold"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x16="0"
        style="@style/2132018187"/>
    <TextView
        textSize="13sp"
        textColor="#030303"
        id="@+id/tv_body"
        paddingLeft="15.5dp"
        paddingRight="15.5dp"
        layout_marginTop="8dp"
        layout_marginBottom="21dp"
        fontFamily="@font/2131296265"
        textAlignment="center"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x15="@+id/tv_header"
        style="@style/2132018187"/>
    <View
        id="@+id/separator_horizontal"
        background="#c74d4d4d"
        layout_width="match_parent"
        layout_height="1px"
        layout_marginTop="20.5dp"
        NOT_FOUND_0x15="@+id/tv_body"/>
    <View
        id="@+id/separator_vertical"
        background="#c74d4d4d"
        layout_width="1px"
        layout_height="40dp"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x15="@+id/separator_horizontal"/>
    <TextView
        textSize="17sp"
        textColor="#007aff"
        gravity="center"
        id="@+id/btn_left"
        background="@drawable/transparent_square_rounded_skeleton"
        clickable="true"
        layout_width="0dp"
        layout_height="wrap_content"
        fontFamily="@font/2131296265"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x13="@+id/separator_vertical"
        NOT_FOUND_0x15="@+id/separator_horizontal"/>
    <TextView
        textSize="17sp"
        textColor="#007aff"
        gravity="center"
        id="@+id/btn_right"
        background="@drawable/transparent_square_rounded_skeleton"
        clickable="true"
        layout_width="0dp"
        layout_height="wrap_content"
        fontFamily="@font/2131296265"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x12="@+id/separator_vertical"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x15="@+id/separator_horizontal"/>
</androidx.constraintlayout.widget.ConstraintLayout>
