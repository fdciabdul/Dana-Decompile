<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/vg_friends_header_root"
    background="?attr/2130969992"
    layout_width="80dp"
    layout_height="wrap_content"
    layout_marginLeft="2dp"
    layout_marginRight="2dp">
    <de.hdodenhof.circleimageview.CircleImageView
        layout_gravity="center_horizontal"
        id="@+id/iv_friends_header_profile_avatar"
        layout_width="52dp"
        layout_height="52dp"
        src="@drawable/avatar_placeholder"
        contentDescription="@string/imgHighlightProfileAvatar"
        NOT_FOUND_0x11="@color/2131099990"
        NOT_FOUND_0x12="1dp"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x16="0"/>
    <androidx.appcompat.widget.AppCompatTextView
        textSize="10sp"
        textColor="@android:color/white"
        ellipsize="end"
        id="@+id/res_0x7f0a1707_tncsummaryactivity_externalsyntheticlambda0"
        layout_width="wrap_content"
        layout_height="14dp"
        layout_marginTop="6dp"
        maxWidth="76dp"
        text="@string/see_all"
        maxLines="1"
        fontFamily="@font/2131296265"
        NOT_FOUND_0x13="@+id/iv_friends_header_profile_avatar"
        NOT_FOUND_0x14="@+id/iv_friends_header_profile_avatar"
        NOT_FOUND_0x15="@+id/iv_friends_header_profile_avatar"/>
</androidx.constraintlayout.widget.ConstraintLayout>
