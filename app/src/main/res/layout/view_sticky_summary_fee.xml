<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/clStickySummaryFee"
    background="@drawable/bg_transparent_rounded_grey_20"
    layout_width="match_parent"
    layout_height="wrap_content">
    <LinearLayout
        orientation="horizontal"
        id="@+id/llAdditionalFee"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_margin="12dp"
        NOT_FOUND_0xd="@+id/llAdditionalExtraFee"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x11="0">
        <androidx.appcompat.widget.AppCompatTextView
            layout_gravity="start"
            layout_width="0dp"
            layout_height="wrap_content"
            text="@string/sendmoney_summary_admin_fee_title"
            layout_weight="1"
            style="@style/2132017405"/>
        <id.dana.richview.CurrencyTextView
            gravity="end"
            id="@+id/tvFeeAmount"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_weight="1"
            style="@style/2132017406"/>
        <androidx.appcompat.widget.AppCompatTextView
            gravity="end"
            id="@+id/res_0x7f0a13b4_defaultmeter_noopdoubleupdowncounter"
            visibility="gone"
            layout_width="0dp"
            layout_height="wrap_content"
            text="@string/sendmoney_confirmation_free"
            layout_weight="1"
            textAllCaps="true"
            style="@style/2132017406"/>
    </LinearLayout>
    <LinearLayout
        orientation="horizontal"
        id="@+id/llAdditionalExtraFee"
        visibility="gone"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_margin="12dp"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="@+id/llAdditionalFee">
        <androidx.appcompat.widget.AppCompatTextView
            layout_gravity="start"
            layout_width="0dp"
            layout_height="wrap_content"
            text="@string/sendmoney_summary_admin_fee_title"
            layout_weight="1"
            style="@style/2132017405"/>
        <id.dana.richview.CurrencyTextView
            gravity="end"
            id="@+id/tvExtraFeeAmount"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_weight="1"
            style="@style/2132017406"/>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
