<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    orientation="vertical"
    id="@+id/allowedTargets"
    background="#f5f5f5"
    focusable="true"
    clickable="true"
    layout_width="match_parent"
    layout_height="match_parent">
    <include
        id="@+id/toolbar"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x14="0"
        layout="@layout/view_cashier_toolbar"/>
    <ScrollView
        layout_width="match_parent"
        layout_height="0dp"
        NOT_FOUND_0x10="@+id/llConfirmBtn"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x13="@+id/toolbar">
        <LinearLayout
            orientation="vertical"
            layout_width="match_parent"
            layout_height="wrap_content">
            <include layout="@layout/view_cashier_add_card_header"/>
            <include layout="@layout/view_cashier_input_card"/>
            <id.dana.cashier.view.CashierPromoView
                id="@+id/cashierPromoView"
                background="#ffffff"
                paddingTop="4dp"
                visibility="gone"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginTop="4dp"/>
            <id.dana.cashier.view.CashierAddOnView
                id="@+id/res_0x7f0a029c_cyfmonitor_challengeactioncallback"
                visibility="gone"
                layout_width="match_parent"
                layout_height="wrap_content"/>
            <View
                background="#f5f5f5"
                layout_width="match_parent"
                layout_height="89dp"/>
        </LinearLayout>
    </ScrollView>
    <LinearLayout
        orientation="vertical"
        id="@+id/llConfirmBtn"
        background="#ffffff"
        layout_width="match_parent"
        layout_height="wrap_content"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="0">
        <id.dana.component.buttoncomponent.DanaButtonPrimaryView
            id="@+id/res_0x7f0a0140_verifypinstatemanager_executeriskchallenge_2_1"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginLeft="16dp"
            layout_marginTop="8dp"
            layout_marginRight="16dp"
            layout_marginBottom="16dp"
            NOT_FOUND_0x15="@string/pay"
            NOT_FOUND_0x16="0"/>
    </LinearLayout>
    <id.dana.richview.DanaProtectionLoadingView
        id="@+id/danaProtectLoadingView"
        contentDescription="@string/icon_secure_lottie"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x14="0"
        style="@style/2132017531"/>
    <FrameLayout
        id="@+id/flDanaLoading"
        background="#80ffffff"
        visibility="gone"
        layout_width="match_parent"
        layout_height="match_parent">
        <id.dana.richview.LogoProgressView
            layout_gravity="center"
            id="@+id/lpvLoading"
            layout_width="wrap_content"
            layout_height="wrap_content"/>
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
