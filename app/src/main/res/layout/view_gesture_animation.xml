<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_width="match_parent"
    layout_height="wrap_content">
    <com.airbnb.lottie.LottieAnimationView
        id="@+id/lavGestureHold"
        layout_width="wrap_content"
        layout_height="wrap_content"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="true"
        NOT_FOUND_0x11="json/lottie/hold_tap.lottie"
        NOT_FOUND_0x12="true"/>
    <FrameLayout
        background="@drawable/rounded_blue"
        layout_marginTop="16dp"
        NOT_FOUND_0xc="@+id/lavGestureHold"
        NOT_FOUND_0xd="@+id/lavGestureHold"
        NOT_FOUND_0xe="@+id/lavGestureHold"
        style="@style/2132018187">
        <TextView
            textSize="12sp"
            textColor="#ffffff"
            gravity="center"
            layout_margin="4dp"
            text="@string/press_and_hold"
            lines="2"
            fontFamily="@font/2131296266"
            style="@style/2132018187"/>
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
