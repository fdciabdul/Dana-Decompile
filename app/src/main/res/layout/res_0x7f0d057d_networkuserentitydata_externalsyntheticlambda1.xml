<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    gravity="center_horizontal"
    layout_width="match_parent"
    layout_height="wrap_content">
    <FrameLayout
        id="@+id/phone_wrapper"
        background="@drawable/btn_common_white_border_grey20"
        layout_width="match_parent"
        layout_height="44dp"
        NOT_FOUND_0x18="0"
        NOT_FOUND_0x1b="0"
        NOT_FOUND_0x1d="0"/>
    <LinearLayout
        layout_gravity="center"
        orientation="horizontal"
        id="@+id/ll_prefix"
        paddingLeft="12dp"
        paddingRight="12dp"
        layout_width="wrap_content"
        layout_height="0dp"
        layout_margin="2dp"
        NOT_FOUND_0x17="@+id/phone_wrapper"
        NOT_FOUND_0x1b="@+id/phone_wrapper"
        NOT_FOUND_0x1d="@+id/phone_wrapper">
        <androidx.appcompat.widget.AppCompatImageView
            layout_gravity="center"
            id="@+id/isLayoutRequested_res_0x7f0a0b28"
            NOT_FOUND_0x1e="@drawable/res_0x7f08091e_networkuserentitydata_externalsyntheticlambda8"
            style="@style/2132018187"/>
        <TextView
            textSize="16sp"
            textColor="@android:color/black"
            layout_gravity="center"
            id="@+id/prefix"
            layout_marginStart="10dp"
            NOT_FOUND_0x16="@font/2131296266"
            style="@style/2132018187"/>
    </LinearLayout>
    <androidx.appcompat.widget.AppCompatTextView
        textSize="24sp"
        textColor="#a4a4a4"
        id="@+id/tv_middle_number"
        layout_width="wrap_content"
        layout_height="wrap_content"
        NOT_FOUND_0x16="@font/2131296266"
        NOT_FOUND_0x17="@+id/ll_prefix"
        NOT_FOUND_0x1a="@+id/ll_prefix"
        NOT_FOUND_0x1d="@+id/ll_prefix"/>
    <EditText
        textSize="24sp"
        textColor="@android:color/black"
        textColorHint="#bfbfbf"
        layout_gravity="center"
        id="@+id/input"
        background="@null"
        paddingRight="10dp"
        layout_width="0dp"
        layout_height="wrap_content"
        hint="@string/lbl_phone_number_placeholder"
        android:maxLength="15"
        inputType="textNoSuggestions|numberPassword"
        contentDescription="@string/txt_area_phone_number"
        NOT_FOUND_0x16="@font/2131296266"
        NOT_FOUND_0x17="@+id/ll_prefix"
        NOT_FOUND_0x19="@+id/iv_left"
        NOT_FOUND_0x1a="@+id/tv_middle_number"
        NOT_FOUND_0x1d="@+id/ll_prefix"/>
    <View
        id="@+id/separator"
        background="#80ffffff"
        visibility="gone"
        layout_width="40dp"
        layout_marginTop="5dp"
        layout_centerHorizontal="true"
        NOT_FOUND_0x18="0"
        NOT_FOUND_0x1b="0"
        NOT_FOUND_0x1c="@+id/phone_wrapper"
        style="@style/2132017642"/>
    <androidx.appcompat.widget.AppCompatImageView
        layout_gravity="center"
        id="@+id/iv_left"
        layout_marginEnd="14dp"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x18="0"
        NOT_FOUND_0x1d="0"
        style="@style/2132018187"/>
</androidx.constraintlayout.widget.ConstraintLayout>
