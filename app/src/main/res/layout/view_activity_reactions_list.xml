<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    focusable="true"
    focusableInTouchMode="true"
    clickable="true"
    layout_width="match_parent"
    layout_height="match_parent">
    <TextView
        id="@+id/tv_reactions_name"
        background="#e3e3e3"
        paddingLeft="16dp"
        paddingTop="4dp"
        paddingBottom="4dp"
        layout_width="match_parent"
        layout_height="wrap_content"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0xf="0"
        style="@style/2132017432"/>
    <id.dana.richview.LogoProgressView
        id="@+id/lpv_activity_reactions"
        layout_width="match_parent"
        layout_height="0dp"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0xe="@+id/tv_reactions_name"/>
    <androidx.recyclerview.widget.RecyclerView
        id="@+id/rv_activity_reactions"
        visibility="invisible"
        layout_width="match_parent"
        layout_height="0dp"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xe="@+id/tv_reactions_name"/>
</androidx.constraintlayout.widget.ConstraintLayout>
