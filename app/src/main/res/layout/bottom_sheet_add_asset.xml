<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_gravity="bottom"
    id="@+id/container"
    background="@drawable/rounded_top_background"
    layout_width="match_parent"
    layout_height="match_parent"
    NOT_FOUND_0xd="@string/bottom_sheet_behavior">
    <androidx.core.widget.NestedScrollView
        layout_gravity="bottom"
        layout_width="match_parent"
        layout_height="wrap_content">
        <androidx.constraintlayout.widget.ConstraintLayout
            layout_width="match_parent"
            layout_height="wrap_content">
            <androidx.appcompat.widget.AppCompatImageView
                layout_gravity="center_horizontal"
                id="@+id/tab_icon"
                layout_width="56dp"
                layout_height="4dp"
                layout_marginTop="8dp"
                NOT_FOUND_0xe="0"
                NOT_FOUND_0xf="0"
                NOT_FOUND_0x11="0"
                NOT_FOUND_0x12="@drawable/ic_flat_dark"/>
            <androidx.appcompat.widget.AppCompatTextView
                textSize="18sp"
                textColor="#313131"
                id="@+id/tv_title_first_section"
                visibility="gone"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginLeft="16dp"
                layout_marginTop="20dp"
                layout_marginRight="16dp"
                text="@string/add_asset_payment_title"
                fontFamily="@font/2131296266"
                NOT_FOUND_0x10="@+id/tab_icon"/>
            <androidx.recyclerview.widget.RecyclerView
                id="@+id/rv_add_first_section"
                visibility="gone"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginTop="16dp"
                NOT_FOUND_0x10="@+id/tv_title_first_section"/>
            <androidx.appcompat.widget.AppCompatTextView
                textSize="18sp"
                textColor="#313131"
                id="@+id/tv_title_second_section"
                visibility="gone"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginLeft="16dp"
                layout_marginTop="20dp"
                layout_marginRight="16dp"
                fontFamily="@font/2131296266"
                NOT_FOUND_0x10="@+id/rv_add_first_section"/>
            <androidx.recyclerview.widget.RecyclerView
                id="@+id/rv_add_second_section"
                visibility="gone"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginTop="16dp"
                NOT_FOUND_0x10="@+id/tv_title_second_section"/>
            <androidx.appcompat.widget.AppCompatTextView
                textSize="18sp"
                textColor="#313131"
                id="@+id/tv_title_third_section"
                visibility="gone"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginLeft="16dp"
                layout_marginTop="20dp"
                layout_marginRight="16dp"
                fontFamily="@font/2131296266"
                NOT_FOUND_0x10="@+id/rv_add_second_section"/>
            <androidx.recyclerview.widget.RecyclerView
                id="@+id/rv_add_third_section"
                visibility="gone"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginTop="16dp"
                NOT_FOUND_0x10="@+id/tv_title_third_section"/>
            <androidx.appcompat.widget.AppCompatTextView
                textSize="18sp"
                textColor="#313131"
                id="@+id/tv_title_fourth_section"
                visibility="gone"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginLeft="16dp"
                layout_marginTop="20dp"
                layout_marginRight="16dp"
                fontFamily="@font/2131296266"
                NOT_FOUND_0x10="@+id/rv_add_third_section"/>
            <androidx.recyclerview.widget.RecyclerView
                id="@+id/rv_add_fourth_section"
                visibility="gone"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginTop="16dp"
                NOT_FOUND_0x10="@+id/tv_title_fourth_section"/>
            <androidx.appcompat.widget.AppCompatTextView
                textSize="18sp"
                textColor="#313131"
                id="@+id/tv_title_fifth_section"
                visibility="gone"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginLeft="16dp"
                layout_marginTop="20dp"
                layout_marginRight="16dp"
                fontFamily="@font/2131296266"
                NOT_FOUND_0x10="@+id/rv_add_fourth_section"/>
            <androidx.recyclerview.widget.RecyclerView
                id="@+id/rv_add_fifth_section"
                visibility="gone"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginTop="16dp"
                NOT_FOUND_0x10="@+id/tv_title_fifth_section"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.core.widget.NestedScrollView>
</FrameLayout>
