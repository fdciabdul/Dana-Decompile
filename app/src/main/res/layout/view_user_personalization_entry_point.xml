<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/cl_explore_promo"
    background="@drawable/bg_home_widget_white_rounded"
    padding="16dp"
    layout_width="match_parent"
    layout_height="wrap_content">
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/iv_personalization_entry_illustration"
        layout_width="56dp"
        layout_height="56dp"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x14="@drawable/ic_personalization_entry"/>
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/iv_personalization_entry_chevron"
        layout_width="16dp"
        layout_height="16dp"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x14="@drawable/arrow_right_gray"
        NOT_FOUND_0x15="@color/2131099993"/>
    <androidx.appcompat.widget.AppCompatTextView
        textColor="#727272"
        id="@+id/tv_personalization_entry_desc"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginLeft="12dp"
        layout_marginTop="4dp"
        layout_marginRight="12dp"
        text="@string/personalization_entry_desc"
        NOT_FOUND_0xc="@+id/iv_personalization_entry_illustration"
        NOT_FOUND_0xf="@+id/iv_personalization_entry_illustration"
        NOT_FOUND_0x10="@+id/iv_personalization_entry_chevron"
        NOT_FOUND_0x12="@+id/tv_personalization_entry_title"
        style="@style/2132017433"/>
    <androidx.appcompat.widget.AppCompatTextView
        ellipsize="end"
        id="@+id/tv_personalization_entry_title"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginLeft="12dp"
        layout_marginRight="12dp"
        text="@string/personalization_entry_title"
        maxLines="1"
        NOT_FOUND_0xd="@+id/tv_personalization_entry_desc"
        NOT_FOUND_0xf="@+id/iv_personalization_entry_illustration"
        NOT_FOUND_0x10="@+id/iv_personalization_entry_chevron"
        NOT_FOUND_0x13="@+id/iv_personalization_entry_illustration"
        style="@style/2132017908"/>
</androidx.constraintlayout.widget.ConstraintLayout>
