<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    orientation="vertical"
    background="@drawable/bg_recent"
    padding="16dp"
    layout_width="wrap_content"
    layout_height="wrap_content">
    <ImageView
        layout_gravity="center"
        background="@drawable/ic_phone_number_already_registered"
        layout_width="128dp"
        layout_height="128dp"
        layout_marginTop="16dp"/>
    <TextView
        textColor="@android:color/black"
        layout_gravity="center"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginLeft="56dp"
        layout_marginTop="16dp"
        layout_marginRight="56dp"
        text="@string/uh_oh_your_dana_account_is_different"
        fontFamily="@font/2131296266"
        textAlignment="center"/>
    <TextView
        textSize="12sp"
        textColor="#313131"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_marginRight="16dp"
        layout_marginBottom="16dp"
        text="@string/please_login"
        fontFamily="@font/2131296265"
        textAlignment="center"/>
    <LinearLayout
        gravity="center"
        orientation="horizontal"
        layout_width="match_parent"
        layout_height="wrap_content">
        <Button
            textSize="12sp"
            textColor="#108ee9"
            id="@+id/btn_cancel"
            background="@android:color/transparent"
            layout_width="match_parent"
            layout_height="32dp"
            text="@string/option_cancel"
            layout_weight="1"
            fontFamily="@font/2131296266"/>
        <Button
            textSize="12sp"
            id="@+id/btn_login"
            layout_width="match_parent"
            layout_height="32dp"
            text="@string/login_title"
            layout_weight="1"
            style="@style/2132017655"/>
        <id.dana.richview.LogoProgressView
            id="@+id/lpv_loading"
            visibility="gone"
            layout_width="match_parent"
            layout_height="32dp"
            layout_weight="1"/>
    </LinearLayout>
</LinearLayout>
