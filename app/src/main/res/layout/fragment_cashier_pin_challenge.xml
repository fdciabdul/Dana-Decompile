<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="#f5f5f5"
    focusable="true"
    clickable="true"
    layout_width="match_parent"
    layout_height="match_parent">
    <include
        id="@+id/vRiskBinding"
        NOT_FOUND_0x1f="0"
        NOT_FOUND_0x23="0"
        NOT_FOUND_0x25="0"
        layout="@layout/view_cashier_risk_binding_pay"/>
    <androidx.appcompat.widget.AppCompatTextView
        gravity="center"
        id="@+id/tvEnterPin"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="24dp"
        text="@string/please_enter_pin"
        contentDescription="@string/sdet_lbl_enter_pin"
        textAlignment="center"
        layout_marginStart="32dp"
        layout_marginEnd="32dp"
        NOT_FOUND_0x1f="0"
        NOT_FOUND_0x23="0"
        NOT_FOUND_0x24="@+id/vRiskBinding"
        style="@style/2132017916"/>
    <androidx.appcompat.widget.AppCompatTextView
        gravity="center"
        id="@+id/tvCancelPin"
        visibility="gone"
        fitsSystemWindows="true"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="76dp"
        layout_marginBottom="28dp"
        text="@string/option_cancel"
        textAllCaps="true"
        textAlignment="center"
        layout_marginStart="32dp"
        layout_marginEnd="32dp"
        NOT_FOUND_0x20="@+id/guideline4"
        NOT_FOUND_0x23="0"
        NOT_FOUND_0x24="@+id/edtInputPin"
        style="@style/2132017911"/>
    <androidx.appcompat.widget.AppCompatTextView
        gravity="center"
        id="@+id/tvForgotPin"
        fitsSystemWindows="true"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="76dp"
        layout_marginBottom="28dp"
        text="@string/forgot_pin_with_question_mark"
        contentDescription="@string/btn_forgot_pin_cashier"
        textAllCaps="true"
        textAlignment="center"
        layout_marginStart="32dp"
        layout_marginEnd="32dp"
        NOT_FOUND_0x1f="0"
        NOT_FOUND_0x23="0"
        NOT_FOUND_0x24="@+id/edtInputPin"
        style="@style/2132017911"/>
    <androidx.appcompat.widget.AppCompatTextView
        gravity="center"
        id="@+id/tvPinError"
        visibility="gone"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="16dp"
        contentDescription="@string/sdet_lbl_incorrect_pin"
        textAlignment="center"
        layout_marginStart="16dp"
        layout_marginEnd="16dp"
        NOT_FOUND_0x1f="0"
        NOT_FOUND_0x23="0"
        NOT_FOUND_0x24="@+id/edtInputPin"
        style="@style/2132017409"/>
    <androidx.constraintlayout.widget.Guideline
        orientation="vertical"
        id="@+id/guideline4"
        layout_width="wrap_content"
        layout_height="wrap_content"
        NOT_FOUND_0x21="0.5"/>
    <id.dana.core.ui.richview.PinEntryEditText
        textSize="16sp"
        textStyle="bold"
        textColor="@android:color/black"
        textColorHint="#e6e6e6"
        id="@+id/edtInputPin"
        paddingTop="26dp"
        paddingBottom="26dp"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="16dp"
        android:maxLength="6"
        digits="1234567890"
        inputType="textVisiblePassword|number"
        isScrollContainer="true"
        contentDescription="@string/sdet_txt_pin"
        textIsSelectable="false"
        layout_marginStart="80dp"
        layout_marginEnd="80dp"
        NOT_FOUND_0x1f="0"
        NOT_FOUND_0x22="0"
        NOT_FOUND_0x23="0"
        NOT_FOUND_0x24="@+id/tvEnterPin"
        NOT_FOUND_0x26="@android:color/transparent"
        NOT_FOUND_0x27="â—"/>
    <id.dana.richview.DanaProtectionLoadingView
        id="@+id/danaProtectLoadingView"
        contentDescription="@string/icon_secure_lottie"
        NOT_FOUND_0x1e="0"
        NOT_FOUND_0x1f="0"
        NOT_FOUND_0x23="0"
        NOT_FOUND_0x25="0"
        style="@style/2132017531"/>
    <FrameLayout
        id="@+id/flDanaLoading"
        background="#80ffffff"
        visibility="gone"
        layout_width="match_parent"
        layout_height="match_parent">
        <id.dana.richview.LogoProgressView
            layout_gravity="center"
            id="@+id/lpvLoading"
            layout_width="wrap_content"
            layout_height="wrap_content"/>
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
