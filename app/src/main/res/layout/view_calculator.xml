<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    orientation="vertical"
    paddingTop="@dimen/2131165324"
    style="@style/2132017490">
    <TextView
        textSize="16sp"
        textColor="#4a4a4a"
        id="@+id/tv_hint_operand_left"
        paddingBottom="12dp"
        fontFamily="@font/2131296266"
        style="@style/2132018187"/>
    <TextView
        textSize="16sp"
        textColor="#4a4a4a"
        id="@+id/tv_hint_operator"
        paddingLeft="5dp"
        paddingRight="5dp"
        paddingBottom="12dp"
        fontFamily="@font/2131296266"
        layout_toEndOf="@+id/tv_hint_operand_left"
        style="@style/2132018187"/>
    <TextView
        textSize="16sp"
        textColor="#4a4a4a"
        id="@+id/tv_hint_operand_right"
        paddingBottom="12dp"
        fontFamily="@font/2131296266"
        layout_toEndOf="@+id/tv_hint_operator"
        style="@style/2132018187"/>
    <View
        id="@+id/vw_calculator_line"
        background="#b8b8b8"
        visibility="gone"
        layout_width="match_parent"
        layout_height="0.5dp"
        layout_below="@+id/tv_hint_operand_left"/>
    <androidx.recyclerview.widget.RecyclerView
        id="@+id/rv_calculator"
        paddingLeft="15dp"
        paddingTop="@dimen/2131165321"
        paddingRight="15dp"
        layout_below="@+id/vw_calculator_line"
        overScrollMode="never"
        style="@style/2132017490"/>
</RelativeLayout>
