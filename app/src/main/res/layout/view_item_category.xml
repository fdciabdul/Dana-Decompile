<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    orientation="vertical"
    layout_width="match_parent"
    layout_height="wrap_content">
    <TextView
        textSize="16sp"
        textColor="#000"
        id="@+id/tv_category"
        background="#ffffff"
        paddingLeft="16dp"
        paddingTop="8dp"
        layout_width="match_parent"
        layout_height="wrap_content"
        fontFamily="@font/2131296266"
        paddingStart="16dp"/>
    <LinearLayout
        background="#ffffff"
        paddingLeft="16dp"
        paddingTop="8dp"
        paddingRight="16dp"
        paddingBottom="8dp"
        layout_width="match_parent"
        layout_height="wrap_content">
        <View
            background="#d1d1d1"
            layout_width="match_parent"
            layout_height="0.5dp"/>
    </LinearLayout>
</LinearLayout>
