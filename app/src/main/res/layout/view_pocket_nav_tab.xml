<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_width="match_parent"
    layout_height="56dp">
    <ImageView
        layout_gravity="center_horizontal"
        id="@+id/nav_tab_icon"
        layout_width="24dp"
        layout_height="24dp"
        layout_marginTop="8dp"
        layout_marginBottom="4dp"
        layout_centerHorizontal="true"
        contentDescription="@string/btn_pocket"
        NOT_FOUND_0x16="0"/>
    <TextView
        textSize="12sp"
        textColor="@drawable/nav_title_color"
        gravity="center"
        layout_gravity="center_horizontal"
        id="@+id/nav_tab_title"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginBottom="4dp"
        layout_below="@+id/nav_tab_icon"
        layout_alignParentBottom="true"
        layout_centerHorizontal="true"
        lineSpacingExtra="4sp"
        fontFamily="@font/2131296265"/>
    <ImageView
        gravity="center"
        id="@+id/nav_tab_pocket_notif"
        paddingLeft="4sp"
        paddingRight="4sp"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginLeft="11dp"
        src="@drawable/notification_pocket"
        layout_marginStart="11dp"
        NOT_FOUND_0x15="@+id/nav_tab_icon"
        NOT_FOUND_0x16="0"
        style="@style/2132018187"/>
</RelativeLayout>
