<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    orientation="vertical"
    id="@+id/cv_promo_merchant_special"
    layout_width="wrap_content"
    layout_height="wrap_content"
    NOT_FOUND_0xa="8dp"
    NOT_FOUND_0xb="0dp">
    <androidx.constraintlayout.widget.ConstraintLayout
        id="@+id/rl_promo_merchant_special"
        background="@drawable/bg_rounded_8dp_border_grey20"
        padding="1dp"
        layout_width="wrap_content"
        layout_height="wrap_content">
        <ImageView
            id="@+id/iv_promo_merchant_special"
            layout_width="160dp"
            layout_height="222dp"
            scaleType="centerInside"
            NOT_FOUND_0xc="0"
            NOT_FOUND_0xe="0"
            NOT_FOUND_0xf="0"
            NOT_FOUND_0x10="0"/>
        <id.dana.component.buttoncomponent.DanaButtonSecondaryView
            id="@+id/btn_promo_merchant_special_detail"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginBottom="6dp"
            contentDescription="@string/cd_promo_center_ads_detail_button"
            layout_marginEnd="8dp"
            NOT_FOUND_0xc="0"
            NOT_FOUND_0xd="1"
            NOT_FOUND_0xe="0"
            NOT_FOUND_0xf="0"
            NOT_FOUND_0x10="0"
            NOT_FOUND_0x11="1"
            NOT_FOUND_0x12="11"
            NOT_FOUND_0x13="@string/detail"
            NOT_FOUND_0x14="1"
            style="@style/2132017895"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>
