<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="#f5f5f5"
    layout_width="match_parent"
    layout_height="match_parent">
    <ScrollView
        layout_width="0dp"
        layout_height="0dp"
        fillViewport="true"
        NOT_FOUND_0x14="@+id/cl_bottom"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x18="0"
        NOT_FOUND_0x1a="0"
        NOT_FOUND_0x1b="0">
        <androidx.constraintlayout.widget.ConstraintLayout
            paddingBottom="16dp"
            layout_width="match_parent"
            layout_height="wrap_content">
            <androidx.appcompat.widget.AppCompatImageView
                id="@+id/iv_banner_agreement"
                background="#108ee9"
                layout_width="match_parent"
                layout_height="156dp"
                src="@drawable/bg_google"
                NOT_FOUND_0x15="0"
                NOT_FOUND_0x18="0"
                NOT_FOUND_0x1a="0"/>
            <androidx.constraintlayout.widget.ConstraintLayout
                background="@drawable/bg_home_widget_white_rounded"
                layout_width="0dp"
                layout_height="wrap_content"
                layout_marginTop="32dp"
                layout_marginStart="16dp"
                layout_marginEnd="16dp"
                NOT_FOUND_0x15="0"
                NOT_FOUND_0x18="0"
                NOT_FOUND_0x1a="0">
                <LinearLayout
                    gravity="center"
                    orientation="horizontal"
                    id="@+id/ll_phone_number"
                    background="@drawable/bg_rounded_24dp_border_grey20"
                    layout_width="wrap_content"
                    layout_height="40dp"
                    layout_marginTop="8dp"
                    NOT_FOUND_0x15="0"
                    NOT_FOUND_0x18="0"
                    NOT_FOUND_0x19="@+id/tv_dana_id">
                    <com.google.android.material.imageview.ShapeableImageView
                        id="@+id/tv_user_avatar"
                        layout_width="24dp"
                        layout_height="24dp"
                        layout_marginLeft="8dp"
                        layout_marginTop="8dp"
                        layout_marginRight="4dp"
                        layout_marginBottom="8dp"
                        NOT_FOUND_0x1e="@style/2132018200"/>
                    <androidx.appcompat.widget.AppCompatTextView
                        id="@+id/tv_phone_number"
                        layout_width="wrap_content"
                        layout_height="wrap_content"
                        layout_marginRight="12dp"
                        contentDescription="@string/lblPhoneNumber"
                        style="@style/2132017413"/>
                </LinearLayout>
                <TextView
                    id="@+id/tv_dana_id"
                    layout_width="wrap_content"
                    layout_height="wrap_content"
                    layout_marginTop="16.5dp"
                    text="@string/dana_qr_binding"
                    NOT_FOUND_0x15="0"
                    NOT_FOUND_0x18="0"
                    NOT_FOUND_0x19="@+id/divider"
                    style="@style/2132017908"/>
                <androidx.appcompat.widget.AppCompatImageView
                    id="@+id/iv_binding_dots"
                    layout_width="40dp"
                    layout_height="8dp"
                    layout_marginTop="42dp"
                    NOT_FOUND_0x15="0"
                    NOT_FOUND_0x18="0"
                    NOT_FOUND_0x1a="0"
                    NOT_FOUND_0x1f="@drawable/ic_binding_dots"/>
                <View
                    id="@+id/divider"
                    background="#d1d1d1"
                    layout_width="0dp"
                    layout_height="1dp"
                    layout_marginTop="16dp"
                    layout_marginStart="12dp"
                    layout_marginEnd="12dp"
                    NOT_FOUND_0x15="0"
                    NOT_FOUND_0x18="0"
                    NOT_FOUND_0x19="@+id/iv_dana_logo"/>
                <com.google.android.material.imageview.ShapeableImageView
                    id="@+id/iv_merchant_logo"
                    layout_width="50dp"
                    layout_height="50dp"
                    layout_marginStart="24dp"
                    NOT_FOUND_0x13="@+id/iv_binding_dots"
                    NOT_FOUND_0x17="@+id/iv_binding_dots"
                    NOT_FOUND_0x1a="@+id/iv_binding_dots"
                    NOT_FOUND_0x1e="@style/2132018200"
                    NOT_FOUND_0x20="@color/2131100473"
                    NOT_FOUND_0x21="1dp"/>
                <com.google.android.material.imageview.ShapeableImageView
                    id="@+id/iv_dana_logo"
                    layout_width="50dp"
                    layout_height="50dp"
                    src="@drawable/res_0x7f080633_networkuserentitydata_externalsyntheticlambda2"
                    layout_marginEnd="24dp"
                    NOT_FOUND_0x13="@+id/iv_binding_dots"
                    NOT_FOUND_0x16="@+id/iv_binding_dots"
                    NOT_FOUND_0x1a="@+id/iv_binding_dots"
                    NOT_FOUND_0x1e="@style/2132018200"/>
                <androidx.appcompat.widget.AppCompatTextView
                    id="@+id/tv_agreement_detail"
                    layout_width="0dp"
                    layout_height="wrap_content"
                    layout_marginTop="16dp"
                    contentDescription="@string/lblMerchantName"
                    layout_marginStart="12dp"
                    layout_marginEnd="12dp"
                    NOT_FOUND_0x15="0"
                    NOT_FOUND_0x18="0"
                    NOT_FOUND_0x19="@+id/ll_phone_number"
                    style="@style/2132017413"/>
                <androidx.recyclerview.widget.RecyclerView
                    id="@+id/rv_scope"
                    layout_width="0dp"
                    layout_height="wrap_content"
                    layout_marginTop="4dp"
                    layout_marginBottom="16dp"
                    contentDescription="@string/lblAgreementDescription"
                    layout_marginStart="12dp"
                    layout_marginEnd="12dp"
                    NOT_FOUND_0x13="0"
                    NOT_FOUND_0x15="0"
                    NOT_FOUND_0x18="0"
                    NOT_FOUND_0x19="@+id/tv_agreement_detail"
                    NOT_FOUND_0x1b="0"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
    <androidx.constraintlayout.widget.ConstraintLayout
        id="@+id/cl_bottom"
        background="#ffffff"
        layout_width="0dp"
        layout_height="wrap_content"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x18="0">
        <androidx.appcompat.widget.AppCompatTextView
            id="@+id/MissingMainCoroutineDispatcher"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="8.5dp"
            contentDescription="@string/lblTnC"
            textAlignment="center"
            layout_marginStart="16dp"
            layout_marginEnd="16dp"
            NOT_FOUND_0x15="0"
            NOT_FOUND_0x18="0"
            NOT_FOUND_0x1a="0"
            style="@style/2132017433"/>
        <id.dana.component.buttoncomponent.DanaButtonPrimaryView
            textSize="16sp"
            id="@+id/btn_allow"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_marginTop="8dp"
            layout_marginBottom="16dp"
            contentDescription="@string/btnContinue_automate"
            layout_marginStart="16dp"
            layout_marginEnd="16dp"
            NOT_FOUND_0x13="0"
            NOT_FOUND_0x15="0"
            NOT_FOUND_0x18="0"
            NOT_FOUND_0x19="@+id/MissingMainCoroutineDispatcher"
            NOT_FOUND_0x1c="@string/btn_continue_google"
            NOT_FOUND_0x1d="1"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
