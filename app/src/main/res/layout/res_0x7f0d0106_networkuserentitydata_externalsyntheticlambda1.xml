                        layout_height="80dp"
                        NOT_FOUND_0x21="@drawable/res_0x7f0808e8_networkuserentitydata_externalsyntheticlambda5"/>
                </FrameLayout>
                <FrameLayout
                    id="@+id/flSendToContact"
                    focusable="true"
                    clickable="true"
                    layout_width="0dp"
                    layout_height="match_parent"
                    layout_weight="1"
                    contentDescription="@string/btn_send_to_friend"
                    layout_marginStart="10dp"
                    layout_marginEnd="22.5dp">
                    <LinearLayout
                        orientation="vertical"
                        background="@drawable/bg_rounded_20dp_solid_blue_10"
                        layout_width="match_parent"
                        layout_height="wrap_content"
                        layout_marginTop="40dp">
                        <androidx.appcompat.widget.AppCompatTextView
                            gravity="center"
                            id="@+id/res_0x7f0a156b_walletsearchpresenter_getpaymentassetsbasedonassettype_1_invoke_inlined_sortedby_1"
