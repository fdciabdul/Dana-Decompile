                            layout_height="wrap_content"
                            layout_marginLeft="12dp"
                            layout_marginTop="48dp"
                            layout_marginRight="12dp"
                            textAlignment="center"
                            style="@style/2132017908"/>
                        <androidx.appcompat.widget.AppCompatTextView
                            gravity="center"
                            id="@+id/tvSendToContactDescription"
                            layout_width="match_parent"
                            layout_height="wrap_content"
                            layout_marginLeft="12dp"
                            layout_marginTop="4dp"
                            layout_marginRight="12dp"
                            layout_marginBottom="20dp"
                            minLines="3"
                            textAlignment="center"
                            style="@style/2132017439"/>
                    </LinearLayout>
                    <androidx.appcompat.widget.AppCompatImageView
                        layout_gravity="top|center_horizontal"
                        id="@+id/ivSendToContact"
                        layout_width="80dp"
                        layout_height="80dp"
                        NOT_FOUND_0x21="@drawable/res_0x7f0808e8_networkuserentitydata_externalsyntheticlambda5"/>
