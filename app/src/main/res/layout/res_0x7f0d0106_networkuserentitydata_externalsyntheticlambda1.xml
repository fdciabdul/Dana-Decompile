                    <androidx.appcompat.widget.AppCompatImageView
                        layout_gravity="top|center_horizontal"
                        id="@+id/res_0x7f0a0a37_splitmerchantconfigentity_getmerchantdetailconfig_2_invokesuspend_lambda_0_inlined_getconfigsonceready_default_1_1"
