            layout_height="match_parent">
            <LinearLayout
                orientation="horizontal"
                id="@+id/llGroupSendSelection"
                layout_width="0dp"
                layout_height="match_parent"
                layout_marginTop="40dp"
                weightSum="2"
                NOT_FOUND_0x18="0"
                NOT_FOUND_0x1b="0"
                NOT_FOUND_0x1c="@+id/tvTitle">
                <FrameLayout
                    id="@+id/res_0x7f0a0734_sizeselectors_orselector"
                    focusable="true"
                    clickable="true"
                    layout_width="0dp"
                    layout_height="match_parent"
                    layout_weight="1"
                    layout_marginStart="22.5dp"
                    layout_marginEnd="10dp">
                    <LinearLayout
                        orientation="vertical"
                        background="@drawable/bg_rounded_20dp_solid_blue_10"
                        layout_width="match_parent"
                        layout_height="wrap_content"
                        layout_marginTop="40dp">
                        <androidx.appcompat.widget.AppCompatTextView
                            gravity="center"
                            id="@+id/tvSendToBankTitle"
                            layout_width="match_parent"
                            layout_height="wrap_content"
                            layout_marginLeft="12dp"
                            layout_marginTop="48dp"
                            layout_marginRight="12dp"
                            textAlignment="center"
                            style="@style/2132017908"/>
                        <androidx.appcompat.widget.AppCompatTextView
                            gravity="center"
                            id="@+id/tvSendToBankDescription"
                            layout_width="match_parent"
                            layout_height="wrap_content"
                            layout_marginLeft="12dp"
                            layout_marginTop="4dp"
                            layout_marginRight="12dp"
                            layout_marginBottom="20dp"
                            minLines="3"
                            textAlignment="center"
                            style="@style/2132017439"/>
