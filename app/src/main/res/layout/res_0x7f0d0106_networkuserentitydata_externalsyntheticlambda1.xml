            <id.dana.component.buttoncomponent.DanaButtonSecondaryView
                id="@+id/btnClose"
                layout_width="0dp"
                layout_height="wrap_content"
                layout_marginLeft="16dp"
                layout_marginTop="32dp"
                layout_marginRight="16dp"
                layout_marginBottom="16dp"
                NOT_FOUND_0x16="0"
                NOT_FOUND_0x18="0"
                NOT_FOUND_0x1b="0"
                NOT_FOUND_0x1c="@+id/llGroupSendSelection"
                NOT_FOUND_0x1e="@string/close"
                NOT_FOUND_0x1f="1"
                NOT_FOUND_0x20="@string/btn_close"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </FrameLayout>
</androidx.coordinatorlayout.widget.CoordinatorLayout>
