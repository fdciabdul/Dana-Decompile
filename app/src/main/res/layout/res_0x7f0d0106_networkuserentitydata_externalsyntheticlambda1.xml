                layout_width="56dp"
                layout_height="4dp"
                layout_marginTop="8dp"
                NOT_FOUND_0x17="@+id/tvTitle"
                NOT_FOUND_0x19="0"
                NOT_FOUND_0x1a="0"
                NOT_FOUND_0x1d="0"
                NOT_FOUND_0x21="@drawable/ic_flat_dark"/>
            <androidx.appcompat.widget.AppCompatTextView
                gravity="center"
                id="@+id/tvTitle"
                layout_width="0dp"
                layout_height="wrap_content"
                layout_marginLeft="24dp"
                layout_marginTop="16dp"
                layout_marginRight="24dp"
                text="@string/send_to_group"
                textAlignment="center"
                NOT_FOUND_0x17="@+id/ivIcon"
                NOT_FOUND_0x18="0"
                NOT_FOUND_0x1b="0"
                NOT_FOUND_0x1c="@+id/secondaryHash_res_0x7f0a08d3"
