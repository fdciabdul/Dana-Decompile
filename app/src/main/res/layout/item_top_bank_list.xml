<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/cv_top_bank_item"
    contentDescription="btn-bank-{bank-name}"
    NOT_FOUND_0xe="@color/colorWhite"
    NOT_FOUND_0xf="8dp"
    NOT_FOUND_0x10="2dp"
    NOT_FOUND_0x11="true"
    style="@style/2132017490">
    <androidx.constraintlayout.widget.ConstraintLayout
        id="@+id/cl_top_bank_item"
        background="?attr/2130969992"
        focusable="true"
        clickable="true"
        layout_width="match_parent"
        layout_height="match_parent"
        style="fill">
        <androidx.constraintlayout.widget.ConstraintLayout
            layout_width="match_parent"
            layout_height="0dp"
            NOT_FOUND_0x12="0"
            NOT_FOUND_0x14="1:1"
            NOT_FOUND_0x15="0"
            NOT_FOUND_0x16="0"
            NOT_FOUND_0x18="0">
            <ImageView
                layout_gravity="center"
                id="@+id/iv_bank_logo"
                layout_width="48dp"
                layout_height="48dp"
                layout_marginTop="16dp"
                scaleType="fitXY"
                NOT_FOUND_0x13="@+id/tv_bank_name"
                NOT_FOUND_0x15="0"
                NOT_FOUND_0x16="0"
                NOT_FOUND_0x18="0"
                NOT_FOUND_0x19="0"
                NOT_FOUND_0x1a="@drawable/ic_bank_placeholder"/>
            <TextView
                id="@+id/tv_bank_name"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginBottom="8dp"
                textAlignment="center"
                layout_marginStart="5dp"
                layout_marginEnd="5dp"
                NOT_FOUND_0x12="0"
                NOT_FOUND_0x15="@+id/iv_bank_logo"
                NOT_FOUND_0x16="@+id/iv_bank_logo"
                NOT_FOUND_0x17="@+id/iv_bank_logo"
                style="@style/2132017405"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>
