<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/vg_reaction_section_root"
    layout_width="match_parent"
    layout_height="wrap_content">
    <View
        id="@+id/view_reaction_section_rv_ruler"
        layout_width="0dp"
        layout_height="0dp"
        NOT_FOUND_0x13="@+id/view_barrier_reaction_section_separator"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x17="0"/>
    <androidx.recyclerview.widget.RecyclerView
        orientation="horizontal"
        id="@+id/rv_reactions"
        clipToPadding="false"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="12dp"
        layout_marginBottom="10dp"
        overScrollMode="never"
        paddingStart="10dp"
        paddingEnd="0dp"
        layout_marginEnd="8dp"
        NOT_FOUND_0x11="androidx.recyclerview.widget.LinearLayoutManager"
        NOT_FOUND_0x13="@+id/view_barrier_reaction_section_separator"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x17="0"/>
    <TextView
        orientation="horizontal"
        id="@+id/tv_reactions_empty_state_view"
        visibility="gone"
        layout_width="0dp"
        layout_height="24dp"
        layout_marginTop="12dp"
        layout_marginBottom="10dp"
        text="@string/lbl_saymoji_empty_on_detail_page"
        contentDescription="@string/txt_empty_saymoji"
        paddingStart="12dp"
        paddingEnd="0dp"
        NOT_FOUND_0x13="@+id/reaction_more"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x17="0"
        style="@style/2132017406"/>
    <id.dana.social.view.ReactionView
        id="@+id/reaction_more"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="12dp"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x18="1F600"
        NOT_FOUND_0x19="true"
        NOT_FOUND_0x1a="+"/>
    <LinearLayout
        id="@+id/vg_reaction_section_info_wrapper"
        paddingTop="8dp"
        layout_width="wrap_content"
        layout_height="wrap_content"
        paddingStart="12dp"
        paddingEnd="8dp"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x16="@+id/rv_reactions">
        <TextView
            id="@+id/tv_saymoji_section_info"
            layout_width="wrap_content"
            layout_height="wrap_content"
            contentDescription="@string/lblSaymojiCount"
            style="@style/2132017431"/>
        <TextView
            id="@+id/tv_comment_section_info"
            layout_width="wrap_content"
            layout_height="wrap_content"
            contentDescription="@string/lblCommentCount"
            style="@style/2132017431"/>
    </LinearLayout>
    <View
        id="@+id/view_reaction_bottom_white_space"
        layout_width="wrap_content"
        layout_height="12dp"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x16="@+id/vg_reaction_section_info_wrapper"/>
    <View
        id="@+id/view_reaction_section_separator"
        background="@drawable/bg_gradient_saymoji_separator"
        visibility="gone"
        layout_width="8dp"
        layout_height="48dp"
        layout_marginEnd="8dp"
        NOT_FOUND_0x13="@+id/view_barrier_reaction_section_separator"
        NOT_FOUND_0x17="0"/>
    <id.dana.social.view.ReactionView
        id="@+id/reaction_add_new"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="12dp"
        contentDescription="@string/btn_add_saymoji"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x18="1F600"
        NOT_FOUND_0x1b="true"/>
    <id.dana.social.view.ReactionView
        id="@+id/reaction_dummy"
        visibility="invisible"
        clickable="false"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="12dp"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x18="1F600"
        NOT_FOUND_0x1b="true"/>
    <View
        id="@+id/view_barrier_reaction_section_separator"
        layout_width="1dp"
        layout_height="1dp"
        NOT_FOUND_0x13="@+id/reaction_dummy"
        NOT_FOUND_0x17="0"/>
</androidx.constraintlayout.widget.ConstraintLayout>
