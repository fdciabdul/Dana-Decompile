<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="@drawable/bg_rounded_white_with_border"
    padding="16dp"
    layout_width="match_parent"
    layout_height="wrap_content">
    <TextView
        textSize="16sp"
        textColor="#4a4a4a"
        id="@+id/tv_kyb_tiering_title"
        layout_width="0dp"
        layout_height="wrap_content"
        text="@string/kyb_tiering_title"
        contentDescription="@string/lbl_tiering_section_title"
        fontFamily="@font/2131296266"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x13="0"/>
    <ImageView
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginEnd="66dp"
        NOT_FOUND_0xd="@+id/iv_kyb_tiering_platinum"
        NOT_FOUND_0xe="@+id/iv_kyb_tiering_platinum"
        NOT_FOUND_0x13="@+id/iv_kyb_tiering_platinum"
        NOT_FOUND_0x14="@drawable/ic_illustration_kyb_state_gold"/>
    <ImageView
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginStart="66dp"
        NOT_FOUND_0xd="@+id/iv_kyb_tiering_platinum"
        NOT_FOUND_0x11="@+id/iv_kyb_tiering_platinum"
        NOT_FOUND_0x13="@+id/iv_kyb_tiering_platinum"
        NOT_FOUND_0x14="@drawable/ic_illustration_kyb_state_silver"/>
    <ImageView
        id="@+id/iv_kyb_tiering_platinum"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="16dp"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="@+id/tv_kyb_tiering_title"
        NOT_FOUND_0x14="@drawable/ic_illustration_kyb_state_platinum"/>
    <id.dana.kyb.view.richview.KybTieringDetailItemView
        id="@+id/kyb_tiering_detail_1"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="16dp"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="@+id/iv_kyb_tiering_platinum"/>
    <id.dana.kyb.view.richview.KybTieringDetailItemView
        id="@+id/kyb_tiering_detail_2"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="16dp"
        NOT_FOUND_0xf="@+id/kyb_tiering_detail_3"
        NOT_FOUND_0x10="@+id/kyb_tiering_detail_1"
        NOT_FOUND_0x12="@+id/iv_kyb_tiering_platinum"/>
    <id.dana.kyb.view.richview.KybTieringDetailItemView
        id="@+id/kyb_tiering_detail_3"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="16dp"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x12="@+id/iv_kyb_tiering_platinum"/>
</androidx.constraintlayout.widget.ConstraintLayout>
