<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="#f5f5f5"
    layout_width="match_parent"
    layout_height="wrap_content">
    <androidx.appcompat.widget.AppCompatTextView
        id="@+id/tvTopUpAndPayMethodTitle"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_marginTop="12dp"
        layout_marginRight="16dp"
        textAllCaps="true"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x12="0"
        style="@style/2132017440"/>
    <androidx.recyclerview.widget.RecyclerView
        id="@+id/rvTopUpAndPayMethod"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="@+id/tvTopUpAndPayMethodTitle"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        id="@+id/otherMethodContainer"
        background="#ffffff"
        paddingLeft="16dp"
        paddingTop="10dp"
        paddingRight="16dp"
        paddingBottom="10dp"
        visibility="gone"
        layout_width="match_parent"
        layout_height="wrap_content"
        NOT_FOUND_0x11="@+id/rvTopUpAndPayMethod">
        <androidx.appcompat.widget.AppCompatTextView
            id="@+id/tvOtherMethodTitle"
            layout_width="0dp"
            layout_height="wrap_content"
            NOT_FOUND_0xf="@+id/ivExpand"
            NOT_FOUND_0x10="0"
            NOT_FOUND_0x12="0"
            style="@style/2132017413"/>
        <androidx.appcompat.widget.AppCompatTextView
            id="@+id/tvOtherMethodDescription"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_marginTop="4dp"
            NOT_FOUND_0xf="@+id/ivExpand"
            NOT_FOUND_0x10="0"
            NOT_FOUND_0x11="@+id/tvOtherMethodTitle"
            style="@style/2132017440"/>
        <androidx.appcompat.widget.AppCompatImageView
            id="@+id/ivExpand"
            layout_width="wrap_content"
            layout_height="wrap_content"
            NOT_FOUND_0xd="0"
            NOT_FOUND_0xe="0"
            NOT_FOUND_0x12="0"
            NOT_FOUND_0x13="@drawable/ic_down_chevron"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
