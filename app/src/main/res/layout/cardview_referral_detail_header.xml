<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="#108ee9"
    NOT_FOUND_0x13="0"
    NOT_FOUND_0x16="0"
    NOT_FOUND_0x18="0"
    style="@style/2132017490">
    <ImageView
        id="@+id/iv_referral_free_voucher"
        layout_marginTop="24dp"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x18="0"
        NOT_FOUND_0x1a="@drawable/ic_referral_free_voucher"
        style="@style/2132018187"/>
    <TextView
        textSize="14sp"
        textColor="#ffffff"
        gravity="center_horizontal"
        id="@+id/tv_referral_title"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        layout_marginBottom="32dp"
        fontFamily="@font/2131296265"
        layout_marginStart="32dp"
        layout_marginEnd="32dp"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x17="@+id/iv_referral_free_voucher"
        NOT_FOUND_0x19="1"/>
    <TextView
        textSize="14sp"
        textColor="#ffffff"
        id="@+id/tv_see_how_it_works_header"
        paddingTop="8dp"
        paddingBottom="8dp"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        text="@string/msg_see_how_it_works_header"
        fontFamily="@font/2131296265"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x17="@+id/tv_referral_title"/>
    <LinearLayout
        paddingBottom="16dp"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x17="@+id/tv_see_how_it_works_header">
        <Button
            id="@+id/btn_referral_tracker_first"
            visibility="gone"
            layout_width="132dp"
            layout_height="32dp"
            layout_marginLeft="4dp"
            layout_marginRight="4dp"
            text="@string/referral_tracker_dana_kaget"
            fontFamily="@font/2131296265"
            style="@style/2132017615"/>
        <Button
            id="@+id/btn_referral_tracker_second"
            visibility="gone"
            layout_width="132dp"
            layout_height="32dp"
            layout_marginLeft="4dp"
            layout_marginRight="4dp"
            text="@string/referral_tracker_send_dana"
            fontFamily="@font/2131296265"
            style="@style/2132017615"/>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
