<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_width="match_parent"
    layout_height="match_parent">
    <include
        id="@+id/include_toolbar"
        layout="@layout/layout_toolbar"/>
    <id.dana.richview.WebProgressView
        id="@+id/view_web_progress"
        layout_width="match_parent"
        layout_height="0dp"
        NOT_FOUND_0xd="@+id/cl_bottom_container"
        NOT_FOUND_0xe="@+id/include_toolbar"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        id="@+id/cl_bottom_container"
        layout_width="match_parent"
        layout_height="64dp"
        NOT_FOUND_0xc="0">
        <View
            id="@+id/view_separator"
            background="#d1d1d1"
            layout_width="match_parent"
            layout_height="0.5dp"
            NOT_FOUND_0xf="0"/>
        <Button
            textSize="16sp"
            textColor="#ffffff"
            id="@+id/btn_lazada_agree"
            background="@drawable/btn_common"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginLeft="16dp"
            layout_marginTop="16sp"
            layout_marginRight="16dp"
            layout_marginBottom="8dp"
            text="@string/lazada_terms_and_conditions_agree"
            fontFamily="@font/2131296266"
            NOT_FOUND_0xc="0"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
