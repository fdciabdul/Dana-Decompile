<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/cvMapAction"
    paddingBottom="50dp"
    layout_width="match_parent"
    layout_height="wrap_content">
    <androidx.constraintlayout.widget.ConstraintLayout
        layout_width="match_parent"
        layout_height="wrap_content"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x15="0">
        <androidx.constraintlayout.widget.ConstraintLayout
            orientation="vertical"
            id="@+id/clAddressInfo"
            background="@drawable/bg_rounded_8dp_border_grey20"
            padding="10dp"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginLeft="16dp"
            layout_marginTop="28dp"
            layout_marginRight="16dp"
            layout_marginBottom="16dp"
            NOT_FOUND_0x13="0"
            NOT_FOUND_0x15="0">
            <androidx.appcompat.widget.AppCompatImageView
                id="@+id/ivPin"
                layout_width="wrap_content"
                layout_height="wrap_content"
                NOT_FOUND_0x13="0"
                NOT_FOUND_0x15="0"
                NOT_FOUND_0x16="@drawable/ic_location_small_blue"/>
            <View
                id="@+id/tvStreet"
                background="@drawable/square_skeleton"
                layout_width="220dp"
                layout_height="16dp"
                layout_marginStart="4dp"
                NOT_FOUND_0x12="@+id/ivPin"
                NOT_FOUND_0x15="0"/>
            <View
                id="@+id/tvStreetDetail"
                background="@drawable/square_skeleton"
                layout_width="0dp"
                layout_height="16dp"
                layout_marginTop="4dp"
                NOT_FOUND_0x11="0"
                NOT_FOUND_0x13="@+id/tvStreet"
                NOT_FOUND_0x14="@+id/tvStreet"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <ProgressBar
            id="@+id/progressBar"
            drawingCacheQuality="high"
            layout_width="20dp"
            layout_height="20dp"
            layout_marginTop="28dp"
            indeterminate="true"
            indeterminateDrawable="@drawable/rotate_loading_spinner"
            NOT_FOUND_0x11="0"
            NOT_FOUND_0x13="0"
            NOT_FOUND_0x14="@+id/clAddressInfo"/>
        <View
            id="@+id/btnSelectLocation"
            background="@drawable/square_skeleton"
            layout_width="match_parent"
            layout_height="44dp"
            layout_marginLeft="16dp"
            layout_marginTop="28dp"
            layout_marginRight="16dp"
            textAlignment="center"
            NOT_FOUND_0x11="0"
            NOT_FOUND_0x13="0"
            NOT_FOUND_0x14="@+id/progressBar"/>
        <ProgressBar
            id="@+id/progressBarBtn"
            drawingCacheQuality="high"
            layout_width="20dp"
            layout_height="20dp"
            indeterminate="true"
            indeterminateDrawable="@drawable/rotate_loading_spinner"
            NOT_FOUND_0x10="@+id/btnSelectLocation"
            NOT_FOUND_0x11="@+id/btnSelectLocation"
            NOT_FOUND_0x13="@+id/btnSelectLocation"
            NOT_FOUND_0x15="@+id/btnSelectLocation"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
