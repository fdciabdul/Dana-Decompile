<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/lyt_custom_gridview"
    paddingBottom="1dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    animateLayoutChanges="true">
    <ImageView
        id="@+id/custom_gridview_image"
        layout_width="32dp"
        layout_height="32dp"
        layout_marginTop="4dp"
        scaleType="fitCenter"
        adjustViewBounds="true"
        layout_centerHorizontal="true"
        contentDescription="@string/navigation_tab_icon"/>
    <TextView
        textSize="10sp"
        textColor="#ffffff"
        gravity="center"
        id="@+id/custom_gridview_image_badges_text"
        background="@drawable/service_badge"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        includeFontPadding="false"
        android:maxLength="5"
        layout_alignBottom="@+id/custom_gridview_image"
        layout_centerHorizontal="true"
        fontFamily="@font/open_sans_bold"
        layout_alignStart="@+id/custom_gridview_image"
        layout_alignEnd="@+id/custom_gridview_image"/>
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/custom_gridview_red_dot"
        background="@drawable/shape_oval_grape_fruit_white_stroke_2dp"
        visibility="gone"
        layout_width="12dp"
        layout_height="12dp"
        layout_alignTop="@+id/custom_gridview_image"
        layout_alignEnd="@+id/custom_gridview_image"/>
    <id.dana.service.ServiceBadgeImageView
        id="@+id/res_0x7f0a0b05_daggermerchantmanagementcomponent_merchantmanagementcomponentimpl_sslpinningrepositoryprovider"
        visibility="gone"
        layout_width="16dp"
        layout_height="16dp"
        src="@drawable/ic_service_selector_edit_save"
        layout_alignTop="@+id/custom_gridview_image"
        layout_marginEnd="24dp"
        layout_alignEnd="@+id/custom_gridview_image"/>
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/custom_gridview_maintenance_badge"
        visibility="gone"
        layout_width="16dp"
        layout_height="16dp"
        layout_alignTop="@+id/custom_gridview_image"
        layout_marginStart="24dp"
        layout_alignStart="@+id/custom_gridview_image"
        NOT_FOUND_0x1f="@drawable/ic_maintenance_service"/>
    <TextView
        gravity="center_horizontal"
        id="@+id/custom_gridview_text"
        paddingLeft="8dp"
        paddingRight="8dp"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        maxLines="4"
        minLines="2"
        layout_below="@+id/custom_gridview_image"
        layout_centerHorizontal="true"
        NOT_FOUND_0x1e="0.5"
        style="@style/2132017439"/>
</RelativeLayout>
