<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    gravity="center_horizontal"
    orientation="vertical"
    id="@+id/lyt_custom_gridview"
    paddingBottom="12dp"
    layout_width="match_parent"
    layout_height="wrap_content">
    <ImageView
        id="@+id/custom_gridview_image"
        layout_width="@dimen/2131165627"
        layout_height="@dimen/2131165627"
        layout_marginTop="4dp"
        src="@drawable/shape_oval_grey"
        scaleType="fitCenter"
        adjustViewBounds="true"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x12="0"/>
    <ImageView
        id="@+id/custom_gridview_text"
        paddingLeft="12dp"
        paddingRight="12dp"
        layout_width="0dp"
        layout_height="12dp"
        layout_marginTop="12dp"
        src="@drawable/shape_rectangle_grey20_6dp"
        paddingStart="12dp"
        paddingEnd="12dp"
        NOT_FOUND_0xe="0.5"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="@+id/custom_gridview_image"/>
</androidx.constraintlayout.widget.ConstraintLayout>
