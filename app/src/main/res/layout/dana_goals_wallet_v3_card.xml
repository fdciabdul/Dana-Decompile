<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/res_0x7f0a042a_imagecaptureviewmodel_gettipuploadphoto_2"
    background="@drawable/bg_rounded_1dp_grey20"
    paddingLeft="16dp"
    paddingTop="16dp"
    paddingRight="16dp"
    paddingBottom="16dp"
    focusable="true"
    clickable="true"
    layout_width="match_parent"
    layout_height="match_parent">
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/iv_dana_logo"
        layout_width="wrap_content"
        layout_height="28dp"
        scaleType="fitStart"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x17="@drawable/ic_dana_goals"/>
    <androidx.appcompat.widget.AppCompatTextView
        id="@+id/tv_title_balance"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="10dp"
        text="@string/goals_card_title"
        NOT_FOUND_0x13="@+id/tv_balance_currency"
        NOT_FOUND_0x15="0"
        style="@style/2132017438"/>
    <androidx.appcompat.widget.AppCompatTextView
        id="@+id/tv_balance_currency"
        layout_width="wrap_content"
        layout_height="wrap_content"
        text="@string/currency_indonesia"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x15="0"
        style="@style/2132017915"/>
    <androidx.appcompat.widget.AppCompatTextView
        id="@+id/tv_balance_content"
        layout_width="wrap_content"
        layout_height="wrap_content"
        maxLines="1"
        NOT_FOUND_0x12="@+id/tv_balance_currency"
        NOT_FOUND_0x14="@+id/tv_balance_currency"
        NOT_FOUND_0x16="@+id/tv_balance_currency"
        style="@style/2132017915"/>
    <TextView
        id="@+id/tv_hidden_pay_content"
        layout_width="wrap_content"
        layout_height="wrap_content"
        text="@string/hidden_balance_placeholder"
        NOT_FOUND_0x12="@+id/tv_balance_currency"
        NOT_FOUND_0x14="@+id/tv_balance_content"
        NOT_FOUND_0x16="@+id/tv_balance_currency"
        style="@style/2132017915"/>
    <androidx.appcompat.widget.AppCompatImageView
        layout_gravity="center_horizontal"
        id="@+id/OkHttpExporterBuilder"
        padding="12dp"
        focusable="true"
        clickable="true"
        layout_width="wrap_content"
        layout_height="wrap_content"
        hapticFeedbackEnabled="true"
        NOT_FOUND_0x12="@+id/tv_hidden_pay_content"
        NOT_FOUND_0x14="@+id/tv_hidden_pay_content"
        NOT_FOUND_0x16="@+id/tv_hidden_pay_content"
        NOT_FOUND_0x17="@drawable/ic_visibility_hide_outlined"/>
    <androidx.constraintlayout.widget.Group
        id="@+id/groupContent"
        layout_width="wrap_content"
        layout_height="wrap_content"
        NOT_FOUND_0x11="tv_balance_currency, tv_title_balance"/>
</androidx.constraintlayout.widget.ConstraintLayout>
