<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_width="match_parent"
    layout_height="wrap_content">
    <View
        id="@+id/view_bubble_feed_bg"
        background="@drawable/bg_left_top_unrounded_white"
        layout_width="0dp"
        layout_height="0dp"
        layout_marginTop="2dp"
        layout_marginStart="8.9dp"
        NOT_FOUND_0xd="@+id/cl_feed"
        NOT_FOUND_0xe="@+id/cl_feed"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="@+id/cl_feed"/>
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/iv_top_left_bubble"
        layout_width="10dp"
        layout_height="20dp"
        src="@drawable/ic_right_triangle"
        scaleY="-1"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="@+id/view_bubble_feed_bg"/>
    <View
        id="@+id/triangle_spacer"
        background="#e3e3e3"
        layout_width="0dp"
        layout_height="1dp"
        layout_marginStart="1dp"
        NOT_FOUND_0xe="@+id/iv_top_left_bubble"
        NOT_FOUND_0x10="@+id/iv_top_left_bubble"
        NOT_FOUND_0x11="@+id/iv_top_left_bubble"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        id="@+id/cl_feed"
        background="@android:color/transparent"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginEnd="4dp"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0xf="@+id/iv_top_left_bubble"
        NOT_FOUND_0x11="0">
        <TextView
            textSize="12sp"
            textColor="#313131"
            id="@+id/tv_desc"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_marginTop="8dp"
            layout_marginBottom="8dp"
            fontFamily="@font/sf_pro_regular"
            layout_marginStart="12dp"
            layout_marginEnd="22dp"
            NOT_FOUND_0xd="0"
            NOT_FOUND_0xe="0"
            NOT_FOUND_0x10="0"
            NOT_FOUND_0x11="0"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
