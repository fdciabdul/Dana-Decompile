<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    orientation="vertical"
    id="@+id/lyt_custom_gridview"
    layout_width="128dp"
    layout_height="wrap_content"
    layout_marginLeft="6dp"
    layout_marginTop="12dp"
    layout_marginRight="6dp"
    layout_marginBottom="12dp">
    <FrameLayout
        layout_gravity="center"
        layout_width="match_parent"
        layout_height="128dp">
        <androidx.appcompat.widget.AppCompatImageView
            layout_gravity="center"
            id="@+id/res_0x7f0a09f1_access_setbufferat"
            background="@drawable/home_component_border"
            layout_width="match_parent"
            layout_height="match_parent"
            scaleType="centerCrop"
            adjustViewBounds="true"/>
        <View
            layout_gravity="center"
            id="@+id/view_sku_logo_skeleton"
            layout_width="match_parent"
            layout_height="match_parent"/>
    </FrameLayout>
    <TextView
        ellipsize="end"
        id="@+id/tvTitle"
        paddingLeft="4dp"
        paddingRight="4dp"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        maxLines="3"
        style="@style/2132017908"/>
    <TextView
        textSize="12sp"
        textColor="#727272"
        ellipsize="end"
        id="@+id/isHelperTextDisplayed"
        paddingLeft="4dp"
        paddingRight="4dp"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="2dp"
        maxLines="1"
        fontFamily="@font/2131296265"
        style="@style/2132017440"/>
    <TextView
        textSize="12sp"
        ellipsize="end"
        id="@+id/FriendListPresenter_Factory"
        paddingLeft="4dp"
        paddingRight="4dp"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="2dp"
        maxLines="1"
        fontFamily="@font/2131296265"
        style="@style/2132017435"/>
</LinearLayout>
