<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_gravity="bottom"
    background="@drawable/rounded_top_background"
    paddingBottom="16dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    NOT_FOUND_0x1a="0">
    <TextView
        id="@+id/tvTitle"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="28dp"
        text="@string/send_money_text_change_display_name"
        layout_marginStart="16dp"
        NOT_FOUND_0x1c="@+id/ivCloseSheetRename"
        NOT_FOUND_0x20="0"
        NOT_FOUND_0x22="0"
        style="@style/2132017924"/>
    <TextView
        id="@+id/tvDesc"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_marginRight="16dp"
        text="@string/send_money_desc_rename_bottom_sheet"
        NOT_FOUND_0x1d="0"
        NOT_FOUND_0x1e="0"
        NOT_FOUND_0x21="@+id/detNormal"
        style="@style/2132017440"/>
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/ivCloseSheetRename"
        background="?attr/2130969992"
        padding="16dp"
        focusable="true"
        clickable="true"
        NOT_FOUND_0x1a="@+id/tvTitle"
        NOT_FOUND_0x1b="0"
        NOT_FOUND_0x1f="@+id/tvTitle"
        NOT_FOUND_0x22="@+id/tvTitle"
        NOT_FOUND_0x25="@drawable/ic_close_black"
        style="@style/2132018187"/>
    <com.google.android.material.textfield.TextInputLayout
        textColorHint="#bfbfbf"
        id="@+id/detNormal"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_marginTop="24dp"
        layout_marginRight="16dp"
        hint="@string/send_money_text_change_alias_name_edit_text"
        NOT_FOUND_0x19="@color/2131100507"
        NOT_FOUND_0x1d="0"
        NOT_FOUND_0x21="@+id/tvTitle">
        <id.dana.component.edittextcomponent.DanaEditTextView
            enabled="true"
            id="@+id/detChangeName"
            focusable="true"
            focusableInTouchMode="true"
            layout_width="match_parent"
            layout_height="wrap_content"
            android:maxLength="20"
            digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ "
            inputType="textPersonName"
            imeOptions="actionDone"
            textCursorDrawable="@color/2131100507"
            NOT_FOUND_0x18="0"/>
    </com.google.android.material.textfield.TextInputLayout>
    <id.dana.component.buttoncomponent.DanaButtonPrimaryView
        id="@+id/btnRenameBankAccount"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_margin="16dp"
        NOT_FOUND_0x21="@+id/tvDesc"
        NOT_FOUND_0x23="@string/send_money_save_rename_bank_account"
        NOT_FOUND_0x24="1"/>
</androidx.constraintlayout.widget.ConstraintLayout>
