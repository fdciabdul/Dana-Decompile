<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="@drawable/bg_outline_grey_20_rad_12"
    padding="12dp"
    layout_width="match_parent"
    layout_height="wrap_content">
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/ivAsset"
        layout_width="81dp"
        layout_height="52dp"
        scaleType="fitXY"
        NOT_FOUND_0x9="0"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0xf="0"/>
    <androidx.appcompat.widget.AppCompatTextView
        textColor="#313131"
        id="@+id/tvTitle"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginStart="12dp"
        layout_marginEnd="8dp"
        NOT_FOUND_0xb="@+id/btnAddAsset"
        NOT_FOUND_0xc="@+id/ivAsset"
        NOT_FOUND_0xf="0"
        style="@style/2132017910"/>
    <androidx.appcompat.widget.AppCompatTextView
        textColor="#727272"
        id="@+id/isHelperTextDisplayed"
        layout_width="0dp"
        layout_height="wrap_content"
        NOT_FOUND_0xa="@+id/tvTitle"
        NOT_FOUND_0xd="@+id/tvTitle"
        NOT_FOUND_0xe="@+id/tvTitle"
        style="@style/2132017433"/>
    <id.dana.component.buttoncomponent.DanaButtonSecondaryView
        id="@+id/btnAddAsset"
        layout_width="90dp"
        layout_height="32dp"
        NOT_FOUND_0x9="0"
        NOT_FOUND_0xa="0"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="10"
        NOT_FOUND_0x11="1"/>
</androidx.constraintlayout.widget.ConstraintLayout>
