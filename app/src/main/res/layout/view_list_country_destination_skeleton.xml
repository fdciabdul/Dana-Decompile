<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_width="match_parent"
    layout_height="wrap_content">
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/ivIconLeftSkeleton"
        background="@drawable/bg_circle_grey_20"
        layout_width="28dp"
        layout_height="28dp"
        layout_margin="16dp"
        NOT_FOUND_0x9="0"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x11="0"/>
    <View
        id="@+id/viewSubTopSkeleton"
        background="@drawable/square_rounded_skeleton"
        layout_width="0dp"
        layout_height="20dp"
        layout_marginTop="10dp"
        layout_marginStart="16dp"
        layout_marginEnd="24dp"
        NOT_FOUND_0xa="@+id/viewSubBottomSkeleton"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xe="@+id/ivIconLeftSkeleton"
        NOT_FOUND_0x11="0"/>
    <View
        id="@+id/viewSubBottomSkeleton"
        background="@drawable/square_rounded_skeleton"
        layout_width="0dp"
        layout_height="16dp"
        layout_marginBottom="10dp"
        layout_marginStart="16dp"
        layout_marginEnd="24dp"
        NOT_FOUND_0x9="0"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xe="@+id/ivIconLeftSkeleton"
        NOT_FOUND_0x10="@+id/viewSubTopSkeleton"/>
    <View
        background="#e3e3e3"
        layout_width="match_parent"
        layout_height="1dp"
        layout_marginTop="10dp"
        layout_marginStart="16dp"
        NOT_FOUND_0x9="0"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xd="0"/>
</androidx.constraintlayout.widget.ConstraintLayout>
