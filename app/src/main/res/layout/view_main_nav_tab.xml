<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/vg_main_nav_tab_root"
    visibility="invisible"
    layout_width="match_parent"
    layout_height="56dp">
    <View
        id="@+id/view_pay_bottom_menu"
        background="@drawable/ic_nav_pay_bg"
        layout_width="56dp"
        layout_height="56dp"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x14="0"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        orientation="vertical"
        layout_width="0dp"
        layout_height="match_parent"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x14="0">
        <ImageView
            layout_gravity="center_horizontal"
            id="@+id/nav_tab_icon"
            layout_width="28dp"
            layout_height="28dp"
            layout_marginTop="8dp"
            contentDescription="@string/navigation_tab_icon"
            NOT_FOUND_0x11="0"
            NOT_FOUND_0x12="0"
            NOT_FOUND_0x14="0"
            NOT_FOUND_0x15="@drawable/ic_nav_pay"/>
        <TextView
            textSize="12sp"
            textColor="#fafafa"
            gravity="center"
            layout_gravity="center_horizontal"
            id="@+id/nav_tab_title"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginBottom="4dp"
            text="@string/nav_pay_title"
            lineSpacingExtra="4sp"
            fontFamily="@font/2131296265"
            NOT_FOUND_0x10="0"
            NOT_FOUND_0x11="0"
            NOT_FOUND_0x12="0"
            NOT_FOUND_0x13="@+id/nav_tab_icon"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
