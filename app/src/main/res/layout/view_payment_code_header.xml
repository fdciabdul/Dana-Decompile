<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/clHeaderContainer"
    background="@drawable/bg_gradient_top_up_and_pay"
    layout_width="match_parent"
    layout_height="wrap_content"
    NOT_FOUND_0xb="0"
    NOT_FOUND_0xc="0"
    NOT_FOUND_0xd="@+id/toolbar">
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/ivHeader"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="6dp"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0xf="@drawable/ic_topup_and_pay_header"/>
    <androidx.appcompat.widget.AppCompatTextView
        id="@+id/tv_header_title"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_marginTop="8dp"
        layout_marginRight="16dp"
        layout_marginBottom="6dp"
        text="@string/topup_and_pay_header_title"
        textAlignment="center"
        NOT_FOUND_0xa="0"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xd="@+id/ivHeader"
        style="@style/2132017438"/>
</androidx.constraintlayout.widget.ConstraintLayout>
