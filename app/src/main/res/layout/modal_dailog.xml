<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    orientation="vertical"
    background="#ffffff"
    layout_width="230dp"
    layout_height="111dp">
    <TextView
        textSize="14dp"
        textColor="#333333"
        gravity="center"
        id="@+id/tv_msg"
        paddingLeft="12.8dp"
        paddingTop="21.8dp"
        paddingRight="12.8dp"
        paddingBottom="21.8dp"
        layout_width="match_parent"
        layout_height="wrap_content"
        text="Hint"
        maxLines="2"/>
    <View
        background="#e5e5e5"
        layout_width="match_parent"
        layout_height="1px"/>
    <TextView
        textSize="18dp"
        textColor="#108ee9"
        gravity="center"
        id="@+id/tv_ok"
        layout_width="match_parent"
        layout_height="42.7dp"
        text="@string/inter_uitools_ok"
        maxLines="1"/>
</LinearLayout>
