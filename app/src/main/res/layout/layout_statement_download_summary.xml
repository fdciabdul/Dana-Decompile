<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    orientation="vertical"
    background="#f5f5f5"
    layout_width="match_parent"
    layout_height="match_parent"
    paddingStart="16dp"
    paddingEnd="16dp">
    <id.dana.statement.view.StatementDateView
        id="@+id/WithdrawSavedCardChannelModel"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="8dp"/>
    <id.dana.statement.view.AccumulateStatementAmountView
        id="@+id/view_accumulate_statement_amount"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="12dp"/>
    <androidx.recyclerview.widget.RecyclerView
        id="@+id/rv_statement_history"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="8dp"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        id="@+id/cl_dana_watermark"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="24dp">
        <TextView
            gravity="center"
            layout_gravity="center"
            id="@+id/tv_powered_by"
            layout_width="wrap_content"
            layout_height="wrap_content"
            text="@string/powered_by"
            drawablePadding="8dp"
            drawableEnd="@drawable/ic_dana_gn_pay"
            NOT_FOUND_0xf="0"
            NOT_FOUND_0x10="0"
            NOT_FOUND_0x12="0"
            style="@style/2132017405"/>
        <TextView
            gravity="center"
            layout_gravity="center"
            id="@+id/tv_tagline_dana"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="4dp"
            text="@string/dana_tagline"
            NOT_FOUND_0xf="0"
            NOT_FOUND_0x10="0"
            NOT_FOUND_0x11="@+id/tv_powered_by"
            style="@style/2132017440"/>
        <TextView
            gravity="center"
            layout_gravity="center"
            id="@+id/tv_dana_url"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="4dp"
            layout_marginBottom="32dp"
            text="@string/dana_website"
            NOT_FOUND_0xe="0"
            NOT_FOUND_0xf="0"
            NOT_FOUND_0x10="0"
            NOT_FOUND_0x11="@+id/tv_tagline_dana"
            style="@style/2132017434"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</LinearLayout>
