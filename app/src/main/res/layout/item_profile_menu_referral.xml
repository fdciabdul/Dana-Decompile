<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="#ffffff"
    layout_width="match_parent"
    layout_height="wrap_content"
    contentDescription="@string/btn_referral">
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/iv_profile_referral"
        layout_width="28dp"
        layout_height="28dp"
        layout_marginTop="16dp"
        layout_marginBottom="16dp"
        scaleType="fitXY"
        layout_marginStart="16dp"
        NOT_FOUND_0x12="@+id/cl_referral_banner"
        NOT_FOUND_0x18="0"
        NOT_FOUND_0x1a="0"
        NOT_FOUND_0x1e="@drawable/ic_referral"/>
    <androidx.appcompat.widget.AppCompatTextView
        ellipsize="end"
        id="@+id/tv_profile_referral"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginLeft="12dp"
        layout_marginRight="12dp"
        maxLines="1"
        NOT_FOUND_0x11="@+id/iv_profile_referral"
        NOT_FOUND_0x14="@+id/CustomPopMenuExtension"
        NOT_FOUND_0x17="@+id/iv_profile_referral"
        NOT_FOUND_0x1a="@+id/iv_profile_referral"
        style="@style/2132017413"/>
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/CustomPopMenuExtension"
        layout_width="24dp"
        layout_height="24dp"
        scaleType="center"
        layout_marginEnd="4dp"
        NOT_FOUND_0x11="@+id/iv_profile_referral"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x1a="@+id/iv_profile_referral"
        NOT_FOUND_0x1e="@drawable/ic_arrow_right_me_page"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        id="@+id/cl_referral_banner"
        background="@drawable/bg_rounded_8dp_grey10"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginLeft="8dp"
        layout_marginRight="8dp"
        layout_marginBottom="8dp"
        NOT_FOUND_0x12="@+id/menu_divider"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x18="0"
        NOT_FOUND_0x19="@+id/iv_profile_referral">
        <androidx.appcompat.widget.AppCompatTextView
            id="@+id/tv_referral_banner_title"
            layout_width="0dp"
            layout_height="wrap_content"
            text="@string/referral_banner_title"
            layout_marginStart="16dp"
            NOT_FOUND_0x12="@+id/tv_referral_banner_subtitle"
            NOT_FOUND_0x15="0"
            NOT_FOUND_0x1a="0"
            NOT_FOUND_0x1b="2"
            style="@style/2132017908"/>
        <androidx.appcompat.widget.AppCompatTextView
            id="@+id/tv_referral_banner_subtitle"
            visibility="gone"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_marginLeft="16dp"
            layout_marginTop="4dp"
            layout_marginRight="16dp"
            fontFamily="@font/sf_pro_regular"
            NOT_FOUND_0x11="0"
            NOT_FOUND_0x18="0"
            NOT_FOUND_0x19="@+id/tv_referral_banner_title"
            style="@style/2132017440"/>
        <com.airbnb.lottie.LottieAnimationView
            id="@+id/lv_referral_banner_gift"
            layout_width="62dp"
            layout_height="62dp"
            layout_marginTop="4dp"
            layout_marginBottom="4dp"
            layout_marginEnd="12dp"
            NOT_FOUND_0x11="0"
            NOT_FOUND_0x16="0"
            NOT_FOUND_0x1a="0"
            NOT_FOUND_0x1c="json/lottie/profile_referral_banner.lottie"
            NOT_FOUND_0x1d="true"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <View
        id="@+id/menu_divider"
        layout_width="0dp"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x18="0"
        style="@style/2132017967"/>
</androidx.constraintlayout.widget.ConstraintLayout>
