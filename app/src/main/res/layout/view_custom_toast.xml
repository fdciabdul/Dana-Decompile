<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    orientation="horizontal"
    id="@+id/cl_custom_toast_container"
    paddingLeft="16dp"
    paddingRight="16dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    paddingStart="16dp"
    paddingEnd="16dp">
    <androidx.constraintlayout.widget.ConstraintLayout
        id="@+id/cl_custom_toast"
        background="@drawable/bg_rounded_toast"
        layout_width="match_parent"
        layout_height="wrap_content"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x1c="0"
        NOT_FOUND_0x1d="0">
        <ImageView
            layout_gravity="center_vertical"
            id="@+id/iv_custom_toast"
            layout_width="24dp"
            layout_height="24dp"
            layout_marginLeft="16dp"
            layout_marginTop="8dp"
            layout_marginBottom="8dp"
            layout_marginStart="16dp"
            NOT_FOUND_0x16="0"
            NOT_FOUND_0x19="0"
            NOT_FOUND_0x1d="0"
            NOT_FOUND_0x1e="@drawable/ic_success"/>
        <TextView
            textSize="12sp"
            textColor="#313131"
            ellipsize="none"
            id="@+id/tv_custom_toast"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginLeft="8dp"
            layout_marginTop="8dp"
            layout_marginRight="16dp"
            layout_marginBottom="8dp"
            maxLines="2"
            fontFamily="@font/2131296265"
            layout_marginStart="8dp"
            layout_marginEnd="16dp"
            NOT_FOUND_0x15="true"
            NOT_FOUND_0x16="@+id/iv_custom_toast"
            NOT_FOUND_0x18="0"
            NOT_FOUND_0x1a="@+id/iv_custom_toast"
            NOT_FOUND_0x1b="0"
            NOT_FOUND_0x1d="@+id/iv_custom_toast"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
