<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="#ffffff"
    paddingLeft="16dp"
    paddingTop="8dp"
    paddingRight="16dp"
    paddingBottom="16dp"
    layout_width="match_parent"
    layout_height="wrap_content">
    <RelativeLayout
        id="@+id/vButton"
        background="@drawable/square_rounded_skeleton_grey_40"
        paddingTop="8dp"
        paddingBottom="8dp"
        layout_width="match_parent"
        layout_height="44dp"
        textAlignment="center"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0xf="0">
        <ProgressBar
            id="@+id/progressBarInBtn"
            drawingCacheQuality="high"
            layout_width="match_parent"
            layout_height="match_parent"
            indeterminate="true"
            indeterminateDrawable="@drawable/rotate_loading_spinner"/>
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
