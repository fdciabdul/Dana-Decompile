<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    orientation="horizontal"
    id="@+id/container_custom_toast_for_unbind"
    background="@drawable/bg_rounded_toast_success"
    paddingLeft="8dp"
    paddingTop="12dp"
    paddingRight="16dp"
    paddingBottom="12dp"
    layout_width="match_parent"
    layout_height="56dp">
    <androidx.appcompat.widget.AppCompatImageView
        layout_gravity="center"
        layout_width="28dp"
        layout_height="28dp"
        NOT_FOUND_0xb="@drawable/ic_success"/>
    <TextView
        layout_gravity="center"
        id="@+id/tv_custom_snackbar_for_unbind"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginStart="8dp"
        style="@style/2132017439"/>
</LinearLayout>
