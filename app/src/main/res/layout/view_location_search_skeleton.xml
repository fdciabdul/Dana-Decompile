<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    paddingTop="16dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    paddingStart="12dp">
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/ivLocationIconShimmer"
        layout_width="0dp"
        layout_height="wrap_content"
        NOT_FOUND_0x9="@+id/tvLocationDetailShimmer"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xd="@+id/tvLocationNameShimmer"
        NOT_FOUND_0xe="@drawable/ic_location_with_background_inactive"/>
    <View
        id="@+id/tvLocationNameShimmer"
        background="@drawable/square_rounded_skeleton_4dp_grey20"
        layout_width="240dp"
        layout_height="22dp"
        layout_marginStart="12dp"
        layout_marginEnd="12dp"
        NOT_FOUND_0xa="@+id/ivLocationIconShimmer"
        NOT_FOUND_0xd="0"/>
    <TextView
        id="@+id/tvLocationDetailShimmer"
        background="@drawable/square_rounded_skeleton_4dp_grey20"
        layout_width="240dp"
        layout_height="14dp"
        layout_marginTop="4dp"
        NOT_FOUND_0xb="@+id/tvLocationNameShimmer"
        NOT_FOUND_0xc="@+id/tvLocationNameShimmer"/>
    <TextView
        id="@+id/tvDistanceShimmer"
        background="@drawable/square_rounded_skeleton_4dp_grey20"
        layout_width="40dp"
        layout_height="14dp"
        layout_marginTop="4dp"
        NOT_FOUND_0xb="@+id/tvLocationDetailShimmer"
        NOT_FOUND_0xc="@+id/tvLocationDetailShimmer"/>
    <View
        background="#d1d1d1"
        layout_width="0dp"
        layout_height="0.5dp"
        layout_marginTop="13dp"
        NOT_FOUND_0xb="@+id/tvDistanceShimmer"
        NOT_FOUND_0xc="@+id/tvDistanceShimmer"/>
</androidx.constraintlayout.widget.ConstraintLayout>
