<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/FamilyManageAvailableServicesAdapter_res_0x7f0a03e4"
    background="#108ee9"
    focusable="true"
    focusableInTouchMode="true"
    animateLayoutChanges="true"
    style="@style/2132017490">
    <androidx.appcompat.widget.Toolbar
        id="@+id/tbSearchView"
        layout_width="match_parent"
        layout_height="?android:attr/actionBarSize"
        NOT_FOUND_0x18="0dp"
        NOT_FOUND_0x19="0dp"
        NOT_FOUND_0x1a="0dp"
        NOT_FOUND_0x1b="0"
        NOT_FOUND_0x1d="0"
        NOT_FOUND_0x1e="0"
        NOT_FOUND_0x20="0"/>
    <TextView
        id="@+id/tvCancel"
        visibility="visible"
        text="@string/btn_cancel_nearby_search"
        contentDescription="@string/btnCancel"
        layout_marginEnd="12dp"
        NOT_FOUND_0x1b="0"
        NOT_FOUND_0x1e="0"
        NOT_FOUND_0x20="0"
        style="@style/2132017542"/>
    <LinearLayout
        id="@+id/onLooperPrepared"
        background="@drawable/searchbox_rounded_white"
        padding="10dp"
        layout_width="0dp"
        layout_height="wrap_content"
        contentDescription="@string/txtAreaSearch"
        layout_marginStart="44dp"
        layout_marginEnd="16dp"
        NOT_FOUND_0x1b="@+id/tbSearchView"
        NOT_FOUND_0x1c="@+id/tvCancel"
        NOT_FOUND_0x1f="0"
        NOT_FOUND_0x20="0">
        <ImageView
            layout_gravity="center"
            layout_width="20dp"
            layout_height="20dp"
            src="@drawable/ic_search_grey"/>
        <FrameLayout
            layout_width="0dp"
            layout_height="match_parent"
            layout_weight="1">
            <EditText
                ellipsize="end"
                layout_gravity="center"
                id="@+id/etSearch"
                background="#00000000"
                focusable="true"
                focusableInTouchMode="true"
                layout_width="match_parent"
                layout_height="wrap_content"
                hint="@string/nearby_search_hint"
                maxLines="1"
                android:maxLength="25"
                inputType="textNoSuggestions"
                imeOptions="actionSearch"
                paddingStart="4dp"
                paddingEnd="20dp"
                style="@style/2132017407"/>
            <androidx.appcompat.widget.AppCompatImageView
                layout_gravity="end|center_vertical"
                id="@+id/addOnSelectionChangedListener"
                visibility="gone"
                layout_width="20dp"
                layout_height="20dp"
                contentDescription="@string/btnX"
                NOT_FOUND_0x21="@drawable/ic_close_round_grey"/>
        </FrameLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
