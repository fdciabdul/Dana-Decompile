<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/cl_empty_state"
    background="#f5f5f5"
    layout_width="match_parent"
    layout_height="match_parent"
    layout_margin="16dp">
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/getCustomAttributesOrThrow"
        layout_width="160dp"
        layout_height="160dp"
        NOT_FOUND_0xb="@+id/btn_try_again_empty_state"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="0.45"
        NOT_FOUND_0x15="@drawable/ic_general_error_somethings_wrong"/>
    <TextView
        id="@+id/res_0x7f0a1921_textviewkt_doaftertextchanged_inlined_addtextchangedlistener_default_1"
        tag="@string/error_message_tag"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="16dp"
        text="@string/something_is_wrong_here"
        textAlignment="center"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="@+id/getCustomAttributesOrThrow"
        style="@style/2132017932"/>
    <TextView
        id="@+id/tv_subtitle_empty_state"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        text="@string/subtitle_no_connection"
        textAlignment="center"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="@+id/res_0x7f0a1921_textviewkt_doaftertextchanged_inlined_addtextchangedlistener_default_1"
        style="@style/2132017917"/>
    <id.dana.component.buttoncomponent.DanaButtonPrimaryView
        id="@+id/btn_try_again_empty_state"
        layout_width="match_parent"
        layout_height="wrap_content"
        contentDescription="@string/btn_try_again"
        NOT_FOUND_0xa="0"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x13="@string/try_again"
        NOT_FOUND_0x14="1"/>
</androidx.constraintlayout.widget.ConstraintLayout>
