<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_width="match_parent"
    layout_height="wrap_content">
    <View
        id="@+id/vMerchantLogo"
        background="@drawable/bg_rounded_grey_20_4dp"
        layout_width="48dp"
        layout_height="48dp"
        NOT_FOUND_0x8="0"
        NOT_FOUND_0xa="0"/>
    <View
        id="@+id/vDate"
        background="@drawable/bg_rounded_grey_20_4dp"
        layout_width="0dp"
        layout_height="16dp"
        layout_marginStart="8dp"
        NOT_FOUND_0x6="0"
        NOT_FOUND_0x7="@+id/vMerchantLogo"
        NOT_FOUND_0xa="0"/>
    <View
        id="@+id/vShopName"
        background="@drawable/bg_rounded_grey_20_4dp"
        layout_width="0dp"
        layout_height="16dp"
        layout_marginTop="8dp"
        layout_marginStart="8dp"
        NOT_FOUND_0x6="0"
        NOT_FOUND_0x7="@+id/vMerchantLogo"
        NOT_FOUND_0x9="@+id/vDate"/>
    <View
        id="@+id/vRating"
        background="@drawable/bg_rounded_grey_20_4dp"
        layout_width="0dp"
        layout_height="40dp"
        layout_marginTop="8dp"
        layout_marginStart="8dp"
        NOT_FOUND_0x6="0"
        NOT_FOUND_0x7="@+id/vMerchantLogo"
        NOT_FOUND_0x9="@+id/vShopName"/>
</androidx.constraintlayout.widget.ConstraintLayout>
