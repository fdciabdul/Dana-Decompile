<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_width="match_parent"
    layout_height="40dp">
    <ImageView
        id="@+id/ivDropdownOptionIcon"
        visibility="gone"
        layout_width="14dp"
        layout_height="14dp"
        layout_marginStart="4dp"
        NOT_FOUND_0xb="@+id/tvDropdownOptionName"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="@+id/tvDropdownOptionName"/>
    <TextView
        ellipsize="end"
        id="@+id/tvDropdownOptionName"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        layout_marginBottom="8dp"
        maxLines="1"
        layout_marginStart="4dp"
        layout_marginEnd="12dp"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xd="@+id/tvDropdownOptionValue"
        NOT_FOUND_0xe="@+id/ivDropdownOptionIcon"
        NOT_FOUND_0x10="0"
        style="@style/2132017405"/>
    <TextView
        id="@+id/tvDropdownOptionValue"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginEnd="12dp"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xd="@+id/ivChecklist"
        NOT_FOUND_0x10="0"
        style="@style/2132017905"/>
    <ImageView
        id="@+id/ivChecklist"
        visibility="invisible"
        layout_width="18dp"
        layout_height="18dp"
        src="@drawable/ic_check_24_green50_filled_circle"
        layout_marginEnd="12dp"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0x10="0"/>
</androidx.constraintlayout.widget.ConstraintLayout>
