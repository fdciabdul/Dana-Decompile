<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="@drawable/btn_common"
    layout_width="match_parent"
    layout_height="44dp">
    <TextView
        textSize="16sp"
        textColor="@android:color/white"
        id="@+id/textView"
        layout_marginLeft="16dp"
        layout_marginRight="16dp"
        text="@string/button_pay_my_part"
        textAllCaps="true"
        fontFamily="@font/2131296266"
        layout_marginStart="16dp"
        layout_marginEnd="16dp"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="0"
        style="@style/2132018187"/>
    <id.dana.richview.CurrencyTextView
        textSize="16sp"
        textColor="@android:color/white"
        id="@+id/ctv_pay_amount"
        layout_marginLeft="16dp"
        fontFamily="@font/2131296266"
        layout_marginStart="16dp"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0xf="@+id/imageview_right_arrow"
        NOT_FOUND_0x10="1"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="0"
        style="@style/2132018187"/>
    <ImageView
        id="@+id/imageview_right_arrow"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x10="0.976"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x13="@drawable/arrow_right_white"
        style="@style/2132018187"/>
</androidx.constraintlayout.widget.ConstraintLayout>
