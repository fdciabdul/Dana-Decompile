<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    paddingTop="11dp"
    paddingBottom="11dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    paddingStart="16dp"
    paddingEnd="16dp">
    <View
        id="@+id/vAvatarSkeleton"
        background="@drawable/bg_circle_skeleton"
        layout_width="48dp"
        layout_height="48dp"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xe="0"/>
    <View
        id="@+id/vNameSkeleton"
        background="@drawable/square_rounded_skeleton_4dp_grey20"
        layout_width="132dp"
        layout_height="18dp"
        layout_marginStart="12dp"
        NOT_FOUND_0xb="@+id/vAvatarSkeleton"
        NOT_FOUND_0xe="0"/>
    <View
        id="@+id/vStarSkeleton"
        background="@drawable/square_rounded_skeleton_4dp_grey20"
        layout_width="96dp"
        layout_height="16dp"
        layout_marginTop="4dp"
        layout_marginStart="12dp"
        NOT_FOUND_0xb="@+id/vAvatarSkeleton"
        NOT_FOUND_0xd="@+id/vNameSkeleton"/>
    <View
        background="@drawable/square_rounded_skeleton_4dp_grey20"
        layout_width="60dp"
        layout_height="16dp"
        layout_marginStart="4dp"
        NOT_FOUND_0xa="@+id/vStarSkeleton"
        NOT_FOUND_0xb="@+id/vStarSkeleton"
        NOT_FOUND_0xe="@+id/vStarSkeleton"/>
</androidx.constraintlayout.widget.ConstraintLayout>
