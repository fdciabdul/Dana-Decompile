<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/fl_home_survey"
    background="@drawable/home_component_border"
    visibility="gone"
    layout_marginTop="4dp"
    style="@style/2132017490">
    <androidx.constraintlayout.widget.ConstraintLayout
        layout_width="match_parent"
        layout_height="match_parent">
        <ImageView
            id="@+id/img_mobile_recharge"
            layout_width="56dp"
            layout_height="56dp"
            layout_marginLeft="8dp"
            layout_marginTop="8dp"
            layout_marginBottom="8dp"
            layout_alignParentLeft="true"
            layout_centerVertical="true"
            contentDescription="@null"
            layout_marginStart="8dp"
            layout_alignParentStart="true"
            NOT_FOUND_0x12="0"
            NOT_FOUND_0x15="0"
            NOT_FOUND_0x17="0"
            NOT_FOUND_0x18="@drawable/ic_survey"/>
        <TextView
            textSize="16sp"
            id="@+id/tv_survey_title"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_marginLeft="16dp"
            layout_marginTop="16dp"
            layout_marginRight="8dp"
            text="@string/home_survey_title"
            fontFamily="@string/see_more_inbox"
            layout_marginStart="16dp"
            layout_marginEnd="8dp"
            NOT_FOUND_0x13="0"
            NOT_FOUND_0x14="@+id/img_mobile_recharge"
            NOT_FOUND_0x17="0"/>
        <TextView
            textSize="14sp"
            id="@+id/tv_survey_description"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_marginLeft="16dp"
            layout_marginTop="8dp"
            layout_marginRight="8dp"
            layout_marginBottom="16dp"
            text="@string/home_survey_desc"
            layout_marginStart="16dp"
            layout_marginEnd="8dp"
            NOT_FOUND_0x12="0"
            NOT_FOUND_0x13="0"
            NOT_FOUND_0x14="@+id/img_mobile_recharge"
            NOT_FOUND_0x16="@+id/tv_survey_title"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>
