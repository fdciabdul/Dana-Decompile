<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    orientation="horizontal"
    id="@+id/llParent"
    layout_width="match_parent"
    layout_height="60dp">
    <RelativeLayout
        id="@+id/rlRadio"
        layout_width="wrap_content"
        layout_height="wrap_content"
        NOT_FOUND_0x11="@+id/ivIcon"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x16="@+id/ivIcon">
        <androidx.appcompat.widget.AppCompatImageView
            layout_gravity="center_vertical"
            id="@+id/ivRadioInactive"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginEnd="16dp"
            NOT_FOUND_0x17="@drawable/ic_radio_button_blue_inactive"/>
        <androidx.appcompat.widget.AppCompatImageView
            layout_gravity="center_vertical"
            id="@+id/ivRadioActive"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginEnd="16dp"
            NOT_FOUND_0x17="@drawable/ic_radio_button_blue_active"/>
    </RelativeLayout>
    <TextView
        textColor="#313131"
        layout_gravity="center_vertical"
        id="@+id/tvName"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_marginRight="16dp"
        NOT_FOUND_0x11="@+id/ivIcon"
        NOT_FOUND_0x13="@+id/rlRadio"
        NOT_FOUND_0x14="@+id/ivIcon"
        NOT_FOUND_0x16="@+id/ivIcon"
        style="@style/2132017407"/>
    <View
        id="@+id/view_divider"
        background="#b8b8b8"
        layout_width="0dp"
        layout_height="0.5dp"
        layout_marginTop="10dp"
        layout_marginStart="16dp"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x15="0"/>
    <de.hdodenhof.circleimageview.CircleImageView
        id="@+id/ivIcon"
        visibility="gone"
        layout_width="25dp"
        layout_height="25dp"
        layout_marginTop="16dp"
        layout_marginBottom="16dp"
        src="@drawable/ic_user_default_placeholder"
        layout_marginStart="16dp"
        layout_marginEnd="8dp"
        NOT_FOUND_0xf="@color/2131099990"
        NOT_FOUND_0x10="1dp"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x16="0"/>
</androidx.constraintlayout.widget.ConstraintLayout>
