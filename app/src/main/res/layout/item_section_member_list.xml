<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/cl_contact"
    background="#ffffff"
    paddingTop="16dp"
    focusable="false"
    layout_width="match_parent"
    layout_height="wrap_content">
    <LinearLayout
        orientation="horizontal"
        id="@+id/llSectionCell"
        layout_width="match_parent"
        layout_height="wrap_content"
        weightSum="1"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x14="0">
        <androidx.constraintlayout.widget.ConstraintLayout
            id="@+id/clLeftCell"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_weight="0.75">
            <id.dana.richview.CircleImageSelectionView
                id="@+id/cisvDisplayPhoto"
                layout_width="28dp"
                layout_height="28dp"
                layout_marginStart="16dp"
                NOT_FOUND_0xf="0"
                NOT_FOUND_0x12="0"
                NOT_FOUND_0x14="0"
                NOT_FOUND_0x15="9dp"/>
            <TextView
                ellipsize="end"
                id="@+id/tvDisplayName"
                layout_width="0dp"
                layout_height="wrap_content"
                maxLines="2"
                layout_marginStart="12dp"
                NOT_FOUND_0x10="0"
                NOT_FOUND_0x11="@+id/cisvDisplayPhoto"
                NOT_FOUND_0x14="0"
                style="@style/2132017413"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <TextView
            gravity="end|center_vertical"
            id="@+id/tvRoles"
            layout_width="0dp"
            layout_height="match_parent"
            layout_weight="0.25"
            layout_marginEnd="16dp"
            style="@style/2132017413"/>
    </LinearLayout>
    <View
        id="@+id/viewDivider"
        background="#b8b8b8"
        layout_width="match_parent"
        layout_height="0.5dp"
        layout_marginTop="16dp"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x13="@+id/llSectionCell"/>
</androidx.constraintlayout.widget.ConstraintLayout>
