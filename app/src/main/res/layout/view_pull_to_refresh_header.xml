<?xml version="1.0" encoding="utf-8"?>
<in.srain.cube.views.ptr.PtrClassicDefaultHeader xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    orientation="vertical"
    id="@+id/pcdh_loading"
    background="#108ee9"
    layout_width="match_parent"
    layout_height="match_parent">
    <id.dana.core.ui.richview.LoadingLottieAnimationView
        layout_gravity="top|center_horizontal"
        id="@+id/lav_loading_animation"
        layout_width="60dp"
        layout_height="60dp"
        layout_marginBottom="16dp"
        NOT_FOUND_0xc="11"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0xe="26"
        NOT_FOUND_0xf="11"
        NOT_FOUND_0x10="json/lottie/loading_animation_faster.lottie"
        NOT_FOUND_0x11="true"
        NOT_FOUND_0x12="44"
        NOT_FOUND_0x13="26"/>
    <TextView
        textSize="10sp"
        textColor="#ffffff"
        layout_gravity="bottom|center_horizontal"
        paddingBottom="8dp"
        layout_width="wrap_content"
        layout_height="wrap_content"
        text="@string/pull_to_refresh_text"
        fontFamily="@font/2131296265"/>
</in.srain.cube.views.ptr.PtrClassicDefaultHeader>
