<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/rl_main_parent"
    layout_width="match_parent"
    layout_height="wrap_content">
    <View
        id="@+id/statement_divider"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x18="0"
        style="@style/2132017967"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        id="@+id/res_0x7f0a03f6_cachebuilderspec_accessdurationparser"
        background="@drawable/bg_statement_rounded_4dp"
        paddingTop="8dp"
        paddingBottom="8dp"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_weight="0.5"
        contentDescription="@string/btnStatementIncome"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x14="@+id/divider"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x17="@+id/statement_divider">
        <androidx.appcompat.widget.AppCompatImageView
            id="@+id/iv_arrow_income"
            background="@drawable/bg_income_statement"
            padding="7dp"
            layout_width="32dp"
            layout_height="32dp"
            layout_marginTop="10dp"
            layout_marginBottom="10dp"
            scaleType="fitXY"
            layout_marginStart="12dp"
            NOT_FOUND_0x12="0"
            NOT_FOUND_0x16="0"
            NOT_FOUND_0x18="0"
            NOT_FOUND_0x19="@drawable/ic_arrow_down_statement"/>
        <androidx.appcompat.widget.AppCompatTextView
            textColor="@color/2131100175"
            id="@+id/tv_income_label"
            focusable="true"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="8dp"
            layout_marginBottom="8dp"
            text="@string/income"
            includeFontPadding="false"
            fontFamily="@font/sf_pro_regular"
            layout_marginStart="12dp"
            NOT_FOUND_0x15="@+id/iv_arrow_income"
            NOT_FOUND_0x18="0"
            style="@style/2132017440"/>
        <androidx.appcompat.widget.AppCompatTextView
            textColor="#313131"
            id="@+id/tv_income_amount"
            focusable="true"
            layout_width="0dp"
            layout_height="wrap_content"
            text="@string/amount_default"
            includeFontPadding="false"
            NOT_FOUND_0x16="@+id/tv_income_label"
            NOT_FOUND_0x17="@+id/tv_income_label"
            style="@style/2132017908"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <View
        id="@+id/divider"
        background="?android:attr/listDivider"
        layout_width="1dp"
        layout_height="40dp"
        layout_marginTop="14dp"
        layout_marginBottom="14dp"
        layout_weight="0"
        NOT_FOUND_0x14="@+id/cl_accumulate_expense"
        NOT_FOUND_0x15="@+id/res_0x7f0a03f6_cachebuilderspec_accessdurationparser"
        NOT_FOUND_0x17="@+id/statement_divider"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        id="@+id/cl_accumulate_expense"
        background="@drawable/bg_statement_rounded_4dp"
        paddingTop="8dp"
        paddingBottom="8dp"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_weight="0.5"
        contentDescription="@string/btnStatementExpense"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x15="@+id/divider"
        NOT_FOUND_0x18="0">
        <androidx.appcompat.widget.AppCompatImageView
            id="@+id/iv_arrow_expense"
            background="@drawable/bg_expense_statement"
            padding="7dp"
            layout_width="32dp"
            layout_height="32dp"
            layout_marginTop="10dp"
            layout_marginBottom="10dp"
            scaleType="fitXY"
            layout_marginStart="12dp"
            NOT_FOUND_0x12="0"
            NOT_FOUND_0x16="0"
            NOT_FOUND_0x18="0"
            NOT_FOUND_0x19="@drawable/ic_arrow_up_statement"/>
        <androidx.appcompat.widget.AppCompatTextView
            textColor="@color/2131100175"
            id="@+id/tv_expense_label"
            focusable="true"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="8dp"
            layout_marginBottom="8dp"
            text="@string/expense"
            includeFontPadding="false"
            fontFamily="@font/sf_pro_regular"
            layout_marginStart="12dp"
            NOT_FOUND_0x15="@+id/iv_arrow_expense"
            NOT_FOUND_0x18="0"
            style="@style/2132017440"/>
        <androidx.appcompat.widget.AppCompatTextView
            textColor="#313131"
            id="@+id/tv_expense_amount"
            focusable="true"
            layout_width="0dp"
            layout_height="wrap_content"
            text="@string/amount_default"
            includeFontPadding="false"
            NOT_FOUND_0x16="@+id/tv_expense_label"
            NOT_FOUND_0x17="@+id/tv_expense_label"
            style="@style/2132017908"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
