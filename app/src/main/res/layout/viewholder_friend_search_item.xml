<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/allowedTargets"
    layout_width="match_parent"
    layout_height="wrap_content"
    paddingStart="16dp">
    <com.google.android.material.imageview.ShapeableImageView
        id="@+id/civProfileAvatar"
        layout_width="28dp"
        layout_height="28dp"
        src="@drawable/avatar_placeholder"
        NOT_FOUND_0x14="@+id/tvName"
        NOT_FOUND_0x1a="0"
        NOT_FOUND_0x1c="@+id/tvUsername"
        NOT_FOUND_0x23="@style/2132018200"
        NOT_FOUND_0x24="@color/2131099990"
        NOT_FOUND_0x25="1dp"/>
    <TextView
        textColor="#313131"
        ellipsize="end"
        id="@+id/tvUsername"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="16dp"
        maxLines="1"
        layout_marginStart="12dp"
        NOT_FOUND_0x15="@+id/tvName"
        NOT_FOUND_0x17="@+id/barrierButton"
        NOT_FOUND_0x18="0"
        NOT_FOUND_0x19="@+id/civProfileAvatar"
        NOT_FOUND_0x1c="0"
        style="@style/2132017894"/>
    <TextView
        textColor="#313131"
        ellipsize="end"
        id="@+id/tvName"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        maxLines="1"
        fontFamily="@font/2131296265"
        layout_marginStart="12dp"
        NOT_FOUND_0x17="@+id/barrierButton"
        NOT_FOUND_0x18="0"
        NOT_FOUND_0x19="@+id/civProfileAvatar"
        NOT_FOUND_0x1b="@+id/tvUsername"
        style="@style/2132017433"/>
    <androidx.constraintlayout.widget.Barrier
        id="@+id/barrierButton"
        layout_width="wrap_content"
        layout_height="wrap_content"
        NOT_FOUND_0x10="5"
        NOT_FOUND_0x11="btnApprove, btnAdd, btnCancel, lpvLoading"/>
    <id.dana.component.buttoncomponent.DanaButtonPrimaryView
        id="@+id/btnApprove"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginEnd="4dp"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x17="@+id/btnDecline"
        NOT_FOUND_0x1c="0"
        NOT_FOUND_0x1d="10"
        NOT_FOUND_0x1e="@string/btn_friend_request_approve"
        NOT_FOUND_0x1f="1"/>
    <id.dana.component.buttoncomponent.DanaButtonSecondaryView
        id="@+id/btnDecline"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginEnd="16dp"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x1c="0"
        NOT_FOUND_0x20="10"
        NOT_FOUND_0x21="@string/btn_friend_request_decline"
        NOT_FOUND_0x22="1"/>
    <id.dana.component.buttoncomponent.DanaButtonPrimaryView
        id="@+id/btnAdd"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginEnd="16dp"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x18="1"
        NOT_FOUND_0x1c="0"
        NOT_FOUND_0x1d="10"
        NOT_FOUND_0x1e="@string/add_friend_button"
        NOT_FOUND_0x1f="1"/>
    <id.dana.component.buttoncomponent.DanaButtonSecondaryView
        id="@+id/btnCancel"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginEnd="16dp"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x18="1"
        NOT_FOUND_0x1c="0"
        NOT_FOUND_0x20="10"
        NOT_FOUND_0x21="@string/cancel_friend_button"
        NOT_FOUND_0x22="1"/>
    <com.google.android.material.progressindicator.CircularProgressIndicator
        layout_gravity="center"
        id="@+id/lpvLoading"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        indeterminate="true"
        layout_marginEnd="16dp"
        NOT_FOUND_0x12="@color/2131099990"
        NOT_FOUND_0x13="16dp"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x19="@+id/barrierButton"
        NOT_FOUND_0x1c="0"/>
    <View
        id="@+id/divider"
        background="#d1d1d1"
        layout_width="match_parent"
        layout_height="0.5dp"
        layout_marginTop="16dp"
        NOT_FOUND_0x1a="@+id/civProfileAvatar"
        NOT_FOUND_0x1b="@+id/civProfileAvatar"/>
</androidx.constraintlayout.widget.ConstraintLayout>
