<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_width="match_parent"
    layout_height="wrap_content"
    layout_marginBottom="8dp">
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/headerIcon"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginStart="16dp"
        NOT_FOUND_0x8="@+id/addOnItemTouchListener"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xc="0"/>
    <androidx.appcompat.widget.AppCompatTextView
        textColor="#313131"
        id="@+id/addOnItemTouchListener"
        layout_width="wrap_content"
        layout_height="wrap_content"
        text="@string/bank_promo_header"
        layout_marginStart="6dp"
        NOT_FOUND_0xa="@+id/headerIcon"
        NOT_FOUND_0xc="0"
        style="@style/2132017916"/>
    <androidx.appcompat.widget.AppCompatTextView
        id="@+id/btnSeeAll"
        layout_width="wrap_content"
        layout_height="wrap_content"
        text="@string/promo_discovery_see_all"
        layout_marginEnd="21dp"
        NOT_FOUND_0x8="@+id/addOnItemTouchListener"
        NOT_FOUND_0x9="0"
        NOT_FOUND_0xc="0"
        style="@style/2132017895"/>
</androidx.constraintlayout.widget.ConstraintLayout>
