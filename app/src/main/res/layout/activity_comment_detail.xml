<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/allowedTargets"
    layout_width="match_parent"
    layout_height="match_parent">
    <include
        id="@+id/tb_social_detail"
        layout_width="match_parent"
        layout_height="wrap_content"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x14="0"
        layout="@layout/layout_toolbar"/>
    <in.srain.cube.views.ptr.PtrClassicFrameLayout
        id="@+id/ptr_activity_detail"
        background="#108ee9"
        clipToPadding="false"
        layout_width="match_parent"
        layout_height="0dp"
        NOT_FOUND_0x10="@+id/feed_input_comment_view"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x13="@+id/tb_social_detail">
        <androidx.coordinatorlayout.widget.CoordinatorLayout
            focusable="true"
            focusableInTouchMode="true"
            clickable="true"
            layout_width="match_parent"
            layout_height="match_parent">
            <com.google.android.material.appbar.AppBarLayout
                id="@+id/app_bar_activity_detail_layout"
                background="#fafafa"
                NOT_FOUND_0xc="0dp"
                style="@style/2132017490">
                <com.google.android.material.appbar.CollapsingToolbarLayout
                    id="@+id/collapsing_toolbar"
                    layout_marginBottom="8dp"
                    NOT_FOUND_0x15="0x1"
                    style="@style/2132017490">
                    <id.dana.social.view.FeedsView
                        id="@+id/feed_view_detail"
                        background="#ffffff"
                        layout_width="match_parent"
                        layout_height="wrap_content"
                        NOT_FOUND_0x17="false"
                        NOT_FOUND_0x18="false"
                        NOT_FOUND_0x19="true"/>
                    <id.dana.social.v2.view.ActivityFeedView
                        id="@+id/newFeedViewDetail"
                        background="#ffffff"
                        visibility="gone"
                        layout_width="match_parent"
                        layout_height="wrap_content"
                        NOT_FOUND_0xb="true"
                        NOT_FOUND_0x16="false"/>
                </com.google.android.material.appbar.CollapsingToolbarLayout>
                <id.dana.social.view.ReactionSectionView
                    id="@+id/reaction_section_view_detail"
                    background="#ffffff"
                    layout_width="match_parent"
                    layout_height="48dp"
                    NOT_FOUND_0xd="true"/>
                <View
                    background="#d1d1d1"
                    layout_width="match_parent"
                    layout_height="0.5dp"/>
            </com.google.android.material.appbar.AppBarLayout>
            <id.dana.social.view.CommentListView
                id="@+id/commentlistview"
                background="#f5f5f5"
                clipChildren="false"
                clipToPadding="false"
                layout_width="match_parent"
                layout_height="match_parent"
                NOT_FOUND_0xe="@string/appbar_scrolling_view_behavior"/>
        </androidx.coordinatorlayout.widget.CoordinatorLayout>
    </in.srain.cube.views.ptr.PtrClassicFrameLayout>
    <View
        background="@drawable/bg_gradient_comment_separator"
        layout_width="match_parent"
        layout_height="8dp"
        NOT_FOUND_0x10="@+id/feed_input_comment_view"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="0"/>
    <id.dana.social.view.FeedInputCommentView
        id="@+id/feed_input_comment_view"
        layout_width="match_parent"
        layout_height="wrap_content"
        NOT_FOUND_0xf="0"/>
</androidx.constraintlayout.widget.ConstraintLayout>
