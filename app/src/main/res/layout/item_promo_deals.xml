<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_width="wrap_content"
    layout_height="wrap_content"
    contentDescription="@string/cardPromo"
    NOT_FOUND_0x12="4dp"
    NOT_FOUND_0x13="0dp"
    NOT_FOUND_0x14="false">
    <androidx.constraintlayout.widget.ConstraintLayout
        background="@drawable/bg_rectangle_grey_frame_4_dp"
        layout_width="wrap_content"
        layout_height="wrap_content"
        paddingEnd="16dp">
        <androidx.appcompat.widget.AppCompatImageView
            id="@+id/ivIcon"
            paddingTop="8dp"
            paddingBottom="8dp"
            clickable="false"
            layout_width="58dp"
            layout_height="54dp"
            layout_margin="1dp"
            adjustViewBounds="true"
            paddingStart="8dp"
            paddingEnd="12dp"
            NOT_FOUND_0x15="0"
            NOT_FOUND_0x18="0"
            NOT_FOUND_0x19="0"
            NOT_FOUND_0x1a="@drawable/ic_promo_tag"/>
        <LinearLayout
            orientation="vertical"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginStart="8dp"
            NOT_FOUND_0x15="0"
            NOT_FOUND_0x16="0"
            NOT_FOUND_0x17="@+id/ivIcon"
            NOT_FOUND_0x19="0">
            <TextView
                ellipsize="end"
                id="@+id/tvTitle"
                layout_width="wrap_content"
                layout_height="wrap_content"
                maxWidth="194dp"
                maxLines="1"
                style="@style/2132017908"/>
            <TextView
                ellipsize="end"
                id="@+id/isHelperTextDisplayed"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginTop="4dp"
                maxWidth="194dp"
                maxLines="1"
                style="@style/2132017440"/>
        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>
