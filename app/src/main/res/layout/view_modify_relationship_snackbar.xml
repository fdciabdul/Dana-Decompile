<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    paddingBottom="30dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    paddingStart="16dp"
    paddingEnd="16dp">
    <androidx.constraintlayout.widget.ConstraintLayout
        id="@+id/cl_base_snackbar"
        background="@drawable/bg_custom_snackbar"
        layout_width="match_parent"
        layout_height="wrap_content"
        paddingStart="16dp"
        paddingEnd="16dp">
        <androidx.constraintlayout.widget.ConstraintLayout
            id="@+id/cl_custom_snackbar"
            layout_width="match_parent"
            layout_height="wrap_content"
            NOT_FOUND_0x13="0"
            NOT_FOUND_0x15="0"
            NOT_FOUND_0x18="0"
            NOT_FOUND_0x19="0">
            <ImageView
                id="@+id/iv_icon"
                layout_width="wrap_content"
                layout_height="wrap_content"
                maxWidth="24dp"
                maxHeight="24dp"
                NOT_FOUND_0x13="0"
                NOT_FOUND_0x15="0"
                NOT_FOUND_0x17="@+id/tv_custom_snackbar"
                NOT_FOUND_0x19="0"/>
            <TextView
                textSize="12sp"
                textColor="#313131"
                ellipsize="end"
                id="@+id/tv_custom_snackbar"
                layout_width="0dp"
                layout_height="wrap_content"
                layout_marginTop="12dp"
                layout_marginBottom="12dp"
                maxLines="3"
                fontFamily="@font/2131296265"
                layout_marginStart="8dp"
                layout_marginEnd="8dp"
                NOT_FOUND_0x13="0"
                NOT_FOUND_0x14="0"
                NOT_FOUND_0x16="@+id/iv_icon"
                NOT_FOUND_0x17="@+id/btn_snackbar"
                NOT_FOUND_0x19="0"/>
            <Button
                textSize="12sp"
                textStyle="bold"
                textColor="#108ee9"
                id="@+id/btn_snackbar"
                background="#00000000"
                layout_width="71dp"
                layout_height="32dp"
                NOT_FOUND_0x13="0"
                NOT_FOUND_0x18="0"
                NOT_FOUND_0x19="0"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>
