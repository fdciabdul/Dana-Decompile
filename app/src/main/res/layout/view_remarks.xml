<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/cl_notes_remarks"
    paddingLeft="16dp"
    paddingTop="14dp"
    paddingRight="16dp"
    paddingBottom="14dp"
    layout_width="match_parent"
    layout_height="wrap_content">
    <Button
        id="@+id/btn_action"
        layout_width="wrap_content"
        minWidth="96dp"
        text="@string/sendmoney_remarks_empty_action"
        contentDescription="@string/btnNotes"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x1a="0"
        style="@style/2132017482"/>
    <ImageView
        id="@+id/SendMoneyHomePresenter"
        layout_width="40dp"
        layout_height="40dp"
        scaleType="center"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x18="0"
        NOT_FOUND_0x1a="0"
        NOT_FOUND_0x1c="@drawable/ic_remarks"/>
    <TextView
        ellipsize="end"
        id="@+id/tv_title"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginLeft="12dp"
        layout_marginTop="2dp"
        layout_marginRight="12dp"
        text="@string/sendmoney_remarks_add"
        maxLines="1"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x14="@+id/btn_action"
        NOT_FOUND_0x15="0.5"
        NOT_FOUND_0x17="@+id/SendMoneyHomePresenter"
        NOT_FOUND_0x19="@+id/tv_label"
        style="@style/2132017413"/>
    <TextView
        ellipsize="end"
        id="@+id/tv_label"
        visibility="gone"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginLeft="12dp"
        layout_marginRight="12dp"
        text="@string/sendmoney_remarks_add"
        maxLines="1"
        NOT_FOUND_0x12="@+id/tv_title"
        NOT_FOUND_0x14="@+id/btn_action"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x17="@+id/SendMoneyHomePresenter"
        NOT_FOUND_0x1a="0"
        NOT_FOUND_0x1b="2"
        style="@style/2132017440"/>
</androidx.constraintlayout.widget.ConstraintLayout>
