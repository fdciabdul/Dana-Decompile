<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_width="match_parent"
    layout_height="match_parent">
    <com.airbnb.lottie.LottieAnimationView
        layout_gravity="center_horizontal"
        id="@+id/lottie_intro_animation"
        layout_width="@dimen/2131165638"
        layout_height="@dimen/2131165638"
        scaleType="fitCenter"
        maxHeight="300dp"
        layout_marginStart="20dp"
        layout_marginEnd="20dp"
        NOT_FOUND_0xf="@+id/tv_intro_title"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x14="true"/>
    <TextView
        textSize="@dimen/2131165643"
        textStyle="bold"
        textColor="@android:color/white"
        gravity="center"
        id="@+id/tv_intro_title"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginBottom="5dp"
        NOT_FOUND_0xd="@font/2131296266"
        NOT_FOUND_0xf="@+id/tv_intro_description"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x13="2"/>
    <TextView
        textSize="@dimen/2131165642"
        textColor="@android:color/white"
        gravity="center"
        id="@+id/tv_intro_description"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginBottom="@dimen/2131165639"
        layout_marginStart="@dimen/2131165641"
        layout_marginEnd="@dimen/2131165641"
        NOT_FOUND_0xd="@font/2131296265"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="0"/>
</androidx.constraintlayout.widget.ConstraintLayout>
