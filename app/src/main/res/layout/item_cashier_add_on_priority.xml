<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="#ffffff"
    layout_width="match_parent"
    layout_height="wrap_content">
    <LinearLayout
        gravity="center"
        orientation="horizontal"
        id="@+id/llAddOnTitle"
        background="@drawable/bg_rounded_add_on_title"
        paddingLeft="8dp"
        paddingTop="2dp"
        paddingRight="8dp"
        paddingBottom="2dp"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="12dp"
        layout_marginStart="12dp"
        NOT_FOUND_0x16="true"
        NOT_FOUND_0x19="@+id/ivInfoPriority"
        NOT_FOUND_0x1a="0"
        NOT_FOUND_0x1b="2"
        NOT_FOUND_0x1c="@+id/cbAddOnPriority"
        NOT_FOUND_0x1f="0">
        <androidx.appcompat.widget.AppCompatImageView
            id="@+id/icAddOnPriority"
            layout_width="wrap_content"
            layout_height="wrap_content"
            src="@drawable/ic_blue_star"
            layout_marginEnd="4dp"/>
        <androidx.appcompat.widget.AppCompatTextView
            textStyle="bold"
            textColor="#246df5"
            ellipsize="end"
            id="@+id/tvAddOnPriorityTitle"
            layout_width="match_parent"
            layout_height="wrap_content"
            maxLines="1"
            contentDescription="@string/sdet_title_addon"
            fontFamily="@font/2131296265"
            style="@style/2132017894"/>
    </LinearLayout>
    <androidx.appcompat.widget.AppCompatCheckBox
        id="@+id/cbAddOnPriority"
        layout_width="28dp"
        layout_height="28dp"
        button="@drawable/selector_addon_check"
        contentDescription="@string/sdet_checkbox_addon"
        layout_marginStart="16dp"
        NOT_FOUND_0x17="@+id/tvAddOnPriorityDesc"
        NOT_FOUND_0x1d="0"
        NOT_FOUND_0x1f="@+id/llAddOnTitle"/>
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/ivInfoPriority"
        layout_width="16dp"
        layout_height="16dp"
        contentDescription="@string/sdet_btn_tooltip_addon"
        layout_marginStart="4dp"
        layout_marginEnd="16dp"
        NOT_FOUND_0x17="@+id/llAddOnTitle"
        NOT_FOUND_0x19="@+id/tvAddOnAmount"
        NOT_FOUND_0x1c="@+id/llAddOnTitle"
        NOT_FOUND_0x1f="@+id/llAddOnTitle"
        NOT_FOUND_0x20="@drawable/ic_info_blue_50"/>
    <androidx.appcompat.widget.AppCompatTextView
        ellipsize="end"
        id="@+id/tvAddOnPriorityDesc"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        layout_marginBottom="12dp"
        maxLines="2"
        contentDescription="@string/sdet_desc_addon"
        layout_marginEnd="16dp"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x19="@+id/tvAddOnAmount"
        NOT_FOUND_0x1d="@+id/llAddOnTitle"
        NOT_FOUND_0x1e="@+id/llAddOnTitle"
        style="@style/2132017440"/>
    <androidx.appcompat.widget.AppCompatTextView
        id="@+id/tvAddOnAmount"
        layout_width="wrap_content"
        layout_height="wrap_content"
        contentDescription="@string/sdet_lbl_amount_addon"
        layout_marginEnd="12dp"
        NOT_FOUND_0x17="@+id/tvAddOnPriorityDesc"
        NOT_FOUND_0x18="0"
        NOT_FOUND_0x1f="@+id/llAddOnTitle"
        style="@style/2132017414"/>
</androidx.constraintlayout.widget.ConstraintLayout>
