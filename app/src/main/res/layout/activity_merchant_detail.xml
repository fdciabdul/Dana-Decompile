<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    fitsSystemWindows="true"
    layout_width="match_parent"
    layout_height="match_parent">
    <com.google.android.material.appbar.AppBarLayout
        theme="@style/2132017847"
        layout_width="match_parent"
        layout_height="wrap_content">
        <com.google.android.material.appbar.CollapsingToolbarLayout
            theme="@style/2132017847"
            id="@+id/collapsing_toolbar"
            layout_width="match_parent"
            layout_height="wrap_content"
            NOT_FOUND_0x16="?attr/2130968968"
            NOT_FOUND_0x1e="0x3">
            <FrameLayout
                id="@+id/rl_content"
                layout_width="match_parent"
                layout_height="wrap_content"
                NOT_FOUND_0x18="2"
                NOT_FOUND_0x19="0.7">
                <androidx.appcompat.widget.AppCompatImageView
                    id="@+id/iv_merchant_header_image"
                    layout_width="match_parent"
                    layout_height="180dp"
                    scaleType="centerCrop"
                    NOT_FOUND_0x21="@drawable/ic_header_null"/>
                <RelativeLayout
                    background="@drawable/shadow_bottom"
                    layout_width="match_parent"
                    layout_height="match_parent">
                    <androidx.recyclerview.widget.RecyclerView
                        id="@+id/ProfileMenuHeaderViewHolder_ViewBinding"
                        paddingBottom="12dp"
                        layout_width="match_parent"
                        layout_height="wrap_content"
                        layout_alignParentBottom="true"
                        paddingStart="12dp"
                        paddingEnd="12dp"/>
                </RelativeLayout>
            </FrameLayout>
            <androidx.appcompat.widget.Toolbar
                id="@+id/toolbar_merchant_detail"
                fitsSystemWindows="true"
                layout_width="match_parent"
                layout_height="?attr/2130968583"
                alpha="0.9"
                NOT_FOUND_0x18="1"
                NOT_FOUND_0x1f="@drawable/arrow_left_white"
                NOT_FOUND_0x20="@style/2132017852">
                <androidx.constraintlayout.widget.ConstraintLayout
                    layout_width="match_parent"
                    layout_height="match_parent">
                    <androidx.appcompat.widget.AppCompatTextView
                        ellipsize="end"
                        gravity="center"
                        id="@+id/tv_toolbar_title"
                        layout_width="0dp"
                        layout_height="wrap_content"
                        lines="1"
                        alpha="0"
                        layout_marginEnd="?android:attr/actionBarSize"
                        NOT_FOUND_0x1a="0"
                        NOT_FOUND_0x1b="0"
                        NOT_FOUND_0x1c="0"
                        NOT_FOUND_0x1d="0"
                        style="@style/2132017420"/>
                    <androidx.appcompat.widget.AppCompatImageView
                        id="@+id/iv_share"
                        layout_width="24dp"
                        layout_height="24dp"
                        layout_marginEnd="16dp"
                        NOT_FOUND_0x1a="0"
                        NOT_FOUND_0x1b="0"
                        NOT_FOUND_0x1d="0"
                        NOT_FOUND_0x21="@drawable/ic_share"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.appcompat.widget.Toolbar>
        </com.google.android.material.appbar.CollapsingToolbarLayout>
    </com.google.android.material.appbar.AppBarLayout>
    <androidx.core.widget.NestedScrollView
        id="@+id/nested_scroll_view"
        paddingBottom="30dp"
        layout_width="match_parent"
        layout_height="wrap_content"
        NOT_FOUND_0x17="@string/appbar_scrolling_view_behavior">
        <LinearLayout
            orientation="vertical"
            layout_width="match_parent"
            layout_height="wrap_content">
            <id.dana.nearbyme.merchantdetail.viewcomponent.MerchantInfoView
                id="@+id/res_0x7f0a1b4d_com_alibaba_ariver_engine_api_extopt_7"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginBottom="4dp"/>
            <id.dana.nearbyme.merchantdetail.merchantphoto.MerchantPhotosView
                id="@+id/view_merchant_photo"
                visibility="gone"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginBottom="4dp"/>
            <id.dana.nearbyme.merchantdetail.viewcomponent.MerchantAverageRatingView
                id="@+id/view_average_rating"
                layout_width="match_parent"
                layout_height="wrap_content"/>
            <id.dana.nearbyme.merchantdetail.viewcomponent.MerchantLatestReviewView
                id="@+id/view_latest_merchant_review"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginBottom="4dp"/>
            <id.dana.nearbyme.merchantdetail.viewcomponent.MerchantDanaDealsView
                id="@+id/view_merchant_dana_deals"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginBottom="4dp"/>
            <View
                id="@+id/view_sticky_padding"
                layout_width="match_parent"
                layout_height="1dp"
                layout_marginBottom="64dp"/>
        </LinearLayout>
    </androidx.core.widget.NestedScrollView>
    <id.dana.nearbyme.merchantdetail.viewcomponent.MerchantDetailMenuView
        layout_gravity="bottom"
        id="@+id/EmojiEditTextHelper"
        background="@drawable/bg_rounded_8dp"
        visibility="gone"
        clipToPadding="false"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginBottom="24dp"
        layout_marginStart="8dp"
        layout_marginEnd="8dp"/>
    <include
        id="@+id/res_0x7f0a0438_sightcameraview_icameraframelistener"
        visibility="gone"
        layout="@layout/view_empty_state_try_again"/>
</androidx.coordinatorlayout.widget.CoordinatorLayout>
