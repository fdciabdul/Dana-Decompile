<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/bs_add_receiver"
    focusable="true"
    clickable="true"
    layout_width="match_parent"
    layout_height="match_parent"
    NOT_FOUND_0x1a="true"
    NOT_FOUND_0x1f="@string/bottom_sheet_behavior">
    <RelativeLayout
        id="@+id/card_view"
        background="@drawable/rounded_top_background"
        clipToPadding="false"
        layout_width="match_parent"
        layout_height="match_parent"
        layout_marginTop="4dp"
        divider="@android:color/transparent"
        dividerHeight="0px">
        <ImageView
            layout_gravity="center_horizontal"
            layout_width="32dp"
            layout_height="4dp"
            layout_marginTop="8dp"
            layout_centerHorizontal="true"
            NOT_FOUND_0x24="@drawable/ic_flat_dark"/>
        <androidx.core.widget.NestedScrollView
            id="@+id/ns_content"
            clipToPadding="false"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="14dp">
            <androidx.constraintlayout.widget.ConstraintLayout
                id="@+id/cl_receiver_input_field"
                paddingLeft="16dp"
                paddingTop="16dp"
                paddingRight="16dp"
                paddingBottom="12dp"
                layout_width="match_parent"
                layout_height="wrap_content">
                <Button
                    enabled="false"
                    textSize="16sp"
                    id="@+id/btn_add_receiver_info"
                    layout_width="match_parent"
                    layout_height="wrap_content"
                    layout_marginTop="64dp"
                    text="@string/tnc_confirm_button"
                    fontFamily="@font/2131296266"
                    NOT_FOUND_0x22="@+id/llv_transfer_agents"
                    style="@style/2132017655"/>
                <TextView
                    textSize="12sp"
                    textColor="#6e6e6e"
                    id="@+id/tv_add_receiver_information"
                    layout_width="match_parent"
                    layout_height="wrap_content"
                    layout_marginTop="24dp"
                    text="@string/receiver_information"
                    textAllCaps="true"
                    fontFamily="@font/open_sans_bold"
                    NOT_FOUND_0x20="0"
                    NOT_FOUND_0x21="0"
                    NOT_FOUND_0x22="@+id/tv_detail"/>
                <TextView
                    textSize="20sp"
                    textColor="#000000"
                    id="@+id/tv_title"
                    layout_width="match_parent"
                    layout_height="wrap_content"
                    text="@string/send_via_agent_input_title"
                    fontFamily="@font/open_sans_bold"
                    NOT_FOUND_0x20="0"
                    NOT_FOUND_0x21="0"
                    NOT_FOUND_0x23="0"/>
                <TextView
                    textSize="12sp"
                    textColor="#6e6e6e"
                    id="@+id/tv_send_via_agent_info"
                    layout_width="match_parent"
                    layout_height="wrap_content"
                    layout_marginLeft="42dp"
                    layout_marginTop="32dp"
                    layout_marginRight="42dp"
                    text="@string/you_can_send_money_via_agent"
                    fontFamily="@font/2131296265"
                    textAlignment="center"
                    NOT_FOUND_0x22="@+id/cet_receiver_date_of_birth"/>
                <TextView
                    textSize="12sp"
                    textColor="#1b1b1b"
                    id="@+id/tv_detail"
                    layout_width="match_parent"
                    layout_height="wrap_content"
                    layout_marginTop="16dp"
                    text="@string/send_via_agent_input_detail"
                    fontFamily="@font/open_sans_bold"
                    NOT_FOUND_0x20="0"
                    NOT_FOUND_0x21="0"
                    NOT_FOUND_0x22="@+id/tv_title"/>
                <id.dana.richview.LogoListView
                    id="@+id/llv_transfer_agents"
                    layout_width="wrap_content"
                    layout_height="wrap_content"
                    layout_marginTop="12dp"
                    NOT_FOUND_0x20="0"
                    NOT_FOUND_0x21="0"
                    NOT_FOUND_0x22="@+id/tv_send_via_agent_info"/>
                <id.dana.richview.CustomEditText
                    id="@+id/cet_receiver_date_of_birth"
                    focusable="false"
                    layout_width="match_parent"
                    layout_height="wrap_content"
                    layout_marginTop="20dp"
                    inputType="date"
                    NOT_FOUND_0x1b="@string/input_receiver_date_of_birth"
                    NOT_FOUND_0x1e="@string/date_of_birth"
                    NOT_FOUND_0x22="@+id/cet_receiver_name"/>
                <id.dana.richview.CustomEditText
                    id="@+id/cet_receiver_phone_number"
                    layout_width="match_parent"
                    layout_height="wrap_content"
                    layout_marginTop="20dp"
                    inputType="phone"
                    NOT_FOUND_0x1b="@string/input_receiver_number"
                    NOT_FOUND_0x1c="@drawable/ic_contact"
                    NOT_FOUND_0x1d="16"
                    NOT_FOUND_0x1e="@string/phone_number"
                    NOT_FOUND_0x22="@+id/tv_add_receiver_information"/>
                <id.dana.richview.CustomEditText
                    id="@+id/cet_receiver_name"
                    layout_width="match_parent"
                    layout_height="wrap_content"
                    layout_marginTop="20dp"
                    inputType="textPersonName"
                    NOT_FOUND_0x1b="@string/input_receiver_name"
                    NOT_FOUND_0x1d="50"
                    NOT_FOUND_0x1e="@string/receiver_name_as_on_id_card"
                    NOT_FOUND_0x22="@+id/cet_receiver_phone_number"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.core.widget.NestedScrollView>
    </RelativeLayout>
</FrameLayout>
