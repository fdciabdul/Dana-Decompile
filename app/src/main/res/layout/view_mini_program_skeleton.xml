<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_width="match_parent"
    layout_height="70dp">
    <View
        id="@+id/viewSkeleton"
        background="@drawable/ellipsize_skeleton"
        layout_width="28dp"
        layout_height="28dp"
        NOT_FOUND_0x7="0"
        NOT_FOUND_0xa="0"
        NOT_FOUND_0xb="0"/>
    <LinearLayout
        orientation="vertical"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginStart="16dp"
        NOT_FOUND_0x7="0"
        NOT_FOUND_0x9="@+id/viewSkeleton"
        NOT_FOUND_0xb="0">
        <View
            background="@drawable/square_rounded_skeleton_grey_40"
            layout_width="195dp"
            layout_height="20dp"/>
        <View
            background="@drawable/square_rounded_skeleton_grey_40"
            layout_width="195dp"
            layout_height="16dp"
            layout_marginTop="4dp"/>
    </LinearLayout>
    <View
        background="@drawable/ellipsize_skeleton"
        layout_width="28dp"
        layout_height="28dp"
        NOT_FOUND_0x7="0"
        NOT_FOUND_0x8="0"
        NOT_FOUND_0xb="0"/>
</androidx.constraintlayout.widget.ConstraintLayout>
