<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    orientation="vertical"
    style="@style/2132017531">
    <androidx.cardview.widget.CardView
        layout_width="match_parent"
        layout_height="0dp"
        NOT_FOUND_0xa="8dp"
        NOT_FOUND_0xb="4dp"
        NOT_FOUND_0xc="true"
        NOT_FOUND_0xd="true"
        NOT_FOUND_0xe="@dimen/2131165330"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="@string/card_pay_view_constraint_ratio"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="0.5">
        <include layout="@layout/fragment_scanner"/>
        <ImageView
            layout_gravity="start"
            id="@+id/iv_gallery"
            background="@drawable/bg_circle_transparent"
            padding="8dp"
            layout_width="40dp"
            layout_height="40dp"
            layout_marginTop="16dp"
            layout_marginStart="16dp"
            NOT_FOUND_0x13="@drawable/ic_gallery_pay_scanner"/>
        <ImageView
            layout_gravity="right"
            id="@+id/iv_flash"
            background="@drawable/bg_circle_transparent"
            padding="8dp"
            layout_width="40dp"
            layout_height="40dp"
            layout_marginTop="16dp"
            layout_marginEnd="16dp"
            NOT_FOUND_0x13="@drawable/ic_flash_pay"/>
    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>
