<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="#f5f5f5"
    focusable="true"
    clickable="true"
    layout_width="match_parent"
    layout_height="match_parent">
    <include
        id="@+id/vRiskBinding"
        NOT_FOUND_0x20="0"
        NOT_FOUND_0x21="0"
        NOT_FOUND_0x23="0"
        layout="@layout/view_cashier_risk_binding_pay"/>
    <TextView
        gravity="center"
        id="@+id/tvTitle"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginLeft="32dp"
        layout_marginTop="24dp"
        layout_marginRight="32dp"
        text="@string/please_enter_cvv"
        NOT_FOUND_0x20="0"
        NOT_FOUND_0x21="0"
        NOT_FOUND_0x22="@+id/vRiskBinding"
        style="@style/2132017916"/>
    <LinearLayout
        id="@+id/llActionContainer"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="24dp"
        layout_marginBottom="28dp"
        NOT_FOUND_0x1f="0"
        NOT_FOUND_0x20="0"
        NOT_FOUND_0x21="0">
        <TextView
            gravity="center"
            id="@+id/tvCancelCvv"
            fitsSystemWindows="true"
            layout_width="match_parent"
            layout_height="wrap_content"
            text="@string/cancel_action"
            layout_weight="1"
            textAllCaps="true"
            layout_marginStart="32dp"
            style="@style/2132017911"/>
        <TextView
            gravity="center"
            id="@+id/tvWhatIsCvv"
            fitsSystemWindows="true"
            layout_width="match_parent"
            layout_height="wrap_content"
            text="@string/what_is_cvv"
            layout_weight="1"
            textAllCaps="true"
            layout_marginEnd="32dp"
            style="@style/2132017911"/>
    </LinearLayout>
    <id.dana.core.ui.richview.PinEntryEditText
        textSize="28sp"
        textStyle="normal"
        textColor="#000000"
        gravity="center"
        id="@+id/etInputCvv"
        background="#b8f1d4"
        paddingTop="15dp"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginLeft="80dp"
        layout_marginTop="26dp"
        layout_marginRight="80dp"
        cursorVisible="false"
        android:maxLength="3"
        digits="1234567890"
        inputType="number"
        imeOptions="actionNone"
        textIsSelectable="false"
        textAlignment="center"
        NOT_FOUND_0x20="0"
        NOT_FOUND_0x21="0"
        NOT_FOUND_0x22="@+id/tvTitle"
        NOT_FOUND_0x24="@drawable/bg_pin_cashier"
        NOT_FOUND_0x25="●"
        NOT_FOUND_0x26="8dp"/>
    <TextView
        gravity="center"
        id="@+id/res_0x7f0a145e_logfilemanager_1"
        visibility="gone"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_marginTop="38dp"
        layout_marginRight="16dp"
        NOT_FOUND_0x20="0"
        NOT_FOUND_0x21="0"
        NOT_FOUND_0x22="@+id/etInputCvv"
        style="@style/2132017409"/>
    <id.dana.richview.DanaProtectionLoadingView
        id="@+id/danaProtectLoadingView"
        contentDescription="@string/icon_secure_lottie"
        NOT_FOUND_0x1f="0"
        NOT_FOUND_0x20="0"
        NOT_FOUND_0x21="0"
        NOT_FOUND_0x23="0"
        style="@style/2132017531"/>
    <FrameLayout
        id="@+id/flDanaLoading"
        background="#80ffffff"
        visibility="gone"
        layout_width="match_parent"
        layout_height="match_parent">
        <id.dana.richview.LogoProgressView
            layout_gravity="center"
            id="@+id/lpvLoading"
            layout_width="wrap_content"
            layout_height="wrap_content"/>
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
