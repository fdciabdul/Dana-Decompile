<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/cl_bottom_help_container"
    background="@drawable/rounded_popup"
    layout_width="match_parent"
    layout_height="wrap_content">
    <androidx.constraintlayout.widget.ConstraintLayout
        id="@+id/clHeaderContent"
        focusable="true"
        focusableInTouchMode="true"
        clickable="true"
        layout_width="match_parent"
        layout_height="wrap_content"
        NOT_FOUND_0x1b="0"
        NOT_FOUND_0x20="0"
        NOT_FOUND_0x22="0">
        <TextView
            textSize="16sp"
            textColor="#000000"
            gravity="center_horizontal"
            id="@+id/handleScrollChange"
            focusable="false"
            clickable="false"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginLeft="16dp"
            layout_marginTop="28dp"
            layout_marginRight="16dp"
            lineSpacingExtra="4sp"
            fontFamily="@font/2131296266"
            NOT_FOUND_0x1d="0"
            NOT_FOUND_0x1e="0"
            NOT_FOUND_0x21="@+id/rectGrey"/>
        <ImageView
            id="@+id/rectGrey"
            focusable="false"
            visibility="gone"
            clickable="false"
            layout_width="56dp"
            layout_height="4dp"
            layout_marginTop="8dp"
            src="@drawable/square_rounded_skeleton_grey_20"
            NOT_FOUND_0x1b="0"
            NOT_FOUND_0x20="0"
            NOT_FOUND_0x22="0"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <Button
        textSize="16sp"
        id="@+id/btnFinishHelp"
        layout_height="44dp"
        layout_marginLeft="16dp"
        layout_marginRight="16dp"
        layout_marginBottom="16dp"
        text="@string/bottom_on_boarding_button_finish"
        contentDescription="@string/btnCloseHelp"
        NOT_FOUND_0x18="0"
        NOT_FOUND_0x1b="0"
        NOT_FOUND_0x20="0"
        style="@style/2132017655"/>
    <ImageView
        id="@+id/ivInfo"
        visibility="gone"
        layout_width="13dp"
        layout_height="13dp"
        layout_marginTop="40dp"
        layout_marginBottom="23dp"
        src="@drawable/ic_info_grey_20"
        layout_marginStart="23dp"
        layout_marginEnd="10dp"
        NOT_FOUND_0x19="@+id/btnFinishHelp"
        NOT_FOUND_0x1c="@+id/tvDescriptionInfo"
        NOT_FOUND_0x20="0"
        NOT_FOUND_0x21="@+id/enableMergePathsForKitKatAndAbove"/>
    <TextView
        textSize="12sp"
        textColor="#727272"
        id="@+id/tvDescriptionInfo"
        visibility="gone"
        layout_width="0dp"
        layout_height="wrap_content"
        fontFamily="@font/sf_pro_regular"
        NOT_FOUND_0x18="@+id/ivInfo"
        NOT_FOUND_0x1b="@+id/btnFinishHelp"
        NOT_FOUND_0x1f="@+id/ivInfo"
        NOT_FOUND_0x22="@+id/ivInfo"/>
    <id.dana.richview.HelpListView
        id="@+id/enableMergePathsForKitKatAndAbove"
        paddingTop="4dp"
        paddingBottom="4dp"
        layout_width="match_parent"
        layout_height="wrap_content"
        NOT_FOUND_0x19="@+id/tvDescriptionInfo"
        NOT_FOUND_0x1a="@string/bottom_on_boarding_viewpager_dimens_ratio"
        NOT_FOUND_0x1b="0"
        NOT_FOUND_0x20="0"
        NOT_FOUND_0x21="@+id/clHeaderContent"/>
</androidx.constraintlayout.widget.ConstraintLayout>
