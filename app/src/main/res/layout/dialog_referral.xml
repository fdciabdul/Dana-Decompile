<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    gravity="center"
    layout_width="match_parent"
    layout_height="match_parent">
    <androidx.constraintlayout.widget.ConstraintLayout
        orientation="vertical"
        background="@drawable/rounded_white_mini"
        padding="12dp"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_margin="20dp"
        NOT_FOUND_0x1b="0"
        NOT_FOUND_0x1c="0"
        NOT_FOUND_0x20="0"
        NOT_FOUND_0x22="0"
        NOT_FOUND_0x23="0.8">
        <ImageView
            id="@+id/iv_referral_dialog_icon"
            layout_width="wrap_content"
            layout_height="wrap_content"
            src="@drawable/ic_referral_dialog"
            NOT_FOUND_0x1c="0"
            NOT_FOUND_0x20="0"
            NOT_FOUND_0x22="0"/>
        <TextView
            textSize="16sp"
            textColor="@android:color/black"
            gravity="center"
            id="@+id/tv_title"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="8dp"
            lineSpacingExtra="4sp"
            fontFamily="@font/2131296266"
            NOT_FOUND_0x20="0"
            NOT_FOUND_0x21="@+id/iv_referral_dialog_icon"/>
        <TextView
            textSize="12sp"
            textColor="#313131"
            gravity="center"
            id="@+id/tv_description"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="8dp"
            lineSpacingExtra="4sp"
            fontFamily="@font/2131296265"
            NOT_FOUND_0x20="0"
            NOT_FOUND_0x21="@+id/tv_title"/>
        <CheckBox
            enabled="true"
            id="@+id/cb_dialog_referral"
            visibility="visible"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="16dp"
            NOT_FOUND_0x1a="@color/2131100378"
            NOT_FOUND_0x1d="@+id/tv_checkbox_description"
            NOT_FOUND_0x1e="2"
            NOT_FOUND_0x20="0"
            NOT_FOUND_0x21="@+id/tv_description"/>
        <TextView
            textSize="12sp"
            textColor="#727272"
            gravity="center"
            id="@+id/tv_checkbox_description"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginLeft="4dp"
            fontFamily="@font/2131296265"
            layout_marginStart="4dp"
            NOT_FOUND_0x17="12sp"
            NOT_FOUND_0x18="10sp"
            NOT_FOUND_0x19="1"
            NOT_FOUND_0x1b="@+id/cb_dialog_referral"
            NOT_FOUND_0x1c="0"
            NOT_FOUND_0x1e="2"
            NOT_FOUND_0x1f="@+id/cb_dialog_referral"
            NOT_FOUND_0x22="@+id/cb_dialog_referral"/>
        <LinearLayout
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="16dp"
            weightSum="2"
            NOT_FOUND_0x20="0"
            NOT_FOUND_0x21="@+id/tv_checkbox_description">
            <Button
                textSize="12sp"
                id="@+id/btn_secondary_action"
                layout_width="match_parent"
                layout_height="32dp"
                layout_marginRight="4dp"
                text="@string/referral_learn_how"
                layout_weight="1"
                layout_marginEnd="4dp"
                style="@style/2132017657"/>
            <Button
                textSize="12sp"
                id="@+id/btn_primary_action"
                layout_width="match_parent"
                layout_height="32dp"
                layout_marginLeft="4dp"
                text="@string/referral_understood"
                layout_weight="1"
                layout_marginStart="4dp"
                style="@style/2132017655"/>
        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
