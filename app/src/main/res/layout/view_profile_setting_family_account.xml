<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/rl_main_parent"
    background="#ffffff"
    layout_width="match_parent"
    layout_height="wrap_content">
    <TextView
        ellipsize="end"
        id="@+id/tvTitle"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginLeft="12dp"
        layout_marginTop="10dp"
        layout_marginRight="12dp"
        maxLines="1"
        NOT_FOUND_0xf="@+id/tvSubtitle"
        NOT_FOUND_0x11="@+id/toOpenSansSemiBold"
        NOT_FOUND_0x12="0.5"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x14="@+id/ivIcon"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x18="2"
        style="@style/2132017413"/>
    <TextView
        ellipsize="end"
        id="@+id/tvSubtitle"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        layout_marginBottom="10dp"
        maxLines="1"
        layout_marginEnd="12dp"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x10="@+id/tvTitle"
        NOT_FOUND_0x11="@+id/toOpenSansSemiBold"
        NOT_FOUND_0x12="0.5"
        NOT_FOUND_0x14="@+id/ivIcon"
        NOT_FOUND_0x15="@+id/tvTitle"
        NOT_FOUND_0x16="@+id/tvTitle"
        style="@style/2132017440"/>
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/CustomPopMenuExtension"
        layout_width="24dp"
        layout_height="24dp"
        scaleType="center"
        layout_marginEnd="8dp"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x19="@drawable/ic_arrow_right_me_page"/>
    <TextView
        ellipsize="end"
        id="@+id/toOpenSansSemiBold"
        visibility="gone"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginEnd="4dp"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x11="@+id/onLoadingGetInvestmentCardAssets"
        NOT_FOUND_0x17="0"
        style="@style/2132017408"/>
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/onLoadingGetInvestmentCardAssets"
        visibility="gone"
        layout_width="12dp"
        layout_height="12dp"
        scaleType="fitCenter"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x11="@+id/CustomPopMenuExtension"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x19="@drawable/red_dot"/>
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/ivIcon"
        layout_width="28dp"
        layout_height="28dp"
        layout_marginTop="16dp"
        layout_marginBottom="16dp"
        scaleType="fitCenter"
        layout_marginStart="16dp"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x19="@drawable/ic_family_account"/>
    <View
        id="@+id/line"
        layout_width="0dp"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x15="0"
        style="@style/2132017967"/>
</androidx.constraintlayout.widget.ConstraintLayout>
