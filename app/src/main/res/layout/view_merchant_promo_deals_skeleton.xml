<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    orientation="horizontal"
    layout_width="match_parent"
    layout_height="wrap_content">
    <include
        id="@+id/clPromoBannerSkeletonFirst"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout="@layout/view_skeleton_item_promo_deals"/>
    <include
        id="@+id/clPromoBannerSkeletonSecond"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginStart="8dp"
        layout_marginEnd="8dp"
        NOT_FOUND_0x6="@+id/clPromoBannerSkeletonFirst"
        layout="@layout/view_skeleton_item_promo_deals"/>
    <include
        id="@+id/clPromoBannerSkeletonThird"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginStart="8dp"
        layout_marginEnd="8dp"
        NOT_FOUND_0x6="@+id/clPromoBannerSkeletonSecond"
        layout="@layout/view_skeleton_item_promo_deals"/>
</androidx.constraintlayout.widget.ConstraintLayout>
