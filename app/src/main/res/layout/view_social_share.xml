<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    orientation="vertical"
    id="@+id/ll_csv_container"
    layout_width="match_parent"
    layout_height="wrap_content">
    <TextView
        gravity="center_horizontal"
        id="@+id/tv_social_share_title"
        layout_width="match_parent"
        layout_height="wrap_content"
        textAlignment="center"
        style="@style/2132017900"/>
    <LinearLayout
        orientation="horizontal"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="12dp"
        baselineAligned="false">
        <LinearLayout
            orientation="vertical"
            id="@+id/ll_share_qr"
            visibility="gone"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_weight="1"
            contentDescription="@string/btnShowQr">
            <de.hdodenhof.circleimageview.CircleImageView
                layout_gravity="center_horizontal"
                id="@+id/res_0x7f0a0324_trackerkey_globalsearchopenproperties"
                layout_width="24dp"
                layout_height="24dp"
                layout_marginBottom="8dp"
                src="@drawable/ic_show_qr"/>
            <TextView
                layout_width="match_parent"
                layout_height="wrap_content"
                text="@string/social_share_show_qr_button_text"
                lines="2"
                minLines="2"
                textAlignment="center"
                style="@style/2132017439"/>
        </LinearLayout>
        <LinearLayout
            orientation="vertical"
            id="@+id/ll_share_first"
            visibility="gone"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_weight="1">
            <de.hdodenhof.circleimageview.CircleImageView
                layout_gravity="center_horizontal"
                id="@+id/civ_social_share_first"
                layout_width="24dp"
                layout_height="24dp"
                layout_marginBottom="8dp"
                src="@drawable/ic_social_whatsapp"/>
            <TextView
                id="@+id/tv_social_share_first"
                layout_width="match_parent"
                layout_height="wrap_content"
                text="@string/social_share_button_text"
                maxLines="1"
                lines="1"
                textAlignment="center"
                style="@style/2132017439"/>
            <TextView
                id="@+id/tv_app_name_first"
                layout_width="match_parent"
                layout_height="wrap_content"
                maxLines="1"
                lines="1"
                textAlignment="center"
                style="@style/2132017439"/>
        </LinearLayout>
        <LinearLayout
            orientation="vertical"
            id="@+id/ll_share_second"
            visibility="gone"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_weight="1">
            <de.hdodenhof.circleimageview.CircleImageView
                layout_gravity="center_horizontal"
                id="@+id/civ_social_share_second"
                layout_width="24dp"
                layout_height="24dp"
                layout_marginBottom="8dp"
                src="@drawable/ic_social_facebook"/>
            <TextView
                id="@+id/tv_social_share_second"
                layout_width="match_parent"
                layout_height="wrap_content"
                text="@string/social_share_button_text"
                lines="1"
                minLines="1"
                textAlignment="center"
                style="@style/2132017439"/>
            <TextView
                id="@+id/tv_app_name_second"
                layout_width="match_parent"
                layout_height="wrap_content"
                maxLines="1"
                lines="1"
                textAlignment="center"
                style="@style/2132017439"/>
        </LinearLayout>
        <LinearLayout
            orientation="vertical"
            id="@+id/ll_share_third"
            visibility="gone"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_weight="1">
            <de.hdodenhof.circleimageview.CircleImageView
                layout_gravity="center_horizontal"
                id="@+id/civ_social_share_third"
                layout_width="24dp"
                layout_height="24dp"
                layout_marginBottom="8dp"
                src="@drawable/ic_social_line"/>
            <TextView
                id="@+id/tv_social_share_third"
                layout_width="match_parent"
                layout_height="wrap_content"
                text="@string/social_share_button_text"
                maxLines="1"
                lines="1"
                textAlignment="center"
                style="@style/2132017439"/>
            <TextView
                id="@+id/tv_app_name_third"
                layout_width="match_parent"
                layout_height="wrap_content"
                maxLines="1"
                lines="1"
                textAlignment="center"
                style="@style/2132017439"/>
        </LinearLayout>
        <LinearLayout
            orientation="vertical"
            id="@+id/ll_share_others"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_weight="1"
            contentDescription="@string/btnShareOthers">
            <de.hdodenhof.circleimageview.CircleImageView
                layout_gravity="center_horizontal"
                id="@+id/civ_share_others"
                layout_width="24dp"
                layout_height="24dp"
                layout_marginBottom="8dp"
                src="@drawable/ic_social_others"/>
            <TextView
                id="@+id/tv_social_share_others"
                layout_width="match_parent"
                layout_height="wrap_content"
                text="@string/social_share_others_button_text"
                lines="2"
                minLines="2"
                textAlignment="center"
                style="@style/2132017439"/>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
