<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="@drawable/bg_card_rounded_10dp"
    layout_width="match_parent"
    layout_height="wrap_content">
    <ImageView
        id="@+id/ivHeader"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="14dp"
        layout_marginStart="16dp"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xe="0"/>
    <TextView
        textColor="#313131"
        id="@+id/tvHeader"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginStart="8dp"
        layout_marginEnd="16dp"
        NOT_FOUND_0x9="@+id/ivHeader"
        NOT_FOUND_0xa="0"
        NOT_FOUND_0xb="@+id/ivHeader"
        NOT_FOUND_0xe="@+id/ivHeader"
        style="@style/2132017902"/>
    <View
        id="@+id/viewSeparator"
        background="#e3e3e3"
        layout_width="match_parent"
        layout_height="1dp"
        layout_marginTop="12dp"
        NOT_FOUND_0xd="@+id/ivHeader"/>
    <androidx.recyclerview.widget.RecyclerView
        id="@+id/rvDynamicForm"
        layout_width="match_parent"
        layout_height="wrap_content"
        overScrollMode="never"
        NOT_FOUND_0xa="0"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xd="@+id/viewSeparator"/>
</androidx.constraintlayout.widget.ConstraintLayout>
