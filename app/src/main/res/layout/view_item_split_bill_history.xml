<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    orientation="horizontal"
    id="@+id/ll_split_bill_history"
    background="?android:attr/selectableItemBackground"
    paddingTop="8dp"
    paddingBottom="8dp"
    focusable="true"
    clickable="true"
    layout_width="match_parent"
    layout_height="wrap_content"
    weightSum="10">
    <ImageView
        layout_gravity="center_vertical"
        id="@+id/iv_status"
        layout_width="24dp"
        layout_height="24dp"/>
    <TextView
        textColor="#313131"
        layout_gravity="center_vertical"
        id="@+id/tv_date_split_bill_history"
        layout_width="45dp"
        layout_height="wrap_content"
        layout_marginLeft="4dp"
        layout_marginStart="4dp"/>
    <TextView
        textColor="#313131"
        ellipsize="end"
        layout_gravity="center_vertical"
        id="@+id/tv_description"
        paddingRight="10dp"
        layout_width="0dp"
        layout_height="wrap_content"
        maxLines="1"
        lines="1"
        layout_weight="5"
        paddingEnd="10dp"/>
    <id.dana.richview.CurrencyTextView
        textColor="#313131"
        layout_gravity="center_vertical"
        id="@+id/ctv_amount"
        layout_width="0dp"
        layout_height="wrap_content"
        maxLines="1"
        lines="1"
        layout_weight="5"/>
    <FrameLayout
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginRight="2dp"
        layout_marginEnd="2dp">
        <de.hdodenhof.circleimageview.CircleImageView
            id="@+id/civ_first_payer"
            layout_width="26dp"
            layout_height="26dp"
            NOT_FOUND_0x19="@color/2131100492"
            NOT_FOUND_0x1a="1dp"
            NOT_FOUND_0x1b="@color/2131100492"/>
        <de.hdodenhof.circleimageview.CircleImageView
            id="@+id/civ_second_payer"
            layout_width="26dp"
            layout_height="26dp"
            layout_marginLeft="13dp"
            layout_marginStart="13dp"
            NOT_FOUND_0x19="@color/2131100492"
            NOT_FOUND_0x1a="1dp"
            NOT_FOUND_0x1b="@color/2131100492"/>
        <de.hdodenhof.circleimageview.CircleImageView
            id="@+id/civ_third_payer"
            layout_width="26dp"
            layout_height="26dp"
            layout_marginLeft="26dp"
            layout_marginStart="26dp"
            NOT_FOUND_0x19="@color/2131100492"
            NOT_FOUND_0x1a="1dp"
            NOT_FOUND_0x1b="@color/2131100492"/>
        <FrameLayout
            id="@+id/fl_sum_payer"
            visibility="gone"
            layout_width="26dp"
            layout_height="26dp"
            layout_marginLeft="26dp"
            layout_marginStart="26dp">
            <de.hdodenhof.circleimageview.CircleImageView
                layout_width="match_parent"
                layout_height="match_parent"
                src="@drawable/ic_sum_split_bill"
                NOT_FOUND_0x19="@color/2131100492"
                NOT_FOUND_0x1a="1dp"/>
            <TextView
                textSize="10sp"
                textColor="#727272"
                layout_gravity="center"
                id="@+id/tv_sum_payer"
                layout_width="wrap_content"
                layout_height="wrap_content"/>
        </FrameLayout>
    </FrameLayout>
    <ImageView
        layout_gravity="center_vertical"
        layout_width="16dp"
        layout_height="17dp"
        NOT_FOUND_0x1c="@drawable/res_0x7f080796_networkuserentitydata_externalsyntheticlambda1"/>
</LinearLayout>
