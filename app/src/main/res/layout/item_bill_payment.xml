<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="#ffffff"
    layout_width="match_parent"
    layout_height="wrap_content"
    layout_marginLeft="1dp"
    layout_marginRight="1dp"
    NOT_FOUND_0x15="0dp"
    NOT_FOUND_0x16="0dp"
    NOT_FOUND_0x17="true"
    NOT_FOUND_0x21="@android:color/transparent">
    <androidx.appcompat.widget.LinearLayoutCompat
        orientation="vertical"
        layout_width="match_parent"
        layout_height="wrap_content">
        <androidx.constraintlayout.widget.ConstraintLayout
            id="@+id/clItemBillPayment"
            paddingLeft="16dp"
            paddingTop="12dp"
            paddingRight="16dp"
            paddingBottom="12dp"
            layout_width="match_parent"
            layout_height="wrap_content">
            <TextView
                ellipsize="end"
                id="@+id/tvBizName"
                layout_width="0dp"
                layout_height="wrap_content"
                maxLines="1"
                layout_marginStart="12dp"
                layout_marginEnd="8dp"
                NOT_FOUND_0x1c="@+id/tvBillAmount"
                NOT_FOUND_0x1d="@+id/civBizLogo"
                NOT_FOUND_0x20="0"
                style="@style/2132017908"/>
            <TextView
                ellipsize="end"
                id="@+id/tvBizDescription"
                layout_width="0dp"
                layout_height="wrap_content"
                layout_marginTop="4dp"
                maxLines="1"
                layout_marginEnd="8dp"
                NOT_FOUND_0x1c="@+id/tvBillDate"
                NOT_FOUND_0x1e="@+id/tvBizName"
                NOT_FOUND_0x1f="@+id/tvBizName"
                style="@style/2132017433"/>
            <TextView
                gravity="end"
                id="@+id/tvBillAmount"
                layout_width="wrap_content"
                layout_height="wrap_content"
                NOT_FOUND_0x1a="@+id/tvBizName"
                NOT_FOUND_0x1c="@+id/getContainerAuth_res_0x7f0a0d22"
                NOT_FOUND_0x20="@+id/tvBizName"
                style="@style/2132017405"/>
            <TextView
                gravity="end"
                id="@+id/tvBillDate"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginTop="4dp"
                NOT_FOUND_0x1b="@+id/tvBillAmount"
                NOT_FOUND_0x1f="@+id/tvBillAmount"
                style="@style/2132017433"/>
            <androidx.appcompat.widget.LinearLayoutCompat
                gravity="center"
                id="@+id/getContainerAuth_res_0x7f0a0d22"
                layout_width="wrap_content"
                layout_height="0dp"
                NOT_FOUND_0x1a="0"
                NOT_FOUND_0x1b="0"
                NOT_FOUND_0x20="0">
                <ImageView
                    id="@+id/ivBillHelp"
                    visibility="gone"
                    layout_width="21dp"
                    layout_height="21dp"
                    src="@drawable/ic_mybills_information"
                    contentDescription="@string/my_bills_content_description_btn"
                    layout_marginStart="8dp"/>
                <id.dana.component.formcomponent.DanaCheckboxButtonView
                    id="@+id/dcBillPaid"
                    visibility="gone"
                    layout_width="21dp"
                    layout_height="21dp"
                    layout_marginStart="8dp"/>
            </androidx.appcompat.widget.LinearLayoutCompat>
            <androidx.cardview.widget.CardView
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginTop="4dp"
                NOT_FOUND_0x15="@dimen/2131165267"
                NOT_FOUND_0x1b="@+id/civBizLogo"
                NOT_FOUND_0x20="0">
                <de.hdodenhof.circleimageview.CircleImageView
                    id="@+id/civBizRecurringLogo"
                    layout_width="12dp"
                    layout_height="12dp"/>
            </androidx.cardview.widget.CardView>
            <de.hdodenhof.circleimageview.CircleImageView
                layout_gravity="center_horizontal"
                id="@+id/civBizLogo"
                layout_width="28dp"
                layout_height="28dp"
                layout_marginTop="@dimen/2131166135"
                NOT_FOUND_0x18="@color/2131099990"
                NOT_FOUND_0x19="1dp"
                NOT_FOUND_0x1e="0"
                NOT_FOUND_0x20="0"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <View
            id="@+id/viewDivider"
            background="#f5f5f5"
            layout_width="match_parent"
            layout_height="1dp"
            layout_marginStart="16dp"
            layout_marginEnd="16dp"/>
    </androidx.appcompat.widget.LinearLayoutCompat>
</androidx.cardview.widget.CardView>
