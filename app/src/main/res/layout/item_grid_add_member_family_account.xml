<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="@drawable/bg_dotted_blue_50"
    padding="12dp"
    layout_width="match_parent"
    layout_height="134dp"
    layout_margin="3dp">
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/ivMemberImage"
        layout_width="32dp"
        layout_height="32dp"
        NOT_FOUND_0xa="0"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xd="@drawable/ic_add_blue"/>
    <TextView
        ellipsize="end"
        id="@+id/tvMemberName"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        text="@string/dashboard_member_add_member"
        maxLines="2"
        NOT_FOUND_0xa="0"
        NOT_FOUND_0xb="@+id/ivMemberImage"
        style="@style/2132017900"/>
    <TextView
        id="@+id/tvMemberInfo"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        text="@string/dashboard_member_add_member_description"
        maxLines="2"
        NOT_FOUND_0xa="0"
        NOT_FOUND_0xb="@+id/tvMemberName"
        style="@style/2132017425"/>
</androidx.constraintlayout.widget.ConstraintLayout>
