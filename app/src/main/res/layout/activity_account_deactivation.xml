<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_width="match_parent"
    layout_height="match_parent">
    <include
        id="@+id/res_0x7f0a0f0e_verifypinstatemanager_executeriskchallenge_2_1"
        layout="@layout/layout_toolbar"/>
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/illustration_banner_general_deactivate_account"
        layout_width="0dp"
        layout_height="wrap_content"
        scaleType="fitXY"
        adjustViewBounds="true"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x18="@+id/res_0x7f0a0f0e_verifypinstatemanager_executeriskchallenge_2_1"
        NOT_FOUND_0x1c="@drawable/illustration_banner_general_deactivate_account"/>
    <LinearLayout
        orientation="vertical"
        id="@+id/header"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="16dp"
        layout_marginBottom="16dp"
        layout_centerHorizontal="true"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x18="@+id/illustration_banner_general_deactivate_account">
        <androidx.appcompat.widget.AppCompatTextView
            textSize="16sp"
            textColor="#000"
            gravity="center"
            id="@+id/tv_account_deactivation_title"
            layout_marginBottom="4dp"
            text="@string/lbl_account_deactivation_title"
            fontFamily="@font/2131296266"
            style="@style/2132017490"/>
        <androidx.appcompat.widget.AppCompatTextView
            textColor="#727272"
            gravity="center"
            id="@+id/tv_account_deactivation_description"
            text="@string/lbl_account_deactivation_description"
            fontFamily="@font/sf_pro_regular"
            style="@style/2132017490"/>
    </LinearLayout>
    <androidx.appcompat.widget.AppCompatTextView
        id="@+id/tv_label"
        visibility="invisible"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="24dp"
        text="@string/lbl_account_deactivation_email_input_placeholder"
        layout_marginStart="20dp"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x18="@+id/header"
        style="@style/2132017901"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        orientation="vertical"
        id="@+id/body"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_centerHorizontal="true"
        layout_marginStart="16dp"
        layout_marginEnd="16dp"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x18="@+id/tv_label">
        <EditText
            textSize="16sp"
            id="@+id/res_0x7f0a0693_daggerkycamleddactivitycomponent_kycamleddactivitycomponentimpl_kycamleddrepositoryprovider"
            layout_width="0dp"
            layout_height="wrap_content"
            hint="@string/lbl_account_deactivation_email_input_placeholder"
            inputType="textEmailAddress"
            fontFamily="@font/sf_pro_regular"
            NOT_FOUND_0x16="0"
            NOT_FOUND_0x17="0"
            NOT_FOUND_0x19="0"/>
        <androidx.constraintlayout.widget.ConstraintLayout
            orientation="horizontal"
            id="@+id/cl_bottom_hint"
            visibility="gone"
            layout_width="match_parent"
            layout_height="wrap_content"
            NOT_FOUND_0x16="0"
            NOT_FOUND_0x17="0"
            NOT_FOUND_0x18="@+id/res_0x7f0a0693_daggerkycamleddactivitycomponent_kycamleddactivitycomponentimpl_kycamleddrepositoryprovider">
            <androidx.appcompat.widget.AppCompatTextView
                id="@+id/getSecondaryProgressTintMode"
                layout_width="0dp"
                layout_height="wrap_content"
                text="@string/lbl_account_deactivation_error_message"
                layout_marginStart="4sp"
                NOT_FOUND_0x15="0"
                NOT_FOUND_0x16="0"
                NOT_FOUND_0x17="0"
                NOT_FOUND_0x19="0"
                style="@style/2132017435"/>
            <androidx.appcompat.widget.AppCompatImageView
                id="@+id/newSslSocketFactory"
                layout_width="16dp"
                layout_height="16dp"
                NOT_FOUND_0x15="0"
                NOT_FOUND_0x16="0"
                NOT_FOUND_0x19="0"
                NOT_FOUND_0x1c="@drawable/ic_warning_16"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <LinearLayout
            orientation="vertical"
            id="@+id/notes_placeholder"
            background="@drawable/bg_rounded_grey_10_border_grey_20_4dp"
            padding="16sp"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="16dp"
            NOT_FOUND_0x18="@+id/cl_bottom_hint">
            <androidx.appcompat.widget.AppCompatTextView
                textSize="14sp"
                textColor="#000"
                gravity="center"
                id="@+id/notes_title"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginBottom="8dp"
                text="@string/lbl_account_deactivation_notes_title"
                fontFamily="@font/2131296266"/>
            <androidx.appcompat.widget.AppCompatTextView
                textSize="12sp"
                textColor="#727272"
                layout_width="wrap_content"
                layout_height="wrap_content"
                text="@string/lbl_account_deactivation_notes_description"
                fontFamily="@font/sf_pro_regular"/>
        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <id.dana.component.buttoncomponent.DanaButtonSecondaryView
        id="@+id/btn_deactivate"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginBottom="16dp"
        layout_marginStart="16dp"
        layout_marginEnd="16dp"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x1a="@string/btn_account_deactivation_deactivate"
        NOT_FOUND_0x1b="0"/>
</androidx.constraintlayout.widget.ConstraintLayout>
