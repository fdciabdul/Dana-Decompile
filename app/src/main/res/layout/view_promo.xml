<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.card.MaterialCardView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/cv_promo_container"
    layout_width="match_parent"
    layout_height="wrap_content"
    layout_marginBottom="16dp"
    layout_marginStart="16dp"
    layout_marginEnd="16dp"
    NOT_FOUND_0x18="8dp"
    NOT_FOUND_0x19="0dp"
    NOT_FOUND_0x1a="false"
    NOT_FOUND_0x33="@color/2131099990"
    NOT_FOUND_0x34="1dp">
    <androidx.constraintlayout.widget.ConstraintLayout
        background="@drawable/rounded_white_border"
        paddingBottom="14dp"
        layout_width="match_parent"
        layout_height="wrap_content">
        <com.google.android.material.textview.MaterialTextView
            textColor="#ffffff"
            gravity="center_vertical"
            id="@+id/tvCountdown"
            background="@drawable/bg_rounded_4dp_solid_yellow_50"
            paddingLeft="4dp"
            paddingTop="1dp"
            paddingRight="4dp"
            paddingBottom="1dp"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginBottom="2dp"
            drawablePadding="4dp"
            layout_marginStart="16dp"
            NOT_FOUND_0x1b="@drawable/ic_timer_countdown"
            NOT_FOUND_0x1c="0"
            NOT_FOUND_0x1f="0"
            NOT_FOUND_0x21="0"
            NOT_FOUND_0x27="0"
            style="@style/2132017899"/>
        <androidx.appcompat.widget.AppCompatImageView
            id="@+id/iv_promo_banner"
            layout_width="match_parent"
            layout_height="0dp"
            scaleType="centerCrop"
            NOT_FOUND_0x1d="@+id/tv_promo_title"
            NOT_FOUND_0x1e="H,1:0.37"
            NOT_FOUND_0x23="0"
            NOT_FOUND_0x26="0"
            NOT_FOUND_0x29="0"
            NOT_FOUND_0x2a="0"/>
        <androidx.appcompat.widget.AppCompatImageView
            id="@+id/iv_hot_promo_ribbon"
            layout_width="90dp"
            layout_height="30dp"
            src="@drawable/ic_hot_promo_ribbon_with_text"
            scaleType="fitXY"
            NOT_FOUND_0x1c="0"
            NOT_FOUND_0x21="1"
            NOT_FOUND_0x23="0"
            NOT_FOUND_0x26="0"
            NOT_FOUND_0x29="0"
            NOT_FOUND_0x2a="0"/>
        <com.google.android.material.imageview.ShapeableImageView
            id="@+id/iv_logo_background"
            visibility="gone"
            layout_width="48dp"
            layout_height="48dp"
            NOT_FOUND_0x1c="0"
            NOT_FOUND_0x21="0"
            NOT_FOUND_0x23="0"
            NOT_FOUND_0x26="0"
            NOT_FOUND_0x29="0"
            NOT_FOUND_0x2a="0"
            NOT_FOUND_0x31="@style/2132017638"
            NOT_FOUND_0x32="@color/2131100488"/>
        <androidx.appcompat.widget.AppCompatImageView
            id="@+id/SendMoneyHomePresenter"
            visibility="gone"
            layout_width="32dp"
            layout_height="32dp"
            NOT_FOUND_0x1c="@+id/iv_logo_background"
            NOT_FOUND_0x23="@+id/iv_logo_background"
            NOT_FOUND_0x26="@+id/iv_logo_background"
            NOT_FOUND_0x29="@+id/iv_logo_background"/>
        <androidx.appcompat.widget.AppCompatImageView
            id="@+id/iv_expired_icon"
            layout_width="wrap_content"
            layout_height="wrap_content"
            adjustViewBounds="true"
            layout_marginStart="16dp"
            NOT_FOUND_0x1c="0"
            NOT_FOUND_0x23="0"
            NOT_FOUND_0x25="@+id/tv_promo_expiry_date"
            NOT_FOUND_0x28="@+id/tv_promo_title"
            NOT_FOUND_0x32="@drawable/ic_glyph_clock_filled"/>
        <com.google.android.material.textview.MaterialTextView
            ellipsize="end"
            id="@+id/tv_promo_title"
            paddingTop="12dp"
            paddingBottom="12dp"
            layout_width="match_parent"
            layout_height="wrap_content"
            maxLines="2"
            paddingStart="16dp"
            paddingEnd="16dp"
            NOT_FOUND_0x23="0"
            NOT_FOUND_0x26="0"
            NOT_FOUND_0x28="@+id/iv_promo_banner"
            NOT_FOUND_0x2a="0"
            style="@style/2132017908"/>
        <com.google.android.material.textview.MaterialTextView
            ellipsize="end"
            gravity="center_vertical"
            id="@+id/tv_promo_expiry_date"
            layout_width="0dp"
            layout_height="wrap_content"
            maxLines="2"
            paddingStart="4dp"
            NOT_FOUND_0x1c="0"
            NOT_FOUND_0x24="@+id/iv_expired_icon"
            NOT_FOUND_0x25="@+id/btn_first_promo_action"
            NOT_FOUND_0x26="0"
            NOT_FOUND_0x28="@+id/tv_promo_title"
            style="@style/2132017432"/>
        <id.dana.component.buttoncomponent.DanaButtonSecondaryView
            id="@+id/btn_first_promo_action"
            visibility="invisible"
            layout_width="wrap_content"
            layout_height="wrap_content"
            contentDescription="@string/cd_promo_center_detail_promo_button"
            NOT_FOUND_0x1c="0"
            NOT_FOUND_0x21="1"
            NOT_FOUND_0x22="2"
            NOT_FOUND_0x24="@+id/btn_first_promo_action"
            NOT_FOUND_0x25="@+id/btn_second_promo_action"
            NOT_FOUND_0x28="@+id/tv_promo_title"
            NOT_FOUND_0x2e="11"
            NOT_FOUND_0x2f="Detail"
            NOT_FOUND_0x30="1"/>
        <id.dana.component.buttoncomponent.DanaButtonPrimaryView
            id="@+id/btn_second_promo_action"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            contentDescription="@string/cd_promo_center_use_promo_button"
            layout_marginStart="4dp"
            NOT_FOUND_0x1c="0"
            NOT_FOUND_0x24="@+id/btn_first_promo_action"
            NOT_FOUND_0x25="@+id/right_guideline"
            NOT_FOUND_0x28="@+id/tv_promo_title"
            NOT_FOUND_0x2b="11"
            NOT_FOUND_0x2c="Pakai"
            NOT_FOUND_0x2d="1"/>
        <androidx.constraintlayout.widget.Guideline
            orientation="vertical"
            id="@+id/right_guideline"
            layout_width="wrap_content"
            layout_height="wrap_content"
            NOT_FOUND_0x20="16dp"
            NOT_FOUND_0x26="0"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</com.google.android.material.card.MaterialCardView>
