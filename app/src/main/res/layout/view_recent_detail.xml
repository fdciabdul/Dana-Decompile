<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    paddingLeft="8dp"
    paddingTop="8dp"
    paddingRight="8dp"
    paddingBottom="8dp"
    layout_width="80dp"
    layout_height="wrap_content">
    <id.dana.richview.CircleImageSelectionView
        id="@+id/cisv_display_photo"
        background="@drawable/white_seven_circle"
        layout_width="48dp"
        layout_height="48dp"
        layout_marginLeft="8dp"
        layout_marginRight="8dp"
        layout_marginStart="8dp"
        layout_marginEnd="8dp"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x18="0"
        NOT_FOUND_0x19="16dp"/>
    <TextView
        textSize="12sp"
        textColor="#4a4a4a"
        ellipsize="end"
        id="@+id/tv_name"
        layout_marginTop="8dp"
        maxLines="1"
        fontFamily="@font/2131296265"
        textAlignment="center"
        NOT_FOUND_0x15="@+id/cisv_display_photo"
        NOT_FOUND_0x16="@+id/cisv_display_photo"
        NOT_FOUND_0x17="@+id/cisv_display_photo"
        style="@style/2132018187"/>
    <TextView
        textSize="10sp"
        textColor="#4a4a4a"
        ellipsize="end"
        id="@+id/tv_detail"
        visibility="gone"
        maxLines="1"
        fontFamily="@font/2131296265"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x15="@+id/cisv_display_photo"
        NOT_FOUND_0x16="@+id/cisv_display_photo"
        NOT_FOUND_0x17="@+id/tv_name"
        style="@style/2132018187"/>
</androidx.constraintlayout.widget.ConstraintLayout>
