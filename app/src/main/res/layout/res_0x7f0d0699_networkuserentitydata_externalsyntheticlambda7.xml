<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_width="match_parent"
    layout_height="match_parent"
    layout_marginBottom="74dp">
    <FrameLayout
        background="@drawable/rounded_white"
        paddingTop="8dp"
        paddingBottom="8dp"
        focusable="true"
        clickable="true"
        paddingStart="12dp"
        paddingEnd="12dp"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x13="0"
        style="@style/2132018187">
        <LinearLayout
            orientation="horizontal"
            id="@+id/llSkipButton"
            style="@style/2132018187">
            <ImageView
                NOT_FOUND_0x14="@drawable/ic_close_filled_blue"
                style="@style/2132018187"/>
            <TextView
                textSize="14sp"
                textColor="#108ee9"
                text="@string/menu_skip"
                fontFamily="@font/2131296266"
                layout_marginStart="8dp"
                style="@style/2132018187"/>
        </LinearLayout>
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
