<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_width="match_parent"
    layout_height="match_parent">
    <FrameLayout
        id="@+id/fl_image_preview"
        background="#313131"
        layout_width="match_parent"
        layout_height="0dp"
        NOT_FOUND_0x1f="@+id/tv_tip_info"
        NOT_FOUND_0x20="0"
        NOT_FOUND_0x23="0"
        NOT_FOUND_0x24="@+id/toolbar">
        <androidx.appcompat.widget.AppCompatImageView
            id="@+id/iv_image_preview"
            layout_width="match_parent"
            layout_height="match_parent"/>
    </FrameLayout>
    <View
        id="@+id/divider"
        background="#d1d1d1"
        layout_width="0dp"
        layout_height="1dp"
        layout_marginBottom="8dp"
        NOT_FOUND_0x1f="@+id/btn_retake_picture"
        NOT_FOUND_0x20="0"
        NOT_FOUND_0x23="0"/>
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/iv_capture"
        background="?attr/2130969992"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginBottom="34dp"
        contentDescription="@string/image_capture_btn_take_photo"
        NOT_FOUND_0x1e="0"
        NOT_FOUND_0x20="0"
        NOT_FOUND_0x23="0"
        NOT_FOUND_0x2a="@drawable/ic_capture"/>
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/iv_gallery"
        background="?attr/2130969992"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginBottom="16dp"
        contentDescription="@string/image_capture_btn_gallery"
        layout_marginStart="16dp"
        NOT_FOUND_0x1f="@+id/iv_capture"
        NOT_FOUND_0x23="0"
        NOT_FOUND_0x2a="@drawable/ic_gallery"/>
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/iv_light"
        background="?attr/2130969992"
        layout_width="wrap_content"
        layout_height="wrap_content"
        contentDescription="@string/image_capture_btn_flash"
        layout_marginEnd="16dp"
        NOT_FOUND_0x1e="@+id/tv_check_tip"
        NOT_FOUND_0x20="0"
        NOT_FOUND_0x25="@+id/tv_check_tip"
        NOT_FOUND_0x2a="@drawable/ic_light"/>
    <androidx.appcompat.widget.AppCompatTextView
        textStyle="bold"
        textColor="#108ee9"
        gravity="center"
        id="@+id/tv_check_tip"
        background="@drawable/bg_photo_tips"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        text="@string/image_capture_see_photo_tip_label"
        contentDescription="@string/image_capture_btn_see_photo_tips"
        textAllCaps="true"
        NOT_FOUND_0x1c="@drawable/ic_arrow_right_24_blue_50"
        NOT_FOUND_0x1e="@+id/iv_gallery"
        NOT_FOUND_0x21="@+id/iv_light"
        NOT_FOUND_0x22="@+id/iv_gallery"
        NOT_FOUND_0x25="@+id/iv_gallery"
        style="@style/2132017399"/>
    <androidx.appcompat.widget.AppCompatTextView
        textStyle="bold"
        textColor="#313131"
        gravity="center_vertical"
        id="@+id/tv_tip_info"
        background="@drawable/bg_photo_tips_info"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="16dp"
        layout_marginBottom="16dp"
        drawablePadding="8dp"
        contentDescription="@string/image_capture_lbl_tips_confirmation_upload"
        NOT_FOUND_0x1d="@drawable/ic_tip_info"
        NOT_FOUND_0x1f="@+id/divider"
        NOT_FOUND_0x20="@+id/btn_upload"
        NOT_FOUND_0x23="@+id/btn_retake_picture"
        NOT_FOUND_0x24="@+id/fl_image_preview"
        style="@style/2132017399"/>
    <androidx.constraintlayout.widget.Group
        id="@+id/groupPicturePreview"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        NOT_FOUND_0x1b="fl_image_preview, divider, btn_upload, btn_retake_picture"/>
    <com.otaliastudios.cameraview.CameraView
        id="@+id/vDanaCamera"
        layout_width="0dp"
        layout_height="0dp"
        layout_marginBottom="16dp"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x15="1"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x18="0"
        NOT_FOUND_0x19="false"
        NOT_FOUND_0x1a="false"
        NOT_FOUND_0x1f="@+id/iv_gallery"
        NOT_FOUND_0x20="0"
        NOT_FOUND_0x23="0"
        NOT_FOUND_0x24="@+id/toolbar">
        <androidx.constraintlayout.widget.ConstraintLayout
            id="@+id/cl_frame"
            background="@drawable/camera_frame_supporting_docs"
            visibility="invisible"
            layout_width="match_parent"
            layout_height="match_parent"
            layout_margin="32dp">
            <androidx.appcompat.widget.AppCompatTextView
                gravity="center"
                layout_gravity="center"
                id="@+id/tv_guidelines"
                visibility="gone"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginLeft="16dp"
                layout_marginRight="16dp"
                text="@string/image_capture_upload_photo_guidelines"
                NOT_FOUND_0x1e="0"
                NOT_FOUND_0x20="0"
                NOT_FOUND_0x23="0"
                NOT_FOUND_0x25="0"
                style="@style/2132017915"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </com.otaliastudios.cameraview.CameraView>
    <id.dana.component.buttoncomponent.DanaButtonPrimaryView
        id="@+id/btn_upload"
        layout_width="0dp"
        layout_height="wrap_content"
        contentDescription="@string/image_capture_btn_upload"
        layout_marginEnd="16dp"
        NOT_FOUND_0x1e="@+id/btn_retake_picture"
        NOT_FOUND_0x20="0"
        NOT_FOUND_0x22="@+id/btn_retake_picture"
        NOT_FOUND_0x26="@string/image_capture_upload_photo_btn_label"
        NOT_FOUND_0x27="1"/>
    <id.dana.component.buttoncomponent.DanaButtonSecondaryView
        id="@+id/btn_retake_picture"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginBottom="16dp"
        contentDescription="@string/image_capture_btn_retake_photo"
        layout_marginStart="16dp"
        layout_marginEnd="8dp"
        NOT_FOUND_0x1e="0"
        NOT_FOUND_0x21="@+id/btn_upload"
        NOT_FOUND_0x23="0"
        NOT_FOUND_0x28="@string/image_capture_retake_photo_btn_label"
        NOT_FOUND_0x29="1"/>
    <include
        id="@+id/toolbar"
        layout="@layout/layout_toolbar"/>
</androidx.constraintlayout.widget.ConstraintLayout>
