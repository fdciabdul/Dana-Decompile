<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="#a9a9a9"
    layout_width="match_parent"
    layout_height="match_parent">
    <Button
        textSize="15dp"
        textColor="#ffffff"
        id="@+id/activity_egg_confirm"
        background="@drawable/btn_easter_egg"
        layout_width="0dp"
        layout_height="50dp"
        layout_marginLeft="8dp"
        layout_marginRight="16dp"
        layout_marginBottom="20dp"
        text="Dismiss"
        layout_marginStart="8dp"
        layout_marginEnd="16dp"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0x10="@+id/activity_egg_copy"
        NOT_FOUND_0x12="0"/>
    <Button
        textSize="15dp"
        textColor="#ffffff"
        id="@+id/activity_egg_copy"
        background="@drawable/btn_easter_egg"
        layout_width="0dp"
        layout_height="50dp"
        layout_marginLeft="16dp"
        layout_marginRight="8dp"
        layout_marginBottom="20dp"
        text="@string/easter_egg_copy_button"
        layout_marginStart="16dp"
        layout_marginEnd="8dp"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x11="@+id/activity_egg_confirm"/>
    <ScrollView
        id="@+id/activity_egg_scroll"
        layout_width="match_parent"
        layout_height="0dp"
        layout_marginLeft="30dp"
        layout_marginTop="30dp"
        layout_marginRight="30dp"
        layout_marginBottom="20dp"
        NOT_FOUND_0xe="@+id/activity_egg_confirm"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x13="0">
        <TextView
            textSize="15dp"
            textColor="#ffffff"
            id="@+id/activity_egg_text"
            layout_width="match_parent"
            layout_height="wrap_content"/>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>
