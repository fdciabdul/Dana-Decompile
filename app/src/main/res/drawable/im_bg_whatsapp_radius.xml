            <solid color="#66f282"/>
            <corners radius="4dp"/>
        </shape>
    </item>
    <item state_focused="true">
        <shape shape="rectangle">
            <solid color="#66f282"/>
            <corners radius="4dp"/>
        </shape>
    </item>
    <item>
        <shape shape="rectangle">
            <solid color="#1bd741"/>
            <corners radius="4dp"/>
        </shape>
    </item>
</selector>
