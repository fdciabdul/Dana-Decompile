<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate
            android:fromDegrees="52"
            android:toDegrees="45"
            android:pivotX="-45%"
            android:pivotY="90%">
            <shape shape="rectangle">
                <solid color="#ffffff"/>
                <stroke
                    width="1dp"
                    color="#e3e3e3"/>
            </shape>
        </rotate>
    </item>
</layer-list>
