<?xml version="1.0" encoding="utf-8"?>
<inset xmlns:android="http://schemas.android.com/apk/res/android"
    insetLeft="4dp"
    insetRight="4dp"
    insetTop="4dp"
    insetBottom="4dp">
    <selector>
        <item
            state_checked="false"
            state_pressed="false">
            <layer-list>
                <item drawable="@drawable/abc_textfield_default_mtrl_alpha"/>
                <item drawable="@drawable/abc_spinner_mtrl_am_alpha"/>
            </layer-list>
        </item>
        <item>
            <layer-list>
                <item drawable="@drawable/abc_textfield_activated_mtrl_alpha"/>
                <item drawable="@drawable/abc_spinner_mtrl_am_alpha"/>
            </layer-list>
        </item>
    </selector>
</inset>
