<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item state_enabled="false">
        <shape>
            <solid color="#9a108ee9"/>
            <corners radius="2dp"/>
        </shape>
    </item>
    <item
        state_enabled="true"
        state_pressed="true">
        <shape>
            <solid color="#0b71ba"/>
            <corners radius="2dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid color="#108ee9"/>
            <corners radius="2dp"/>
        </shape>
    </item>
</selector>
