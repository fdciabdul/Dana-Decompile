<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        id="@android:id/background"
        drawable="@drawable/abc_scrubber_track_mtrl_alpha"/>
    <item id="@android:id/secondaryProgress">
        <scale android:scaleWidth="100%">
            <selector>
                <item state_enabled="false">
                    <color color="@android:color/transparent"/>
                </item>
                <item drawable="@drawable/abc_scrubber_primary_mtrl_alpha"/>
            </selector>
        </scale>
    </item>
    <item id="@android:id/progress">
        <scale android:scaleWidth="100%">
            <selector>
                <item state_enabled="false">
                    <color color="@android:color/transparent"/>
                </item>
                <item drawable="@drawable/abc_scrubber_primary_mtrl_alpha"/>
            </selector>
        </scale>
    </item>
</layer-list>
