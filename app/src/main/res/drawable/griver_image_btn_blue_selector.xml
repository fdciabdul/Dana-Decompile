<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item state_pressed="true">
        <shape shape="rectangle">
            <solid color="#80108ee9"/>
            <corners radius="2dp"/>
        </shape>
    </item>
    <item state_focused="true">
        <shape shape="rectangle">
            <solid color="#80108ee9"/>
            <corners radius="2dp"/>
        </shape>
    </item>
    <item>
        <shape shape="rectangle">
            <solid color="#ff108ee9"/>
            <corners radius="2dp"/>
        </shape>
    </item>
</selector>
