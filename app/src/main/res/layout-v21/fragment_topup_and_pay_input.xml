<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    orientation="vertical"
    background="#f5f5f5"
    focusable="true"
    clickable="true"
    layout_width="match_parent"
    layout_height="match_parent">
    <include
        id="@+id/toolbar"
        NOT_FOUND_0x1d="0"
        NOT_FOUND_0x1e="0"
        NOT_FOUND_0x20="0"
        layout="@layout/view_cashier_toolbar"/>
    <include
        id="@+id/clHeaderContainer"
        NOT_FOUND_0x1d="0"
        NOT_FOUND_0x1e="0"
        NOT_FOUND_0x1f="@+id/toolbar"
        layout="@layout/view_payment_code_header"/>
    <id.dana.cashier.view.CashierInputAmountView
        id="@+id/topupInputField"
        layout_width="match_parent"
        layout_height="wrap_content"
        contentDescription="@string/topup_txt_amount"
        NOT_FOUND_0x1d="0"
        NOT_FOUND_0x1e="0"
        NOT_FOUND_0x1f="@+id/clHeaderContainer"/>
    <id.dana.cashier.view.CashierPayWithCvvView
        id="@+id/isAdvancedMarkersAvailable"
        visibility="gone"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        contentDescription="@string/topup_txt_cvv"
        NOT_FOUND_0x1d="0"
        NOT_FOUND_0x1e="0"
        NOT_FOUND_0x1f="@+id/topupInputField"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        layout_gravity="bottom"
        id="@+id/clCardAndButton"
        NOT_FOUND_0x1b="0"
        NOT_FOUND_0x1d="0"
        NOT_FOUND_0x1e="0"
        style="@style/2132017490">
        <FrameLayout
            theme="@style/2132017169"
            NOT_FOUND_0x1b="0"
            NOT_FOUND_0x1d="0"
            NOT_FOUND_0x1e="0"
            style="@style/2132017490">
            <com.google.android.material.card.MaterialCardView
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginTop="12dp"
                NOT_FOUND_0x19="16dp">
                <androidx.constraintlayout.widget.ConstraintLayout
                    layout_width="match_parent"
                    layout_height="wrap_content">
                    <include
                        id="@+id/res_0x7f0a1ac8_types_typevariableimpl"
                        NOT_FOUND_0x1d="0"
                        NOT_FOUND_0x1e="0"
                        NOT_FOUND_0x20="0"
                        layout="@layout/view_payment_method_source_no_expand"/>
                    <View
                        id="@+id/vDividerButton"
                        background="#b8b8b8"
                        visibility="gone"
                        layout_width="match_parent"
                        layout_height="0.5dp"
                        NOT_FOUND_0x1c="@+id/btnPay"
                        NOT_FOUND_0x1d="0"
                        NOT_FOUND_0x1e="0"/>
                    <id.dana.component.buttoncomponent.DanaButtonPrimaryView
                        id="@+id/btnPay"
                        paddingLeft="16dp"
                        paddingTop="8dp"
                        paddingRight="16dp"
                        paddingBottom="16dp"
                        layout_width="match_parent"
                        layout_height="wrap_content"
                        contentDescription="@string/btn_topup_pay_submit"
                        NOT_FOUND_0x1b="0"
                        NOT_FOUND_0x1d="0"
                        NOT_FOUND_0x1e="0"
                        NOT_FOUND_0x1f="@+id/res_0x7f0a1ac8_types_typevariableimpl"
                        NOT_FOUND_0x21="@string/pay"
                        NOT_FOUND_0x22="1"
                        NOT_FOUND_0x23="false"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </com.google.android.material.card.MaterialCardView>
        </FrameLayout>
        <androidx.appcompat.widget.AppCompatTextView
            textSize="10sp"
            textColor="#525252"
            id="@+id/res_0x7f0a157c_totalsavingamountview_getsavingconfigmodule_1"
            background="@drawable/bg_rounded_white_12dp"
            paddingTop="2dp"
            paddingBottom="2dp"
            visibility="invisible"
            text="@string/smart_pay_text_logo"
            drawablePadding="4dp"
            fontFamily="@font/sf_pro_heavy_italic"
            paddingStart="8dp"
            paddingEnd="4dp"
            layout_marginEnd="16dp"
            letterSpacing="-0.06"
            NOT_FOUND_0x1a="@drawable/ic_info_grey_20"
            NOT_FOUND_0x1d="0"
            NOT_FOUND_0x20="0"
            style="@style/2132018188"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <id.dana.richview.DanaProtectionLoadingView
        id="@+id/danaProtectLoadingView"
        NOT_FOUND_0x1b="0"
        NOT_FOUND_0x1d="0"
        NOT_FOUND_0x1e="0"
        NOT_FOUND_0x20="0"
        style="@style/2132017531"/>
    <FrameLayout
        id="@+id/flDanaLoading"
        background="#80ffffff"
        visibility="gone"
        layout_width="match_parent"
        layout_height="match_parent">
        <id.dana.richview.LogoProgressView
            layout_gravity="center"
            id="@+id/lpvLoading"
            layout_width="wrap_content"
            layout_height="wrap_content"/>
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
