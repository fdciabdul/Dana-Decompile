<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    visibility="gone"
    layout_width="match_parent"
    layout_height="wrap_content">
    <ProgressBar
        id="@+id/pb_transaction_progress"
        layout_width="0dp"
        layout_height="8dp"
        layout_marginTop="8dp"
        progressTint="#108ee9"
        progressBackgroundTint="#e3e3e3"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="@+id/tv_monthly_transaction_limit"
        style="@android:style/Widget.ProgressBar.Horizontal"/>
    <TextView
        textColor="#727272"
        id="@+id/tv_monthly_transaction_limit"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        text="@string/kyb_monthly_transaction_limit"
        contentDescription="@string/lbl_title_monthly_limit_trx"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="@+id/view_top_line"
        style="@style/2132017433"/>
    <TextView
        textColor="#727272"
        id="@+id/tv_transaction_progress"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        contentDescription="@string/lbl_limit_amount"
        layout_marginStart="4dp"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xe="@+id/iv_limit_almost_reached"
        NOT_FOUND_0x10="@+id/pb_transaction_progress"
        NOT_FOUND_0x12="0dp"
        style="@style/2132017407"/>
    <View
        id="@+id/view_top_line"
        layout_width="0dp"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x11="0"
        style="@style/2132017967"/>
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/iv_limit_almost_reached"
        visibility="gone"
        layout_width="16dp"
        layout_height="16dp"
        layout_marginTop="4dp"
        contentDescription="@string/img_icon_high_urgency"
        NOT_FOUND_0xb="@+id/tv_transaction_progress"
        NOT_FOUND_0xd="@+id/tv_transaction_progress"
        NOT_FOUND_0x11="@+id/tv_transaction_progress"
        NOT_FOUND_0x13="@drawable/ic_warning_red"/>
</androidx.constraintlayout.widget.ConstraintLayout>
