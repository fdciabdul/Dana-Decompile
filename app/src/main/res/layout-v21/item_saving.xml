<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    background="@drawable/bg_rounded_white_with_border"
    paddingTop="12dp"
    paddingBottom="12dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    contentDescription="@string/btnGoalList"
    paddingStart="12dp">
    <ProgressBar
        id="@+id/progress_bar"
        layout_width="match_parent"
        layout_height="4dp"
        layout_marginTop="12dp"
        layout_marginEnd="12dp"
        progressTint="#008ef0"
        progressBackgroundTint="#e3e3e3"
        NOT_FOUND_0x1d="@+id/iv_icon"
        style="@android:style/Widget.ProgressBar.Horizontal"/>
    <id.dana.richview.HideableBalanceView
        id="@+id/tv_hideable_balance"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        contentDescription="@string/lblCurrentAmount"
        NOT_FOUND_0x16="@+id/tv_progress"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x19="@+id/tv_progress"
        NOT_FOUND_0x1c="0"
        NOT_FOUND_0x1d="@+id/progress_bar"/>
    <TextView
        gravity="end"
        id="@+id/tv_progress"
        layout_width="0dp"
        layout_height="wrap_content"
        NOT_FOUND_0x14="@+id/tv_hideable_balance"
        NOT_FOUND_0x16="@+id/tv_hideable_target_balance"
        NOT_FOUND_0x19="@+id/tv_hideable_target_balance"
        NOT_FOUND_0x1e="@+id/tv_hideable_balance"
        style="@style/2132017406"/>
    <id.dana.richview.HideableBalanceView
        id="@+id/tv_hideable_target_balance"
        layout_width="wrap_content"
        layout_height="wrap_content"
        contentDescription="@string/lblGoalAmount"
        layout_marginEnd="12dp"
        NOT_FOUND_0x14="@+id/tv_hideable_balance"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x18="@+id/tv_progress"
        NOT_FOUND_0x1a="0"
        NOT_FOUND_0x1e="@+id/tv_hideable_balance"/>
    <TextView
        gravity="end"
        id="@+id/tv_goal_achieved"
        background="@drawable/bg_rounded_fill_green50_9dp"
        paddingTop="1dp"
        paddingBottom="1dp"
        layout_width="wrap_content"
        layout_height="wrap_content"
        text="@string/goal_achieved"
        includeFontPadding="false"
        drawablePadding="2dp"
        paddingStart="1dp"
        paddingEnd="12dp"
        layout_marginEnd="12dp"
        NOT_FOUND_0x13="@drawable/ic_check_filled_circle_white_16dp"
        NOT_FOUND_0x14="@+id/tv_hideable_balance"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x1a="0"
        NOT_FOUND_0x1e="@+id/tv_hideable_balance"
        style="@style/2132017899"/>
    <TextView
        id="@+id/tv_title"
        layout_width="0dp"
        layout_height="wrap_content"
        contentDescription="@string/lblGoalName"
        layout_marginStart="8dp"
        layout_marginEnd="4dp"
        NOT_FOUND_0x14="@+id/iv_icon"
        NOT_FOUND_0x16="@+id/CustomPopMenuExtension"
        NOT_FOUND_0x18="@+id/iv_icon"
        NOT_FOUND_0x19="@+id/CustomPopMenuExtension"
        NOT_FOUND_0x1b="@+id/iv_icon"
        NOT_FOUND_0x1e="0"
        style="@style/2132017916"/>
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/CustomPopMenuExtension"
        padding="8dp"
        layout_width="32dp"
        layout_height="32dp"
        NOT_FOUND_0x14="@+id/iv_icon"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x1a="0"
        NOT_FOUND_0x1e="0"
        NOT_FOUND_0x1f="@drawable/res_0x7f080796_networkuserentitydata_externalsyntheticlambda1"/>
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/iv_icon"
        layout_width="64dp"
        layout_height="64dp"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x1c="0"
        NOT_FOUND_0x1e="0"
        NOT_FOUND_0x1f="@drawable/res_0x7f080633_networkuserentitydata_externalsyntheticlambda2"/>
</androidx.constraintlayout.widget.ConstraintLayout>
