<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/clBoardError"
    background="@android:color/transparent"
    paddingLeft="42dp"
    paddingRight="42dp"
    clipToPadding="false"
    layout_width="match_parent"
    layout_height="match_parent"
    paddingHorizontal="42dp"
    NOT_FOUND_0x16="0"
    NOT_FOUND_0x18="0">
    <androidx.appcompat.widget.AppCompatTextView
        gravity="center"
        id="@+id/res_0x7f0a13e7_playbackstatecompat_mediakeyaction"
        layout_width="wrap_content"
        layout_height="wrap_content"
        text="@string/board_error_title"
        textAlignment="center"
        NOT_FOUND_0x12="18sp"
        NOT_FOUND_0x13="12sp"
        NOT_FOUND_0x14="1"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x18="0"
        style="@style/2132017923"/>
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/ivBoardErrorIcon"
        layout_width="wrap_content"
        layout_height="wrap_content"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x17="@+id/res_0x7f0a13e7_playbackstatecompat_mediakeyaction"
        NOT_FOUND_0x19="@drawable/ic_board_error"/>
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/ivBoardErrorShadow"
        background="@drawable/bg_danapoly_error_state_shadow"
        layout_width="120dp"
        layout_height="12dp"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x17="@+id/ivBoardErrorIcon"/>
    <androidx.appcompat.widget.AppCompatButton
        textColor="#ffffff"
        id="@+id/btnTryAgain"
        background="@drawable/bg_button_rounded_white_4dp"
        paddingLeft="16dp"
        paddingTop="8dp"
        paddingRight="16dp"
        paddingBottom="8dp"
        layout_width="wrap_content"
        layout_height="32dp"
        layout_marginTop="16dp"
        minWidth="95dp"
        text="@string/board_error_button"
        textAllCaps="true"
        paddingHorizontal="16dp"
        paddingVertical="8dp"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x17="@+id/ivBoardErrorShadow"
        style="@style/2132017900"/>
</androidx.constraintlayout.widget.ConstraintLayout>
