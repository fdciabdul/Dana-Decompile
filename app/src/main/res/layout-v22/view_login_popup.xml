<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    orientation="vertical"
    style="@style/2132017490">
    <include
        id="@+id/viewSimplePopup"
        layout="@layout/view_simple_popup"/>
    <LinearLayout
        orientation="horizontal"
        id="@+id/ll_action_dialog"
        paddingLeft="12dp"
        paddingRight="12dp"
        visibility="gone"
        layout_width="match_parent"
        layout_height="44dp"
        paddingHorizontal="12dp">
        <Button
            textSize="12sp"
            textColor="#108ee9"
            id="@+id/btn_negative"
            visibility="gone"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_weight="1"
            fontFamily="@font/2131296266"
            style="@style/2132017657"/>
        <Button
            textSize="12sp"
            id="@+id/btn_positive"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_weight="1"
            fontFamily="@font/2131296266"
            style="@style/2132017655"/>
    </LinearLayout>
</LinearLayout>
