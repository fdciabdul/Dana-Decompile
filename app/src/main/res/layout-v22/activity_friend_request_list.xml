<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    orientation="vertical"
    layout_width="match_parent"
    layout_height="match_parent">
    <include
        id="@+id/toolbarFeeds"
        layout_width="match_parent"
        layout_height="wrap_content"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x17="0"
        layout="@layout/layout_toolbar"/>
    <id.dana.social.v2.view.FeedPullToRefreshView
        id="@+id/ptrFriendRequest"
        clipToPadding="false"
        layout_width="match_parent"
        layout_height="match_parent">
        <androidx.constraintlayout.widget.ConstraintLayout
            id="@+id/onTooManyRedirects_res_0x7f0a0352"
            background="#ffffff"
            layout_width="match_parent"
            layout_height="match_parent">
            <androidx.recyclerview.widget.RecyclerView
                id="@+id/rvFriendRequests"
                clipToPadding="false"
                layout_width="match_parent"
                layout_height="0dp"
                layout_marginTop="2dp"
                NOT_FOUND_0x15="0"
                NOT_FOUND_0x17="0"/>
            <LinearLayout
                gravity="center"
                orientation="vertical"
                id="@+id/llErrorState"
                layout_width="match_parent"
                layout_height="0dp"
                NOT_FOUND_0x11="@+id/btnTryAgain"
                NOT_FOUND_0x17="0">
                <androidx.appcompat.widget.AppCompatImageView
                    id="@+id/ivErrorState"
                    layout_width="wrap_content"
                    layout_height="wrap_content"
                    NOT_FOUND_0x13="0"
                    NOT_FOUND_0x14="0"
                    NOT_FOUND_0x17="0"
                    NOT_FOUND_0x1a="@drawable/ic_network_error"/>
                <TextView
                    textColor="#313131"
                    gravity="center"
                    id="@+id/tvErrorTitle"
                    layout_width="wrap_content"
                    layout_height="wrap_content"
                    layout_marginLeft="16dp"
                    layout_marginTop="32dp"
                    layout_marginRight="16dp"
                    text="@string/general_network_error_title"
                    layout_marginHorizontal="16dp"
                    NOT_FOUND_0x12="0"
                    NOT_FOUND_0x15="0"
                    NOT_FOUND_0x16="@+id/ivErrorState"
                    style="@style/2132017926"/>
                <TextView
                    gravity="center"
                    id="@+id/MerchantReviewFormAnalyticTracker"
                    layout_width="wrap_content"
                    layout_height="wrap_content"
                    layout_marginLeft="16dp"
                    layout_marginTop="8dp"
                    layout_marginRight="16dp"
                    text="@string/general_network_error_description"
                    layout_marginHorizontal="16dp"
                    NOT_FOUND_0x12="0"
                    NOT_FOUND_0x15="0"
                    NOT_FOUND_0x16="@+id/tvErrorTitle"
                    style="@style/2132017414"/>
            </LinearLayout>
            <id.dana.component.buttoncomponent.DanaButtonSecondaryView
                id="@+id/btnTryAgain"
                layout_width="0dp"
                layout_height="wrap_content"
                layout_marginLeft="16dp"
                layout_marginRight="16dp"
                layout_marginBottom="16dp"
                layout_marginHorizontal="16dp"
                NOT_FOUND_0x10="0"
                NOT_FOUND_0x12="0"
                NOT_FOUND_0x15="0"
                NOT_FOUND_0x18="@string/try_again"
                NOT_FOUND_0x19="1"/>
            <androidx.constraintlayout.widget.Group
                id="@+id/groupErrorState"
                visibility="gone"
                layout_width="wrap_content"
                layout_height="wrap_content"
                NOT_FOUND_0xf="btnTryAgain, llErrorState"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </id.dana.social.v2.view.FeedPullToRefreshView>
</LinearLayout>
