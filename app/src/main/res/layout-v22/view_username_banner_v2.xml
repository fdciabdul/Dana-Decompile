<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.card.MaterialCardView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    layout_width="match_parent"
    layout_height="wrap_content"
    layout_margin="16dp"
    NOT_FOUND_0xd="@dimen/2131165294">
    <androidx.constraintlayout.widget.ConstraintLayout
        layout_width="match_parent"
        layout_height="wrap_content">
        <com.google.android.material.imageview.ShapeableImageView
            id="@+id/iv_icon"
            layout_width="64dp"
            layout_height="64dp"
            layout_marginTop="16dp"
            layout_marginBottom="16dp"
            layout_marginStart="16dp"
            layout_marginVertical="16dp"
            NOT_FOUND_0xe="0"
            NOT_FOUND_0x12="0"
            NOT_FOUND_0x14="0"
            NOT_FOUND_0x15="@style/2132018200"
            NOT_FOUND_0x16="@drawable/ic_awesome"/>
        <androidx.appcompat.widget.AppCompatTextView
            textColor="#313131"
            id="@+id/tv_title"
            layout_width="0dp"
            layout_height="wrap_content"
            text="@string/create_username"
            lines="1"
            layout_marginStart="12dp"
            NOT_FOUND_0xf="0"
            NOT_FOUND_0x11="@+id/iv_icon"
            NOT_FOUND_0x14="@+id/iv_icon"
            style="@style/2132017916"/>
        <androidx.appcompat.widget.AppCompatTextView
            gravity="top"
            id="@+id/tv_description"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_marginTop="4dp"
            text="@string/create_username_desc"
            layout_marginStart="12dp"
            NOT_FOUND_0x10="@+id/iv_right"
            NOT_FOUND_0x11="@+id/iv_icon"
            NOT_FOUND_0x13="@+id/tv_title"
            style="@style/2132017406"/>
        <androidx.appcompat.widget.AppCompatImageView
            id="@+id/iv_right"
            layout_width="24dp"
            layout_height="24dp"
            layout_marginEnd="12dp"
            NOT_FOUND_0xe="0"
            NOT_FOUND_0xf="0"
            NOT_FOUND_0x14="0"
            NOT_FOUND_0x16="@drawable/ic_arrow_right_mini"
            NOT_FOUND_0x17="@color/2131099993"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</com.google.android.material.card.MaterialCardView>
