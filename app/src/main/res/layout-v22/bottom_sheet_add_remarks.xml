<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    id="@+id/fl_container"
    background="@drawable/rounded_top_background"
    layout_width="match_parent"
    layout_height="wrap_content"
    NOT_FOUND_0x1c="@string/bottom_sheet_behavior">
    <ImageView
        layout_gravity="top|center_horizontal"
        id="@+id/secondaryHash_res_0x7f0a08d3"
        layout_width="56dp"
        layout_height="4dp"
        layout_marginTop="8dp"
        NOT_FOUND_0x25="@drawable/ic_flat_dark"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        id="@+id/res_0x7f0a041b_fontscontractcompat_fontrequestcallback"
        paddingLeft="16dp"
        paddingTop="28dp"
        paddingRight="16dp"
        paddingBottom="16dp"
        layout_width="match_parent"
        layout_height="wrap_content"
        paddingHorizontal="16dp">
        <Button
            textSize="16sp"
            id="@+id/btn_positive"
            focusable="true"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_marginTop="32dp"
            contentDescription="@string/btnContinue"
            layout_marginStart="4dp"
            NOT_FOUND_0x1d="0"
            NOT_FOUND_0x20="@+id/btn_negative"
            NOT_FOUND_0x22="@+id/tv_hint"
            NOT_FOUND_0x24="0dp"
            style="@style/2132017581"/>
        <Button
            textSize="16sp"
            id="@+id/btn_negative"
            focusable="true"
            visibility="gone"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_marginTop="32dp"
            contentDescription="@string/btnSkip"
            layout_marginEnd="4dp"
            NOT_FOUND_0x1e="@+id/btn_positive"
            NOT_FOUND_0x1f="2"
            NOT_FOUND_0x21="0"
            NOT_FOUND_0x22="@+id/tv_hint"
            style="@style/2132017482"/>
        <EditText
            enabled="true"
            gravity="start|top"
            id="@+id/et_remarks"
            background="@drawable/selector_bg_text_view"
            paddingLeft="10dp"
            paddingTop="12dp"
            paddingRight="10dp"
            paddingBottom="12dp"
            focusable="true"
            focusableInTouchMode="true"
            scrollbars="vertical"
            layout_width="match_parent"
            layout_height="83dp"
            layout_marginTop="16dp"
            hint="@string/sendmoney_remarks_edit_text_hint"
            maxLines="3"
            minLines="3"
            inputType="textMultiLine"
            contentDescription="@string/txtAreaNotes"
            textCursorDrawable="@drawable/cursor_tangerine"
            paddingHorizontal="10dp"
            paddingVertical="12dp"
            NOT_FOUND_0x1d="0"
            NOT_FOUND_0x21="0"
            NOT_FOUND_0x22="@+id/tv_title"
            style="@style/2132017413"/>
        <TextView
            id="@+id/tv_title"
            layout_width="match_parent"
            layout_height="wrap_content"
            text="@string/sendmoney_remarks_add_title"
            NOT_FOUND_0x1d="0"
            NOT_FOUND_0x21="0"
            NOT_FOUND_0x23="0"
            style="@style/2132017924"/>
        <TextView
            id="@+id/tv_hint"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="8dp"
            text="@string/sendmoney_remarks_add_hint"
            NOT_FOUND_0x1d="0"
            NOT_FOUND_0x21="0"
            NOT_FOUND_0x22="@+id/et_remarks"
            style="@style/2132017440"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>
