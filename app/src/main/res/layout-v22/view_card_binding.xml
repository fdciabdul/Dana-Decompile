<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    orientation="vertical"
    focusable="true"
    focusableInTouchMode="true"
    layout_width="match_parent"
    layout_height="wrap_content">
    <id.dana.cardbinding.view.CardView
        id="@+id/cardView"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="16dp"
        layout_marginStart="16dp"
        layout_marginEnd="16dp"/>
    <id.dana.cashier.view.InputCardNumberView
        id="@+id/inputCardNumberView"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="22dp"
        layout_marginStart="16dp"
        layout_marginEnd="16dp"/>
    <include
        id="@+id/viewOneKlikNotice"
        visibility="gone"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="16dp"
        layout_marginBottom="16dp"
        layout_marginVertical="@dimen/BuiltInFictitiousFunctionClassFactory_res_0x7f070055"
        layout="@layout/view_oneklik_bca"/>
    <androidx.recyclerview.widget.RecyclerView
        id="@+id/res_0x7f0a112b_protoserializer_externalsyntheticlambda1"
        clipToPadding="false"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        layout_marginBottom="2dp"
        overScrollMode="never"
        layout_marginStart="16dp"
        layout_marginEnd="16dp"/>
</LinearLayout>
