<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    paddingLeft="16dp"
    paddingTop="12dp"
    paddingRight="16dp"
    paddingBottom="12dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    paddingHorizontal="16dp"
    paddingVertical="12dp">
    <com.google.android.material.imageview.ShapeableImageView
        id="@+id/civProfileAvatar"
        layout_width="40dp"
        layout_height="40dp"
        src="@drawable/ic_avatar_grey_default"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x19="0"
        NOT_FOUND_0x20="@style/2132018200"
        NOT_FOUND_0x21="@color/2131099990"
        NOT_FOUND_0x22="1dp"/>
    <TextView
        id="@+id/tvName"
        layout_width="0dp"
        layout_height="wrap_content"
        fontFamily="@font/sf_pro_regular"
        layout_marginStart="8dp"
        NOT_FOUND_0x14="@+id/btnApprove"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x16="@+id/civProfileAvatar"
        NOT_FOUND_0x19="0"
        style="@style/2132017405"/>
    <TextView
        id="@+id/tvDate"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="6dp"
        fontFamily="@font/sf_pro_regular"
        layout_marginStart="8dp"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x16="@+id/civProfileAvatar"
        NOT_FOUND_0x18="@+id/tvName"
        style="@style/2132017440"/>
    <id.dana.component.buttoncomponent.DanaButtonPrimaryView
        id="@+id/btnApprove"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginStart="12dp"
        NOT_FOUND_0x16="@+id/tvName"
        NOT_FOUND_0x19="0"
        NOT_FOUND_0x1a="10"
        NOT_FOUND_0x1b="@string/btn_friend_request_approve"
        NOT_FOUND_0x1c="1"/>
    <id.dana.component.buttoncomponent.DanaButtonSecondaryView
        id="@+id/btnDecline"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginStart="4dp"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x16="@+id/btnApprove"
        NOT_FOUND_0x19="0"
        NOT_FOUND_0x1d="10"
        NOT_FOUND_0x1e="@string/btn_friend_request_decline"
        NOT_FOUND_0x1f="1"/>
    <com.google.android.material.progressindicator.CircularProgressIndicator
        layout_gravity="center"
        id="@+id/lpvLoading"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        indeterminate="true"
        NOT_FOUND_0x11="@color/2131099990"
        NOT_FOUND_0x12="16dp"
        NOT_FOUND_0x13="@+id/btnDecline"
        NOT_FOUND_0x15="0.5"
        NOT_FOUND_0x17="@+id/btnApprove"
        NOT_FOUND_0x19="0"/>
    <androidx.constraintlayout.widget.Group
        id="@+id/groupButtons"
        layout_width="wrap_content"
        layout_height="wrap_content"
        NOT_FOUND_0x10="btnApprove, btnDecline"/>
</androidx.constraintlayout.widget.ConstraintLayout>
